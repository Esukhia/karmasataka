msgid "རྒྱུན་ དུ་ ཞུགས་པ␣འི་ འབྲས་བུ་ མངོན་སུམ་ དུ་ བྱས་ ཏེ །"
msgstr "ils manifestèrent le résultat de l'entrée dans le courant, puis"

msgctxt "line 120, 01.txt"
msgid "དེ␣ས་ བདེན་པ་ མཐོང་ ནས་"
msgstr "le père de famille (lit. il) vit les vérités, et"

msgctxt "line 121, 01.txt"
msgid "སྐྱབས་ སུ་ འགྲོ་བ་ དང་ བསླབ་པ␣འི་ གཞི་ རྣམས་ བླངས་ སོ ། །"
msgstr "prit refuge et [prit] les vœux (lit. bases de l'entraînement)."

msgctxt "line 122, 01.txt"
msgid ""
"དེ་ནས་ ཚེ་ དང་ ལྡན་པ་ ཤཱ་རི␣འི་ བུས་ ཁྱིམ་བདག་ དེ་ ཆོས་ ཀྱི་ གཏམ་ གྱིས་ "
"ཡང་དག་པ␣ར་ བསྟན །"
msgstr ""
"Ensuite, l'honorable Śāriputra instruisit parfaitement le père de famille "
"par un discours sur le Dharma,"

# ཡང་དག་པར་འཛིན་དུ་བཅུག<«གཡུང་»«པེ་»+པ།> །
msgctxt "line 123, 01.txt"
msgid "ཡང་དག་པ␣ར་ འཛིན་ དུ་ བཅུག  །"
msgstr "il le lui fit assimiler en entier,"

msgctxt "line 124, 01.txt"
msgid "ཡང་དག་པ␣ར་ གཟེངས་བསྟོད །"
msgstr "l'encouragea grandement,"

# ཡང་དག་པ་<«གཡུང་»«ལི་»«པེ་»«སྣར་»«ཅོ་»«ཞོལ་»པར།>དགའ་བར་བྱས་ནས་
msgctxt "line 125, 01.txt"
msgid "ཡང་དག་པ་ དགའ་བ␣ར་ བྱས་ ནས་"
msgstr "le réjouit entièrement. Puis,"

# སྟན་<«གཡུང་»«པེ་»བསྟན།>ལས་ལངས་ཏེ་
msgctxt "line 126, 01.txt"
msgid "སྟན་ ལ␣ས་ ལངས་ ཏེ་"
msgstr "il se leva de son siège et"

msgctxt "line 127, 01.txt"
msgid "སོང་ ངོ་ ། །"
msgstr "s'en alla."

msgctxt "line 128, 01.txt"
msgid "ཁྱིམ་བདག་ དེ་ཡང་ བདེན་པ་ མཐོང་བ␣ར་ གྱུར་ ནས །"
msgstr "Ce père de famille, après avoir vu les vérités,"

msgctxt "line 129, 01.txt"
msgid "སྦྱིན་པ་ དག་ སྦྱིན་ ཞིང་"
msgstr "il pratiqua la générosité et"

# བསོད་ནམས་དག་<«གཡུང་»–དག>བྱེད་དོ། །
msgctxt "line 130, 01.txt"
msgid "བསོད་ནམས་ དག་བྱེད་ དོ ། །"
msgstr "accumula des mérites."

# དེའི་ཁྱིམ་དེ་སློང་བའི་སྐྱེ་བོ་རྣམས་ཀྱི་ཆུ་དོང་དང་<«གཡུང་»«པེ་»–དང་།>འདྲ་བར་གྱུར་ཏོ།
# །
msgctxt "line 131, 01.txt"
msgid ""
"དེ␣འི་ ཁྱིམ་ དེ་ སློང་བ␣འི་ སྐྱེ་བོ་ རྣམས་ ཀྱི་ ཆུ་དོང་ དང་ འདྲ་བ␣ར་ གྱུར་ "
"ཏོ ། །"
msgstr "Sa maison devint comme un puits pour les personnes qui mendiaient."

msgctxt "line 132, 01.txt"
msgid ""
"ཁྱིམ་བདག་ དེ␣ས་ ཀྱང་ །  ཚེ་ དང་ ལྡན་པ་ ཤཱ་རི␣འི་ བུ་ ཡང་ དང་ ཡང་ དུ་ ཁྱིམ་ "
"གྱི་ ནང་ དུ་ བཤོས་ གསོལ་བ␣ར་ བྱེད་ དོ ། །"
msgstr ""
"Ce père de famille invitait régulièrement (lit. encore et encore) "
"l'honorable Śāriputra à prendre son repas chez lui (lit. chez ce père de "
"famille)."

msgctxt "line 133, 01.txt"
msgid ""
"དེ་ནས་ ཚེ་ དང་ ལྡན་པ་ ཤཱ་རི␣འི་ བུ་ དང་ ཁྱིམ་བདག་ དེ␣ས་ ཁྱི་ མོ་ དེ་ ལ་ཁ་ "
"ཟས་ བྱིན་ ནས་"
msgstr ""
"Alors, l'honorable Śāriputra et ce père de famille donnaient de la "
"nourriture à la chienne et"

msgctxt "line 134, 01.txt"
msgid "ཆོས་ མཉན་པ␣འི་ ཕྱིར་ མདུན་ དུ་ འདུག་ གོ ། །"
msgstr "[le père de famille] s'asseyait pour écouter l'enseignement."

msgctxt "line 135, 01.txt"
msgid "ཁྱི་ མོ་ དེ་ཡང་ ཚེ་ དང་ ལྡན་པ་ ཤཱ་རི␣འི་ བུ␣འི་ མདུན་ དུ་ འདུག་ ནས་"
msgstr "La chienne s'asseyait aussi devant l'honorable Śāriputra et"

msgctxt "line 136, 01.txt"
msgid "ཆོས་ ཉན་ ཏོ ། །"
msgstr "écoutait le Dharma."

msgctxt "line 137, 01.txt"
msgid "དེ་ནས་ ཁྱིམ་བདག་ དེ␣ས་ བསམས་པ་"
msgstr "Alors, le père de famille pensa : «"

# བདག་གིས་ལེགས་<«གཡུང་»«པེ་»ལགས།>པ་འདི་ལྟ་བུ་རྟོགས་པ་<«ཅོ་»–པ།>ཅི་ཡང་<«གཡུང་»«ལི་»«པེ་»«ཅོ་»ཅིའང་།>རུང་བ་དེ་དག་ཐམས་ཅད་ནི་ཁྱི་མོ་འདིའི་<«གཡུང་»«ལི་»«པེ་»«སྣར་»«ཅོ་»«ཞོལ་»མོའི།>མཐུ་ལས་བྱུང་བ་ཡིན་ནོ།
# །སྙམ་དུ་
msgctxt "line 138, 01.txt"
msgid ""
"བདག་ གིས་ ལེགས་པ་ འདི་ ལྟ་བུ་ རྟོགས་པ་ ཅི་ཡང་ རུང་བ་ དེ་དག་ ཐམས་ཅད་ ནི་ ཁྱི་"
" མོ་ འདི␣འི་ མཐུ་ ལ␣ས་ བྱུང་བ་ ཡིན་ ནོ ། ། སྙམ་ དུ་"
msgstr ""
"Toutes les [choses] merveilleuses comme [celles-ci] que j'ai comprises, "
"autant qu'elles sont, elles me sont arrivées (lit. proviennent) grâce à "
"cette chienne. »"

msgctxt "line 139, 01.txt"
msgid "རིག་ གོ ། །"
msgstr "vit-il."

msgctxt "line 140, 01.txt"
msgid "རིག་ ནས་ ཀྱང་ དེ་ ལ་ ཤིན་ཏུ་ སྐྱོང་བ␣ར་ བྱེད་ དོ ། །"
msgstr "l'ayant vu, il s'occupa beaucoup d'elle."

# དེ་ནས་ཕྱི་ཞིག་ན་<«གཡུང་»«པེ་»ནས།>ཁྱི་མོ་དེ་ནད་ཀྱིས་ཐེབས་ནས།
msgctxt "line 141, 01.txt"
msgid "དེ་ནས་ ཕྱི་ ཞིག་ ན་ ཁྱི་ མོ་ དེ་ ནད་ ཀྱིས་ ཐེབས་ ནས །"
msgstr "Alors, après un certain temps, cette chienne tomba malade et"

# ཤཱ་རིའི་བུས་དེ་ལ་སྨྲས་པ<«གཡུང་»མ།>།
msgctxt "line 142, 01.txt"
msgid "ཤཱ་རི␣འི་ བུས་ དེ་ ལ་ སྨྲས་པ །"
msgstr "Śāriputra lui dit : «"

msgctxt "line 143, 01.txt"
msgid "བུ་"
msgstr "Enfant,"

msgctxt "line 144, 01.txt"
msgid "འདི་ལྟར་"
msgstr "Eh bien (lit. comme ceci)"

msgctxt "line 145, 01.txt"
msgid "འདུ་བྱེད་ ཐམས་ཅད་ ནི་ མི་རྟག་པ␣འོ ། །"
msgstr "Tous les [phénomènes] composés sont impermanents."

msgctxt "line 146, 01.txt"
msgid "འདུ་བྱེད་ ཐམས་ཅད་ ནི་ སྡུག་བསྔལ་བ␣འོ ། །"
msgstr "Tous les [phénomènes] composés apportent (lit. sont) la souffrance."

msgctxt "line 147, 01.txt"
msgid "ཆོས་ ཐམས་ཅད་ ནི་ བདག་མེད་པ␣འོ ། །"
msgstr ""
"Tous les [phénomènes] composés n'ont pas d'existence réelle (lit. sont le "
"non-moi)."

msgctxt "line 148, 01.txt"
msgid "མྱ་ངན་ ལ␣ས་ འདས་པ་ ནི་ ཞི་བ␣འོ ། །"
msgstr "L'au-delà de la souffrance est l'apaisement."

# ང་ལ་སེམས་དགའ་བར་<«ཅོ་»བ།>གྱིས་ཤིག་དང་།
msgctxt "line 149, 01.txt"
msgid "ང་ ལ་ སེམས་ དགའ་བ␣ར་ གྱིས་ ཤིག་ དང་ །"
msgstr ""
"Emplis ton cœur (lit. esprit) de joie à ma pensée (lit. aies de la joie pour"
" moi)."

msgctxt "line 150, 01.txt"
msgid "དུད་འགྲོ␣འི་ སྐྱེ་གནས་ དང་ འབྲལ་བ␣ར་ འགྱུར་ ཡང་སྲིད་ དོ་ ཞེས་"
msgstr ""
"il se pourrait que tu sois débarassée (lit. séparée) des naissances "
"animales. »"

msgctxt "line 151, 01.txt"
msgid "སྨྲ␣ས་ ནས །"
msgstr "dit-il. Puis,"

msgctxt "line 152, 01.txt"
msgid "ཚེ་ དང་ ལྡན་པ་ ཤཱ་རི␣འི་ བུ་ སོང་ ངོ་ ། །"
msgstr "l'honorable Śāriputra s'en alla."

# དེ་<«གཡུང་»«པེ་»+ན།>སོང་ནས་རིང་པོ་མ་ལོན་པར་ཁྱི་མོ་དེ་ཡང་ཚེ་དང་ལྡན་པ་ཤཱ་རིའི་བུ་ལ་སེམས་དགའ་བར་གྱུར་ནས་ཤིའོ།
# །
msgctxt "line 153, 01.txt"
msgid ""
"དེ་ སོང་ ནས་ རིང་པོ་ མ་ ལོན་པ␣ར་ ཁྱི་ མོ་ དེ་ཡང་ ཚེ་ དང་ ལྡན་པ་ ཤཱ་རི␣འི་ "
"བུ་ ལ་ སེམས་ དགའ་བ␣ར་ གྱུར་ ནས་ ཤི␣འོ ། །"
msgstr ""
"Peu de temps après son départ, la chienne mourut le cœur (lit. esprit) plein"
" de joie à la pensée de l'honorable Śāriputra."

msgctxt "line 154, 01.txt"
msgid "དེ་ ཤི་འཕོས་ ཏེ་"
msgstr "Elle mourut et"

msgctxt "line 155, 01.txt"
msgid "འདས་ ནས་"
msgstr "passa au-delà [de cette vie], puis"

msgctxt "line 156, 01.txt"
msgid ""
"ཁྱིམ་ དེ་ཉིད་ དུ་ ཁྱིམ་བདག་ དེ␣འི་ ཆུང་མ་ ཆེན་མ␣འི་ མངལ་ དུ་ སྐྱེས་ སོ ། །"
msgstr ""
"dans cette même maison, elle renaquit dans le sein de l'épouse principale de"
" ce père de famille."

msgctxt "line 157, 01.txt"
msgid ""
"དེ་ ཤི་བ␣འི་ ཚེ་ཚེ་ དང་ ལྡན་པ་ ཤཱ་རི␣འི་ བུ་ སླར་ ཁྱིམ་ དེ␣ར་ འོངས་ ནས །"
msgstr ""
"Après la mort de la chienne (lit. sa mort), l'honorable Śāriputra était "
"revenu à cette maison et"

# ཁྱིམ་བདག་གིས་ཚེ་དང་ལྡན་པ་ཤཱ་རིའི་བུ་ལ་བསྙད་<«གཡུང་»བརྙད།>དེ།
msgctxt "line 158, 01.txt"
msgid "ཁྱིམ་བདག་ གིས་ ཚེ་ དང་ ལྡན་པ་ ཤཱ་རི␣འི་ བུ་ ལ་ བསྙད་ དེ །"
msgstr ""
"le père de famille avait raconté [la mort de la chienne] à l'honorable "
"Śāriputra : «"

# བཙུན་པ་ཤཱ་<«གཡུང་»«པེ་»ཤ>རིའི་བུ་
msgctxt "line 159, 01.txt"
msgid "བཙུན་པ་ ཤཱ་རི␣འི་ བུ་"
msgstr "Vénérable Śāriputra,"

msgctxt "line 160, 01.txt"
msgid "ཁྱི་ མོ་ དེ་ གུམ་ ལགས་སོ་ ཞེས་"
msgstr "cette chienne est morte »"

msgctxt "line 161, 01.txt"
msgid "བྱས་ སོ ། །"
msgstr "dit-il."

msgctxt "line 162, 01.txt"
msgid "ཚེ་ དང་ ལྡན་པ་ ཤཱ་རི␣འི་ བུས་ སྨྲས་པ །"
msgstr "L'honorable Śāriputra dit : «"

msgctxt "line 163, 01.txt"
msgid "ཁྱི་ མོ་ དེ␣འི་ རོ་ དེ་ ཕྱོགས་ དབེན་པ་ ཞིག་ ཏུ་ ཞོག་ ཤིག་ དང་ །"
msgstr "Dépose le cadavre de cette chienne dans un endroit isolé."

msgctxt "line 164, 01.txt"
msgid "རུས་པ་ དེ་དག་ ཕྱིས་ དེ་ཉིད་ ལ་ ཕན་ འདོགས་པ␣ར་ འགྱུར་ རོ ། །"
msgstr "Plus tard, ces ossements lui seront utiles. »"

msgctxt "line 165, 01.txt"
msgid "ཁྱིམ་བདག་ གིས་ སྨྲས་པ །"
msgstr "Le père de famille dit : «"

# བཙུན་པ་ཤཱ་རིའི་བུས་ཇི་སྐད་བཀའ་སྩལ་པ་བཞིན་དུ་<«གཡུང་»«ལི་»«པེ་»«སྣར་»«ཅོ་»«ཞོལ»–དུ།>བགྱིའོ་ཞེས་
msgctxt "line 166, 01.txt"
msgid "བཙུན་པ་ ཤཱ་རི␣འི་ བུས་ ཇི་སྐད་ བཀའ་ སྩལ་པ་ བཞིན་ དུ་ བགྱི␣འོ་ ཞེས་"
msgstr "Je ferai comme le vénérable Śāriputra l'a demandé. »"

msgctxt "line 167, 01.txt"
msgid "སྨྲ␣ས་ ནས །"
msgstr "dit-il. Puis,"

msgctxt "line 168, 01.txt"
msgid ""
"ཁྱིམ་བདག་ དེ␣ས་ ཁྱི་རོ་ དེ་ གང་ དུ་ སུ␣ས་ ཀྱང་ མི་ མཐོང་བ␣ར་ འགྱུར་བ་ དེ་ལྟ་"
" བུ␣འི་ ཕྱོགས་ ཤིག་ ཏུ་ བཞག་ གོ ། །"
msgstr ""
"Ce père de famille déposa le cadavre de chienne à un endroit où personne ne "
"le verrait."

# དེ་ནས་ཁྱིམ་བདག་གི་ཆུང་མ་དེ་ཟླ་བ་དགུའམ་བཅུ་ལོན་པ་དང་བུ་བྱུང་སྟེ<«ཁུ་»–བུ་བྱུང་སྟེ།>།
msgctxt "line 169, 01.txt"
msgid ""
"དེ་ནས་ ཁྱིམ་བདག་ གི་ ཆུང་མ་ དེ་ ཟླ་བ་ དགུ␣འམ་ བཅུ་ ལོན་པ་ དང་ བུ་ བྱུང་ སྟེ "
"།"
msgstr ""
"Ensuite, l'épouse de ce père de famille accoucha après que neuf ou dix mois "
"soient passés. Ainsi,"

# བུ་མོ་གཟུགས་བཟང་ཞིང་བལྟ་<«ལི་»«སྣར་»«ཅོ་»ལྟ།>ན་སྡུག་ལ་མཛེས་པ་<«གཡུང་»«པེ་»ལ།>མདོག་བཟང་བ་གསེར་གྱི་ཁ་དོག་ཅན་མགོ་བོ་གདུགས་ལྟར་འདུག་པ།
# ལག་པ་རིང་བ་དཔྲལ་བའི་དབྱེས་ཆེ་བ་སྣ་མཐོ་བ་སྣ་ལེགས་པ་སྨིན་མ་སྟུག་པ་ཞིག་བཙས་སོ།
# །
msgctxt "line 170, 01.txt"
msgid ""
"བུ་མོ་ གཟུགས་བཟང་ ཞིང་ བལྟ་ ན་ སྡུག་ ལ་ མཛེས་པ་ མདོག་ བཟང་བ་ གསེར་ གྱི་ "
"ཁ་དོག་ ཅན་ མགོ་བོ་ གདུགས་ ལྟར་ འདུག་པ །  ལག་པ་ རིང་བ་ དཔྲལ་བ␣འི་ དབྱེས་ "
"ཆེ་བ་ སྣ་ མཐོ་བ་ སྣ་ ལེགས་པ་ སྨིན་མ་ སྟུག་པ་ ཞིག་ བཙས་ སོ ། །"
msgstr ""
"naquit une fille au corps harmonieux (lit. noble), qui était belle et "
"agréable au regard, dont la teinte noble [était] la couleur de l'or, dont la"
" tête [se] tenait comme une ombrelle, dont les mains étaient longues, dont "
"la largeur (lit. l'étendue) du front était grande, dont le nez était haut, "
"dont le nez était bien [fait] et dont les sourcils étaient denses."

# དེ་ནས་དེའི་བཙས་སྟོན་ཡང་རྒྱས་པར་<«གཡུང་»+བྱས་སོ།
# །དེ་ནས་དེའི་ཙས་སྟོན་ཡང་རྒྱས་པར། «པེ་»+བྱས་སོ།
# །དེ་ནས་དེའི་བཙས་སྟོན་ཡང་རྒྱས་པར།>བྱས་ནས།
msgctxt "line 171, 01.txt"
msgid "དེ་ནས་ དེ␣འི་ བཙས་ སྟོན་ ཡང་ རྒྱས་པ␣ར་ བྱས་ ནས །"
msgstr ""
"Ensuite, la fête de sa naissance fut faite [de manière] grandiose (lit. "
"vastement) et"

msgctxt "line 172, 01.txt"
msgid "རིགས་ དང་ འཐུན་པ␣འི་ མིང་ བཏགས་ ཏེ །"
msgstr "on lui donna un nom en accord avec sa caste. Puis,"

# འོ་མ་དང་མར་དང་ཞོ་དང་ཞུན་མར་དང་མར་གྱི་ཉིང་གུས་<«གཡུང་»«ལི་»«པེ་»«སྣར་»སྙིང་གུས།
# «ཅོ་»«ཞོལ་»སྙིང་ཁུས།>བསྐྱེད་བསྲིངས་ཏེ།
msgctxt "line 173, 01.txt"
msgid ""
"འོ་མ་ དང་ མར་ དང་ ཞོ་ དང་ ཞུན་མར་ དང་ མར་ གྱི་ ཉིང་ གུས་ བསྐྱེད་ བསྲིངས་ ཏེ "
"།"
msgstr ""
"elle se développa au fil du temps grâce à du lait, du yaourt, du beurre, du "
"beurre purifié (lit. fondu) et du beurre sur-purifié (lit. l'essence du "
"beurre). Ainsi,"

msgctxt "line 174, 01.txt"
msgid "མཚོ␣འི་ ནང་ གི་ པད་མ་ བཞིན་ དུ་ སྐྱེ་ དྲག་པ␣ར་ གྱུར་ ཏོ ། །"
msgstr "comme un lotus dans un lac, elle grandit avec force."

# དེ་ནས་དེ་གང་<«གཡུང་»«ལི་»«པེ་»«སྣར་»«ཅོ་»དག>གི་ཚེ་ཆེན་མོར་གྱུར་པ་དེའི་ཚེ་ཚེ་དང་ལྡན་པ་ཤཱ་རིའི་བུས་སྨྲས་པ།
msgctxt "line 175, 01.txt"
msgid ""
"དེ་ནས་ དེ་ གང་ གི་ ཚེ་ ཆེན་ མོ␣ར་ གྱུར་པ་ དེ␣འི་ ཚེ་ཚེ་ དང་ ལྡན་པ་ ཤཱ་རི␣འི་"
" བུས་ སྨྲས་པ །"
msgstr "Ensuite, lorsqu'elle eut grandi, l'honorable Śāriputra lui dit : «"

msgctxt "line 176, 01.txt"
msgid "བུ་"
msgstr "Enfant,"

msgctxt "line 177, 01.txt"
msgid "འདུག་ ལ་"
msgstr "assieds-toi et"

msgctxt "line 178, 01.txt"
msgid "ཆོས་ ཉོན་ ཅིག་ ཅེས་"
msgstr "écoute le Dharma. »"

# བྱས་ན<«གཡུང་»«པེ་»ནས།>།
msgctxt "line 179, 01.txt"
msgid "བྱས་ ན །"
msgstr "dit-il. À ce moment,"

# དེ་བྱད་<«གཡུང་»«པེ་»བྱེད།>གཟུགས་དང་ལང་ཚོའི་<«གཡུང་»ཙོའི།>རྒྱགས་པས་དྲེགས་ནས་
msgctxt "line 180, 01.txt"
msgid "དེ་ བྱད་གཟུགས་ དང་ ལང་ཚོ␣འི་ རྒྱགས་པ␣ས་ དྲེགས་ ནས་"
msgstr "Enorgueillie, [très] imbue de sa beauté et de sa jeunesse,"

msgctxt "line 181, 01.txt"
msgid "ཕན་ཚུན་ དུ་ གཡེངས་ ཏེ་"
msgstr "elle se distrayait ici et là et"

msgctxt "line 182, 01.txt"
msgid "ཆོས་ ཉན་ དུ་ མི་ བཏུབ་བོ ། །"
msgstr "ne parvenait pas à écouter le Dharma."

msgctxt "line 183, 01.txt"
msgid "དེ་ནས་ ཕྱིས་ ཚེ་ དང་ ལྡན་པ་ ཤཱ་རི␣འི་ བུས་ དེ་ ཤེད་ བྱེ་ བར་ རིག་ ནས །"
msgstr ""
"Ensuite, plus tard, l'honorable Śāriputra pensa lui ouvrir les yeux (lit. le"
" visage) et"

msgctxt "line 184, 01.txt"
msgid "ཁྱི་ མོ␣འི་ རུས་བུ་ དེ་དེ་ ཉིད་ ཀྱི་ མདུན་ དུ་ བཞག་ སྟེ །"
msgstr "posa les ossements de cette chienne devant elle et"

msgctxt "line 185, 01.txt"
msgid ""
"ཅི་ ནས་ ཀྱང་ བུ་མོ་ དེ␣ས་ ཚེ་རབས་ དྲན་པ␣ར་ འགྱུར་བ་ དེ་ལྟ་ བུར་ བྱིན་ གྱིས་ "
"བརླབས་ ནས །"
msgstr ""
"la bénit pour qu'immanquablement, cette fille se souvienne de la suite de "
"ses vies, Alors,"

# ཚེ་རབས་དྲན་མ་ཐག་ཏུ་བུ་མོ་དེ་<«ཁུ་»དེས།>ཤས་ཆེར་སྐྱོ་བ་སྐྱེས་སོ། །
msgctxt "line 186, 01.txt"
msgid "ཚེ་རབས་ དྲན་ མ་ ཐག་ ཏུ་ བུ་མོ་ དེ་ ཤས་ ཆེ␣ར་ སྐྱོ་བ་ སྐྱེས་ སོ ། །"
msgstr ""
"dès qu'elle se souvint de la suite de ses vies, elle ressentit (lit. faire "
"naître) un désenchantement intense."

msgctxt "line 187, 01.txt"
msgid "དེ་ སྐྱོ་བ་ སྐྱེས་ ནས་"
msgstr "Ayant ressenti (lit. fait naître) le désenchantement,"

msgctxt "line 188, 01.txt"
msgid "བསམས་པ །"
msgstr "elle pensa : «"

msgctxt "line 189, 01.txt"
msgid ""
"འཕགས་པ་ ཤཱ་རི␣འི་ བུ་ ནི །  བདག་ ལ་ དཀའ་བ་ བྱེད་ བྱེད་པ་ ཡིན་པ་ ལྟ་ ཞིག་ སྟེ"
" །"
msgstr ""
"Regarde toutes les difficultés que le sublime Śāriputra a endurées pour moi."

# འདི་ལས་<«ཞོལ་»ལ།>བརྟེན་ནས།
msgctxt "line 190, 01.txt"
msgid "འདི་ ལ␣ས་ བརྟེན་ ནས །"
msgstr "Grâce à lui,"

msgctxt "line 191, 01.txt"
msgid "བདག་ གིས་ དུད་འགྲོ␣འི་ སྐྱེ་གནས་ སྤངས་ སོ་ སྙམ་ དུ་"
msgstr "je me suis débarrassée des naissances animales. »"

msgctxt "line 192, 01.txt"
msgid "རིག་ ནས །"
msgstr "vit-elle. Puis,"

msgctxt "line 193, 01.txt"
msgid "དགའ་བ་ སྐྱེས་ ཏེ་"
msgstr "[ceci] la combla (lit. faire naître) de joie et"

msgctxt "line 194, 01.txt"
msgid ""
"ཆོས་ མཉན་པ␣འི་ ཕྱིར །  ཚེ་ དང་ ལྡན་པ་ ཤཱ་རི␣འི་ བུ␣འི་ མདུན་ དུ་ འདུག་ གོ ། "
"།"
msgstr "elle s'assit devant l'honorable Śāriputra pour écouter le Dharma."

msgctxt "line 195, 01.txt"
msgid ""
"དེ་ནས་ ཚེ་ དང་ ལྡན་པ་ ཤཱ་རི␣འི་ བུས་ དེ␣འི་ བསམ་པ་ དང་ བག་ ལ་ ཉལ་ དང་ །  "
"ཁམས་ དང་ རང་བཞིན་ རྟོགས་ ནས་"
msgstr ""
"Alors, l'honorable Śāriputra discerna ses pensées, ses tendances "
"habituelles, son tempérament (lit. élément) et son caractère (lit. la "
"nature) et"

msgctxt "line 196, 01.txt"
msgid "དེ་ དང་ འཐུན་པ␣འི་ ཆོས་ བསྟན་ ཏེ །"
msgstr ""
"il leur prodigua un enseignement lui (lit. à ceci) correspondant. Alors,"

msgctxt "line 197, 01.txt"
msgid "དེ་ ཐོས་ ནས་"
msgstr "l'ayant écouté,"

# བུ་མོ་དེས་སྟན་དེ་<«ལི་»«སྣར་»«ཅོ་»ཏེ།>ཁོ་ན་ལ་འདུག་བཞིན་དུ་འཇིག་ཚོགས་ལ་ལྟ་བའི་རིའི་<«གཡུང་»–རིའི།>རྩེ་མོ་མཐོན་པོ་ཉི་ཤུ་ཡེ་ཤེས་རྡོ་རྗེས་བཅོམ་ནས།
msgctxt "line 198, 01.txt"
msgid ""
"བུ་མོ་ དེ␣ས་ སྟན་ དེ་ ཁོ་ན་ ལ་ འདུག་ བཞིན་ དུ་ འཇིག་ཚོགས་ ལ་ ལྟ་བ␣འི་ རི␣འི་"
" རྩེ་མོ་ མཐོན་པོ་ ཉི་ཤུ་ ཡེ་ཤེས་ རྡོ་རྗེ␣ས་ བཅོམ་ ནས །"
msgstr ""
"cette fille détruisit les vingt cimes élevées des vues concernant l'amas "
"périssable à l'aide du diamant de la sagesse et"

msgctxt "line 199, 01.txt"
msgid "རྒྱུན་ དུ་ ཞུགས་པ␣འི་ འབྲས་བུ་ མངོན་སུམ་ དུ་ བྱས་ ཏེ །"
msgstr "elle manifesta le résultat de l'entrée dans le courant, puis"

# དེས་བདེན་པ་<«ཅོ་»+མ།>མཐོང་ནས་
msgctxt "line 200, 01.txt"
msgid "དེ␣ས་ བདེན་པ་ མཐོང་ ནས་"
msgstr "elle vit les vérités, et"

msgctxt "line 201, 01.txt"
msgid "སྟན་ ལ␣ས་ ལངས་ ཏེ་"
msgstr "se leva de son siège et"

msgctxt "line 202, 01.txt"
msgid "བླ་གོས་ ཕྲག་པ་ གཅིག་ ཏུ་ གཟར་ ནས །"
msgstr ""
"elle [replia] son châle sur une épaule et le laissa retomber [devant elle],"

# ཚེ་དང་ལྡན་པ་ཤཱ་རིའི་བུ་ག་ལ་བ་དེ་<«གཡུང་»«ལི་»«པེ་»«སྣར་»«ཅོ་»དེར།>ལོགས་སུ་ཐལ་མོ་སྦྱར་[4b]བ་བཏུད་ནས།
msgctxt "line 203, 01.txt"
msgid ""
"ཚེ་ དང་ ལྡན་པ་ ཤཱ་རི␣འི་ བུ་ག་ ལ་བ་ དེ་ ལོགས་ སུ་ ཐལ་མོ་ སྦྱར་བ་ བཏུད་ ནས །"
msgstr ""
"joignit les mains et s'inclina en direction de l'honorable Śāriputra et"

msgctxt "line 204, 01.txt"
msgid "ཚེ་ དང་ ལྡན་པ་ ཤཱ་རི␣འི་ བུ་ ལ་ འདི་སྐད་ ཅེས་ སྨྲ␣ས་ སོ ། །"
msgstr "Elle dit ceci à l'honorable Śāriputra : «"

msgctxt "line 205, 01.txt"
msgid "བཙུན་པ་ ཤཱ་རི␣འི་ བུ་"
msgstr "Vénérable Śāriputra,"

msgctxt "line 206, 01.txt"
msgid ""
"བདག་ ལེགས་པ␣ར་ གསུངས་པ␣འི་ ཆོས་ འདུལ་བ་ ལ་ རབ་ཏུ་ འབྱུང་བ་ དང་ བསྙེན་པ␣ར་ "
"རྫོགས་ ཤིང་ དགེ་སློང་ མ␣འི་ དངོས་པོ་ འཐོབ་ ཏུ་ རུང་ ན་"
msgstr ""
"s'il est possible de s'extraire du monde selon le Vinaya, le Dharma si bien "
"exprimé, et [s'il est possible] de parfaire l'approche [de la libération], "
"d'obtenir la condition de nonne pleinement ordonnée,"

msgctxt "line 207, 01.txt"
msgid ""
"བདག་ ཀྱང་ །  བཅོམ་ལྡན་འདས་ ཀྱི་ ཐད་ དུ་ ཚངས་པ␣ར་ སྤྱོད་པ་ སྤྱད་པ␣ར་ འཚལ་ ལོ "
"། །"
msgstr ""
"moi aussi, j'aimerais vivre une vie chaste (lit. suivre le brahmacarya) "
"auprès du Bienheureux. »"

# དེ་ནས་ཚེ་དང་ལྡན་པ་<«གཡུང་»«པེ་»–པ།>ཤཱ་རིའི་<«གཡུང་»རཱིའི།>བུས་དེའི་ཕ་མ་ལ་དད་པ་སྨྲས་ནས།
msgctxt "line 208, 01.txt"
msgid "དེ་ནས་ ཚེ་ དང་ ལྡན་པ་ ཤཱ་རི␣འི་ བུས་ དེ␣འི་ ཕ་མ་ ལ་ དད་པ་ སྨྲ␣ས་ ནས །"
msgstr ""
"Ensuite, l'honorable Śāriputra fit part de ses aspirations à ses parents, "
"puis"

# སྐྱེ་དགུའི་<«ཞོལ་»རྒུའི།>བདག་མོ་ཆེན་མོ་ལ་གཏད་དེ་
msgctxt "line 209, 01.txt"
msgid "སྐྱེ་དགུ␣འི་ བདག་མོ་ ཆེན་ མོ་ལ་ གཏད་ དེ་"
msgstr "la remit à Mahā­prajāpatī, puis"

# སྐྱེ་དགུའི་<«ཞོལ་»རྒུའི།>བདག་མོ་ཆེན་མོས་<«ཁུ་»མོ།>དེ་རབ་ཏུ་ཕྱུང་ནས་
msgctxt "line 210, 01.txt"
msgid "སྐྱེ་དགུ␣འི་ བདག་མོ་ ཆེན་ མོས་ དེ་ རབ་ཏུ་ ཕྱུང་ ནས་"
msgstr "Mahā­prajāpatī lui fit renoncer au monde, puis"

msgctxt "line 211, 01.txt"
msgid "བསྙེན་པ␣ར་ རྫོགས་པ␣ར་ བྱས་ ཏེ །"
msgstr "lui donna l'ordination complète et"

msgctxt "line 212, 01.txt"
msgid "དེ་ ལ་ལུང་ ཡང་ ཕོག་ གོ ། །"
msgstr "lui [accorda] la transmission orale."

# དེས་ཀྱང་བརྩོན་པ་དང་བསྒྲུབ་<«ཞོལ»སྒྲུབ།>པ་དང་འབད་པས་ཉོན་མོངས་པ་ཐམས་ཅད་སྤངས་ནས།
msgctxt "line 213, 01.txt"
msgid ""
"དེ␣ས་ ཀྱང་ བརྩོན་པ་ དང་ བསྒྲུབ་པ་ དང་ འབད་པ␣ས་ ཉོན་མོངས་པ་ ཐམས་ཅད་ སྤངས་ ནས "
"།"
msgstr ""
"De son côté, elle s'efforça, s'appliqua et s'évertua à éliminer toutes les "
"émotions perturbatrices et"

msgctxt "line 214, 01.txt"
msgid "དགྲ་བཅོམ་པ་ ཉིད་ མངོན་སུམ་ དུ་ བྱས་ ཏེ །"
msgstr "ils manifestèrent l'état d'arhat."

msgctxt "line 215, 01.txt"
msgid "དེ་ དགྲ་བཅོམ་པ་ ཁམས་ གསུམ་པ␣འི་ འདོད་ཆགས་ དང་ བྲལ་བ␣ར་ གྱུར་ ནས །"
msgstr "elle devint une arhat libre de l'attachement aux trois mondes. Ainsi,"

# གསེར་དང་བོང་རྡོར་<«གཡུང་»«པེ་»བ།>མཉམ་པ་
msgctxt "line 216, 01.txt"
msgid "གསེར་ དང་ བོང་ རྡོར་ མཉམ་པ་"
msgstr ""
"[un morceau] d'or et une motte de terre étaient identiques [à ses yeux] ;"

msgctxt "line 217, 01.txt"
msgid "ནམ་མཁའ་ དང་ ལག་མཐིལ་ དུ་ འདྲ་བ␣འི་ སེམས་ དང་ ལྡན་པ་"
msgstr ""
"elle possédait un esprit [pour lequel] l'espace et la paume de la main sont "
"similaires ;"

msgctxt "line 218, 01.txt"
msgid "ཙན་དན་ སྦངས་པ་ ལྟ་བུ␣ར་ བསིལ་བ␣ར་ གྱུར་པ་"
msgstr "elle [possédait] la fraîcheur du [bois de] santal trempé ;"

# རིག་པས་མ་རིག་པའི་<«གཡུང་»«ལི་»«པེ་»«སྣར་»«ཅོ་»–མ་རིག་པའི།>སྒོ་ངའི་སྦུབས་བཅོམ་པ་
msgctxt "line 219, 01.txt"
msgid "རིག་པ␣ས་ མ་རིག་པ␣འི་ སྒོ་ང␣འི་ སྦུབས་ བཅོམ་པ་"
msgstr "sa sagesse avait brisé la coquille de l'œuf ;"

msgctxt "line 220, 01.txt"
msgid "རིག་པ་ དང་ མངོན་པར་ ཤེས་པ་ དང་ །  སོ་སོ་ ཡང་དག་པ␣ར་ རིག་པ་ ཐོབ་པ་"
msgstr ""
"elle avait obtenu la connaissance, la clairvoyance et la parfaite "
"discrimination ;"

msgctxt "line 221, 01.txt"
msgid "སྲིད་པ␣འི་ རྙེད་པ་ དང་ ཆགས་པ་ དང་ བཀུར་སྟི་ ལ་ རྒྱབ་ ཀྱིས་ ཕྱོགས་པ་"
msgstr ""
"elle avait tourné le dos aux biens du samsara, aux désirs et aux éloges ;"

# དབང་པོ་དང་ཉེ་དབང་དང་བཅས་པའི་ལྷ་རྣམས་ཀྱིས་མཆོད་ཅིང་རྗེད་<«གཡུང་»«སྣར་»བརྗེད།
# «པེ་»བརྗོད།>པ་དང་། གུས་པར་སྨྲ་བའི་<«ཁུ་»སྨྲས་པའི།>གནས་སུ་གྱུར་ཏོ། །
msgctxt "line 222, 01.txt"
msgid ""
"དབང་པོ་ དང་ ཉེ་དབང་ དང་ བཅས་པ␣འི་ ལྷ་ རྣམས་ ཀྱིས་ མཆོད་ ཅིང་ རྗེད་པ་ དང་ །  "
"གུས་པ␣ར་ སྨྲ་བ␣འི་ གནས་ སུ་ གྱུར་ ཏོ ། །"
msgstr ""
"elle était désormais digne (lit. un objet de) des offrandes, de la "
"vénération et de la révérence (lit. paroles respectueuses) d'Indra, "
"d'Upendra et des autres dieux."

msgctxt "line 223, 01.txt"
msgid "དེ་ དགྲ་བཅོམ་པ␣ར་ གྱུར་ ནས །"
msgstr "Après être devenue une arhat,"

msgctxt "line 224, 01.txt"
msgid "ཚེ་ དང་ ལྡན་པ་ ཤཱ་རི␣འི་ བུ␣འི་ སྔོན་ གྱི་ དྲིན་ དྲན་ ཏེ །"
msgstr "elle vit se souvint de la bonté de Śāriputra et"

msgctxt "line 225, 01.txt"
msgid "ཡང་ དང་ ཡང་ དུ་ ཐད་ དུ་ འགྲོ་ ཞིང་"
msgstr "allait régulièrement auprès de lui,"

msgctxt "line 226, 01.txt"
msgid "རྐང་པ་ ལ་ ཕྱག་ འཚལ་ ནས །"
msgstr "se prosternait à ses pieds et"

msgctxt "line 227, 01.txt"
msgid "འདི་སྐད་ དུ་"
msgstr "[s'adressait à lui] en ces termes : «"

# འཕགས་པ་ཤཱ་རིའི་བུ་ལས་<«ཞོལ་»ལ།>བརྟེན་ཏེ་
msgctxt "line 228, 01.txt"
msgid "འཕགས་པ་ ཤཱ་རི␣འི་ བུ་ ལ␣ས་ བརྟེན་ ཏེ་"
msgstr "Grâce au sublime Śāriputra,"

msgctxt "line 229, 01.txt"
msgid "བདག་ གིས་ དུད་འགྲོ␣འི་ སྐྱེ་གནས་ སྤངས་ ནས །"
msgstr "je me suis débarrassée des naissances animales et"

msgctxt "line 230, 01.txt"
msgid "ཡོན་ཏན་ འདི་ ལྟ་བུ་ དག་ ཀྱང་ ཐོབ་པ␣ར་ གྱུར་པ␣ས་ ན །"
msgstr "j'ai obtenu toutes les qualités comme celles-ci. C'est pourquoi,"

msgctxt "line 231, 01.txt"
msgid "འཕགས་པ་ ནི་ བདག་ ལ་ དཀའ་བ་ མཛད་ མཛད་པ་ ལགས་སོ་ ཞེས་"
msgstr "[être] sublime, vous avez enduré tant de difficultés pour moi. »"

msgctxt "line 232, 01.txt"
msgid "ཟེར་ རོ ། །"
msgstr "disait-elle."

msgctxt "line 233, 01.txt"
msgid "དེ་ཡང་ དང་ ཡང་ དུ་ ཟེར་བ་ དགེ་སློང་ མང་པོ␣ས་ ཐོས་ ནས་"
msgstr ""
"Alors, de nombreux moines l'entendirent répéter ceci encore et encore, et"

# ཐོས་མ་ཐག་ཏུ་<«གཡུང་»«ལི་»«པེ་»དུ།>དེ་དག་གིས་ཚེ་དང་ལྡན་པ་ཤཱ་རིའི་བུ་ལ་དྲིས་པ།
msgctxt "line 234, 01.txt"
msgid "ཐོས་ མ་ ཐག་ ཏུ་ དེ་དག་ གིས་ ཚེ་ དང་ ལྡན་པ་ ཤཱ་རི␣འི་ བུ་ ལ་ དྲིས་པ །"
msgstr "après l'avoir entendu, ils demandèrent à l'honorable Śāriputra : «"

msgctxt "line 235, 01.txt"
msgid "ཚེ་ དང་ ལྡན་པ་ ཤཱ་རི␣འི་ བུ་"
msgstr "Honorable Śāriputra,"

# དགེ་སློང་མ་འདིས་ཅི་ལས་བསམས་ཏེ<«གཡུང་»བསིལ་ཏེ། «ཞོལ་»བསམ་སྟེ།>།
msgctxt "line 236, 01.txt"
msgid "དགེ་སློང་ མ་ འདི␣ས་ ཅི་ ལ␣ས་ བསམས་ ཏེ །"
msgstr "à quoi pense cette nonne quand"

# དེ་ཡང་དང་ཡང་དུ་དེ་སྐད་<«གཡུང་»«པེ་»+དེ་སྐད།>མཆི།
msgctxt "line 237, 01.txt"
msgid "དེ་ཡང་ དང་ ཡང་ དུ་ དེ་སྐད་ མཆི །"
msgstr "elle répète (lit. se trouve [à dire]) encore et encore ceci ? »"

# ཚེ་དང་ལྡན་པ་ཤཱ་<«གཡུང་»«པེ་»ཤ>རིའི་བུས་སྨྲས་པ།
msgctxt "line 238, 01.txt"
msgid "ཚེ་ དང་ ལྡན་པ་ ཤཱ་རི␣འི་ བུས་ སྨྲས་པ །"
msgstr "L'honorable Śāriputra dit : «"

msgctxt "line 239, 01.txt"
msgid ""
"ཁྱེད་ ཀྱིས་ ཁྱིམ་བདག་ འདི␣འི་ ཁྱིམ་ ན་ ཁྱི་ མོ་ ཞིག་ འདུག་པ་ མཐོང་ ངམ །"
msgstr "Avez-vous vu une chienne dans la maison de ce père de famille ? »"

msgctxt "line 240, 01.txt"
msgid "དེ་དག་ གིས་ སྨྲས་པ །"
