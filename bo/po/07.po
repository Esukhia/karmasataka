msgctxt "line 1, 07.txt"
msgid "རྒྱལ་མཚན་  ཞེས་ བྱ་བ་  ནི  ། –"
msgstr ""

msgctxt "line 2, 07.txt"
msgid "གླེང་ གཞི་  མཉན་ དུ་ཡོད་པ་  ན་  བཞུགས་  ཏེ  ། –"
msgstr ""

msgctxt "line 3, 07.txt"
msgid ""
"དེ  –␣འི་  ཚེ་  གྲོང་ཁྱེར་  བཱ་རཱ་ཎ་སཱི་  ན་  རྒྱལ་པོ་  ཚངས་པ␣ས་ བྱིན་  ཞེས་"
" བྱ་བ་  རྒྱལ་པོ་  བྱེད་  དེ  ། –"
msgstr ""

msgctxt "line 4, 07.txt"
msgid ""
"དེ  –␣འི་  རིང་  ལ་  འབྱོར་པ་  དང་  རྒྱས་པ་  དང་  བདེ་བ་  དང་  ལོ་ ལེགས་པ་  "
"དང་  ། –  སྐྱེ་བོ་  དང་  མི་  མང་པོ  –␣ས་  གང་བ་  དང་  རྩོད་པ་  དང་  འཐབ་པ་ "
" ཞི་བ་  དང་  འཁྲུག་པ་  དང་  ནང་འཁྲུག་  དང་  ཆོམ་རྐུན་  དང་  མུ་གེ་  དང་  "
"ནད་  རྣམས་  མེད་པ་  དང་  འབྲས་  སཱ་ ལུ་  དང་  བུ་རམ་ཤིང་  དང་ བ་ ལང་  དང་  "
"མ་ཧེ་  དང་  ལྡན་པ  –␣ར་  འདུག་  སྟེ  ། –"
msgstr ""

msgctxt "line 5, 07.txt"
msgid ""
"བུ་  གཅིག་པ་  སྡུག་པ་  ལ་  བྱ་བ་  བཞིན་  དུ་  ཆོས་ བཞིན་  དུ་  རྒྱལ་སྲིད་  "
"བྱེད་  དོ  ། – །"
msgstr ""

msgctxt "line 6, 07.txt"
msgid ""
"དེ་ནས་  ཕྱི་  ཞིག་  ན་  རྒྱལ་པོ་  དེ  –␣འི་  བཙུན་མོ་  ལ་  བུ་  ཆགས་  ནས་"
msgstr ""

msgctxt "line 7, 07.txt"
msgid "དེ་  ཟླ་བ་  དགུ  –␣འམ་  བཅུ་  ལོན་པ་  དང་  ། –"
msgstr ""

msgctxt "line 8, 07.txt"
msgid ""
"བུ་མོ་  གཟུགས་ བཟང་  ཞིང་  བལྟ་  ན་  སྡུག་  ལ་  མཛེས་པ་  ཞིག་  བཙས་  ནས  ། –"
msgstr ""

msgctxt "line 9, 07.txt"
msgid "དེ  –␣འི་  བཙས་ སྟོན་  རྒྱས་པ  –␣ར་  བྱས་  ཏེ  ། –"
msgstr ""

msgctxt "line 10, 07.txt"
msgid "བུ་མོ་  འདི  –␣འི་  མིང་  ཇི་སྐད་  གདགས་  ཞེས་"
msgstr ""

msgctxt "line 11, 07.txt"
msgid "མིང་ འདོགས་པ  –␣ར་  བྱེད་  དོ  ། – །"
msgstr ""

msgctxt "line 12, 07.txt"
msgid "ཀཱ་ ཤི  –␣འི་  རྒྱལ་པོ  –␣འི་  བུ་མོ་  ཡང་  ཡིན་  ལ་"
msgstr ""

msgctxt "line 13, 07.txt"
msgid "གཟུགས་  ཀྱང་  མཛེས་པ  –␣ས་  ན  ། –"
msgstr ""

msgctxt "line 14, 07.txt"
msgid "བུ་མོ་  འདི  –␣འི་  མིང་  ཀཱ་ ཤི་ མཛེས་ལྡན་ མ་  ཞེས་  གདགས་  སོ་  ཞེས་"
msgstr ""

msgctxt "line 15, 07.txt"
msgid "ཟེར་  རོ  ། – །"
msgstr ""

msgctxt "line 16, 07.txt"
msgid ""
"དེ་ནས་  བུ་མོ་  ཀཱ་ ཤི་ མཛེས་ལྡན་ མ་  པང་  ན་  འཚོ་བ  –␣འི་  མ་མ་  གཉིས་  "
"དང་  ། –  ནུ་མ་  སྣུན་པ  –␣འི་  མ་མ་  གཉིས་  དང་  ། –  དྲི་མ་  འཁྲུ་བ  "
"–␣འི་  མ་མ་  གཉིས་  དང་  ། –  རྩེ་བ  –␣འི་  མ་མ་  གཉིས་  ཏེ་  མ་མ་  བརྒྱད་  "
"ལ་  གཏད་  ནས  ། –"
msgstr ""

msgctxt "line 17, 07.txt"
msgid ""
"དེ་  མ་མ་  བརྒྱད་  ཀྱིས་  འོ་མ་  དང་  ཞོ་  དང་  མར་  དང་  ། –  ཞུན་མར་  དང་ "
" མར་  གྱི་  ཉིང་ གུ  –␣ས་  བསྐྱེད་  བསྲིངས་  ཏེ་"
msgstr ""

msgctxt "line 18, 07.txt"
msgid ""
"མཚོ  –␣འི་  ནང་  གི་  པད་མ་  བཞིན་  དུ་  སྐྱེ་  དྲག་པ  –␣ར་  གྱུར་  ཏོ  ། – "
"།"
msgstr ""

msgctxt "line 19, 07.txt"
msgid "དེ་  གང་  གི་  ཚེ་  ཆེན་ མོ  –␣ར་  གྱུར་  ཏེ  ། –"
msgstr ""

msgctxt "line 20, 07.txt"
msgid ""
"སྒྱུ་རྩལ་  ཤེས་པ  –␣ར་  གྱུར་པ་  དེ  –␣འི་  ཚེ  ། –  དེ  –␣ས་  "
"བཅོམ་ལྡན་འདས་  ཀྱི་  བསྟན་པ་  ལ་  དད་པ་  རྙེད་  ནས་"
msgstr ""

msgctxt "line 21, 07.txt"
msgid "དེ  –␣ས་  ཕ་མ་  གཉིས་  ལ་  གསོལ་  ཏེ  ། –"
msgstr ""

msgctxt "line 22, 07.txt"
msgid "སངས་རྒྱས་  དང་  ཆོས་  དང་  དགེ་འདུན་  ལ་  བཀུར་སྟི་  བྱས་  སོ  ། – །"
msgstr ""

msgctxt "line 23, 07.txt"
msgid ""
"དེ་  གང་  གི་  ཚེ་  ཆེན་ མོ  –␣ར་  གྱུར་པ་  དེ  –␣འི་  ཚེ  ། –  ཀཱ་ ཤི  "
"–␣འི་  རྒྱལ་པོ  –␣འི་  བུ་མོ་  གཟུགས་ བཟང་  ཞིང་  བལྟ་  ན་  སྡུག་  ལ་  "
"མཛེས་པ་  ཞིག་  ཡོད་  དེ  ། –"
msgstr ""

msgctxt "line 24, 07.txt"
msgid "འཇིག་རྟེན་  ན་  བུ་མོ་  དེ་  དང་  འདྲ་བ་  གཞན་  མེད་  དོ་  ཞེས་"
msgstr ""

msgctxt "line 25, 07.txt"
msgid "ཀུན་  ཏུ་  གྲགས་པ␣ས་  རྒྱས་པ  –␣ར་  གྱུར་  ཏོ  ། – །"
msgstr ""

msgctxt "line 26, 07.txt"
msgid ""
"དེ་ནས་  དེ་  ཐོས་  མ་ ཐག་  ཏུ་  ཉི་འོག་  གི་  རྒྱལ་པོ་  དྲུག་  འདོད་ཆགས་  "
"ཀྱི་  མད  –␣ས་  ཕོག་  ནས  ། –"
msgstr ""

msgctxt "line 27, 07.txt"
msgid ""
"དེ་དག་  ཐམས་ཅད་  ཀྱིས་  ཀཱ་ ཤི  –␣འི་  རྒྱལ་པོ་  ཚངས་པ␣ས་ བྱིན་  ལ་  ཕོ་ཉ་  "
"བཏང་  སྟེ  ། –"
msgstr ""

msgctxt "line 28, 07.txt"
msgid "དེ་དག་  རེ་རེ་  ནས་  བུ་མོ་  དེ་  ང་  ལ་  བྱིན་  ཅིག་  ཅེས་"
msgstr ""

msgctxt "line 29, 07.txt"
msgid "སྤྲིང་  ངོ་  ། – །"
msgstr ""

msgctxt "line 30, 07.txt"
msgid ""
"དེ་ནས་  དེ་  ཐོས་  མ་ ཐག་  ཏུ་  རྒྱལ་པོ་  ཚངས་པ␣ས་ བྱིན་  སེམས་  ཁོངས་  སུ་ "
" ཆུད་  ཅིང་  འདུག་  སྟེ  ། –"
msgstr ""

msgctxt "line 31, 07.txt"
msgid ""
"གལ་ཏེ་  བུ་མོ་  འདི་  གཅིག་  ལ་  བྱིན་  ན་  ནི  ། –  གཞན་  རྣམས་  བདག་  གི་ "
" དགྲ  –␣ར་  འགྱུར་  ན་"
msgstr ""

msgctxt "line 32, 07.txt"
msgid "སུ་  ལ་  བྱིན་པ  –␣ར་  བྱ་  སྙམ་"
msgstr ""

msgctxt "line 33, 07.txt"
msgid "བསམས་  ནས་"
msgstr ""

msgctxt "line 34, 07.txt"
msgid "གང་  ཡང་  སྦྱིན་  མི་འདོད་པ  –␣ར་  གྱུར་  ཏོ  ། – །"
msgstr ""

msgctxt "line 35, 07.txt"
msgid ""
"དེ་ནས་  ཕྱི་  ཞིག་  ན་  ཉི་འོག་  གི་  རྒྱལ་པོ་  དྲུག་  གིས་  བསམས་པ  ། –"
msgstr ""

msgctxt "line 36, 07.txt"
msgid ""
"དེ  –␣ས་ བདག་ ཅག་  ལ་  དད་པ␣ས་  བུ་མོ་  བྱིན་པ  –␣ར་  མི་  འགྱུར་  གྱིས་"
msgstr ""

msgctxt "line 37, 07.txt"
msgid "དེ  –␣ར་  སོང་  ལ་"
msgstr ""

msgctxt "line 38, 07.txt"
msgid "མཐུ  –␣ས་  དབྲོག་  གོ་  སྙམ་  དུ་"
msgstr ""

msgctxt "line 39, 07.txt"
msgid "བསམས་  ཏེ  ། –"
msgstr ""

msgctxt "line 40, 07.txt"
msgid ""
"ཐམས་ཅད་  ཀྱིས་  ཉི་མ་  གཅིག་  ཏུ་  དཔུང་  གི་  ཚོགས་  ཡན་ལག་  བཞི་  གོ་  "
"བསྐོན་  ནས་"
msgstr ""

msgctxt "line 41, 07.txt"
msgid "གྲོང་ཁྱེར་  བཱ་རཱ་ཎ་སཱི  –␣ར་  དོང་  སྟེ  ། –"
msgstr ""

msgctxt "line 42, 07.txt"
msgid "ཁོ་ར་  ཁོར་ཡུག་  ནས་  བསྐོར་  ཏོ  ། – །"
msgstr ""

msgctxt "line 43, 07.txt"
msgid ""
"དེ་ནས་  རྒྱལ་པོ་  ཚངས་པ␣ས་ བྱིན་  གྱིས་  བདག་ཉིད་  ཕ་རོལ་  གྱི་  དམག་ཚོགས་  "
"ཀྱིས་  གཙེས་པ  –␣ར་  རིག་  ནས་"
msgstr ""

msgctxt "line 44, 07.txt"
msgid "ཁང་  བཟངས་  ཀྱི་  སྟེང་  དུ་  སོང་  སྟེ  ། –"
