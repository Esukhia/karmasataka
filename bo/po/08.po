msgctxt "line 23, 08.txt"
msgid "འདུག་ གོ ། །"
msgstr ""

# དེ་ནས་ཕྱི་ཞིག་ན་དེ་ཁོ་ན་འཁོད་བཞིན་དུ་རྒྱལ་པོ་གསལ་[33a]རྒྱལ་གྱི་བུ་མོ་གཟུགས་བཟང་ཞིང་བལྟ་<«ལི་»«སྣར་»«ཅོ་»ལྟ།>ན་སྡུག་ལ་མཛེས་པ་ཞིག་བཙས་སོ།
# །
msgctxt "line 24, 08.txt"
msgid ""
"དེ་ནས་ ཕྱི་ ཞིག་ ན་ དེ་ ཁོ་ན་ འཁོད་ བཞིན་ དུ་ རྒྱལ་པོ་ གསལ་རྒྱལ་ གྱི་ བུ་མོ་"
" གཟུགས་བཟང་ ཞིང་ བལྟ་ ན་ སྡུག་ ལ་ མཛེས་པ་ ཞིག་ བཙས་ སོ ། །"
msgstr ""

# ཀཱ་<«གཡུང་»«པེ་»ཀ>ཤིའི་རྒྱལ་པོ་ཚངས་པས་བྱིན་གྱི་བུ་གཟུགས་བཟང་ཞིང་བལྟ་<«ལི་»«སྣར་»«ཅོ་»ལྟ།>ན་སྡུག་ལ་མཛེས་པ་ཞིག་ཀྱང་བཙས་ནས།
msgctxt "line 25, 08.txt"
msgid ""
"ཀཱ་ ཤི␣འི་ རྒྱལ་པོ་ ཚངས་པ␣ས་ བྱིན་ གྱི་ བུ་ གཟུགས་བཟང་ ཞིང་ བལྟ་ ན་ སྡུག་ ལ་"
" མཛེས་པ་ ཞིག་ ཀྱང་ བཙས་ ནས །"
msgstr ""

msgctxt "line 26, 08.txt"
msgid ""
"དེ␣ར་ དེ་ གཉིས་ སོ་སོ␣འི་ ཕོ་བྲང་ འཁོར་ དུ་ དགའ་བ␣འི་ རོལ་མོ་ བརྡུངས་ སོ ། །"
msgstr ""

msgctxt "line 27, 08.txt"
msgid "དེ་ནས་ དེ་ གཉིས་ ཀྱིས་ ཕན་ཚུན་ དུ་ ཐོས་ ནས་"
msgstr ""

# འདིར་དགའ་བའི་རོལ་མོ་བརྡུངས་པ་དེ་<«གཡུང་»«པེ་»ནི།
# «ལི་»«སྣར་»«ཅོ་»«ཞོལ་»ན།>ཅི་ཞིག་ཅེས་
msgctxt "line 28, 08.txt"
msgid "འདི␣ར་ དགའ་བ␣འི་ རོལ་མོ་ བརྡུངས་པ་ དེ་ ཅི་ཞིག་ ཅེས་"
msgstr ""

msgctxt "line 29, 08.txt"
msgid "དྲིས་ ནས་"
msgstr ""

