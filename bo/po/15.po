msgctxt "line 1, 15.txt"
msgid "ཤིང་རྟ་  གཉིས་ པ་  ནི  །  "
msgstr ""

msgctxt "line 2, 15.txt"
msgid "གླེང་ གཞི་  མཉན་ དུ་ཡོད་པ་  ན་  བཞུགས་  ཏེ  །  "
msgstr ""

msgctxt "line 3, 15.txt"
msgid ""
"དེ  –␣འི་  ཚེ་  བྲམ་ཟེ  –␣འི་  ཁྱེའུ་  ཞིག་  གིས་  མཆོད་སྦྱིན་  བྱས་  ནས་"
msgstr ""

msgctxt "line 4, 15.txt"
msgid "ཤིང་རྟ་  ལ་  ཞོན་  ཏེ་"
msgstr ""

msgctxt "line 5, 15.txt"
msgid "འཁོར་  གྱི་  ཚོགས་  ཀྱིས་  གཡོག་  བྱས་  ནས་"
msgstr ""

msgctxt "line 6, 15.txt"
msgid "མཉན་ དུ་ཡོད་པ  –␣འི་  ནང་  དུ་  ཆ␣ས་  སོ  །   །"
msgstr ""

msgctxt "line 7, 15.txt"
msgid "བཅོམ་ལྡན་འདས་  ཀྱང་  མཉན་ དུ་ཡོད་པ  –␣ར་  བསོད་སྙོམས་  མཛད་  ནས  །  "
msgstr ""

msgctxt "line 8, 15.txt"
msgid "མཉན་ དུ་ཡོད་པ་  ནས་  སླར་  གཤེགས་པ་  དང་"
msgstr ""

msgctxt "line 9, 15.txt"
msgid "དེ་  དགའ་བ་  སྐྱེས་  ནས་"
msgstr ""

msgctxt "line 10, 15.txt"
msgid "བཅོམ་ལྡན་འདས་  ལ་  བལྟས་  སོ  །   །"
msgstr ""

msgctxt "line 11, 15.txt"
msgid "དེ  –␣འི་  སྐབས་  སུ་  བཅོམ་ལྡན་འདས་  ཀྱིས་  འཛུམ་པ་  མཛད་  དེ  །  "
msgstr ""

msgctxt "line 12, 15.txt"
msgid ""
"འདི་  ནི་  སངས་རྒྱས་  བཅོམ་ལྡན་འདས་  རྣམས་  གང་  གི་  ཚེ་  འཛུམ་པ་  མཛད་པ  "
"–␣འི་  ཆོས་ཉིད་  དེ  །  "
msgstr ""

msgctxt "line 13, 15.txt"
msgid ""
"དེ  –␣འི་  ཚེ་  ཞལ་  ནས་  འོད་ཟེར་  སྔོན་པོ་  དང་  །    སེར་པོ་  དང་  །    "
"དམར་པོ་  དང་  །    དཀར་པོ་  རྣམས་  བྱུང་  ནས་"
msgstr ""

msgctxt "line 14, 15.txt"
msgid "ཁ་ཅིག་  ནི་  འོག་  ཏུ་  འགྲོ  –␣འོ  །   །"
msgstr ""

msgctxt "line 15, 15.txt"
msgid "ཁ་ཅིག་  ནི་  སྟེང་  དུ་  འགྲོ  –␣འོ  །   །"
msgstr ""

msgctxt "line 16, 15.txt"
msgid ""
"འོག་  ཏུ་  འགྲོ་བ་  གང་ ཡིན་པ་  དེ་དག་  ནི་  སེམས་ཅན་  དམྱལ་བ་  ཡང་སོས་  "
"དང་  ཐིག་ནག་  དང་  །    བསྡུས་འཇོམས་  དང་  །    ངུ་འབོད་  དང་  །    "
"ངུ་འབོད་  ཆེན་པོ་  དང་  །    ཚ་བ་  དང་  །    རབ་ ཏུ་ ཚ་ བ་  དང་  །    "
"མནར་མེད་ པ་  དང་  །    ཆུ་བུར་  ཅན་  དང་  །    ཆུ་བུར་  རྡོལ་  དང་  །    "
"ས་ཐམས་  ཐམས་  དང་  །    ཀྱི་ཧུད་  ཟེར་ བ་  དང་  །    ཨ་ཆུ་  ཟེར་ བ་  དང་  ། "
"   ཨུད་ པལ་  ལྟར་  གས་པ་  དང་  །    པད་མ་  ལྟར་  གས་པ་  དང་  །    པད་མ་  "
"ལྟར་  གས་པ་  ཆེན་པོ  –␣ར་  སོང་  ནས  །  "
msgstr ""

msgctxt "line 17, 15.txt"
msgid ""
"ཚ་བ  –␣འི་  སེམས་ཅན་  དམྱལ་བ་  གང་ ཡིན་པ་  དེ་དག་  ཏུ་  བསིལ་བ  –␣ར་  གྱུར་ "
" ཏེ་"
msgstr ""

msgctxt "line 18, 15.txt"
msgid "འབབ་ བོ  །   །"
msgstr ""

msgctxt "line 19, 15.txt"
msgid ""
"གྲང་བ  –␣འི་  སེམས་ཅན་  དམྱལ་བ་  གང་ ཡིན་པ་  དེ་དག་  ཏུ་  ནི་  དྲོ་བ  –␣ར་  "
"གྱུར་  ཏེ་"
msgstr ""

msgctxt "line 20, 15.txt"
msgid "འབབ་ བོ  །   །"
msgstr ""

msgctxt "line 21, 15.txt"
msgid ""
"དེ  –␣ས་  ན་  སེམས་ཅན་  དེ་དག་  གི་  གནོད་པ  –␣འི་  ཁྱད་པར་  རྣམས་  རྩེ་  "
"ཞིལ་  བར་  གྱུར་  ཏོ  །   །"
msgstr ""

msgctxt "line 22, 15.txt"
msgid "དེ་ནས་  དེ་དག་  འདི་  སྙམ་  དུ་  སེམས་  ཏེ  །  "
msgstr ""

msgctxt "line 23, 15.txt"
msgid "ཀྱེ་"
msgstr ""

msgctxt "line 24, 15.txt"
msgid "ཅི་  བདག་ཅག་  འདི་  ནས་  ཤི་ འཕོས་  སམ་"
msgstr ""

msgctxt "line 25, 15.txt"
msgid "འོན་ཏེ་  གཞན་  དུ་  སྐྱེས་  སམ་  སྙམ་  མོ  །   །"
msgstr ""

msgctxt "line 26, 15.txt"
msgid ""
"དེ་ནས་  དེ་དག་  དད་པ་  བསྐྱེད་པ  –␣འི་  ཕྱིར་  བཅོམ་ལྡན་འདས་  སྤྲུལ་པ་  "
"འགྱེད་པ  –␣ར་  མཛད་  དེ  །  "
msgstr ""

msgctxt "line 27, 15.txt"
msgid "དེ་དག་  གིས་  སྤྲུལ་པ་  མཐོང་  ནས་"
msgstr ""

msgctxt "line 28, 15.txt"
msgid "འདི་  སྙམ་  དུ་  སེམས་  ཏེ  །"
msgstr ""

msgctxt "line 29, 15.txt"
msgid "ཀྱེ་མ་"
msgstr ""

msgctxt "line 30, 15.txt"
msgid "ཅི་  བདག་ཅག་  ནི་  འདི་  ནས་  ཀྱང་  ཤི་ འཕོས་པ  –␣ར་ མ་ གྱུར་  ལ་"
msgstr ""

msgctxt "line 31, 15.txt"
msgid "གཞན་  དུ་  ཡང་  མ་  སྐྱེས་  མོད་  ཀྱི  །  "
msgstr ""

msgctxt "line 32, 15.txt"
msgid ""
"འོན་ཀྱང་  སྔོན་མ་  བྱུང་བ  –␣འི་  སེམས་ཅན་  སྣང་བ་  འདི  –␣འི་  མཐུ  –␣ས  ། "
"   བདག་ཅག་  གི་  གནོད་པ  –␣འི་  ཁྱད་པར་  རྣམས་  རྩེ་  ཞིལ་བ    ར་  གྱུར་  "
"ཏོ་  སྙམ་  མོ  །   །"
msgstr ""

msgctxt "line 33, 15.txt"
msgid "དེ་ནས་  དེ་དག་  སྤྲུལ་པ་  ལ་  སེམས་  མངོན་པར་  དགའ་བ  –␣ར་  གྱུར་  ནས་"
msgstr ""

msgctxt "line 34, 15.txt"
msgid "སེམས་ཅན་  དམྱལ་བ་  མྱོང་བ  –␣ར་  འགྱུར་བ  –␣འི་  ལ␣ས་  དེ་  ཟད་  དེ་"
msgstr ""

msgctxt "line 35, 15.txt"
msgid ""
"ལྷ་  དང་  མི་  རྣམས་  ཀྱི་  ནང་  དུ་  གང་  དུ་  བདེན་པ་  རྣམས་  ཀྱི་  སྣོད་ "
" དུ་  རུང་བ  –␣ར་  འགྱུར་བ་  དེ  –␣ར་  ཉིང་མཚམས་  འཛིན་པ  –␣ར་  འགྱུར་  རོ "
" །   །"
msgstr ""

msgctxt "line 36, 15.txt"
msgid ""
"སྟེང་  དུ་  འགྲོ་བ་  གང་ ཡིན་པ་  དེ་དག་  ནི་  རྒྱལ་ཆེན་  བཞི  –␣འི་  རིས་  "
"རྣམས་  དང་  །    སུམ་ཅུ་རྩ་གསུམ་པ་  རྣམས་  དང་  །    འཐབ་བྲལ་  རྣམས་  དང་  །"
"    དགའ་ལྡན་  རྣམས་  དང་  །    འཕྲུལ་དགའ་  རྣམས་  དང་  །    གཞན་འཕྲུལ་  "
"དབང་  བྱེད་  རྣམས་  དང་  །    ཚངས་རིས་  རྣམས་  དང་  །    ཚངས་པ  –␣འི་  "
"མདུན་ན་འདོན་  རྣམས་  དང་  །    ཚངས་པ་  ཆེན་པོ་  རྣམས་  དང་  །    འོད་ཆུང་  "
"རྣམས་  དང་  །    ཚད་  མེད་  འོད་  རྣམས་  དང་  །    འོད་གསལ་  རྣམས་  དང་  ། "
"   དགེ་ཆུང་  རྣམས་  དང་  །    ཚད་  མེད་  དགེ་  རྣམས་  དང་  །    དགེ་རྒྱས་  "
"རྣམས་  དང་  །    སྤྲིན་  མེད་  རྣམས་  དང་  །    བསོད་ནམས་  སྐྱེས་  རྣམས་  "
"དང་  །    འབྲས་བུ་  ཆེ་  རྣམས་  དང་  །    མི་ཆེ་བ་  རྣམས་  དང་  །    "
"མི་གདུང་བ་  རྣམས་  དང་  །    གྱ་ནོམ་  སྣང་  རྣམས་  དང་  །    ཤིན་ཏུ་  མཐོང་ "
" རྣམས་  དང་  །  འོག་མིན་  གྱི་  ལྷ་  རྣམས་  ཀྱི་  བར་  དུ་  སོང་  ནས་"
msgstr ""

