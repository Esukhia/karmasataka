msgctxt "line 29, C002.txt"
msgid "འདི་སྐད་དུ་"
msgstr ""

# ཀྱེ་<«ཞོལ་»ཀྱཻ།>གཽ་ཏ་མ་ཚུར་སྤྱོན། 
msgctxt "line 30, C002.txt"
msgid "ཀྱེ་ གཽ་ ཏ་ མ་ ཚུར་སྤྱོན །"
msgstr ""

# ཀྱེ་<«ཞོལ་»ཀྱཻ།>གཽ་ཏ་མ་ཁྱོད་གཤེགས་པ་ལེགས་སོ། །
msgctxt "line 31, C002.txt"
msgid "ཀྱེ་ གཽ་ ཏ་ མ་ ཁྱོད་ གཤེགས་པ་ ལེགས་ སོ ། །"
msgstr ""

# ཀྱེ་གཽ་ཏ་མ་ཁྱོད་གདན་བཤམས་པ་ལ་བཞུགས་ཞེས་<«གཡུང་»«ལི་»«པེ་»«ཅོ་»ཤེས།>
msgctxt "line 32, C002.txt"
