msgctxt "line 1, C002.txt"
msgid "﻿དབྱིག་དགའ་ཞེས་བྱ་བ་ནི། "
msgstr ""

# བཅོམ་ལྡན་འདས་ཡུལ་:ཀཱ་ཤིའི་<«གཡུང་»«པེ་»ཀ་ཤའི། «སྣར་»ཀཱ་ཤེའི།
# «ཅོ་»ཀཱ་ཤའི།>ཕྱོགས་སུ་ལྗོངས་རྒྱུ་ཞིང་གཤེགས་པ་ལས་
msgctxt "line 2, C002.txt"
msgid "བཅོམ་ལྡན་འདས་ ཡུལ་ ཀཱ་ ཤི␣འི་ ཕྱོགས་ སུ་ ལྗོངས་རྒྱུ་ ཞིང་གཤེགས་པ་ ལ␣ས་"
msgstr ""

# བ་རཱ་ཎ་སཱིར་བྱོན་<«ཅོ་»བྱིན།>ཏེ། 
msgctxt "line 3, C002.txt"
msgid "བ་ རཱ་ ཎ་ སཱིར་ བྱོན་ ཏེ །"
msgstr ""

# དེའི་ཚེ་བཱ་རཱ་ཎ་སཱི་ན་དྲང་སྲོང་སྨྲ་བ་རི་དགས་ཀྱི་ནགས་<«གཡུང་»«ལི་»«པེ་»«སྣར་»«ཅོ་»«ཞོལ་»གནས།>ན་དགེ་སློང་ལྔ་སྡེ་རྣམས་གནས་སོ།
# །
msgctxt "line 4, C002.txt"
msgid ""
"དེ␣འི་ ཚེ་ བཱ་རཱ་ཎ་སཱི་ ན་ དྲང་སྲོང་ སྨྲ་བ་ རི་ དག␣ས་ ཀྱི་ ནགས་ ན་ དགེ་སློང་"
" ལྔ་ སྡེ་ རྣམས་ གནས་ སོ ། །"
msgstr ""

# དེ་ནས་དགེ་སློང་ལྔ་སྡེ་རྣམས་ཀྱིས་<«གཡུང་»«པེ་»-ཀྱིས།>ཐག་རིང་པོ་ཞིག་ནས་བཅོམ་ལྡན་འདས་མཐོང་ངོ་།
# །
msgctxt "line 5, C002.txt"
msgid ""
"དེ་ནས་ དགེ་སློང་ ལྔ་ སྡེ་ རྣམས་ ཀྱིས་ ཐག་ རིང་པོ་ ཞིག་ ནས་ བཅོམ་ལྡན་འདས་ "
"མཐོང་ ངོ་ ། །"
msgstr ""

msgctxt "line 6, C002.txt"
msgid "མཐོང་ནས་ཀྱང་ཕན་ཚུན་ནས་འདུས་ཏེ་"
msgstr ""

msgctxt "line 7, C002.txt"
msgid "ནང་ཁྲིམས་བཅས་ནས་"
msgstr ""

msgctxt "line 8, C002.txt"
msgid "འདི་སྐད་དུ། "
msgstr ""

# ཀྱེ་དགེ་སྦྱོང་<«གཡུང་»སློང་།>གཽ་ཏ་མ་དེ་ནི་གཡེལ་བ་མང་དུ་སོགས་<«གཡུང་»«ལི་»«པེ་»«སྣར་»«ཅོ་»སོག
# «ཞོལ་»གསོག>པ་མང་དུ་:སོགས་ཤིང་<«གཡུང་»«ལི་»«པེ་»«སྣར་»«ཅོ་»སོག་ཅིང་།
# «ཞོལ་»གསོག་ཅིང་།>འཚོ་བ་སྤོང་བ་ལས་ཉམས་པ་སྟེ།
msgctxt "line 9, C002.txt"
msgid ""
"ཀྱེ་ དགེ་སྦྱོང་ གཽ་ ཏ་ མ་ དེ་ ནི་ གཡེལ་བ་ མང་ དུ་ སོགས་པ་ མང་ དུ་ སོགས་ ཤིང་"
" འཚོ་བ་ སྤོང་བ་ ལ␣ས་ ཉམས་པ་ སྟེ །"
msgstr ""

# དེ་:ནི་ད་<«པེ་»ནི་ང་།
# «ཅོ་»ཉིད།>ཁ་ཟས་མང་དུ་ཟ་ཞིང་འབྲས་ཅན་དང་ཙང་<«གཡུང་»«པེ་»ཙངས།
# «ཅོ་»གཙང་།>ཕྱེ་ཟ་ལ་ཞུན་མར་དང་ཏིལ་མར་གྱིས་ལུས་རྣམས་སྐུད་པར་བྱེད་ཅིང་
msgctxt "line 10, C002.txt"
msgid ""
"དེ་ ནི་ ད་ ཁ་ཟས་ མང་ དུ་ ཟ་ ཞིང་ འབྲས་ ཅན་ དང་ ཙང་ ཕྱེ་ ཟ་ ལ་ ཞུན་མར་ དང་ "
"ཏིལ་མར་ གྱིས་ ལུས་ རྣམས་ སྐུད་པ␣ར་ བྱེད་ ཅིང་"
msgstr ""

# ཆུ་དྲོན་པོས་ལུས་འཁྲུ་བར་བྱེད་དེ། <«གཡུང་»«ཁུ་»དོ།>
msgctxt "line 11, C002.txt"
msgid "ཆུ་ དྲོན་པོ␣ས་ ལུས་ འཁྲུ་བ␣ར་ བྱེད་ དེ །"
msgstr ""

msgctxt "line 12, C002.txt"
msgid "དེ་འུ་བུ་ཅག་གི་ཐད་དུ་འོང་:གིས། "
msgstr ""

# ཀྱེ་<«ཞོལ་»གི། ཀྱཻ།>གཽ་ཏ་མ་སྟན་དག་ཡོད་ཀྱིས་<«ཞོལ་»ཀྱི།>
msgctxt "line 13, C002.txt"
msgid "ཀྱེ་ གཽ་ ཏ་ མ་ སྟན་ དག་ ཡོད་ ཀྱིས་"
msgstr ""

msgctxt "line 14, C002.txt"
msgid "[96b]གལ་ཏེ་འདོད་པ་ལྟ་ན་འདུག་ཅིག་ཅེས་"
msgstr ""

# དེ་སྐད་ཙམ་དག་སྨྲ་བར་བྱའི<«གཡུང་»«པེ་»«ཅོ་»བྱ་བའི།>་
msgctxt "line 15, C002.txt"
msgid "དེ་སྐད་ ཙམ་ དག་ སྨྲ་བ␣ར་ བྱ␣འི་"
msgstr ""

msgctxt "line 16, C002.txt"
msgid "གུས་པར་སྨྲ་བར་མི་བྱའོ། །"
