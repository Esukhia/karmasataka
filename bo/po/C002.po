msgctxt "line 18, C002.txt"
msgid "ལྡང་བར་མི་བྱའོ། །"
msgstr ""

msgctxt "line 19, C002.txt"
msgid "ལངས་ཏེ་"
msgstr ""

# སྟན་བསྟབ་<«གཡུང་»«པེ་»བསྟབས།>པར་མི་བྱའོ་ཞེས་
msgctxt "line 20, C002.txt"
msgid "སྟན་ བསྟབ་པ␣ར་ མི་ བྱ␣འོ་ ཞེས་"
msgstr ""

msgctxt "line 21, C002.txt"
msgid "ཟེར་རོ། །"
msgstr ""

# དེ་ནས་བཅོམ་ལྡན་འདས་དགེ་སློང་ལྔ་སྡེ་རྣམས་དང་ཇི་<«གཡུང་»«ལི་»«པེ་»«སྣར་»«ཅོ་»ཅི།>ཙམ་དུ་ཉེ་བར་བྱོན་པ།
# དེ་<«གཡུང་»-དེ།>ཙམ་དུ་དགེ་སློང་ལྔ་སྡེ་རྣམས་བཅོམ་ལྡན་འདས་ཀྱི་དཔལ་དང་གཟི་བརྗིད་མ་<«གཡུང་»«ལི་»«པེ་»«སྣར་»«ཅོ་»མི།>བཟོད་ནས་
msgctxt "line 22, C002.txt"
msgid ""
"དེ་ནས་ བཅོམ་ལྡན་འདས་ དགེ་སློང་ ལྔ་ སྡེ་ རྣམས་ དང་ ཇི་ཙམ་ དུ་ ཉེ་བར་ བྱོན་པ "
"།  དེ་ཙམ་ དུ་ དགེ་སློང་ ལྔ་ སྡེ་ རྣམས་ བཅོམ་ལྡན་འདས་ ཀྱི་ དཔལ་ དང་ "
"གཟི་བརྗིད་ མ་ བཟོད་ ནས་"
msgstr ""

