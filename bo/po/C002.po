msgstr ""

msgctxt "line 24, C002.txt"
msgid "ཁ་ཅིག་ནི་སྟན་ལས་ལངས་ནས་"
msgstr ""

# བཅོམ་ལྡན་འདས་ཀྱི་<«གཡུང་»«པེ་»ཀྱིས།>གདན་ཤོམ་མོ། །
msgctxt "line 25, C002.txt"
msgid "བཅོམ་ལྡན་འདས་ ཀྱི་ གདན་ ཤོམ་ མོ ། །"
msgstr ""

