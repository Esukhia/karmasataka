msgctxt "line 380, C002.txt"
msgid "སྦྱིན་པ་ དག་ སྦྱིན་པ་ དང་ བསོད་ནམས་ དག་བྱེད་པ་ ལ་ ཞུགས་ སོ ། །"
msgstr ""

msgctxt "line 381, C002.txt"
msgid "དེའི་ཚེ་དེ་མ་ཡིན་པའི་གྲོང་ཁྱེར་ལྔ་ན་དེད་དཔོན་ལྔ་གནས་ཏེ། "
