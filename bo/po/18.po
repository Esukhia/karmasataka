msgctxt "line 1, 18.txt"
msgid "སྒམ་པོ་ ཞེས་ བྱ་བ་ ནི །"
msgstr ""

# གླེང་གཞི་<«གཡུང་»བཞི།>མཉན་དུ་ཡོད་པ་ན་བཞུགས་ཏེ། 
msgctxt "line 2, 18.txt"
msgid "གླེང་ གཞི་ མཉན་ དུ་ཡོད་པ་ ན་ བཞུགས་ ཏེ །"
msgstr ""

# དེའི་ཚེ་སེམས་ཅན་ཞིག་<«སྣར་»–ཞིག>སེམས་ཅན་དམྱལ་བར་སྐྱེས་ཏེ་
msgctxt "line 3, 18.txt"
msgid "དེ␣འི་ ཚེ་ སེམས་ཅན་ ཞིག་ སེམས་ཅན་ དམྱལ་བ␣ར་ སྐྱེས་ ཏེ་"
msgstr ""

msgctxt "line 4, 18.txt"
msgid "ལུས་ ཆེ་ ཞིང་"
msgstr ""

msgctxt "line 5, 18.txt"
msgid "དམུས་ལོང་ དུ་ འདུག་ ལ་"
msgstr ""

msgctxt "line 6, 18.txt"
msgid "དེ␣འི་ ལུས་ རིལ་ གྱིས་ རྨ␣ས་ ཞུ་ལུབ་ ཏུ་ ཡོག་ སྟེ །"
msgstr ""

msgctxt "line 7, 18.txt"
msgid ""
"དེ␣འི་ སྤྱི་བོ་ མན་ཆད་ རྐང་མཐིལ་ ཡན་ཆད་ སྲིན་ ཞུགས་པ␣ས་ དེ་ ནད་མ་ བཟོད་ ནས་"
msgstr ""

msgctxt "line 8, 18.txt"
msgid "ཕན་ཚུན་ རྒྱུག་པ␣ར་ བྱེད་ ལ །"
msgstr ""

msgctxt "line 9, 18.txt"
msgid "དེ་ གལ་ཏེ་ ཐང་ལ་ འགྲོ་ ན་ ནི །"
msgstr ""

msgctxt "line 10, 18.txt"
msgid ""
"དེ་ ལ་ དེ་ཉིད་ ནས་ སེང་གེ་ དང་ །  སྟག་ དང་ །  གཟིག་ དང་ །  དྲེད་ ལ་སོགས་པ་ "
"ལྕགས་ ཀྱི་ མཆེ་བ་ ཅན་ དག་ འབྱུང་བ␣ར་ འགྱུར་ ཏེ །"
msgstr ""

msgctxt "line 11, 18.txt"
msgid "དེ་དག་ གིས་ དེ␣འི་ ཤ་ རྣམས་ བཅད་ ཅིང་"
msgstr ""

# བཅད་ཅིང་<«གཡུང་»–བཅད་ཅིང་།>
msgctxt "line 12, 18.txt"
msgid "བཅད་ ཅིང་"
msgstr ""

msgctxt "line 13, 18.txt"
msgid "ཟ␣འོ ། །"
msgstr ""

# དེ་སྟེ་ཆུ་ལ་འབོག་ན་ནི་དེ་ལ་དེ་ཉིད་ནས་ཆུ་སྲིན་མ་ཀ་ར་ལྕགས་ཀྱི་མཆེ་བ་ཅན་དག་<«གཡུང་»–དག>འབྱུང་བར་འགྱུར་ཏེ།
msgctxt "line 14, 18.txt"
msgid ""
"དེ་སྟེ་ ཆུ་ ལ་ འབོག་ ན་ ནི་ དེ་ ལ་ དེ་ཉིད་ ནས་ཆུ་ སྲིན་ མ་ ཀ་ར་ ལྕགས་ ཀྱི་ "
"མཆེ་བ་ ཅན་ དག་ འབྱུང་བ␣ར་ འགྱུར་ ཏེ །"
msgstr ""

msgctxt "line 15, 18.txt"
msgid "དེ་དག་ གིས་ ཀྱང་ དེ␣འི་ ཤ་ རྣམས་ བཅད་ ཅིང་"
msgstr ""

# བཅད་ཅིང་<«གཡུང་»–བཅད་ཅིང་།>
msgctxt "line 16, 18.txt"
msgid "བཅད་ ཅིང་"
msgstr ""

msgctxt "line 17, 18.txt"
msgid "ཟ␣འོ ། །"
msgstr ""

# དེ་སྟེ་<«གཡུང་»«ལི་»«སྣར་»«ཅོ་»«ཞོལ་»–སྟེ།>སྟེང་གི་ནམ་མཁའ་ལ་འགྲོ་ན་ནི་དེ་ལ་དེ་ཉིད་ནས།
# བྱ་རོག་དང་བྱ་རྒོད་དང་ཆུན་ལག་ལྕགས་ཀྱི་མཆུ་ཅན་དག་འབྱུང་བར་འགྱུར་ཏེ།
msgctxt "line 18, 18.txt"
msgid ""
"དེ་སྟེ་ སྟེང་ གི་ ནམ་མཁའ་ ལ་ འགྲོ་ ན་ ནི་ དེ་ ལ་ དེ་ཉིད་ ནས །  བྱ་རོག་ དང་ "
"བྱ་རྒོད་ དང་ ཆུན་ལག་ ལྕགས་ ཀྱི་ མཆུ་ཅན་ དག་ འབྱུང་བ␣ར་ འགྱུར་ ཏེ །"
msgstr ""

msgctxt "line 19, 18.txt"
msgid "དེ་དག་ གིས་ ཀྱང་ དེ␣འི་ ཤ་ རྣམས་ བཅད་ ཅིང་"
msgstr ""

msgctxt "line 20, 18.txt"
msgid "བཅད་ ཅིང་"
msgstr ""

msgctxt "line 21, 18.txt"
msgid "ཟ␣འོ ། །"
msgstr ""

# དེ་སྟེ་ནགས་སྟུག་<«གཡུང་»ནག་སྡུག
# «པེ་»ནགས་སྡུགཔོ>པོའི་ནང་དུ་[60a]འགྲོ་ན་ནི་དེ་ལ་དེ་ཉིད་ནས་ཤིང་ཤལ་མ་ལིའི་<«གཡུང་»«པེ་»རིའི།>ཚལ་འབྱུང་བར་འགྱུར་ཏེ།
msgctxt "line 22, 18.txt"
msgid ""
"དེ་སྟེ་ ནགས་ སྟུག་པོ␣འི་ ནང་ དུ་ འགྲོ་ ན་ ནི་ དེ་ ལ་ དེ་ཉིད་ ནས་ ཤིང་ "
"ཤལ་མ་ལི␣འི་ ཚལ་ འབྱུང་བ␣ར་ འགྱུར་ ཏེ །"
msgstr ""

# དེ་ལ་དེར་རལ་གྲི་དང་། མདུང་ཐུང་དང་། མདའ་བོ་ཆེ་དང་། རྡོ་རྗེ་རྩེ་གཅིག་པ་དང་།
# མདུང་ཁ་ལེབ་རྣམས་རླུང་གིས་སྤགས་<«གཡུང་»བླགས།>ཏེ་
msgctxt "line 23, 18.txt"
msgid ""
"དེ་ ལ་ དེ␣ར་ རལ་གྲི་ དང་ །  མདུང་ཐུང་ དང་ །  མདའ་བོ་ ཆེ་ དང་ །  རྡོ་རྗེ་ "
"རྩེ་གཅིག་པ་ དང་ །  མདུང་ ཁ་ལེབ་ རྣམས་ རླུང་ གིས་ སྤགས་ ཏེ་"
msgstr ""

msgctxt "line 24, 18.txt"
msgid "དེ␣འི་ སྟེང་ དུ་ འབབ་བོ ། །"
msgstr ""

msgctxt "line 25, 18.txt"
msgid ""
"དེ་སྟེ་ རྩིག་པ␣འི་ གསེབ་ བམ་ རི␣འི་ སུལ་ལམ་ བྱ་སྐྱིབས་ དག་ ཏུ་ འགྲོ་བ␣ར་ "
"འདོད་ ན་ ཡང་"
msgstr ""

msgctxt "line 26, 18.txt"
msgid ""
"དེ་ ལ་ དེ་ཉིད་ ནས་ ལ␣ས་ ལ␣ས་ སྐྱེས་པ␣འི་ མི་ དག་ འབྱུང་བ␣ར་ འགྱུར་ ཏེ །"
msgstr ""

# རལ་གྲི་དང་། མདུང་ཐུང་དང་། མདའ་བོ་ཆེ་དང་།
# རྡོ་རྗེ་རྩེ་གཅིག་པ་<«གཡུང་»«ལི་»«པེ་»«སྣར་»«ཞོལ་»–པ།>དང་།
# མདུང་ཁ་ལེབ་རྣམས་ཐོགས་ནས་
msgctxt "line 27, 18.txt"
msgid ""
"རལ་གྲི་ དང་ །  མདུང་ཐུང་ དང་ །  མདའ་བོ་ ཆེ་ དང་ །  རྡོ་རྗེ་ རྩེ་གཅིག་པ་ དང་ "
"།  མདུང་ ཁ་ལེབ་ རྣམས་ ཐོགས་ ནས་"
msgstr ""

msgctxt "line 28, 18.txt"
msgid "དེ␣འི་ ལུས་ ལ་ འདེབས་ ཏེ །"
msgstr ""

# དེ་དག་གིས་དེའི་ལུས་ཐམས་ཅད་བཅད་<«སྣར་»ཅི་བཅད། «ཞོལ་»གཅོད།>ཅིང་
msgctxt "line 29, 18.txt"
msgid "དེ་དག་ གིས་ དེ␣འི་ ལུས་ ཐམས་ཅད་ བཅད་ ཅིང་"
msgstr ""

msgctxt "line 30, 18.txt"
msgid "གཏུབ་པ␣ར་ བྱེད་ དེ །"
msgstr ""

# རྒྱུ་དེས་ན་དེས་སྡུག་བསྔལ་དྲག་པ་དང་མི་བཟད་<«གཡུང་»«ལི་»«པེ་»«སྣར་»«ཅོ་»ཟད།>པ་དང་ཚ་བ་དང་ཡིད་དུ་མི་འོང་བའི་ཚོར་<«གཡུང་»+ད།
# «པེ»+དེ།>བ་མྱོང་སྟེ།
msgctxt "line 31, 18.txt"
msgid ""
"རྒྱུ་ དེས་ན་ དེ␣ས་ སྡུག་བསྔལ་ དྲག་པ་ དང་ མི་བཟད་པ་ དང་ ཚ་བ་ དང་ ཡིད་ དུ་ མི་"
" འོང་བ␣འི་ ཚོར་བ་ མྱོང་ སྟེ །"
msgstr ""

msgctxt "line 32, 18.txt"
msgid "ཉམ་ཐག་པ␣འི་ སྐད་ འབྱིན་ ཅིང་"
msgstr ""

msgctxt "line 33, 18.txt"
msgid "ངུའོ ། །"
msgstr ""

# སངས་རྒྱས་བཅོམ་ལྡན་འདས་བགྲོད་པ་གཅིག་པུའི་<«གཡུང་»«ལི་»«པེ་»«སྣར་»«ཅོ་»«ཞོལ་»པའི།>ལམ་སྟོན་པ།
msgctxt "line 34, 18.txt"
msgid "སངས་རྒྱས་ བཅོམ་ལྡན་འདས་ བགྲོད་པ་ གཅིག་པུ␣འི་ ལམ་ སྟོན་པ །"
msgstr ""

msgctxt "line 35, 18.txt"
msgid "རྣམ་པ་ གཉིས་ ཀྱི་ ཤེས་བྱ་ དང་ ཡེ་ཤེས་ ལ་ མངའ་ བརྙེས་པ །"
msgstr ""

# མ་འདྲེས་པའི་དྲན་པ་ཉེ་བར་གཞག་<«སྣར་»«ཞོལ་»བཞག>པ་གསུམ་ལ་མཁས་པ། 
msgctxt "line 36, 18.txt"
msgid "མ་ འདྲེས་པ␣འི་ དྲན་པ་ ཉེ་བར་ གཞག་པ་ གསུམ་ ལ་ མཁས་པ །"
msgstr ""

msgctxt "line 37, 18.txt"
msgid "མི་ འཇིགས་པ་ བཞི␣ས་ མི་ འཇིགས་པ །"
msgstr ""

msgctxt "line 38, 18.txt"
msgid "འགྲོ་བ་ ལྔ␣ར་ འགྲོ་བ་ ལ␣ས་ རྣམ་པ␣ར་ གྲོལ་བ །"
msgstr ""

msgctxt "line 39, 18.txt"
msgid "སྐྱེ་མཆེད་ དྲུག་ ལ་ མཁས་པ །"
msgstr ""

# བྱང་ཆུབ་ཀྱི་ཡན་ལག་བདུན་ལ་སྤྱོད་ཡུལ་བ<«ལི་»«སྣར་»«ཅོ་»«ཞོལ་»པ།>། 
msgctxt "line 40, 18.txt"
msgid "བྱང་ཆུབ་ ཀྱི་ ཡན་ལག་ བདུན་ ལ་ སྤྱོད་ཡུལ་བ །"
msgstr ""

# རྣམ་པར་ཐར་<«ལི་»«ཅོ་»མཐར།>པ་བརྒྱད་ལ་བསམ་གཏན་པ། 
msgctxt "line 41, 18.txt"
msgid "རྣམ་པ␣ར་ ཐར་པ་ བརྒྱད་ ལ་ བསམ་གཏན་པ །"
