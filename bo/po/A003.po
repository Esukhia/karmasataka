msgctxt "line 1, A003.txt"
msgid "﻿གསེར་མདོག་ཅེས་བྱ་བ་ནི། "
msgstr ""

# གླེང་གཞི་<«གཡུང་»བཞི།
# «ཅོ་»གཞི་ནི།>མཉན་དུ་ཡོད་པ་ན་བཞུགས་ཏེ<«གཡུང་»«པེ་»སྟེ།>།
msgctxt "line 2, A003.txt"
msgid "གླེང་གཞི་མཉན་དུ་ཡོད་པ་ན་བཞུགས་ཏེ།"
msgstr ""

msgctxt "line 3, A003.txt"
msgid ""
"དེའི་ཚེ་མཉན་དུ་ཡོད་པ་ན། "
"རིགས་ངན་ཞིག་གནས་པ་དེས་ཐབས་ཟླར་བབ་པ་ལ་ཆུང་མ་བླངས་པ་ལས་"
msgstr ""

# ཕྱི་ཞིག་ན་དེའི་ཆུང་མ་:ལ་བུ་<«གཡུང་»འབུ།>ཆགས་ནས་
msgctxt "line 4, A003.txt"
msgid "ཕྱི་ཞིག་ན་དེའི་ཆུང་མ་ལ་བུ་ཆགས་ནས་"
msgstr ""

# དེ་ཟླ་བ་དགུའམ་བཅུ་ལོན་པ་དང་བུ་མོ་མི་སྡུག་པ་<«ལི་»«སྣར་»«ཅོ་»«ཞོལ་»པའི།>མཆོག་ཏུ་མི་སྡུག་ལ་མི་སྡུག་པའི་སྣ་:གྲངས་བཅྭ་<«གཡུང་»«པེ་»«སྣར་»«ཅོ་»གྲངས་བཅོ།
# «ཞོལ་»དྲངས་བཅོ།>བརྒྱད་དང་ལྡན་པ་ཞིག་བཙས་ནས་
msgctxt "line 5, A003.txt"
msgid ""
"དེ་ཟླ་བ་དགུའམ་བཅུ་ལོན་པ་དང་བུ་མོ་མི་སྡུག་པ་མཆོག་ཏུ་མི་སྡུག་ལ་མི་སྡུག་པའི་སྣ་གྲངས་བཅྭ་བརྒྱད་དང་ལྡན་པ་ཞིག་བཙས་ནས་"
msgstr ""

# དེ་མཐོང་མ་ཐག་ཏུ་ཕ་མ་གཉིས་ཤིན་ཏུ་མི་དགའ་བར་གྱུར་ཏེ<«ལི་»«སྣར་»«ཅོ་»«ཞོལ་»ཏོ།>།
msgctxt "line 6, A003.txt"
msgid "དེ་མཐོང་མ་ཐག་ཏུ་ཕ་མ་གཉིས་ཤིན་ཏུ་མི་དགའ་བར་གྱུར་ཏེ། "
msgstr ""

# བདག་ཅག་གཉིས་ལ་བུ་མོ་འདི་འདྲ་བ་མི་དགོས་ཀྱིས་<«ཞོལ་»ཀྱི།>
msgctxt "line 7, A003.txt"
msgid "བདག་ཅག་གཉིས་ལ་བུ་མོ་འདི་འདྲ་བ་མི་དགོས་ཀྱིས་"
msgstr ""

msgctxt "line 8, A003.txt"
msgid "ནམ་སྲོས་པ་དང་ཕྱིར་ཕྱུང་ལ་"
msgstr ""

msgctxt "line 9, A003.txt"
msgid "ཁྱི་རྣམས་ལ་སྦྱིན་ནོ་ཞེས་"
msgstr ""

msgctxt "line 10, A003.txt"
msgid "ཟེར་རོ། །"
msgstr ""

msgctxt "line 11, A003.txt"
msgid "དེ་ནས་ཆུང་མ་ན་རེ་"
msgstr ""

# ཇོ་བོ་དེ་སྐད་མ་གསུང་<«ཞོལ་»གསུངས།>ཞིག །
msgctxt "line 12, A003.txt"
msgid "ཇོ་བོ་དེ་སྐད་མ་གསུང་ཞིག །"
msgstr ""

# འཇིག་རྟེན་ན་མི་གསོད་<«གཡུང་»«པེ་»བསོད།>པ་ནི་རྣམ་པར་སྨད་<«ལི་»«སྣར་»དམད།>པ་ལགས་ཀྱིས<«ཞོལ་»ཀྱི།>།
msgctxt "line 13, A003.txt"
msgid "འཇིག་རྟེན་ན་མི་གསོད་པ་ནི་རྣམ་པར་སྨད་པ་ལགས་ཀྱིས། "
msgstr ""

# འདི་འདི་<«གཡུང་»«ལི་»«པེ་»«སྣར་»«ཅོ་»-འདི།>ཁོ་ན་ལྟར་མི་སྡུག་པའི་རྣམ་<«ཅོ་»སྣ།>གྲངས་བཅྭ་<«གཡུང་»«པེ་»«སྣར་»«ཞོལ་»བཅོ།>བརྒྱད་དང་ལྡན་དུ་ལགས་ཀྱིས་<«ཞོལ་»-ཀྱིས།>ཀྱང་<«གཡུང་»«ལི་»«པེ་»«སྣར་»«ཅོ་»-ཀྱང་།>།
# འདི་བདག་ཅག་གཉིས་ཀྱི་བུ་མོ་ལགས་པས་ན།
# འདི་བཙས་མ་ཐག་ཏུ་བདག་ཅག་གིས་དོར་མི་ནུས་ཀྱིས་<«ཞོལ་»ཀྱི།>
msgctxt "line 14, A003.txt"
msgid ""
"འདི་འདི་ཁོ་ན་ལྟར་མི་སྡུག་པའི་རྣམ་གྲངས་བཅྭ་བརྒྱད་དང་ལྡན་དུ་ལགས་ཀྱིས་ཀྱང་། "
"འདི་བདག་ཅག་གཉིས་ཀྱི་བུ་མོ་ལགས་པས་ན། "
"འདི་བཙས་མ་ཐག་ཏུ་བདག་ཅག་གིས་དོར་མི་ནུས་ཀྱིས་"
msgstr ""

msgctxt "line 15, A003.txt"
msgid "ནམ་ཆེར་སྐྱེས་པ་ན་བསྐྲད་པར་བགྱིའོ། །"
msgstr ""

# འོན་ཀྱང་འདི་དེའི་ཚེ་ཅིས་ཀྱང་[76b]འཚོ་བ་ཚོལ་ནུས་པར་འགྱུར་རོ་<«གཡུང་»«པེ་»«ཁུ་»-རོ།>ཞེས་
msgctxt "line 16, A003.txt"
msgid "འོན་ཀྱང་འདི་དེའི་ཚེ་ཅིས་ཀྱང་[76b]འཚོ་བ་ཚོལ་ནུས་པར་འགྱུར་རོ་ཞེས་"
msgstr ""

msgctxt "line 17, A003.txt"
msgid "བྱས་སོ། །"
msgstr ""

msgctxt "line 18, A003.txt"
msgid "དེ་ནས་འོ་ན་དེ་ལྟར་བྱའོ་ཞེས་"
msgstr ""

msgctxt "line 19, A003.txt"
msgid "སྨྲས་ནས། "
msgstr ""

# དེ་ཕ་མ་གཉིས་ཀྱིས་ལྐོག་<«གཡུང་»རྐོག>ཏུ་གསོས་ཏེ། 
msgctxt "line 20, A003.txt"
msgid "དེ་ཕ་མ་གཉིས་ཀྱིས་ལྐོག་ཏུ་གསོས་ཏེ། "
msgstr ""

msgctxt "line 21, A003.txt"
msgid "ཅི་ནས་སུས་ཀྱང་མི་ཤེས་པར་བྱས་སོ། །"
msgstr ""

# དེ་ནས་དེ་གང་གི་ཚེ་འགྲོ་ནུས་པར་གྱུར་པ་དེའི་ཚེ་དེ་མི་སྡུག་ཆེས་<«གཡུང་»ཅེས།>ནས།
# ཕ་མ་གཉིས་ཀྱིས་ཁྱིམ་ནས་བསྐྲད་<«གཡུང་»«པེ་»སྐད། «མི་»«སྣར་»«ཅོ་»སྐྲད།>དེ་
msgctxt "line 22, A003.txt"
msgid ""
"དེ་ནས་དེ་གང་གི་ཚེ་འགྲོ་ནུས་པར་གྱུར་པ་དེའི་ཚེ་དེ་མི་སྡུག་ཆེས་ནས། "
"ཕ་མ་གཉིས་ཀྱིས་ཁྱིམ་ནས་བསྐྲད་དེ་"
msgstr ""

msgctxt "line 23, A003.txt"
msgid "བཏང་ངོ་། །"
msgstr ""

# དེ་ནས་དེ་སློངས་<«སྣར་»«ཞོལ་»སློང་།>མོ་བྱེད་ཅིང་སྲང་དེ་ཡན་མན་ན་གནས་པ་ལས། 
msgctxt "line 24, A003.txt"
msgid "དེ་ནས་དེ་སློངས་མོ་བྱེད་ཅིང་སྲང་དེ་ཡན་མན་ན་གནས་པ་ལས། "
msgstr ""

# དེས་ས་སྒྲེན་པོ་<«གཡུང་»སྒྲ་བོ།
# «མི་»«པེ་»«སྣར་»«ཅོ་»«ཞོལ་»སྒྲེ་བོ།>ལ་གནས་པ་དང་ཁ་ཟས་ངན་པ་ཟ་ཟ་བས་ལུས་ལ་མཛེ་ནད་<«གཡུང་»«ལི་»«པེ་»«སྣར་»«ཅོ་»«ཞོལ»-ནད།>དང་རྨ་བྱུང་བར་གྱུར་ཏེ།
msgctxt "line 25, A003.txt"
msgid ""
"དེས་ས་སྒྲེན་པོ་ལ་གནས་པ་དང་ཁ་ཟས་ངན་པ་ཟ་ཟ་བས་ལུས་ལ་མཛེ་ནད་དང་རྨ་བྱུང་བར་གྱུར་ཏེ།"
" "
msgstr ""

msgctxt "line 26, A003.txt"
msgid "དེ་ཕྱི་ཞིག་ན་ནད་བུ་ཅན་དུ་གྱུར་ནས། "
msgstr ""

msgctxt "line 27, A003.txt"
msgid "སྲང་དེ་ན་འགྱེལ་ཞིང་འདུག་སྟེ། "
msgstr ""

msgctxt "line 28, A003.txt"
msgid "ཤི་ལ་ཐུག་པ་དང་ཚེ་དང་ལྡན་པ་ཀུན་དགའ་བོས་མཐོང་སྟེ། "
msgstr ""

msgctxt "line 29, A003.txt"
msgid "མཐོང་མ་ཐག་ཏུ་ཚེ་དང་ལྡན་པ་ཀུན་དགའ་བོ་སྙིང་རྗེ་སྐྱེས་ནས་"
msgstr ""

msgctxt "line 30, A003.txt"
msgid "དེ་དེའི་ཐད་དུ་སོང་སྟེ་"
msgstr ""

msgctxt "line 31, A003.txt"
msgid "སྨྲས་པ། "
msgstr ""

# ཆེ་གེ་<«གཡུང་»ཞེས།>ཅི་ཉེས་
msgctxt "line 32, A003.txt"
msgid "ཆེ་གེ་ཅི་ཉེས་"
msgstr ""

msgctxt "line 33, A003.txt"
msgid "དེས་སྨྲས་པ། "
msgstr ""

# བཙུན་པ་ཀུན་:དགའ་བོ་<«ལི་»དགའོ།>བདག་གིས་<«ཞོལ་»གི།>སྔོན་བསོད་ནམས་དག་མ་བགྱིས་པས་ན།
# དེས་བདག་འདི་ཁོ་ན་ལྟ་བུར་གྱུར་པ་ལགས་ཀྱིས<«ཞོལ་»ཀྱི།>།
msgctxt "line 34, A003.txt"
msgid ""
"བཙུན་པ་ཀུན་དགའ་བོ་བདག་གིས་སྔོན་བསོད་ནམས་དག་མ་བགྱིས་པས་ན། "
"དེས་བདག་འདི་ཁོ་ན་ལྟ་བུར་གྱུར་པ་ལགས་ཀྱིས། "
msgstr ""

# བཙུན་པ་ཀུན་དགའ་བོ་ཅི་ནས་བདག་གི་<«གཡུང་»གིས།>མི་དགེ་བའི་ལས་འདི་ཡོངས་སུ་ཟད་པར་འགྱུར་བར་མཛད་དུ་གསོལ།
msgctxt "line 35, A003.txt"
msgid ""
"བཙུན་པ་ཀུན་དགའ་བོ་ཅི་ནས་བདག་གི་མི་དགེ་བའི་ལས་འདི་ཡོངས་སུ་ཟད་པར་འགྱུར་བར་མཛད་དུ་གསོལ།"
" "
msgstr ""

