msgctxt "line 1, 12.txt"
msgid "ལ␣ས་ བརྒྱ་ ཐམ་པ །  བམ་པོ་ བཞི་པ །"
msgstr ""

msgctxt "line 2, 12.txt"
msgid "འདུས་ མོ་ ཞེས་ བྱ་བ་ ནི །"
msgstr ""

# གླེང་གཞི་<«གཡུང་»བཞི།>རྒྱལ་པོའི་ཁབ་ན་བཞུགས་ཏེ། 
msgctxt "line 3, 12.txt"
msgid "གླེང་ གཞི་ རྒྱལ་པོ␣འི་ ཁབ་ ན་ བཞུགས་ ཏེ །"
msgstr ""

# དེའི་ཚེ་ཡུལ་ཏཀྵ་ཤཱི་<«གཡུང་»«པེ་»«སྣར་»«ཅོ་»ཏག་ཤ་ཤི། «ལི་»ཏག་ཤ་ཤ
# «ཞོལ་»ཏ་ཀྵེ་ཤཱ།>ལ་ན། རྒྱལ་པོ་པད་མའི་སྙིང་པོ་ཅན་ཞེས་བྱ་བ་རྒྱལ་པོ་བྱེད་དེ།
msgctxt "line 4, 12.txt"
msgid ""
"དེ␣འི་ ཚེ་ ཡུལ་ ཏཀྵ་ ཤཱི་ ལ་ ན །  རྒྱལ་པོ་ པད་མ␣འི་ སྙིང་པོ་ ཅན་ ཞེས་ བྱ་བ་ "
"རྒྱལ་པོ་ བྱེད་ དེ །"
msgstr ""

# དེའི་རིང་ལ་འབྱོར་པ་དང་། རྒྱས་པ་དང་། བདེ་བ་དང་། ལོ་ལེགས་པ་དང་། སྐྱེ་བོ་དང་།
# མི་མང་པོས་གང་བ་དང་། རྩོད་<«གཡུང་»བཙོད།>པ་དང་། འཐབ་པ་ཞི་བ་དང་། འཁྲུག་པ་དང་།
# ནང་འཁྲུག་<«གཡུང་»ཁྲུག>དང་། ཆོམ་རྐུན་དང་། མུ་གེ་དང་། ནད་རྣམས་མེད་པ་དང་།
# འབྲས་སཱ་<«གཡུང་»«པེ་»ས།>ལུ་དང་། བུ་རམ་ཤིང་དང་། བ་ལང་དང་།
# མ་ཧེ་དང་ལྡན་པར་འདུག་སྟེ
msgctxt "line 5, 12.txt"
msgid ""
"དེ␣འི་ རིང་ ལ་ འབྱོར་པ་ དང་ །  རྒྱས་པ་ དང་ །  བདེ་བ་ དང་ །  ལོ་ལེགས་པ་ དང་ "
"།  སྐྱེ་བོ་ དང་ །  མི་ མང་པོ␣ས་ གང་བ་ དང་ །  རྩོད་པ་ དང་ །  འཐབ་པ་ ཞི་བ་ དང་"
" །  འཁྲུག་པ་ དང་ །  ནང་འཁྲུག་ དང་ །  ཆོམ་རྐུན་ དང་ །  མུ་གེ་ དང་ །  ནད་ "
"རྣམས་ མེད་པ་ དང་ །  འབྲས་ སཱ་ ལུ་ དང་ །  བུ་རམ་ཤིང་ དང་ །  བ་ལང་ དང་ །  "
"མ་ཧེ་ དང་ ལྡན་པ␣ར་ འདུག་ སྟེ"
msgstr ""

msgctxt "line 6, 12.txt"
msgid ""
"བུ་ གཅིག་པ་ སྡུག་པ་ ལ་ བྱ་བ་ བཞིན་ དུ་ ཆོས་བཞིན་ དུ་ རྒྱལ་སྲིད་ བྱེད་ དོ ། །"
msgstr ""

msgctxt "line 7, 12.txt"
msgid ""
"དེ་ནས་ དེ་ བཙུན་མོ་ དང་ ལྷན་ཅིག་ ཏུ་ རྩེ་ ཞིང་ དགའ་ ལ་ དགའ་མགུ␣ར་ སྤྱོད་པ་ "
"ལ␣ས །"
msgstr ""

msgctxt "line 8, 12.txt"
msgid "བཙུན་མོ་ ལ་ བུ་ ཆགས་ ནས །"
msgstr ""

msgctxt "line 9, 12.txt"
msgid ""
"དེ␣འི་ ཡིད་ ལ་ གཞུང་ སྨྲ་བ་ རྣམས་ དང་ ལྷན་ཅིག་ ཏུ་ རྩོད་ ཀྱང་ ཅི་མ་རུང་ སྙམ་"
" དུ་ ཡིད་ ལ་ འདོད་པ །  དེ་ལྟ་ བུ་ ཞིག་ སྐྱེས་ སོ ། །"
msgstr ""

# དེ་ནས་རྒྱལ་པོས་དེ་ཐོས་མ་ཐག་ཏུ་ལྟས་མཁན་རྣམས་<«པེ་»«སྣར་»«ཁུ་»«ཞོལ་»རྣམ།>ལ་བསྟན་ཏེ།
msgctxt "line 10, 12.txt"
msgid "དེ་ནས་ རྒྱལ་པོ␣ས་ དེ་ ཐོས་ མ་ ཐག་ ཏུ་ ལྟས་མཁན་ རྣམས་ ལ་ བསྟན་ ཏེ །"
msgstr ""

msgctxt "line 11, 12.txt"
msgid "ལྟས་མཁན་ རྣམས་ ན་རེ་"
msgstr ""

msgctxt "line 12, 12.txt"
msgid "ལྷ་"
msgstr ""

msgctxt "line 13, 12.txt"
msgid "འདི་ ནི་ ལྟོ་ ན་ མཆིས་པ␣འི་ སེམས་ཅན་ དེ␣འི་ མཐུ་ ལགས་ ཏེ །"
msgstr ""

msgctxt "line 14, 12.txt"
msgid ""
"དེ␣ས་ ནི་ བསྟན་བཅོས་ ཐམས་ཅད་ རྟོགས་པ␣ར་ འགྱུར་ ལ་ གཞུང་ སྨྲ་བ་ ཐམས་ཅད་ "
"ཚར་གཅོད་པ␣ར་ འགྱུར་ ཏེ །"
msgstr ""

msgctxt "line 15, 12.txt"
msgid ""
"དེ␣འི་ སླད་ དུ་ འདི␣འི་ ཡིད་ ལ་ འདོད་པ །  དེ་ལྟ་ བུ་ སྐྱེས་པ་ ལགས་སོ་ ཞེས་"
msgstr ""

msgctxt "line 16, 12.txt"
msgid "ཟེར་ རོ ། །"
msgstr ""

msgctxt "line 17, 12.txt"
msgid "རྒྱལ་པོ␣ས་ སྨྲས་པ །"
msgstr ""

msgctxt "line 18, 12.txt"
msgid "ཤེས་ལྡན་ དག་"
msgstr ""

msgctxt "line 19, 12.txt"
msgid ""
"གལ་ཏེ །  བཙུན་མོ␣འི་ ཡིད་ ལ་ འདོད་པ་ སྐྱེས་པ་ དེ་ མ་ བསལ་ ན །  དེ་ ཅིར་ "
"འགྱུར་"
msgstr ""

msgctxt "line 20, 12.txt"
msgid "ལྟས་མཁན་ རྣམས་ ཀྱིས་ སྨྲས་པ །"
msgstr ""

msgctxt "line 21, 12.txt"
msgid "སྲས་ དེ་ཡན་ ལག་ མ་ཚང་ བར་ སྐྱེ་བ␣ར་ འགྱུར་ ལགས་སོ ། །"
msgstr ""

msgctxt "line 22, 12.txt"
msgid "དེ་ནས་ རྒྱལ་པོ␣ས་ བསམས་པ །"
msgstr ""

# བདག་གི་བུ་ཡིན་ལ་ཡན་ལག་མ་ཚང་བར་འགྱུར་<«ཞོལ་»གྱུར།>ན་མི་རུང་ངོ་སྙམ་ནས་
msgctxt "line 23, 12.txt"
msgid "བདག་ གི་ བུ་ ཡིན་ ལ་ ཡན་ལག་ མ་ཚང་ བར་ འགྱུར་ ན་ མི་ རུང་ ངོ་ སྙམ་ ནས་"
msgstr ""

# བཙུན་མོ་ཡོལ་བའི་ནང་དུ་བཞག་<«གཡུང་»བཞུག>སྟེ། 
msgctxt "line 24, 12.txt"
msgid "བཙུན་མོ་ ཡོལ་བ␣འི་ ནང་ དུ་ བཞག་ སྟེ །"
msgstr ""

# གཞུང་སྨྲ་བ་ཐམས་ཅད་བསགས་<«གཡུང་»«ལི་»«པེ་»«སྣར་»«ཅོ་»བསོགས།>ནས་
msgctxt "line 25, 12.txt"
msgid "གཞུང་ སྨྲ་བ་ ཐམས་ཅད་ བསགས་ ནས་"
msgstr ""

# གཟུ་བོ་<«གཡུང་»མོ།>རྣམས་ཀྱང་བསྐོས་<«གཡུང་»«པེ་»«ཅོ་»བསྒོས།>ཏེ། 
msgctxt "line 26, 12.txt"
msgid "གཟུ་བོ་ རྣམས་ ཀྱང་ བསྐོས་ ཏེ །"
msgstr ""

# གཞུང་གིས་རྩོད་<«གཡུང་»«པེ་»བརྩོད།>དུ་བཅུག་གོ། །
msgctxt "line 27, 12.txt"
msgid "གཞུང་ གིས་ རྩོད་ དུ་ བཅུག་ གོ ། །"
msgstr ""

msgctxt "line 28, 12.txt"
msgid "དེ་ནས་ དེ␣ས་ གཞུང་ སྨྲ་བ་ ཐམས་ཅད་ ཚར་ བཅད་ ནས །"
msgstr ""

# དེའི་ཡིད་ལ་<«གཡུང་»–ལ།>འདོད་པ་སྐྱེས་<«གཡུང་»«པེ་»ལ་བསྐྱེས།>པ་དེ་མེད་པར་གྱུར་ཏོ།
# །
msgctxt "line 29, 12.txt"
msgid "དེ␣འི་ ཡིད་ ལ་ འདོད་པ་ སྐྱེས་པ་ དེ་ མེད་པ␣ར་ གྱུར་ ཏོ ། །"
msgstr ""

# དེ་ནས་དེ་ཟླ་བ་དགུའམ་བཅུ་ལོན་པ་དང་།
# བུ་མོ་གཟུགས་བཟང་ཞིང་བལྟ་<«ལི་»«སྣར་»«ཅོ་»ལྟ།>ན་སྡུག་ལ་མཛེས་པ་ཞིག་བཙས་ནས།
msgctxt "line 30, 12.txt"
msgid ""
"དེ་ནས་ དེ་ ཟླ་བ་ དགུ␣འམ་ བཅུ་ ལོན་པ་ དང་ །  བུ་མོ་ གཟུགས་བཟང་ ཞིང་ བལྟ་ ན་ "
"སྡུག་ ལ་ མཛེས་པ་ ཞིག་ བཙས་ ནས །"
msgstr ""

msgctxt "line 31, 12.txt"
msgid "དེ␣འི་ བཙས་ སྟོན་ རྒྱས་པ␣ར་ བྱས་ ཏེ །"
msgstr ""

# བུ་མོ་འདིའི་མིང་ཅི་སྐད་གདགས་ཞེས་<«གཡུང་»«ལི་»«པེ་»«ཅོ་»ཤེས།>མིང་འདོགས་པར་བྱེད་དེ།
msgctxt "line 32, 12.txt"
msgid "བུ་མོ་ འདི␣འི་ མིང་ ཅི་ སྐད་ གདགས་ ཞེས་ མིང་འདོགས་པ␣ར་ བྱེད་ དེ །"
msgstr ""

# བུ་མོ་འདིའི་<«ཞོལ་»འདིས།>མས་འདུས་པའི་ནང་དུ་གཞུང་སྨྲ་བ་ཐམས་ཅད་ཚར་བཅད་པས་ན་བུ་མོ་འདིའི་མིང་འདུས་མོ་ཞེས་གདགས་སོ་ཞེས་
msgctxt "line 33, 12.txt"
msgid ""
"བུ་མོ་ འདི␣འི་ མས་ འདུས་པ␣འི་ ནང་ དུ་ གཞུང་ སྨྲ་བ་ ཐམས་ཅད་ ཚར་ བཅད་པ␣ས་ ན་ "
"བུ་མོ་ འདི␣འི་ མིང་ འདུས་ མོ་ ཞེས་ གདགས་ སོ་ ཞེས་"
msgstr ""

msgctxt "line 34, 12.txt"
msgid "ཟེར་ རོ ། །"
msgstr ""

# དེ་ནས་བུ་མོ་འདུས་མོ་པང་ན་འཚོ་བའི་མ་མ་གཉིས་དང་།
