msgctxt "line 22, 06.txt"
msgid "ཡུལ་  ཀཽ་ ཤཱཾ་ བཱི  –␣ར་  ཕྱིན་  ནས  །"
msgstr ""

msgctxt "line 23, 06.txt"
msgid ""
"ཀཽ་ ཤཱཾ་ བཱི་  ན་  ཁྱིམ་བདག་  གདངས་ ལ␣ས་ རིག་  གི་  ཀུན་དགའ་ར་བ་  ན་  གནས་  "
"སོ  ། – །"
msgstr ""

msgctxt "line 24, 06.txt"
msgid ""
"ཚེ་  དང་  ལྡན་པ་  དེ་ ཡང་  བསོད་ནམས་  ཆེན་པོ་  དང་  ལྡན་པ  ། –  "
"མདུན་ན་འདོན་  གྱི་  བུ་  ལ␣ས་  རབ་ ཏུ་  བྱུང་བ་  ཡིན་  ནོ་  ཞེས་"
msgstr ""

msgctxt "line 25, 06.txt"
msgid "གྲགས་  སོ  ། – །"
msgstr ""

msgctxt "line 26, 06.txt"
msgid ""
"དེ་ནས་  ཀཽ་ ཤཱཾ་ བཱི་  ན་  གནས་པ  –␣འི་  སྐྱེ་བོ་  མང་པོ  –␣ས་  འཕགས་པ་  "
"པིཎྜོ་ ལ་ བ་ ར་ དྷཱ་ ཛ་  ལྗོངས་ རྒྱུ་  ཞིང་  ཡུལ་  ཀཽ་ ཤཱཾ་ བཱི  –␣ར་  "
"འོངས་  ཏེ  ། –"
msgstr ""

msgctxt "line 27, 06.txt"
msgid ""
"ཀཽ་ ཤཱཾ་ བཱི་  ན་  ཁྱིམ་བདག་  གདངས་ ལ␣ས་ རིག་  གི་  ཀུན་དགའ་ར་བ་  ན་  འདུག་ "
" གོ་  ཞེས་"
msgstr ""

msgctxt "line 28, 06.txt"
msgid "ཐོས་  སོ  ། – །"
msgstr ""

msgctxt "line 29, 06.txt"
msgid "ཐོས་  ནས་  ཀྱང་"
msgstr ""

msgctxt "line 30, 06.txt"
msgid "ཚོགས་  ནས་"
msgstr ""

msgctxt "line 31, 06.txt"
msgid "ཚོགས་  དང་  སྡེ་ཚན་  ནས་  སྡེ་ཚན་  དུ་  འདུས་  ཤིང་  ཚོགས་  ཏེ  ། –"
msgstr ""

msgctxt "line 32, 06.txt"
msgid "ཀཽ་ ཤཱཾ་ བཱི་  ནས་  བྱུང་  ནས  ། –"
msgstr ""

msgctxt "line 33, 06.txt"
msgid ""
"ཚེ་  དང་  ལྡན་པ་  པིཎྜོ་ ལ་ བ་ ར་ དྷྭ་ ཛ་  ག་ལ་བ་  དེ  –␣ར་  སོང་  སྟེ་  "
"ལྷགས་  ནས  ། –"
msgstr ""

msgctxt "line 34, 06.txt"
msgid ""
"ཚེ་  དང་  ལྡན་པ་  པིཎྜོ་ ལ་ བ་ ར་ དྷྭ་ ཛ  –␣འི་  རྐང་པ་  ལ་  ཕྱག་  འཚལ་  ཏེ་"
msgstr ""

msgctxt "line 35, 06.txt"
msgid "ཆོས་  མཉན་པ  –␣འི་  ཕྱིར་  མདུན་  དུ་  འཁོད་  དོ  ། – །"
msgstr ""

msgctxt "line 36, 06.txt"
msgid ""
"དེ་ནས་  ཚེ་  དང་  ལྡན་པ་  པིཎྜོ་ ལ་ བ་ ར་ དྷྭ་ ཛ  –␣ས  ། –  དེ་དག་  གི་  "
"བསམ་པ་  དང་  བག་  ལ་  ཉལ་  དང་  ཁམས་  དང་  རང་བཞིན་  རྟོགས་  ནས་"
msgstr ""

msgctxt "line 37, 06.txt"
msgid "དེ་  དང་  འཐུན་པ  –␣འི་  ཆོས་  བསྟན་  ཏེ  ། –"
msgstr ""

msgctxt "line 38, 06.txt"
msgid ""
"ཀཽ་ ཤཱཾ་ བཱི་  ན་  གནས་པ  –␣འི་  སྐྱེ་བོ  –␣འི་  ཚོགས་ འདུ␣ས་པ་  རྣམས་  "
"ཀྱིས་  སྟན་  དེ་ཉིད་  ལ་  འཁོད་  བཞིན་  དུ་  ཁ་ཅིག་  གིས་  ནི་  དྲོ་བ  –␣ར་ "
" གྱུར་པ་  བསྐྱེད་  དོ  ། – །"
msgstr ""

msgctxt "line 39, 06.txt"
msgid ""
"ཁ་ཅིག་  གིས་  ནི་  སྤྱི་བོ་  དང་  བདེན་པ་  དང་  འཐུན་པ  –␣འི་  བཟོད་པ་  དང་ "
" འཇིག་རྟེན་  གྱི་  ཆོས་  ཀྱི་  མཆོག་  དང་  མཐོང་བ  –␣འི་  སྙོམས་པ  –␣ར་  "
"འཇུག་པ་  བསྐྱེད་  དོ  ། – །"
msgstr ""

msgctxt "line 40, 06.txt"
msgid ""
"ཁ་ཅིག་  གིས་  ནི་  རྒྱུན་  དུ་  ཞུགས་པ  –␣འི་  འབྲས་བུ་  མངོན་སུམ་  དུ་  "
"བྱས་  སོ  ། – །"
msgstr ""

msgctxt "line 41, 06.txt"
msgid ""
