msgctxt "line 1, 06.txt"
msgid "འཆར་ཀ་  ཞེས་ བྱ་བ་  ནི  ། –"
msgstr ""

msgctxt "line 2, 06.txt"
msgid "གླེང་ གཞི་  རྒྱལ་པོ␣འི་ ཁབ་  ན་  བཞུགས་  ཏེ་"
msgstr ""

msgctxt "line 3, 06.txt"
msgid ""
"དེ  –␣ར་  གང་  གི་  ཚེ  ། –  ཚེ་  དང་  ལྡན་པ་  པིཎྜོ་ ལ་ བ་ ར་ དྷྭ་ ཛ  –␣ས་ "
" ཉོན་མོངས་པ་  ཐམས་ཅད་  སྤངས་  ནས  ། –"
msgstr ""

msgctxt "line 4, 06.txt"
msgid ""
"དགྲ་བཅོམ་པ་  ཉིད་  མངོན་སུམ་  དུ་  བྱས་པ་  དེ  –␣འི་  ཚེ་  དེ  –␣ས་  བསམས་པ "
" ། –"
msgstr ""

msgctxt "line 5, 06.txt"
msgid ""
"བཅོམ་ལྡན་འདས་  ཀྱིས་  བདག་  གི་  སྡུག་བསྔལ་  དང་  ཡིད་  མི་ བདེ་བ་  རྣམ་པ་  "
"དུ་མ་  ནི་  བསལ  །"
msgstr ""

msgctxt "line 6, 06.txt"
msgid "བདེ་བ་  དང་  ཡིད་  བདེ་བ་  རྣམ་པ་  དུ་མ་  ནི་  བསྒྲུབས  ། –"
msgstr ""

msgctxt "line 7, 06.txt"
msgid "སྡིག་པ་  མི་དགེ་བ  –␣འི་  ཆོས་  རྣམ་པ་  དུ་མ་  ནི་  བསལ  ། –"
msgstr ""

msgctxt "line 8, 06.txt"
msgid "དགེ་བ  –␣འི་  ཆོས་  རྣམ་པ་  དུ་མ་  ནི་  བསྒྲུབས་  ན  ། –"
msgstr ""

msgctxt "line 9, 06.txt"
msgid ""
"བདག་  གིས་  བཅོམ་ལྡན་འདས་  ཀྱི་  བཀའ་དྲིན་  ཇི་ལྟ  –␣ར་  བསབ་པ  –␣ར་  བྱ་  "
"སྙམ་  མོ  ། – །"
msgstr ""

msgctxt "line 10, 06.txt"
msgid "དེ་ནས་  དེ  –␣ས་  བསམས་པ་"
msgstr ""

msgctxt "line 11, 06.txt"
msgid ""
"སངས་རྒྱས་  འཇིག་རྟེན་  དུ་  འབྱུང་བ་  གང་ཡང་  རུང་བ་  དེ་དག་  ཐམས་ཅད་  ནི་  "
"སེམས་ཅན་  ལ་  ཕན་པ  –␣འི་  ཕྱིར་  འབྱུང་བ  –␣ར་  ཟད་  ཀྱིས  ། –"
msgstr ""

msgctxt "line 12, 06.txt"
msgid "མ་ལ་"
msgstr ""

msgctxt "line 13, 06.txt"
msgid "བདག་  གིས་  ཀྱང་  སེམས་ཅན་  ལ་  ཕན་པ  –␣ར་  བྱ་  གོར་མ་ ཆག་  སྙམ་  ནས་"
