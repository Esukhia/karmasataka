msgstr ""

msgctxt "line 9, 16.txt"
msgid "བཅོམ་ལྡན་འདས་ མཐོང་ ནས །"
msgstr ""

msgctxt "line 10, 16.txt"
msgid "བཀྲ་ མི་ ཤི␣ས་ སོ་ སྙམ་ སྟེ་"
msgstr ""

# ཕན་ཚུན་དུ་བྲོས་སོ<«གཡུང་»«པེ་»སྟེ།>། ། 
msgctxt "line 11, 16.txt"
msgid "ཕན་ཚུན་ དུ་ བྲོས་ སོ ། །"
msgstr ""

# བཅོམ་ལྡན་འདས་ཀྱིས་ཀྱང་།
# དེ་ལ་ཐུགས་བརྩེ་བའི་ཕྱིར་དེ་དང་དེ་ཁོ་ན་ན་<«གཡུང་»«པེ་»–ན།>བཞུགས་པར་གྱུར་ཏོ། །
msgctxt "line 12, 16.txt"
msgid ""
"བཅོམ་ལྡན་འདས་ ཀྱིས་ ཀྱང་ །  དེ་ ལ་ ཐུགས་ བརྩེ་བ␣འི་ ཕྱིར་ དེ་ དང་ དེ་ ཁོ་ན་ "
"ན་ བཞུགས་པ␣ར་ གྱུར་ ཏོ ། །"
msgstr ""

# དེ་ནས་རེ་ཞིག་ན་གྲོང་ཁྱེར་གྱི་སྒོ་བཞི་ཆར་དུ་<«གཡུང་»«ལི་»«པེ་»«ཅོ་»–དུ།>ཡང་དེའི་མདུན་ན་བཅོམ་ལྡན་འདས་བཞུགས་པར་གྱུར་ནས་
msgctxt "line 13, 16.txt"
msgid ""
"དེ་ནས་ རེ་ཞིག་ ན་ གྲོང་ཁྱེར་ གྱི་ སྒོ་ བཞི་ཆ␣ར་ དུ་ ཡང་ དེ␣འི་ མདུན་ ན་ "
"བཅོམ་ལྡན་འདས་ བཞུགས་པ␣ར་ གྱུར་ ནས་"
msgstr ""

msgctxt "line 14, 16.txt"
msgid "དེ␣ས་ བསམས་པ་"
msgstr ""

# དགེ་སྦྱོང་<«གཡུང་»སློང་།>གཽ་ཏ་མ་འདི་ནི་རྫུ་འཕྲུལ་ཆེ་ཞིང་
msgctxt "line 15, 16.txt"
msgid "དགེ་སྦྱོང་ གཽ་ ཏ་ མ་ འདི་ ནི་ རྫུ་འཕྲུལ་ ཆེ་ ཞིང་"
msgstr ""

# མཐུ་ཆེ་<«གཡུང་»ཆེད།>བ་ཡིན་ནོ་སྙམ་ནས། 
msgctxt "line 16, 16.txt"
msgid "མཐུ་ ཆེ་བ་ ཡིན་ ནོ་ སྙམ་ ནས །"
msgstr ""

msgctxt "line 17, 16.txt"
msgid "དེ་ བཅོམ་ལྡན་འདས་ ལ་ དགའ་བ་ སྐྱེས་ སོ ། །"
msgstr ""

msgctxt "line 18, 16.txt"
msgid "དེ་ དགའ་བ་ སྐྱེས་ ནས །"
msgstr ""

msgctxt "line 19, 16.txt"
msgid "བཅོམ་ལྡན་འདས་ ཀྱི་ སྟེང་ དུ་ མེ་ཏོག་ སྤར་གང་ གཏོར་ ཏོ ། །"
msgstr ""

msgctxt "line 20, 16.txt"
msgid "དེ␣འི་ སྐབས་ སུ་ བཅོམ་ལྡན་འདས་ ཀྱིས་ འཛུམ་པ་ མཛད་ དེ །"
msgstr ""

# འདི་<«ཁུ་»དེ།>ནི་སངས་རྒྱས་བཅོམ་ལྡན་འདས་རྣམས་གང་གི་ཚེ་འཛུམ་པ་མཛད་པའི་ཆོས་ཉིད་དེ།
msgctxt "line 21, 16.txt"
msgid ""
"འདི་ ནི་ སངས་རྒྱས་ བཅོམ་ལྡན་འདས་ རྣམས་ གང་ གི་ ཚེ་ འཛུམ་པ་ མཛད་པ␣འི་ "
"ཆོས་ཉིད་ དེ །"
msgstr ""

msgctxt "line 22, 16.txt"
msgid ""
"དེ␣འི་ ཚེ་ ཞལ་ ནས་ འོད་ཟེར་ སྔོན་པོ་ དང་ །  སེར་པོ་ དང་ །  དམར་པོ་ དང་ །  "
"དཀར་པོ་ རྣམས་ བྱུང་ ནས་"
msgstr ""

msgctxt "line 23, 16.txt"
msgid "ཁ་ཅིག་ ནི་ འོག་ ཏུ་ འགྲོ␣འོ ། །"
msgstr ""

msgctxt "line 24, 16.txt"
msgid "ཁ་ཅིག་ ནི་ སྟེང་ དུ་ འགྲོ␣འོ ། །"
msgstr ""

# འོག་ཏུ་འགྲོ་བ་གང་ཡིན་པ་དེ་དག་ནི་སེམས་ཅན་དམྱལ་བ་ཡང་སོས་དང་། ཐིག་ནག་དང་།
# བསྡུས་འཇོམས་དང་། ངུ་འབོད་དང་། ངུ་འབོད་ཆེན་པོ་དང་། ཚ་བ་དང་། རབ་ཏུ་ཚ་བ་དང་།
# མནར་མེད་པ་དང་། ཆུ་བུར་ཅན་དང་། ཆུ་བུར་རྡོལ་བ་<«གཡུང་»«པེ་»–བ།>དང་།
# སོ་ཐམས་ཐམས་<«སྣར་»ཐམ་ཐམ།>དང་། ཀྱི་ཧུད་ཟེར་<«གཡུང་»«པེ་»«སྣར་»«ཅོ་»+བ།>དང་།
# ཨ་ཆུ་ཟེར་བ་<«གཡུང་»–བ།>དང་། ཨུད་པལ་<«གཡུང་»དཔལ།>ལྟར་གས་པ་དང་།
# པད་མ་ལྟར་གས་པ་དང་། པད་མ་ལྟར་གས་པ་<«སྣར་»ཕ།>ཆེན་པོར་སོང་ནས།
msgctxt "line 25, 16.txt"
msgid ""
"འོག་ ཏུ་ འགྲོ་བ་ གང་ཡིན་པ་ དེ་དག་ ནི་ སེམས་ཅན་ དམྱལ་བ་ ཡང་སོས་ དང་ །  "
"ཐིག་ནག་ དང་ །  བསྡུས་འཇོམས་ དང་ །  ངུ་འབོད་ དང་ །  ངུ་འབོད་ ཆེན་པོ་ དང་ །  "
"ཚ་བ་ དང་ །  རབ་ཏུ་ ཚ་བ་ དང་ །  མནར་མེད་པ་ དང་ །  ཆུ་བུར་ ཅན་ དང་ །  ཆུ་བུར་ "
"རྡོལ་བ་ དང་ །  སོ་ ཐམས་ ཐམས་ དང་ །  ཀྱི་ཧུད་ ཟེར་ དང་ །  ཨ་ཆུ་ ཟེར་བ་ དང་ ། "
" ཨུད་ པལ་ ལྟར་ གས་པ་ དང་ །  པད་མ་ ལྟར་ གས་པ་ དང་ །  པད་མ་ ལྟར་ གས་པ་ "
"ཆེན་པོ␣ར་ སོང་ ནས །"
msgstr ""

msgctxt "line 26, 16.txt"
msgid "ཚ་བ␣འི་ སེམས་ཅན་ དམྱལ་བ་ གང་ཡིན་པ་ དེ་དག་ ཏུ་ ནི་ བསིལ་བ␣ར་ གྱུར་ ཏེ་"
msgstr ""

msgctxt "line 27, 16.txt"
msgid "འབབ་བོ ། །"
msgstr ""

msgctxt "line 28, 16.txt"
msgid ""
"གྲང་བ␣འི་ སེམས་ཅན་ དམྱལ་བ་ གང་ཡིན་པ་ དེ་དག་ ཏུ་ ནི་ དྲོ་བ␣ར་ གྱུར་ ཏེ་ "
"འབབ་བོ ། །"
msgstr ""

# དེས་ན་<«གཡུང་»དེ་ནས།
# «ལི་»«པེ་»«ཅོ་»དེ་ན།>སེམས་ཅན་དེ་དག་གི་གནོད་པའི་ཁྱད་པར་རྣམས་[56b]རྩེ་ཞིལ་བར་གྱུར་ཏོ<«གཡུང་»«ལི་»«པེ་»«སྣར་»«ཅོ་»«ཞོལ་»འགྱུར་རོ།>།
# །
msgctxt "line 29, 16.txt"
msgid ""
"དེས་ན་ སེམས་ཅན་ དེ་དག་ གི་ གནོད་པ␣འི་ ཁྱད་པར་ རྣམས་ རྩེ་ ཞིལ་ བར་ གྱུར་ ཏོ "
"། །"
msgstr ""

msgctxt "line 30, 16.txt"
msgid "དེ་ནས་ དེ་དག་ འདི་ སྙམ་ དུ་ སེམས་ ཏེ །"
msgstr ""

msgctxt "line 31, 16.txt"
msgid "ཀྱེ་"
msgstr ""

msgctxt "line 32, 16.txt"
msgid "ཅི་ བདག་ཅག་ འདི་ ནས་ འཆི་ འཕོས་ སམ་"
msgstr ""

msgctxt "line 33, 16.txt"
msgid "འོན་ཏེ་ གཞན་ དུ་ སྐྱེས་ སམ་ སྙམ་ མོ ། །"
msgstr ""

msgctxt "line 34, 16.txt"
msgid ""
"དེ་ནས་ དེ་དག་ དད་པ་ བསྐྱེད་པ␣འི་ ཕྱིར་ བཅོམ་ལྡན་འདས་ སྤྲུལ་པ་ འགྱེད་པ␣ར་ "
"མཛད་ དེ །"
msgstr ""

msgctxt "line 35, 16.txt"
msgid "དེ་དག་ གིས་ སྤྲུལ་པ་ མཐོང་ ནས་"
msgstr ""

msgctxt "line 36, 16.txt"
msgid "འདི་ སྙམ་ དུ་ སེམས་ ཏེ །"
msgstr ""

msgctxt "line 37, 16.txt"
msgid "ཀྱེ་"
msgstr ""

msgctxt "line 38, 16.txt"
msgid "བདག་ཅག་ ནི་ འདི་ ནས་ ཀྱང་ ཤི་འཕོས་ པར་མ་ གྱུར་ ལ་"
msgstr ""

msgctxt "line 39, 16.txt"
msgid "གཞན་ དུ་ ཡང་ མ་ སྐྱེས་ མོད་ ཀྱི །"
msgstr ""

# འོན་ཀྱང་སྔོན་མ་བྱུང་བའི་སེམས་ཅན་སྣང་བ་འདིའི་མཐུས་བདག་ཅག་<«གཡུང་»ཅིག>གི་གནོད་པའི་ཁྱད་པར་རྣམས་རྩེ་ཞིལ་བར་གྱུར་ཏོ་སྙམ་མོ།
# །
msgctxt "line 40, 16.txt"
