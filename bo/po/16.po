msgctxt "line 1, 16.txt"
msgid "ཤིང་རྟ་ གསུམ་པ་ ནི །"
msgstr ""

# གླེང་གཞི་<«ཞོལ་»བཞི།>མཉན་དུ་ཡོད་པ་ན་བཞུགས་ཏེ། 
msgctxt "line 2, 16.txt"
msgid "གླེང་ གཞི་ མཉན་ དུ་ཡོད་པ་ ན་ བཞུགས་ ཏེ །"
msgstr ""

msgctxt "line 3, 16.txt"
msgid "བཅོམ་ལྡན་འདས་ སྔ་དྲོ་ ཤམ་ཐབས་ དང་ ཆོས་གོས་ གསོལ་ ཏེ །"
msgstr ""

