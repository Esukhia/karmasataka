msgctxt "line 1, E001.txt"
msgid "﻿འཐབ་མོ་ཞེས་བྱ་བ་ནི། "
msgstr ""

# བམ་པོ་སུམ་ཅུ་<«གཡུང་»«པེ་»བཅུ། «ལི་»བཅུ་རྩ། །
# «ཅོ་»+རྩ།>བདུན་པ་:སྟེ་ཐ་མ<«སྣར་»-སྟེ་ཐ་མ།>།
msgctxt "line 2, E001.txt"
msgid "བམ་པོ་ སུམ་ཅུ་ བདུན་པ་ སྟེ་ ཐ་མ །"
msgstr ""

# འཐབ་<«གཡུང་»«པེ་»«སྣར་»ཐབ།>མོ་ཞེས་བྱ་བ་ནི། 
msgctxt "line 3, E001.txt"
msgid "འཐབ་མོ་ ཞེས་ བྱ་བ་ ནི །"
msgstr ""

msgctxt "line 4, E001.txt"
msgid "གླེང་གཞི་མཉན་དུ་ཡོད་པ་ན་བཞུགས་ཏེ། "
msgstr ""

# དེའི་ཚེ་མཉན་དུ་ཡོད་<«གཡུང་»«པེ་»ཡོན།>པ་ན་སྐྱེ་བོ་ཕལ་པོ་ཆེ་<«གཡུང་»«པེ་»-ཆེ།>འཐབ་པ་དང་།
# :མཚང་འདྲུ་<«གཡུང་»«ལི་»«པེ་»འཚང་འདྲུ། «སྣར་»«ཅོ་»«ཁུ་»མཚང་འབྲུ།>བ་དང་།
# འགྱེད་པ་དང་། རྩོད་<«གཡུང་»«པེ་»བརྩོད།>པ་ལ་ཞུགས་ཤིང་འཁོད་དོ། །
msgctxt "line 5, E001.txt"
msgid ""
"དེ␣འི་ ཚེ་ མཉན་ དུ་ཡོད་པ་ ན་ སྐྱེ་བོ་ ཕལ་པོ་ ཆེ་ འཐབ་པ་ དང་ །  མཚང་ འདྲུ་བ་ "
"དང་ །  འགྱེད་པ་ དང་ །  རྩོད་པ་ ལ་ ཞུགས་ཤིང་ འཁོད་ དོ ། །"
msgstr ""

msgctxt "line 6, E001.txt"
msgid "སངས་རྒྱས་བཅོམ་ལྡན་འདས་བགྲོད་པ་གཅིག་པུའི་ལམ་སྟོན་པ། "
msgstr ""

# རྣམ་པ་གཉིས་ཀྱི་ཤེས་བྱ་དང་། ཡེ་ཤེས་ལ་མངའ་བརྙེས་<«ལི་»«ཅོ་»བསྙེས༑>པ། 
msgctxt "line 7, E001.txt"
msgid "རྣམ་པ་ གཉིས་ ཀྱི་ ཤེས་བྱ་ དང་ །  ཡེ་ཤེས་ ལ་ མངའ་ བརྙེས་པ །"
msgstr ""

msgctxt "line 8, E001.txt"
msgid "མ་འདྲེས་པའི་དྲན་པ་ཉེ་བར་གཞག་པ་གསུམ་ལ་མཁས་པ། "
msgstr ""

msgctxt "line 9, E001.txt"
msgid "མི་འཇིགས་པ་བཞིས་མི་འཇིགས་པ། "
msgstr ""

# འགྲོ་བ་<«སྣར་»«ཞོལ་»-བ།>ལྔར་འགྲོ་བ་ལས་རྣམ་པར་གྲོལ་བ། 
msgctxt "line 10, E001.txt"
msgid "འགྲོ་བ་ ལྔ␣ར་ འགྲོ་བ་ ལ␣ས་ རྣམ་པ␣ར་ གྲོལ་བ །"
msgstr ""

msgctxt "line 11, E001.txt"
msgid "སྐྱེ་མཆེད་དྲུག་ལ་མཁས་པ། "
msgstr ""

msgctxt "line 12, E001.txt"
msgid "བྱང་ཆུབ་ཀྱི་ཡན་ལག་བདུན་ལ་སྤྱོད་ཡུལ་བ། "
msgstr ""

msgctxt "line 13, E001.txt"
msgid "རྣམ་པར་ཐར་པ་བརྒྱད་ལ་བསམ་གཏན་པ། "
msgstr ""

msgctxt "line 14, E001.txt"
msgid "མཐར་གྱིས་གནས་པའི་སྙོམས་པར་འཇུག་པ་དགུ་ལ་སྙོམས་པར་འཇུག་པ། "
msgstr ""

msgctxt "line 15, E001.txt"
msgid "སྟོབས་བཅུའི་སྟོབས་དང་ལྡན་པ། "
msgstr ""

# ཡང་དག་པའི་སེང་གེའི་སྒྲ་ཆེན་པོ་སྒྲོགས་<«ཞོལ་»སྒྲོག>པ་རྣམས་ནི་ཆོས་ཉིད་ཀྱིས་<«སྣར་»ཀྱི།>ཉིན་དུས་གསུམ་མཚན་དུས་གསུམ་སྟེ།
# ཉིན་མཚན་དུས་དྲུག་ཏུ་སངས་རྒྱས་ཀྱི་སྤྱན་གྱིས་འཇིག་རྟེན་ལ་གཟིགས་ཏེ།
msgctxt "line 16, E001.txt"
msgid ""
"ཡང་དག་པ␣འི་ སེང་གེ␣འི་ སྒྲ་ ཆེན་པོ་ སྒྲོགས་པ་ རྣམས་ ནི་ ཆོས་ཉིད་ ཀྱིས་ ཉིན་ "
"དུས་གསུམ་ མཚན་ དུས་གསུམ་ སྟེ །  ཉིན་མཚན་ དུས་ དྲུག་ ཏུ་ སངས་རྒྱས་ ཀྱི་ སྤྱན་"
" གྱིས་ འཇིག་རྟེན་ ལ་ གཟིགས་ ཏེ །"
msgstr ""

msgctxt "line 17, E001.txt"
msgid "སུ་ནི་རྒུད། "
msgstr ""

msgctxt "line 18, E001.txt"
msgid "སུ་ནི་དར། "
msgstr ""

# སུ་ནི་ཕོངས་<«ལི་»«ཅོ་»འཕོངས།>ཤིང་འདུག །
msgctxt "line 19, E001.txt"
msgid "སུ་ ནི་ ཕོངས་ ཤིང་ འདུག  །"
msgstr ""

# སུ་ནི་ཉམ་<«ལི་»«ཅོ་»ཉམས།>ང་བར་གྱུར<«ལི་»«ཅོ་»འགྱུར།>། 
msgctxt "line 20, E001.txt"
msgid "སུ་ ནི་ ཉམ་ང་ བར་ གྱུར །"
msgstr ""

msgctxt "line 21, E001.txt"
msgid "སུ་ནི་གནོད་པ་དང་ལྡན། "
msgstr ""

# སུ་ནི་ཕོངས་<«ལི་»«ཅོ་»འཕོངས།>པ་དང་ཉམ་<«ལི་»«སྣར་»ཉམས།>ང་བ་དང་གནོད་པ་དང་ལྡན། 
msgctxt "line 22, E001.txt"
msgid "སུ་ ནི་ ཕོངས་པ་ དང་ ཉམ་ང་བ་ དང་ གནོད་པ་ དང་ ལྡན །"
msgstr ""

msgctxt "line 23, E001.txt"
msgid "སུ་ནི་ངན་སོང་དུ་གཞོལ། "
msgstr ""

msgctxt "line 24, E001.txt"
msgid "སུ་ནི་ངན་སོང་དུ་འབབ། "
msgstr ""

msgctxt "line 25, E001.txt"
msgid "སུ་ནི་ངན་སོང་དུ་བབ། "
msgstr ""

# སུ་ནི་ངས་<«ལི་»«ཅོ་»-ངས།>ངན་སོང་ནས་ཕྱུང་སྟེ<«ལི་»«ཅོ་»ཏེ།>། 
msgctxt "line 26, E001.txt"
msgid "སུ་ ནི་ ངས་ ངན་སོང་ ནས་ ཕྱུང་ སྟེ །"
msgstr ""

msgctxt "line 27, E001.txt"
msgid "མཐོ་རིས་དང་ཐར་པ་དང་འབྲས་བུ་ལ་གཞག་པར་བྱ། "
msgstr ""

msgctxt "line 28, E001.txt"
msgid "སུ་ནི་ཉེས་པར་སྤྱོད་པའི་འདམ་དུ་བྱིང་བ་ལས་ལག་ནས་དྲང་བར་བྱ། "
msgstr ""

msgctxt "line 29, E001.txt"
msgid "སུ་ནི་འཕགས་པའི་ནོར་བདུན་མེད་པ་ལས། "
msgstr ""

msgctxt "line 30, E001.txt"
msgid "འཕགས་པའི་ནོར་བདུན་གྱི་དབང་ཕྱུག་ལ་དབང་བྱེད་དུ་གཞུག་པར་བྱ། "
msgstr ""

# སུ་ནི་དགེ་བའི་རྩ་བ་རྣམས་མ་བསྐྱེད་<«གཡུང་»«པེ་»«སྣར་»སྐྱེད།
# «ལི་»«ཅོ་»སྐྱེས།>པ་ལས་
msgctxt "line 31, E001.txt"
msgid "སུ་ ནི་ དགེ་བ␣འི་ རྩ་བ་ རྣམས་ མ་ བསྐྱེད་པ་ ལ␣ས་"
msgstr ""

msgctxt "line 32, E001.txt"
msgid "བསྐྱེད་པར་བྱ། "
msgstr ""

# སུ་ནི་དགེ་བའི་རྩ་བ་རྣམས་<«ལི་»«ཅོ་»-རྣམས།>བསྐྱེད་ཟིན་པ་ལས་
msgctxt "line 33, E001.txt"
msgid "སུ་ ནི་ དགེ་བ␣འི་ རྩ་བ་ རྣམས་ བསྐྱེད་ ཟིན་པ་ ལ␣ས་"
msgstr ""

msgctxt "line 34, E001.txt"
msgid "ཡོངས་སུ་སྨིན་པར་བྱ། "
msgstr ""

msgctxt "line 35, E001.txt"
msgid "སུ་ནི་དགེ་བའི་རྩ་བ་རྣམས་ཡོངས་སུ་སྨིན་ཟིན་པ་ལས་"
msgstr ""

# ཡེ་ཤེས་:ཀྱི་མཚོན་<«ལི་»ཀྱིས་ཚོན། «ཅོ་»ཀྱི་པ་ཚོན།>གྱིས་བརྟོལ་བར་བྱ། 
msgctxt "line 36, E001.txt"
msgid "ཡེ་ཤེས་ ཀྱི་ མཚོན་ གྱིས་ བརྟོལ་བ␣ར་ བྱ །"
msgstr ""

# སུ་ལ་ནི་སངས་རྒྱས་བྱུང་<«གཡུང་»«པེ་»འབྱུང༌།>བས་བརྒྱན་པའི་[116b]འཇིག་རྟེན་འབྲས་བུ་ཡོད་པར་བྱ་སྙམ་དུ་
msgctxt "line 37, E001.txt"
msgid ""
"སུ་ ལ་ ནི་ སངས་རྒྱས་ བྱུང་བ␣ས་ བརྒྱན་པ␣འི་ འཇིག་རྟེན་ འབྲས་བུ་ ཡོད་པ␣ར་ བྱ་ "
"སྙམ་ དུ་"
msgstr ""

# ཡེ་ཤེས་གཟིགས་<«གཡུང་»«པེ་»+ལ།>པ་འཇུག་གོ། །
msgctxt "line 38, E001.txt"
msgid "ཡེ་ཤེས་ གཟིགས་པ་ འཇུག་ གོ ། །"
msgstr ""

# རྒྱ་མཚོ་ཆུ་སྲིན་རྣམས་ཀྱི་<«གཡུང་»ཀྱིས།>གནས། །
msgctxt "line 39, E001.txt"
msgid "རྒྱ་མཚོ་ ཆུ་སྲིན་ རྣམས་ ཀྱི་ གནས ། །"
msgstr ""

msgctxt "line 40, E001.txt"
msgid "དུས་རླབས་ཡོལ་བར་འགྱུར་ཡང་སྲིད། །"
msgstr ""

msgctxt "line 41, E001.txt"
msgid "གདུལ་བར་བྱ་བའི་སྲས་རྣམས་ལ། །སངས་རྒྱས་དུས་ལས་ཡོལ་བ་མེད། །"
msgstr ""

msgctxt "line 42, E001.txt"
msgid "དེ་ནས་བཅོམ་ལྡན་འདས་ཀྱིས་དགོངས་པ། "
msgstr ""

# སྐྱེ་བོ་ཕལ་པོ་<«སྣར་»«ཞོལ་»-པོ།>ཆེ་གདུལ་བའི་དུས་ལ་བབ་སྟེ། 
msgctxt "line 43, E001.txt"
msgid "སྐྱེ་བོ་ ཕལ་པོ་ ཆེ་ གདུལ་བ␣འི་ དུས་ ལ་ བབ་ སྟེ །"
msgstr ""

