msgctxt "fce9551f263945369c1235b5a6402910"
msgid ""
"དེ་སྟེ་ སྟེང་ གི་ ནམ་མཁའ་ ལ་ འགྲོ་ ན་ ནི་ དེ་ ལ་ དེ་ཉིད་ ནས་ བྱ་རོག་ དང་ "
"བྱ་རྒོད་ དང་ ཆུན་ལག་ ལྕགས་ ཀྱི་ མཆུ་ཅན་ དག་ འབྱུང་བ␣ར་ འགྱུར་ ཏེ །"
msgstr ""

msgctxt "c2ab1170f0c046d4aae76b3d9ed3c7f7"
msgid "དེ་དག་ གིས་ ཀྱང་ དེ␣འི་ ཤ་ རྣམས་ བཅད་ ཅིང་ བཅད་ ཅིང་"
msgstr ""

msgctxt "d66c258687ae412fb1732fadf269e07d"
msgid "ཟ␣འོ ། །"
msgstr ""

# དེ་སྟེ་ནགས་སྟུག་<«ལི་»«ཁུ་»སྡུག>པོའི་ནང་དུ་འགྲོ་ན་ནི་དེ་ལ་དེ་ཉིད་ནས་ཤིང་ཤལ་<«གཡུང་»ཤམ།>མ་ལིའི་ཚལ་འབྱུང་བར་འགྱུར་ཏེ།
msgctxt "dd0adc220d1f4795aa525f6c2ca926b9"
msgid ""
"དེ་སྟེ་ ནགས་ སྟུག་པོ␣འི་ ནང་ དུ་ འགྲོ་ ན་ ནི་ དེ་ ལ་ དེ་ཉིད་ ནས་ ཤིང་ "
"ཤལ་མ་ལི␣འི་ ཚལ་ འབྱུང་བ␣ར་ འགྱུར་ ཏེ །"
msgstr ""

# དེ་ལ་དེ་ཉིད་དུ་རལ་གྲི་དང་། མདུང་ཐུང་དང་། མདའ་བོ་ཆེ་དང་།
# རྡོ་རྗེ་རྩེ་གཅིག་པ་དང་། མདུང་ཁ་ལེབ་རྣམས་རླུང་གིས་སྤགས་<«གཡུང་»བླགས།>ཏེ་
msgctxt "276147c574b949bc9d66395ff339d7ff"
msgid ""
"དེ་ ལ་ དེ་ཉིད་ དུ་ རལ་གྲི་ དང་ །  མདུང་ཐུང་ དང་ །  མདའ་བོ་ ཆེ་ དང་ །  "
"རྡོ་རྗེ་ རྩེ་གཅིག་པ་ དང་ །  མདུང་ ཁ་ལེབ་ རྣམས་ རླུང་ གིས་ སྤགས་ ཏེ་"
msgstr ""

msgctxt "846baaaac599488faaf63e71374cc98b"
msgid "དེ␣འི་ སྟེང་ དུ་ འབབ་བོ ། །"
msgstr ""

# དེ་སྟེ་རྩིག་པའི་གསེབ་[61a]བམ་<«པེ་»+ར།>རིའི་སུལ་ལམ་བྱ་སྐྱིབས་དག་ཏུ་འགྲོ་བར་འདོད་ན་ཡང་དེ་ལ་དེ་<«ཅོ་»འདི།>ཉིད་ནས་ལས་ལས་སྐྱེས་པའི་མི་དག་འབྱུང་བར་འགྱུར་ཏེ།
msgctxt "4f2c2e4e05684e1fa6d4565834caa143"
msgid ""
"དེ་སྟེ་ རྩིག་པ␣འི་ གསེབ་ བམ་ རི␣འི་ སུལ་ལམ་ བྱ་སྐྱིབས་ དག་ ཏུ་ འགྲོ་བ␣ར་ "
"འདོད་ ན་ ཡང་ དེ་ ལ་ དེ་ཉིད་ ནས་ ལ␣ས་ ལ␣ས་ སྐྱེས་པ␣འི་ མི་ དག་ འབྱུང་བ␣ར་ "
"འགྱུར་ ཏེ །"
msgstr ""

msgctxt "828b5ccab9e448cba583cb66d25c9e83"
msgid ""
"རལ་གྲི་ དང་ །  མདུང་ཐུང་ དང་ །  མདའ་བོ་ ཆེ་ དང་ །  རྡོ་རྗེ་ རྩེ་གཅིག་པ་ དང་ "
"།  མདུང་ ཁ་ལེབ་ རྣམས་ ཐོགས་ ནས་"
msgstr ""

# དེའི་ལུས་ལ་འདེབས་ཏེ<«ཅོ་»«ཞོལ་»དེ།>།
msgctxt "17481065d41848cfa7ca7a7861cc2abf"
msgid "དེ␣འི་ ལུས་ ལ་ འདེབས་ ཏེ །"
msgstr ""

# དེ་དག་གིས་དེའི་ལུས་<«གཡུང་»«ལི་»«པེ་»«སྣར་»«ཅོ་»+ལ།>ཐམས་ཅད་བཅད་<«ཞོལ་»གཅོད།>ཅིང་གཏུབ་པར་བྱེད་དེ།
msgctxt "2c7fc3b5b8614ebba317618d1131aa30"
msgid "དེ་དག་ གིས་ དེ␣འི་ ལུས་ ཐམས་ཅད་ བཅད་ ཅིང་ གཏུབ་པ␣ར་ བྱེད་ དེ །"
msgstr ""

# རྒྱུ་དེས་ན་སྡུག་བསྔལ་དྲག་པ་དང་། མི་བཟད་<«གཡུང་»«པེ་»ཟད།>པ་དང་། ཚ་བ་དང་།
# ཡིད་དུ་མི་འོང་བའི་ཚོར་བ་མྱོང་སྟེ་
msgctxt "cb0c1136a6004ab29b25d52765394b6c"
msgid ""
"རྒྱུ་ དེས་ན་ སྡུག་བསྔལ་ དྲག་པ་ དང་ །  མི་བཟད་པ་ དང་ །  ཚ་བ་ དང་ །  ཡིད་ དུ་ "
"མི་ འོང་བ␣འི་ ཚོར་བ་ མྱོང་ སྟེ་"
msgstr ""

# ཉམ་ཐག་པའི་སྐད་འབྱིན་ཅིང་<«གཡུང་»«པེ་»–ཅིང་།>
msgctxt "bbed7c45ad224680962db7f0247cca73"
msgid "ཉམ་ཐག་པ␣འི་ སྐད་ འབྱིན་ ཅིང་"
msgstr ""

msgctxt "37f9fb8a77a14b89ba05dadf70af7f8d"
msgid "ངུའོ ། །"
msgstr ""

# དེ་ནས་དེའི་ཀུ་ཅོའི་<«ཁུ་»ཅོའོ།>སྒྲས་མཉན་དུ་ཡོད་པ་ཐམས་ཅད་གང་བར་གྱུར་ཏོ། །
msgctxt "3247d669ccc44a4ab5a8e0b4d4053973"
msgid ""
"དེ་ནས་ དེ␣འི་ ཀུ་ཅོ␣འི་ སྒྲ␣ས་ མཉན་ དུ་ཡོད་པ་ ཐམས་ཅད་ གང་བ␣ར་ གྱུར་ ཏོ ། །"
msgstr ""

msgctxt "ae337062ea41446db5e9082b71e61e41"
msgid ""
"མཉན་ དུ་ཡོད་པ␣འི་ སྐྱེ་བོ་ ཕལ་པོ་ ཆེས་ འཇིགས་ སུ་ རུང་བ␣འི་ སྒྲ་ དེ་ལྟ་ བུ་ "
"ཐོས་ སོ ། །"
msgstr ""

msgctxt "0d430ea4b5b44ef2a7d01c2b43228fc0"
msgid "ཐོས་ ནས་ ཀྱང་"
msgstr ""

msgctxt "5d367de60a984d89b377826fdfb53635"
msgid "སེམས་ཅན་ སྟོང་ཕྲག་ དུ་མ་ མཉན་ དུ་ཡོད་པ་ ནས་ བྱུང་ སྟེ །"
msgstr ""

# སྒྲ་གྲག་<«ལི་»«སྣར་»«ཅོ་»«ཞོལ་»དྲག>པའི་ལོགས་སུ་ཆུ་ཀླུང་ཁང་<«གཡུང་»+པ།
# «པེ་»+ཕ།>ལྡན་ག་ལ་བ་དེར་དོང་ནས་
msgctxt "db28b36b51e74aea818c0b985c1d09a5"
msgid "སྒྲ་ གྲག་པ␣འི་ ལོགས་ སུ་ ཆུ་ ཀླུང་ཁང་ ལྡན་ ག་ལ་བ་ དེ␣ར་ དོང་ ནས་"
msgstr ""

# སྐྱེ་བོ་ཕལ་པོ་ཆེ་<«གཡུང་»–ཆེ།>དེ་དག་གིས།
# ཐག་རིང་པོ་ཞིག་ནས་<«གཡུང་»«ལི་»«པེ་»«སྣར་»«ཅོ་»«ཞོལ་»གིས།>སེམས་ཅན་དེ་དེ་ལྟ་བུའི་གནོད་པའི་སྡུག་བསྔལ་ཉམས་སུ་མྱོང་བ་མཐོང་ངོ་།
# །
msgctxt "f0e4868bea424b029d1225886651aaba"
msgid ""
"སྐྱེ་བོ་ ཕལ་པོ་ ཆེ་ དེ་དག་ གིས །  ཐག་ རིང་པོ་ ཞིག་ ནས་ སེམས་ཅན་ དེ་དེ་ "
"ལྟ་བུ␣འི་ གནོད་པ␣འི་ སྡུག་བསྔལ་ ཉམས་ སུ་ མྱོང་བ་ མཐོང་ ངོ་ ། །"
msgstr ""

msgctxt "580ca624ad96441fbf0099c52bb471fc"
msgid "མཐོང་ ནས་ ཀྱང་"
msgstr ""

msgctxt "f7d312978abe4792ad42b50333bc230a"
msgid "དེ་དག་ གིས་ བསམས་པ །"
msgstr ""

# སེམས་ཅན་འདི་ལྟ་བུ་<«གཡུང་»«པེ་»བུའི།>གནོད་པའི་སྡུག་བསྔལ་འདི་འདྲ་བ་ཉམས་སུ་མྱོང་བ་འདི་སུ་ཞིག་སྙམ་མོ།
# །
msgctxt "5fdaa5e5b11b4a38a7ea69bcdec53436"
msgid ""
"སེམས་ཅན་ འདི་ ལྟ་བུ་ གནོད་པ␣འི་ སྡུག་བསྔལ་ འདི་འདྲ་བ་ ཉམས་ སུ་ མྱོང་བ་ འདི་ "
"སུ་ཞིག་ སྙམ་ མོ ། །"
msgstr ""

# སངས་རྒྱས་བཅོམ་ལྡན་འདས་བཞུགས་ཤིང་གཞེས་<«ཁུ་»བཞེས།>ལ་འཚོ་བ་རྣམས་ནི་ཆོས་ཉིད་ཀྱིས་དུས་དུས་སུ།
