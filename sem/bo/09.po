msgctxt "adf47f9ba1b04e648db1a6f1946ff9a8"
msgid ""
"ཚེ་ གཅིག་ ཏུ་ སྐྱབས་ སུ་ འགྲོ་བ་ དང་ བསླབ་པ␣འི་ གཞི་ རྣམས་ ཀྱང་ བསྲུངས་ ནས །"
msgstr ""

msgctxt "5bda0b4f7dbe4e91a04c4595789be80b"
msgid "ལྷ␣འི་ ནང་ དུ་ སྐྱེས་ སོ ། །"
msgstr ""

msgctxt "427e1ff389394d30a57de9c7133b4c19"
msgid "དེ␣ར་ སྐྱེས་ ནས་"
msgstr ""

