msgctxt "6009891f17d84af4adb902c8d935cb1b"
msgid "དེ་ མཉེས་པ␣ར་ བྱེད་པ␣ར་ གྱུར་ཅིག  །"
msgstr ""

msgctxt "5a8a289e72e24054ab8e95535af2f01a"
msgid "མི་ མཉེས་པ␣ར་ བྱེད་པ␣ར་ མ་ གྱུར་ཅིག་ ཅེས་"
msgstr ""

msgctxt "766c6da6223c49b3ba7b585a90c3ccef"
msgid "བྱས་ སོ ། །"
msgstr ""

# བུ་མོ་ཅིག་<«གཡུང་»«པེ་»མོ་གཅིག
# «ལི་»«སྣར་»«ཅོ་»«ཞོལ་»བུ་མོ་གཅིག>ཤོས་ཀྱིས་སྨོན་ལམ་བཏབ་པ།
msgctxt "015ed2861b284e00a0e33c992010af32"
msgid "བུ་མོ་ ཅིག་ཤོས་ ཀྱིས་ སྨོན་ལམ་ བཏབ་པ །"
msgstr ""

msgctxt "d4501d0c127b415d91eac4038875cdd3"
msgid ""
"བདག་ ཀྱང་ གང་དང་གང་ དུ་ སྐྱེ་བ་ དེ་ དང་ དེ␣ར་ ཁྱོད་ དང་ སྤུན་ དུ་ གྱུར་ཅིག "
" །"
msgstr ""

# བདག་འདི་ལྟ་བུའི་སྐྱེས་བུ་རིན་<«གཡུང་»ཆིན།>པོ་ཆེ་དེ་<«གཡུང་»«ལི་»«པེ་»«ཅོ་»–དེ།>འཚོ་བར་གྱུར་ཅིག་ཅེས་
msgctxt "b300ecfdf6b54f93b7b3cd466c0da302"
msgid "བདག་ འདི་ ལྟ་བུ␣འི་ སྐྱེས་བུ་ རིན་པོ་ཆེ་ དེ་ འཚོ་བ␣ར་ གྱུར་ཅིག་ ཅེས་"
msgstr ""

msgctxt "dd21b10304224c519dfcfe6ba090b432"
msgid "བྱས་ སོ ། །"
msgstr ""

msgctxt "24929bba167443e4b52edb570c701408"
msgid ""
"དེ་ སྤུན་ གཉིས་ ཀྱི་ སྐྲ་མཁན་ བྱེད་པ␣འི་ བུ་མོ་ དེ་ཡང་ དེ་ ན་ འདུག་པ␣ར་ "
"གྱུར་ ཏེ །"
msgstr ""

msgctxt "798e6a441012476abd77f89682c03a13"
msgid ""
"བུ་མོ་ དེ␣ས་ ཀྱང་ དེ་ གཉིས་ ཀྱི་ སྨོན་ལམ་ དེ་ལྟ་ བུ་ དེ་ བཏབ་པ་ ཐོས་ ནས །"
msgstr ""

# ཐོས་མ་ཐག་<«ཞོལ་»ཐམ>ཏུ་
msgctxt "bbd94b6e819e42b7ba4f54d1fdfa58c1"
msgid "ཐོས་ མ་ ཐག་ ཏུ་"
msgstr ""

# དེས་ཀྱང་ཡང་དག་པར་རྫོགས་པའི་སངས་རྒྱས་རྣམ་པར་གཟིགས་ལ་<«ལི་»«ཅོ་»–ལ།>བཀུར་སྟི་བྱས་ཏེ།
msgctxt "e5eca050b9004a8ebde98851e57ad6d5"
msgid ""
"དེ␣ས་ ཀྱང་ ཡང་དག་པ␣ར་ རྫོགས་པ␣འི་ སངས་རྒྱས་ རྣམ་པ␣ར་ གཟིགས་ ལ་ བཀུར་སྟི་ "
"བྱས་ ཏེ །"
msgstr ""

msgctxt "e91b29364c7c4c30a734b9d8264af69f"
msgid "སྨོན་ལམ་ བཏབ་པ་"
msgstr ""

msgctxt "117a23561ee846ecb11c6c29a263cc3b"
msgid "ཀྱེ་མ་"
msgstr ""

msgctxt "68202b05db3949da854fc0357ea63a19"
msgid ""
"བདག་ ཀྱང་ དགེ་བ␣འི་ རྩ་བ་ འདི␣ས །  འདི་ གཉིས་ གང་དང་གང་ དུ་ སྐྱེས་པ་ དེ་ དང་"
" དེ␣ར་ བདག་ འདི་ གཉིས་ ཀྱི་ སྐྲ་མཁན་ མ་ བསྙེན་བཀུར་ བྱེད་པ␣ར་ གྱུར་ ནས །"
msgstr ""

# འདི་གཉིས་ཀྱི་<«ལི་»«སྣར་»«ཅོ་»«ཞོལ་»–ཀྱི།>བུ་རིན་པོ་ཆེ་དེ་<«ཞོལ་»–དེ།>མཉེས་པར་བྱེད་པར་གྱུར་ཅིག་
msgctxt "8f55056600e54c47b3522daf50fccd17"
msgid "འདི་ གཉིས་ ཀྱི་ བུ་ རིན་པོ་ཆེ་ དེ་ མཉེས་པ␣ར་ བྱེད་པ␣ར་ གྱུར་ཅིག་"
msgstr ""

# མི་མཉེས་པར་<«ལི་»«ཅོ་»པ།>བྱེད་པར་མ་གྱུར་ཅིག་ཅེས་
msgctxt "708006cc1ca945eba5bed1c639d45f7e"
msgid "མི་ མཉེས་པ␣ར་ བྱེད་པ␣ར་ མ་ གྱུར་ཅིག་ ཅེས་"
msgstr ""

msgctxt "d9561a9e88f0401ca0c06c9321c37688"
msgid "བྱས་ ཏེ་"
msgstr ""

msgctxt "036538c69598475c8bb4e9349273c6b5"
msgid "དེ␣ར་ འདི␣ས་ སྨོན་ལམ་ འདི་ ལྟ་བུ་ བཏབ་བོ ། །"
msgstr ""

msgctxt "a640d6fc5529475d8c0533087a53d39d"
msgid "བཙུན་པ་"
msgstr ""

msgctxt "000c8e34787043edbf39a09f9dc7052f"
msgid "ལ␣ས་ ཅི་ བགྱིས་ ན །"
msgstr ""

msgctxt "5f93d4e008c548dba7506e5d8438a060"
msgid "འདི␣ས་ བཅོམ་ལྡན་འདས་ ཀྱང་ མཉེས་པ␣ར་ བགྱིས་ ཏེ་"
msgstr ""

msgctxt "d8965098e69948dea2379613b1b333aa"
msgid "མི་ མཉེས་པ␣ར་ མ་ བགྱིས་ ཏེ །"
msgstr ""

# བཅོམ་ལྡན་འདས་ཀྱི་<«ལི་»«སྣར་»«ཅོ་»ཀྱིས།>བསྟན་པ་ལ་རབ་ཏུ་བྱུང་ནས།
msgctxt "369601334bf3478a928c425e243e336f"
msgid "བཅོམ་ལྡན་འདས་ ཀྱི་ བསྟན་པ་ ལ་ རབ་ཏུ་ བྱུང་ ནས །"
msgstr ""

msgctxt "6f60306790cf4073a7f163952bce6268"
msgid "ཉོན་མོངས་པ་ ཐམས་ཅད་ སྤངས་ ཏེ །"
msgstr ""

# དགྲ་བཅོམ་པ་ཉིད་<«གཡུང་»–ཉིད།>མངོན་སུམ་དུ་བགྱིས་ནས།
msgctxt "2b1c204862ae48aca21603ebb891027b"
msgid "དགྲ་བཅོམ་པ་ ཉིད་ མངོན་སུམ་ དུ་ བགྱིས་ ནས །"
msgstr ""

msgctxt "c02939527f4f4888acb0e9bfa4107e53"
msgid ""
"བཅོམ་ལྡན་འདས་ ཀྱིས་ བརྩོན་འགྲུས་ བརྩམས་པ་ རྣམས་ ཀྱི་ མཆོག་ ཏུ་ ཡང་ བསྟན་ ལགས"
" །"
msgstr ""

msgctxt "37025119509642c3900f4e670f329c29"
msgid "བཅོམ་ལྡན་འདས་ ཀྱིས་ བཀའ་ སྩལ་པ །"
msgstr ""

msgctxt "673911d901024eecbf4674915bf29f8f"
msgid ""
"འདི་ ཡང་དག་པ␣ར་ རྫོགས་པ␣འི་ སངས་རྒྱས་ འོད་སྲུང་ གི་ གསུང་རབ་ ལ་ རབ་ཏུ་ བྱུང་"
" སྟེ །"
msgstr ""

# གང་ལས་རབ་ཏུ་བྱུང་བའི་མཁན་མོ་<«གཡུང་»«ལི་»«པེ་»«སྣར་»«ཅོ་»པོ།>དེ་ནི་ཡང་དག་པར་རྫོགས་པའི་སངས་རྒྱས་འོད་སྲུང་གིས་བརྩོན་འགྲུས་བརྩམས་པ་རྣམས་ཀྱི་མཆོག་ཏུ་བསྟན་ཏོ།
# །
msgctxt "11cdde756098494d97dacfbc0760e719"
msgid ""
"གང་ ལ␣ས་ རབ་ཏུ་ བྱུང་བ␣འི་ མཁན་མོ་ དེ་ ནི་ ཡང་དག་པ␣ར་ རྫོགས་པ␣འི་ སངས་རྒྱས་ "
"འོད་སྲུང་ གིས་ བརྩོན་འགྲུས་ བརྩམས་པ་ རྣམས་ ཀྱི་ མཆོག་ ཏུ་ བསྟན་ ཏོ ། །"
msgstr ""

# དེ་ནས་དེས་<«ཞོལ་»དེའི།>ཚེ་གཅིག་ཏུ་ཚངས་པར་སྤྱོད་པ་སྤྱད་ནས།
msgctxt "153f7032f63d49f0a0ddb51e3bc440ea"
msgid "དེ་ནས་ དེ␣ས་ ཚེ་ གཅིག་ ཏུ་ ཚངས་པ␣ར་ སྤྱོད་པ་ སྤྱད་ ནས །"
msgstr ""

# འཆི་ཀར་<«ཞོལ་»ཁར།>སྨོན་ལམ་བཏབ་པ།
msgctxt "4d8913f28ade42ee9236e3a5ff3b8923"
msgid "འཆི་ཀ␣ར་ སྨོན་ལམ་ བཏབ་པ །"
msgstr ""

# འདི་ལྟར་བདག་གིས་<«ཞོལ་»ཉིད།>ཡང་དག་པར་རྫོགས་པའི་<«ཅོ་»–པའི།>སངས་རྒྱས་འོད་སྲུང་གི་གསུང་<«ཅོ་»སུང་།>རབ་ལ་ཚངས་པར་སྤྱོད་པ་སྤྱད་ཀྱང་།
# ཡོན་ཏན་གྱི་ཚོགས་ཅི་<«གཡུང་»«པེ་»–ཅི།>ཡང་མ་ཐོབ་ཀྱིས་<«ཞོལ་»ཀྱི།>
msgctxt "b675593c4e1f41e098a9c7f05ac51a2b"
msgid ""
"འདི་ལྟར་ བདག་ གིས་ ཡང་དག་པ␣ར་ རྫོགས་པ␣འི་ སངས་རྒྱས་ འོད་སྲུང་ གི་ གསུང་རབ་ "
"ལ་ ཚངས་པ␣ར་ སྤྱོད་པ་ སྤྱད་ ཀྱང་ །  ཡོན་ཏན་ གྱི་ ཚོགས་ ཅི་ཡང་ མ་ ཐོབ་ ཀྱིས་"
msgstr ""

# ཡང་དག་པར་རྫོགས་པའི་སངས་རྒྱས་འོད་སྲུང་གིས་བྲམ་ཟེའི་ཁྱེའུ་བླ་མ་ལུང་བསྟན་པ་གང་ཡིན་པའི་སངས་རྒྱས་བཅོམ་ལྡན་འདས་དེ་བདག་གིས་མཉེས་པར་<«གཡུང་»«པེ་»པས།>བྱེད་པར་གྱུར་ཅིག
# །
msgctxt "7b27491e795940deaabba5a9b3e7cc22"
msgid ""
"ཡང་དག་པ␣ར་ རྫོགས་པ␣འི་ སངས་རྒྱས་ འོད་སྲུང་ གིས་ བྲམ་ཟེ␣འི་ ཁྱེའུ་ བླ་མ་ "
"ལུང་བསྟན་པ་ གང་ཡིན་པ␣འི་ སངས་རྒྱས་ བཅོམ་ལྡན་འདས་ དེ་ བདག་ གིས་ མཉེས་པ␣ར་ "
"བྱེད་པ␣ར་ གྱུར་ཅིག  །"
msgstr ""

msgctxt "2be5df0ee2634378a76dd1f4ed5e74f5"
msgid "མི་ མཉེས་པ␣ར་ བྱེད་པ␣ར་ མ་ གྱུར་ཅིག  །"
msgstr ""

msgctxt "7918228a49d948a48924d90f453a2f25"
msgid "དེ་ ཁོ་ན␣འི་ བསྟན་པ་ ལ་ རབ་ཏུ་ བྱུང་ ནས་"
msgstr ""

msgctxt "3d2e18e80f1f4fbf9a42ff5936a9deec"
msgid "ཉོན་མོངས་པ་ ཐམས་ཅད་ སྤངས་ ཏེ །"
msgstr ""

msgctxt "062ae6fcb3774eaa80969c6ecc678745"
msgid "དགྲ་བཅོམ་པ་ ཉིད་ མངོན་སུམ་ དུ་ བྱེད་པ␣ར་ གྱུར་ཅིག  །"
msgstr ""

# ཇི་ལྟར་བདག་གིས་<«ཁུ་»«ཞོལ་»གི།>མཁན་མོ་<«གཡུང་»«པེ་»པོ།>ཡང་དག་པར་རྫོགས་པའི་སངས་རྒྱས་འོད་སྲུང་གིས་བརྩོན་འགྲུས་བརྩམས་པ་རྣམས་ཀྱི་མཆོག་ཏུ་བསྟན་པ་དེ་བཞིན་དུ་བདག་ཀྱང་ཤཱཀྱ་ཐུབ་པ།
# ཤཱཀྱའི་རྒྱལ་པོ་དེས་བརྩོན་འགྲུས་བརྩམས་པ་རྣམས་ཀྱི་མཆོག་ཏུ་བསྟན་པར་གྱུར་ཅིག་ཅེས་
msgctxt "01799d375a374947bd165423c81581b4"
msgid ""
"ཇི་ལྟར་ བདག་ གིས་ མཁན་མོ་ ཡང་དག་པ␣ར་ རྫོགས་པ␣འི་ སངས་རྒྱས་ འོད་སྲུང་ གིས་ "
"བརྩོན་འགྲུས་ བརྩམས་པ་ རྣམས་ ཀྱི་ མཆོག་ ཏུ་ བསྟན་པ་ དེ་བཞིན་ དུ་ བདག་ ཀྱང་ "
"ཤཱཀྱ་ ཐུབ་པ །  ཤཱཀྱའི་ རྒྱལ་པོ་ དེ␣ས་ བརྩོན་འགྲུས་ བརྩམས་པ་ རྣམས་ ཀྱི་ མཆོག་"
" ཏུ་ བསྟན་པ␣ར་ གྱུར་ཅིག་ ཅེས་"
msgstr ""

msgctxt "3c2b4363ad444526918cc16d428fbf32"
msgid "བྱས་ སོ ། །"
msgstr ""

msgctxt "56566cd278714570a95f60a80d2010a2"
msgid "དགེ་སློང་ དག་"
msgstr ""

msgctxt "dee44a170de74430b56aa04697cda0af"
msgid "ཇི་སྙམ་ དུ་ སེམས །"
msgstr ""

msgctxt "5493327610ef465080f07b663a0f9849"
msgid ""
"དེ␣འི་ ཚེ་ ཡང་དག་པ␣ར་ རྫོགས་པ␣འི་ སངས་རྒྱས་ འོད་སྲུང་ གི་ བསྟན་པ་ ལ་ རབ་ཏུ་ "
"བྱུང་བ␣འི་ དགེ་སློང་ མ་ གང་ཡིན་པ་ དེ་ ནི་ སྐྲ་ ལྡན་ མ་ འདི་ཉིད་ ཡིན་ ཏེ །"
msgstr ""

# དེས་དེར་ཚེ་གཅིག་<«གཡུང་»«པེ་»ཅིག>ཏུ་ཚངས་པར་སྤྱོད་པ་སྤྱད་ནས་
msgctxt "8ea3e1f0e59a45b0809c3c0e7424acbf"
msgid "དེ␣ས་ དེ␣ར་ ཚེ་ གཅིག་ ཏུ་ ཚངས་པ␣ར་ སྤྱོད་པ་ སྤྱད་ ནས་"
msgstr ""

# འཆི་ཀར་<«ཞོལ་»ཁར།>སྨོན་ལམ་བཏབ་སྟེ<«གཡུང་»«པེ་»ཏེ།>།
msgctxt "e64adc58363f4ff0868d846db49cdf12"
msgid "འཆི་ཀ␣ར་ སྨོན་ལམ་ བཏབ་ སྟེ །"
msgstr ""

# ཡང་དག་པར་རྫོགས་པའི་སངས་རྒྱས་འོད་སྲུང་གིས་<«ཅོ་»གི།>བྲམ་ཟེའི་ཁྱེའུ་བླ་མ་ལུང་བསྟན་པ་གང་ཡིན་པའི་སངས་རྒྱས་བཅོམ་ལྡན་འདས་དེ་བདག་གིས་མཉེས་པར་བྱེད་པར་གྱུར་ཅིག
# །
msgctxt "70f740a86280436c8bc6799c898cd437"
msgid ""
"ཡང་དག་པ␣ར་ རྫོགས་པ␣འི་ སངས་རྒྱས་ འོད་སྲུང་ གིས་ བྲམ་ཟེ␣འི་ ཁྱེའུ་ བླ་མ་ "
"ལུང་བསྟན་པ་ གང་ཡིན་པ␣འི་ སངས་རྒྱས་ བཅོམ་ལྡན་འདས་ དེ་ བདག་ གིས་ མཉེས་པ␣ར་ "
"བྱེད་པ␣ར་ གྱུར་ཅིག  །"
msgstr ""

msgctxt "b81d30d12ae549a3872e091301b73620"
msgid "མི་ མཉེས་པ␣ར་ བྱེད་པ␣ར་ མ་ གྱུར་ཅིག  །"
msgstr ""

msgctxt "414d312da56d481b9aecdc5850b3ed1f"
msgid "དེ␣འི་ བསྟན་པ་ ལ་ རབ་ཏུ་ བྱུང་ ནས་"
msgstr ""

msgctxt "0a0fe9d8dc5f450e882159e8c75611bf"
msgid "ཉོན་མོངས་པ་ ཐམས་ཅད་ སྤངས་ ཏེ་"
msgstr ""

msgctxt "a1484b75ebdb4aa3a150b708a71acace"
msgid "དགྲ་བཅོམ་པ་ ཉིད་ མངོན་སུམ་ དུ་ བྱེད་པ␣ར་ གྱུར་ཅིག  །"
msgstr ""

# ཇི་ལྟར་བདག་<«ལི་»གདག>གི་མཁན་མོ་<«གཡུང་»«ལི་»«པེ་»«སྣར་»«ཅོ་»པོ།>ཡང་དག་པར་རྫོགས་པའི་སངས་རྒྱས་འོད་སྲུང་གིས་བརྩོན་འགྲུས་བརྩམས་པ་རྣམས་ཀྱི་མཆོག་ཏུ་བསྟན་<«གཡུང་»ཆོག་ཏུ་སྟན།>པ་དེ་བཞིན་དུ་བདག་ཀྱང་ཤཱཀྱ་ཐུབ་པ་[68b]ཤཱཀྱའི་<«གཡུང་»ཤཱཀྱི།
# «པེ་»«ཞོལ་»ཤཱཀྱ།>རྒྱལ་པོ་དེས་བརྩོན་འགྲུས་བརྩམས་པ་རྣམས་ཀྱི་མཆོག་ཏུ་ལུང་བསྟན་<«གཡུང་»«པེ་»སྟན།>པར་གྱུར་ཅིག་ཅེས་
msgctxt "1a4f1b3c52a3486d90dc666427a29a9f"
msgid ""
"ཇི་ལྟར་ བདག་ གི་ མཁན་མོ་ ཡང་དག་པ␣ར་ རྫོགས་པ␣འི་ སངས་རྒྱས་ འོད་སྲུང་ གིས་ "
"བརྩོན་འགྲུས་ བརྩམས་པ་ རྣམས་ ཀྱི་ མཆོག་ ཏུ་ བསྟན་པ་ དེ་བཞིན་ དུ་ བདག་ ཀྱང་ "
"ཤཱཀྱ་ ཐུབ་པ་ ཤཱཀྱའི་ རྒྱལ་པོ་ དེ␣ས་ བརྩོན་འགྲུས་ བརྩམས་པ་ རྣམས་ ཀྱི་ མཆོག་ "
"ཏུ་ ལུང་བསྟན་པ␣ར་ གྱུར་ཅིག་ ཅེས་"
msgstr ""

msgctxt "a7ce4e35d67646dc8962b63b906d84ef"
msgid "བྱས་ སོ ། །"
msgstr ""

msgctxt "15f34ad8f3bd429ebe1da072d561f9da"
msgid "དགེ་སློང་ དག་"
msgstr ""

# ང་<«ལི་»«ཅོ་»–ང་།>ཡང་དག་པར་རྫོགས་པའི་སངས་རྒྱས་འོད་སྲུང་དང་ཤིན་ཏུ་མཉམ་པ་དང་ཤུགས་མཉམ་པ་དང་མཛད་པ་མཉམ་པ་དང་ཐབས་མཉམ་པ་བརྙེས་<«གཡུང་»«ལི་»«པེ་»«སྣར་»–«ཞོལ་»རྙེད།
# «ཅོ་»བརྙེད།>པར་གྱུར་ནས།
msgctxt "dc4d5a52df7f4a0387ae6fd7b609980e"
msgid ""
"ང་ ཡང་དག་པ␣ར་ རྫོགས་པ␣འི་ སངས་རྒྱས་ འོད་སྲུང་ དང་ ཤིན་ཏུ་ མཉམ་པ་ དང་ ཤུགས་ "
"མཉམ་པ་ དང་ མཛད་པ་ མཉམ་པ་ དང་ ཐབས་ མཉམ་པ་ བརྙེས་པ␣ར་ གྱུར་ ནས །"
msgstr ""

msgctxt "2a0b0e8667134559961744c5dfeac174"
msgid "འདི␣ས་ ང་ མཉེས་པ␣ར་ བྱས་ ཏེ་"
msgstr ""

msgctxt "09ad6509058a4914af1110531489cc66"
msgid "མི་ མཉེས་པ␣ར་ མ་ བྱས་ ལ །"
msgstr ""

msgctxt "07df4195158c42f086f3aba03ab0963f"
msgid "ང་ཉིད་ ཀྱི་ བསྟན་པ་ ལ་ རབ་ཏུ་ བྱུང་ ནས་"
msgstr ""

msgctxt "d8ca6e53d14e46438d4943e075b5668c"
msgid "ཉོན་མོངས་པ་ ཐམས་ཅད་ སྤངས་ ཏེ་"
msgstr ""

msgctxt "b91238c7f656461cbb42c1be24268870"
msgid "དགྲ་བཅོམ་པ་ ཉིད་ མངོན་སུམ་ དུ་ བྱས་ ནས །"
msgstr ""

# ད་ལྟར་<«གཡུང་»ལྟའ།>ངས་བརྩོན་འགྲུས་བརྩམས་<«ཅོ་»བརྩོམས།>པ་རྣམས་ཀྱི་མཆོག་<«པེ་»ཆོག>ཏུ་བསྟན་<«གཡུང་»སྟན།>ཏོ།
# ། ། །
msgctxt "f049736591ee42d586af1cdb5df16636"
msgid "ད་ལྟར་ ངས་ བརྩོན་འགྲུས་ བརྩམས་པ་ རྣམས་ ཀྱི་ མཆོག་ ཏུ་ བསྟན་ ཏོ ། ། ། །"
msgstr ""
