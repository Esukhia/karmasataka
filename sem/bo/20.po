# སྐྲ་<«གཡུང་»སྐུ། «པེ་»སྒྲ།>ལྡན་མ་ཞེས་བྱ་བ་ནི། 
msgctxt "line 1, 20.txt"
msgid "སྐྲ་ ལྡན་ མ་ ཞེས་ བྱ་བ་ ནི །"
msgstr ""

# བྱང་ཆུབ་སེམས་དཔའ་དགའ་ལྡན་གྱི་གནས་ན་<«སྣར་»«ཞོལ་»སུ།>བཞུགས་ཙམ་ན་ཡུལ་སེར་སྐྱའི་གནས་ན་རྒྱལ་པོ་སེང་གེའི་འགྲམ་<«ཁུ་»བགྲམ།>རྒྱལ་པོ་བྱེད་དེ།
msgctxt "line 2, 20.txt"
msgid ""
"བྱང་ཆུབ་ སེམས་དཔའ་ དགའ་ལྡན་ གྱི་ གནས་ ན་ བཞུགས་ ཙམ་ ན་ ཡུལ་ སེར་སྐྱ␣འི་ གནས་"
" ན་ རྒྱལ་པོ་ སེང་གེ␣འི་ འགྲམ་ རྒྱལ་པོ་ བྱེད་ དེ །"
msgstr ""

msgctxt "line 3, 20.txt"
msgid ""
"དེ་ བཙུན་མོ་ དང་ ལྷན་ཅིག་ ཏུ་ རྩེ་ ཞིང་ དགའ་ ལ་ དགའ་མགུ␣ར་ སྤྱོད་པ་ ལ␣ས་"
msgstr ""

# སྔ་<«གཡུང་»ལྔ།>ཕྱིར་བུ་ཕོ་བཞི་དང་བུ་མོ་བཞི་བཙས་ཏེ་
msgctxt "line 4, 20.txt"
msgid "སྔ་ཕྱི␣ར་ བུ་ ཕོ་ བཞི་ དང་ བུ་མོ་ བཞི་ བཙས་ ཏེ་"
msgstr ""

# ཟས་གཙང་<«གཡུང་»བཙང་།>དང་ཟས་དཀར་དང་བྲེ་བོ་ཟས་དང་བདུད་རྩི་ཟས་དང་གཙང་མ་དང་དཀར་མོ་དང་བྲེ་བོ་མ་དང་བདུད་རྩི་མ་རྣམས་སོ།
# །
msgctxt "line 5, 20.txt"
msgid ""
"ཟས་གཙང་ དང་ ཟས་དཀར་ དང་ བྲེ་བོ་ ཟས་ དང་ བདུད་རྩི་ ཟས་ དང་ གཙང་མ་ དང་ དཀར་མོ་"
" དང་ བྲེ་བོ་ མ་ དང་ བདུད་རྩི་ མ་ རྣམས་ སོ ། །"
msgstr ""

# དེའི་ཚེ་ཡུལ་སྤོང་བྱེད་ན་རྒྱལ་པོ་ཤཱཀྱ་རབ་སད་<«གཡུང་»«པེ་»སངས།>གནས་ཏེ། 
msgctxt "line 6, 20.txt"
msgid "དེ␣འི་ ཚེ་ ཡུལ་ སྤོང་ བྱེད་ ན་ རྒྱལ་པོ་ ཤཱཀྱ་ རབ་སད་ གནས་ ཏེ །"
msgstr ""

# དེའི་བཙུན་མོ་དང་ལྷན་ཅིག་ཏུ་རྩེ་ཞིང་དགའ་ལ་<«གཡུང་»«ལི་»«པེ་»«སྣར་»«ཅོ་»«ཞོལ་»–དགའ་ལ།>དགའ་མགུར་སྤྱོད་པ་ལ་བུ་མོ་གཉིས་བཙས་ཏེ།
msgctxt "line 7, 20.txt"
msgid ""
"དེ␣འི་ བཙུན་མོ་ དང་ ལྷན་ཅིག་ ཏུ་ རྩེ་ ཞིང་ དགའ་ ལ་ དགའ་མགུ␣ར་ སྤྱོད་པ་ ལ་ "
"བུ་མོ་ གཉིས་ བཙས་ ཏེ །"
msgstr ""

msgctxt "line 8, 20.txt"
msgid "གཅིག་ གི་ མིང་ ནི་ སྒྱུ་འཕྲུལ་ ཞེས་"
msgstr ""

msgctxt "line 9, 20.txt"
msgid "བཏགས་ སོ ། །"
msgstr ""

# གཅིག་གི་མིང་ནི་སྒྱུ་འཕྲུལ་ཆེན་མོ་<«གཡུང་»«ལི་»«པེ་»«སྣར་»«ཅོ་»པོ>ཞེས་
msgctxt "line 10, 20.txt"
msgid "གཅིག་ གི་ མིང་ ནི་ སྒྱུ་འཕྲུལ་ ཆེན་ མོ་ ཞེས་"
msgstr ""

msgctxt "line 11, 20.txt"
msgid "བཏགས་ སོ ། །"
msgstr ""

# དེ་ནས་དེ་གཉིས་འོ་མ་དང་<«ཅོ»–དང་།>། ཞོ་དང་། མར་དང་། ཞུན་མར་དང་།
# མར་གྱི་སྙིང་གུས་<«གཡུང་»«ལི་»«པེ་»«སྣར་»«ཅོ་»«ཞོལ་»ཁུས།>བསྐྱེད་བསྲིངས་ཏེ།
msgctxt "line 12, 20.txt"
msgid ""
"དེ་ནས་ དེ་ གཉིས་ འོ་མ་ དང་ །  ཞོ་ དང་ །  མར་ དང་ །  ཞུན་མར་ དང་ །  མར་ གྱི་ "
"སྙིང་ གུས་ བསྐྱེད་ བསྲིངས་ ཏེ །"
msgstr ""

msgctxt "line 13, 20.txt"
msgid "མཚོ␣འི་ ནང་ གི་ པད་མ་ བཞིན་ དུ་ སྐྱེ་ དྲག་པ␣ར་ གྱུར་ ཏོ ། །"
msgstr ""

msgctxt "line 14, 20.txt"
msgid "དེ་ནས་ དེ་ གཉིས་ ལ་ མཚན་མཁན་ རྣམས་ ཀྱིས་ བལྟས་ ནས །"
msgstr ""

# བུ་<«གཡུང་»ལྷ།>མོ་སྒྱུ་འཕྲུལ་ལས་ནི་བུ་མཚན་ཕུན་སུམ་ཚོགས་པ་ཞིག་བཙའ་བར་འགྱུར་ལ།
msgctxt "line 15, 20.txt"
msgid ""
"བུ་མོ་ སྒྱུ་འཕྲུལ་ ལ␣ས་ ནི་ བུ་ མཚན་ ཕུན་སུམ་ ཚོགས་པ་ ཞིག་ བཙའ་བ␣ར་ འགྱུར་ ལ"
" །"
msgstr ""

msgctxt "line 16, 20.txt"
msgid ""
"སྒྱུ་འཕྲུལ་ ཆེན་ མོ་ལ␣ས་ ནི་ འཁོར་ལོ␣ས་ སྒྱུར་བ␣འི་ རྒྱལ་པོ་ བཙའ་བ␣ར་ འགྱུར་"
" རོ་ ཞེས་"
msgstr ""

msgctxt "line 17, 20.txt"
msgid "ལུང་བསྟན་ ཏེ །"
msgstr ""

# དེ་གཉིས་ཀྱི་<«སྣར་»«ཞོལ་»ཉིད་ཀྱི།
# «ཅོ་»གཉིས་ཀྱིས།>སྐྲ་ཏིལ་མར་གྱི་ཐིགས་པ་ཆུར་<«ཞོལ་»ཆུང་།>བླུགས་པ་བཞིན་དུ་རྒྱས་པར་གྱུར་ཏོ།
# །
msgctxt "line 18, 20.txt"
msgid ""
"དེ་ གཉིས་ ཀྱི་ སྐྲ་ ཏིལ་མར་ གྱི་ ཐིགས་པ་ ཆུར་ བླུགས་པ་ བཞིན་ དུ་ རྒྱས་པ␣ར་ "
"གྱུར་ ཏོ ། །"
msgstr ""

msgctxt "line 19, 20.txt"
msgid "དེ་ནས་ རྒྱལ་པོ་ སེང་གེ་ འགྲམ་ གྱིས་ ཐོས་ ནས །"
msgstr ""

# རྒྱལ་པོ་རབ་སད་<«གཡུང་»«པེ་»སངས།>ལ་ཕོ་ཉ་བཏང་སྟེ། 
msgctxt "line 20, 20.txt"
msgid "རྒྱལ་པོ་ རབ་སད་ ལ་ ཕོ་ཉ་ བཏང་ སྟེ །"
msgstr ""

# ཁྱོད་ཀྱི་བུ་མོ་དེ་གཉིས་བདག་གི་བུའི་ཆུང་མར་བྱིན་ཅིག །<«གཡུང་»«པེ་»གཅིག> 
msgctxt "line 21, 20.txt"
msgid "ཁྱོད་ ཀྱི་ བུ་མོ་ དེ་ གཉིས་ བདག་ གི་ བུ␣འི་ ཆུང་མ␣ར་ བྱིན་ ཅིག  །"
msgstr ""

# འདི་ན་<«ཅོ་»ནི།>ཤཱཀྱ་རྣམས་ཀྱིས་ནི་སུས་ཀྱང་ཐབས་ཟླར་བབ་པའི་ཆུང་མ་གཉིས་མི་བླང་བའི་ནང་ཁྲིམས་དེ་ལྟ་བུ་བཅས་མོད་ཀྱི།
msgctxt "line 22, 20.txt"
msgid ""
"འདི་ ན་ ཤཱཀྱ་ རྣམས་ ཀྱིས་ ནི་ སུ␣ས་ ཀྱང་ ཐབས་ ཟླ␣ར་ བབ་པ␣འི་ ཆུང་མ་ གཉིས་ "
"མི་ བླང་བ␣འི་ ནང་ཁྲིམས་ དེ་ལྟ་ བུ་ བཅས་ མོད་ ཀྱི །"
msgstr ""

# གལ་ཏེ་བདག་གིས་ཤཱཀྱ་རྣམས་ཀྱིས་དད་པ་གནང་བར་བྱེད་ནུས་ན་ཕྱིས་གཉི་ག་<«གཡུང་»«ལི་»«པེ་»«སྣར་»«ཅོ་»«ཁུ་»གཉིས་ག>བླང་ངོ་ཞེས་
msgctxt "line 23, 20.txt"
msgid ""
"གལ་ཏེ་ བདག་ གིས་ ཤཱཀྱ་ རྣམས་ ཀྱིས་ དད་པ་ གནང་ བར་ བྱེད་ ནུས་ ན་ ཕྱིས་ གཉི་ག་"
" བླང་ ངོ་ ཞེས་"
msgstr ""

# སྤྲིང་<«ཞོལ་»སྤྲིངས།>ནས། 
msgctxt "line 24, 20.txt"
msgid "སྤྲིང་ ནས །"
msgstr ""

msgctxt "line 25, 20.txt"
msgid "སེང་གེ་ འགྲམ་ གྱིས་ ཤཱཀྱ་ རྣམས་ ལ་ གསོལ་བ་ བཏབ་ ནས །"
msgstr ""

# དེའི་བུ་མོ་གཉི་ག་<«གཡུང་»«ལི་»«པེ་»«སྣར་»«ཅོ་»«ཞོལ་»གཉིས།>ཁྱིམ་ན་གནས་པ་རྣམས་ཀྱི་ཁྱིམ་པའི་ཆོས་བཞིན་དུ་བྱས་ཏེ་
msgctxt "line 26, 20.txt"
msgid ""
"དེ␣འི་ བུ་མོ་ གཉི་ག་ ཁྱིམ་ ན་ གནས་པ་ རྣམས་ ཀྱི་ ཁྱིམ་པ␣འི་ ཆོས་བཞིན་ དུ་ "
"བྱས་ ཏེ་"
msgstr ""

msgctxt "line 27, 20.txt"
msgid "ཟས་གཙང་ གི་ བཙུན་མོ␣ར་ བླངས་ སོ ། །"
msgstr ""

# དེར་རྒྱལ་པོ་རབ་སད་<«གཡུང་»«པེ་»སངས།>ཀྱིས་དེ་གཉིས་ལ་བུ་མོ་སྐྲ་ལྡན་མ་ཞེས་བྱ་བ་<«གཡུང་»«ལི་»«པེ་»«སྣར་»«ཅོ་»«ཞོལ་»+སྐྲ།>སྐྲ་མཁན་དུ་བྱིན་ནོ།
# །
msgctxt "line 28, 20.txt"
msgid ""
"དེ␣ར་ རྒྱལ་པོ་ རབ་སད་ ཀྱིས་ དེ་ གཉིས་ ལ་ བུ་མོ་ སྐྲ་ ལྡན་ མ་ ཞེས་ བྱ་བ་ "
"སྐྲ་མཁན་ དུ་ བྱིན་ ནོ ། །"
msgstr ""

msgctxt "line 29, 20.txt"
msgid "དེ་ནས་ གང་ གི་ ཚེ་ བྱང་ཆུབ་ སེམས་དཔའ་ བཙས་པ་ ན །"
