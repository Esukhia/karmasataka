msgctxt "38669e7404054c698a1f231061beaf53"
msgid "﻿སྲིན་ ཞེས་ བྱ་བ་ ནི །"
msgstr ""

msgctxt "b3c19312a1bc446499751b4f34de7161"
msgid "གླེང་ གཞི་ མཉན་ དུ་ཡོད་པ་ ན་ བཞུགས་ ཏེ །"
msgstr ""

msgctxt "0884f75c8a3245d5a5ae2c423f9ad759"
msgid ""
"དེ␣འི་ ཚེ་ མཉན་ དུ་ཡོད་པ་ ན་ ཕྱི་ས␣འི་ འདམ་ ཞིག་ ཏུ་ སྲིན་ མགོ་བོ་ དགེ་སློང་"
" གི་ མགོ་བོ་ འདྲ་བ་ ཞིག་ བྱུང་ སྟེ །"
msgstr ""

msgctxt "fc0d69b4709c4732a6a1ac9aba007d95"
msgid ""
"དེ␣འི་ ལུས་ ཐམས་ཅད་ སྲིན་བུ་ ཁབ་ ཀྱི་ རྩེ་མོ་ འདྲ་བ་ ཤ་སྟག་ གིས་ གང་ སྟེ །"
msgstr ""

msgctxt "e2e05d048ce64fca9c40343d24a043ad"
msgid ""
"དེ་ ལ་བ␣འི་ སྤུ་ བཞིན་ དུ་ དེ␣འི་ ལུས་ ལ␣ས་ སྲིན་བུ་ དེ་དག་ བྱུང་ ངོ་ ། །"
msgstr ""

# དེ་དག་དེ་ལ་ཟ་བས་ན་ཚ་མི་བཟོད་<«ཞོལ་»གཟོད།>ནས་
msgctxt "dbae54ae0bf8407f8014efbfc739acb7"
msgid "དེ་དག་ དེ་ ལ་ ཟ་བ␣ས་ ན་ཚ་ མི་ བཟོད་ ནས་"
msgstr ""

# ཕྱི་སའི་འདམ་དེའི་ནང་ན་ཕན་<«ལི་»ཕུན།>ཚུན་རྒྱུག་:ལ་
msgctxt "f54c3110c5ab4176812bd6e114931126"
msgid "ཕྱི་ས␣འི་ འདམ་ དེ␣འི་ ནང་ ན་ ཕན་ཚུན་ རྒྱུག་ ལ་"
msgstr ""

# མི་<«གཡུང་»བ་མ།>བཟོད་<«གཡུང་»«ལི་»«པེ་»«སྣར་»«ཅོ་»བཟད།>པར་བཀྲེས་ཏེ་
msgctxt "cfee701588114f338b8010121f75de12"
msgid "མི་ བཟོད་པ␣ར་ བཀྲེས་ ཏེ་"
msgstr ""

msgctxt "2df9a5a6c5ff4cc0836da9d6ea8a7542"
msgid "ཕྱི་ས་ ཟ␣འོ ། །"
msgstr ""

msgctxt "daf2a1fdd8fc49588e2a018fcf41fd16"
msgid "དེ་ ན་ཚ་ མི་ བཟོད་ དེ་"
msgstr ""

# ཕན་ཚུན་འཁོར་བས་ཕྱི་སའི་འདམ་དེའི་ནང་ཐམས་ཅད་དཀྲུགས་སློངས་<«གཡུང་»«ལི་»«པེ་»«སྣར་»«ཞོལ་»གློངས།
# «ཅོ་»བློངས།>སོ། །
msgctxt "e97e9372b5064c2fb58c26ab09e02ab3"
msgid ""
"ཕན་ཚུན་ འཁོར་བ␣ས་ ཕྱི་ས␣འི་ འདམ་ དེ␣འི་ ནང་ ཐམས་ཅད་ དཀྲུགས་ སློངས་ སོ ། །"
msgstr ""

msgctxt "508fe63a1b75435886efd38c2590d1e5"
msgid "སངས་རྒྱས་ བཅོམ་ལྡན་འདས་ བགྲོད་པ་ གཅིག་པུ␣འི་ ལམ་ སྟོན་པ །"
msgstr ""

# རྣམ་པ་གཉིས་ཀྱི་ཤེས་བྱ་དང་ཡེ་ཤེས་ལ་མངའ་བརྙེས་<«གཡུང་»བརྙེད།>པ་
msgctxt "7633f22b2e8e4fbcaf956586bccf03c1"
msgid "རྣམ་པ་ གཉིས་ ཀྱི་ ཤེས་བྱ་ དང་ ཡེ་ཤེས་ ལ་ མངའ་ བརྙེས་པ་"
msgstr ""

# མ་འདྲེས་པའི་དྲན་པ་ཉེ་བར་གཞག་<«སྣར་»«ཁུ་»«ཞོལ་»བཞག>པ་གསུམ་ལ་མཁས་པ།
msgctxt "f9b526a180ef4ad2affea5132a1c26a8"
msgid "མ་ འདྲེས་པ␣འི་ དྲན་པ་ ཉེ་བར་ གཞག་པ་ གསུམ་ ལ་ མཁས་པ །"
msgstr ""

msgctxt "22c52dd29d1c47ea8ceea1403a048b13"
msgid "མི་ འཇིགས་པ་ བཞི␣ས་ མི་ འཇིགས་པ །"
msgstr ""

msgctxt "c567b5d211674732a5de7c7aeba17cc7"
msgid "འགྲོ་བ་ ལྔ␣ར་ འགྲོ་བ་ ལ␣ས་ རྣམ་པ␣ར་ གྲོལ་བ །"
msgstr ""

msgctxt "4a6cb96fa6d348369562d9b7eebc9e5e"
msgid "སྐྱེ་མཆེད་ དྲུག་ ལ་ མཁས་པ །"
msgstr ""

msgctxt "518d38861400489d832c025ba1561460"
msgid "བྱང་ཆུབ་ ཀྱི་ ཡན་ལག་ བདུན་ ལ་ སྤྱོད་ཡུལ་བ །"
msgstr ""

msgctxt "66cff4e140c944b9b371ef99e9f67073"
msgid "རྣམ་པ␣ར་ ཐར་པ་ བརྒྱད་ ལ་ བསམ་གཏན་པ །"
msgstr ""

# མཐར་གྱིས་གནས་པའི་སྙོམས་པར་འཇུག་པ་དགུ་ལ་སྙོམས་པར་འཇུག་<«པེ་»«ཅོ་»འདུག>པ།
msgctxt "030adf7872b44df48e76cddca22aa7e2"
msgid "མཐར་གྱིས་ གནས་པ␣འི་ སྙོམས་པར་ འཇུག་པ་ དགུ་ ལ་ སྙོམས་པར་ འཇུག་པ །"
msgstr ""

msgctxt "ad965bc4c2574529bfc1552bea4a3214"
msgid "སྟོབས་ བཅུ␣འི་ སྟོབས་ དང་ ལྡན་པ །"
msgstr ""

# ཡང་དག་པའི་སེང་<«ཅོ་»སེ།>གེའི་སྒྲ་སྒྲོགས་<«ཞོལ་»སྒྲོག>པ་རྣམས་ནི་ཆོས་ཉིད་ཀྱིས་ཉིན་དུས་གསུམ་མཚན་དུས་གསུམ་སྟེ<«གཡུང་»«པེ་»ཏེ།>།
# ཉིན་མཚན་དུས་དྲུག་ཏུ་སངས་རྒྱས་ཀྱི་སྤྱན་གྱིས་འཇིག་རྟེན་ལ་གཟིགས་ཏེ།
msgctxt "5bbad89720e544c089523c1bb1c770ef"
msgid ""
"ཡང་དག་པ␣འི་ སེང་གེ␣འི་ སྒྲ་ སྒྲོགས་པ་ རྣམས་ ནི་ ཆོས་ཉིད་ ཀྱིས་ ཉིན་ "
"དུས་གསུམ་ མཚན་ དུས་གསུམ་ སྟེ །  ཉིན་མཚན་ དུས་ དྲུག་ ཏུ་ སངས་རྒྱས་ ཀྱི་ སྤྱན་"
" གྱིས་ འཇིག་རྟེན་ ལ་ གཟིགས་ ཏེ །"
msgstr ""

msgctxt "508cafb3f6744c7c8a6c16c3892ffa62"
msgid "སུ་ ནི་ རྒུད་"
msgstr ""

msgctxt "dcf2b7be59d54cebb802ef1d4c9b5052"
msgid "སུ་ ནི་ དར་"
msgstr ""

msgctxt "42be23ac2fd140be824a21c340a4950d"
msgid "སུ་ ནི་ ཕོངས་ ཤིང་ འདུག་"
msgstr ""

msgctxt "dac644695abc4b6fb5062bd1d7f40152"
msgid "སུ་ ནི་ ཉམ་ང་ བར་ གྱུར་"
msgstr ""

msgctxt "980e26bb57224236b16f456e704cc09b"
msgid "སུ་ ནི་ གནོད་པ་ དང་ ལྡན་"
msgstr ""

msgctxt "e1b79a4ea43a4e69bba1528c7fcbb2eb"
msgid "སུ་ ནི་ ཕོངས་པ་ དང་ ཉམ་ང་བ་ དང་ གནོད་པ་ དང་ ལྡན་"
msgstr ""

msgctxt "f7271493d4864849bbdc39bd9a9c40dc"
msgid "སུ་ ནི་ ངན་སོང་ དུ་ གཞོལ་"
msgstr ""

msgctxt "ea6951a5bce6475683b71737b024ca8f"
msgid "སུ་ ནི་ ངན་སོང་ དུ་ འབབ་"
msgstr ""

msgctxt "f9f51b40eb9a480cad9055ec6721554a"
msgid "སུ་ ནི་ ངན་སོང་ དུ་ བབ་"
msgstr ""

msgctxt "d8118a6197044ebc96cd0bac17ae0c18"
msgid "སུ་ ནི་ ངས་ ངན་སོང་ ནས་ ཕྱུང་ སྟེ་"
msgstr ""

# མཐོ་རིས་དང་ཐར་པ་དང་འབྲས་བུ་ལ་གཞག་<«ལི་»«སྣར་»«ཅོ་»བཞག>པར་བྱ།
msgctxt "bfd3e15554d048f58b26c98c490625fb"
msgid "མཐོ་རིས་ དང་ ཐར་པ་ དང་ འབྲས་བུ་ ལ་ གཞག་པ␣ར་ བྱ །"
msgstr ""

# སུ་ནི་ཉེས་པར་སྤྱོད་པའི་འདམ་དུ་<«གཡུང་»«པེ་»བུ།>བྱིང་བ་ལས་
msgctxt "1ee3b2ad08794678b5b294975c5eeb0f"
msgid "སུ་ ནི་ ཉེས་པ␣ར་ སྤྱོད་པ␣འི་ འདམ་ དུ་ བྱིང་བ་ ལ␣ས་"
msgstr ""

msgctxt "3ca18960b44d485fac51673b23ff0352"
msgid "ལག་ནས་ དྲང་བ␣ར་ བྱ །"
msgstr ""

msgctxt "37e22fe360484e26a254929d865922f7"
msgid "སུ་ ནི་ འཕགས་པ␣འི་ ནོར་ བདུན་ མེད་པ་ ལ␣ས་"
msgstr ""

msgctxt "7439605b223240b3873825da1aa5ec67"
msgid "འཕགས་པ␣འི་ ནོར་ བདུན་ གྱི་ དབང་ཕྱུག་ ལ་ དབང་ བྱེད་ དུ་ གཞུག་པ␣ར་ བྱ །"
msgstr ""

# སུ་ནི་དགེ་བའི་རྩ་བ་རྣམས་མ་བསྐྱེད་པ་ལས་<«གཡུང་»«ལི་»«པེ་»«སྣར་»«ཅོ་»ལ།>
msgctxt "424b6823d431463cadf65819745c23c3"
msgid "སུ་ ནི་ དགེ་བ␣འི་ རྩ་བ་ རྣམས་ མ་ བསྐྱེད་པ་ ལ␣ས་"
msgstr ""

msgctxt "4172c16dcbac441b8c2190ed0df458c7"
msgid "བསྐྱེད་པ␣ར་ བྱ །"
msgstr ""

msgctxt "42f04f6eb48b4125bc92b9b3f2a1d891"
msgid "སུ་ ནི་ དགེ་བ␣འི་ རྩ་བ་ རྣམས་ བསྐྱེད་ ཟིན་པ་ ལ␣ས་"
msgstr ""

msgctxt "cd3052515bb94f288167843911f4fe25"
msgid "ཡོངས་སུ་ སྨིན་པ␣ར་ བྱ །"
msgstr ""

msgctxt "91d356a26d1944b4ac451e9925c369f6"
msgid "སུ་ ནི་ དགེ་བ␣འི་ རྩ་བ་ རྣམས་ ཡོངས་སུ་ སྨིན་ ཟིན་པ་ ལ␣ས་"
msgstr ""

# ཡེ་ཤེས་ཀྱི་<«གཡུང་»«པེ་»ཀྱིས།>མཚོན་གྱིས་བརྟོལ་བར་བྱ།
msgctxt "9742a19aa1da418481df23f05d4c4aa3"
msgid "ཡེ་ཤེས་ ཀྱི་ མཚོན་ གྱིས་ བརྟོལ་བ␣ར་ བྱ །"
msgstr ""

msgctxt "1d22e871d97345ff860d5508cbb83d7b"
msgid ""
"སུ་ ལ་ ནི་ སངས་རྒྱས་ བྱུང་བ␣ས་ བརྒྱན་པ␣འི་ འཇིག་རྟེན་ འབྲས་བུ་ དང་ བཅས་པ␣ར་ "
"བྱ་"
msgstr ""

# སྙམ་དུ་ཡེ་ཤེས་གཟིགས་པ་<«གཡུང་»«པེ་»+ལ།>འཇུག་གོ། །
msgctxt "fe183afafc6e433797d9f3869bec22f9"
msgid "སྙམ་ དུ་ ཡེ་ཤེས་ གཟིགས་པ་ འཇུག་ གོ ། །"
msgstr ""

msgctxt "7d8f217e128849a4a5ddcaed35c899f9"
msgid "རྒྱ་མཚོ་ ཆུ་སྲིན་ རྣམས་ ཀྱི་ གནས ། །"
msgstr ""

# དུས་རླབས་<«ཅོ་»བརླབས། །>ཡོལ་བར་འགྱུར་ཡང་སྲིད། །
msgctxt "24199658ed0b43b49fc7f77049e9f33f"
msgid "དུས་རླབས་ ཡོལ་བ␣ར་ འགྱུར་ ཡང་སྲིད ། །"
msgstr ""

# གདུལ་བར་བྱ་བའི་སྲས་རྣམས་ལ། །སངས་རྒྱས་དུས་ལས་:ཡོལ་བ་<«གཡུང་»ཡོད་པ།>མེད། །
msgctxt "f85e2485e9d845a5b49dc8979e085033"
msgid "གདུལ་བ␣ར་ བྱ་བ␣འི་ སྲས་ རྣམས་ ལ ། ། སངས་རྒྱས་ དུས་ ལ␣ས་ ཡོལ་བ་ མེད ། །"
msgstr ""

msgctxt "a39f87cc917445b59a87853b43f0e7fd"
msgid "དེ་ནས་ བཅོམ་ལྡན་འདས་ ཀྱིས་ དགོངས་པ །"
msgstr ""

msgctxt "736c4d19d91e48b289665ac8585a0945"
msgid "སྲིན་ ལུང་བསྟན་པ␣འི་ དུས་ ལ་ བབ་ སྟེ །"
msgstr ""

# དེ་ལ་བརྟེན་ནས་བདག་གིས་གདུལ་བྱ་ཕལ་པོ་ཆེ་དྲང་དགོས་སོ་<«གཡུང་»«པེ་»-སོ།>སྙམ་དུ་
msgctxt "cd89fe460403406790a44c368939bd83"
msgid ""
"དེ་ ལ་ བརྟེན་ ནས་ བདག་ གིས་ གདུལ་བྱ་ ཕལ་པོ་ ཆེ་ དྲང་ དགོས་ སོ་ སྙམ་ དུ་"
msgstr ""

msgctxt "0f1782a76618426e9844ef5d508e0a37"
msgid "དགོངས་ ནས་"
msgstr ""

# བཅོམ་ལྡན་འདས་ཀྱིས་སྐྱེ་བོ་ཕལ་པོ་:ཆེ་དེ་<«གཡུང་»ཆེན།>དག་དྲང་བའི་ཕྱིར་ཅི་ནས་ཕྱི་སའི་འདམ་དེའི་དྲི་:མནམ་བས་<«ལི་»«སྣར་»«ཅོ་»«ཞོལ་»མནམས་པས།>མཉན་དུ་ཡོད་པ་ཐམས་ཅད་གང་བར་འགྱུར་བ་དེ་ལྟ་བུར་བྱིན་གྱིས་བརླབས་སོ།
# །
msgctxt "15669a20c9f84fbaa50060bb418ae1c7"
msgid ""
"བཅོམ་ལྡན་འདས་ ཀྱིས་ སྐྱེ་བོ་ ཕལ་པོ་ ཆེ་ དེ་དག་ དྲང་བ␣འི་ ཕྱིར་ ཅི་ ནས་ "
"ཕྱི་ས␣འི་ འདམ་ དེ␣འི་ དྲི་ མནམ་ བས་ མཉན་ དུ་ཡོད་པ་ ཐམས་ཅད་ གང་བ␣ར་ འགྱུར་བ་ "
"དེ་ལྟ་ བུར་ བྱིན་ གྱིས་ བརླབས་ སོ ། །"
msgstr ""

# དེ་ནས་དེར་སྐྱེ་བོ་ཕལ་པོ་ཆེ་<«ལི་»ཚེ།>སྔོན་གྱི་<«གཡུང་»«པེ་»གྱིས།>དགེ་བའི་རྩ་བས་བསྐུལ་བ་དེ་དག་ངོ་མཚར་དུ་གྱུར་ཏེ་
msgctxt "872212792d93404a857a7e2b72f2787b"
msgid ""
"དེ་ནས་ དེ␣ར་ སྐྱེ་བོ་ ཕལ་པོ་ ཆེ་ སྔོན་ གྱི་ དགེ་བ␣འི་ རྩ་བ␣ས་ བསྐུལ་བ་ "
"དེ་དག་ ངོ་མཚར་ དུ་ གྱུར་ ཏེ་"
msgstr ""

# དྲི་མནམ་པ་འདི་དག་<«གཡུང་»«ལི་»«པེ་»«སྣར་»«ཅོ་»«ཞོལ་»-དག>གང་ནས་བྱུང་སྙམ་ནས་
msgctxt "a690431a2fdc452680ec7d80ed806846"
msgid "དྲི་ མནམ་པ་ འདི་དག་ གང་ ནས་ བྱུང་ སྙམ་ ནས་"
msgstr ""

msgctxt "0184a8ca20a34938bb3680dd67443090"
msgid "སྐྱེ་བོ་ སྟོང་ཕྲག་ དུ་མ་ ལྟད་མོ་ ལྟ་ འདོད་པ␣ར་ གྱུར་ ཏེ །"
msgstr ""

msgctxt "394e3737b63344538b75c93e6a4d78ff"
msgid "དྲི་ མནམ་པ␣འི་ ཕྱོགས་ སུ་ ཕྱི་ས␣འི་ འདམ་ ག་ལ་བ་ དེ␣ར་ དོང་བ་ དང་"
msgstr ""

# སྐྱེ་བོ་ཕལ་པོ་དེ་དག་གིས་<«ཞོལ་»གི།>ཕྱི་སའི་འདམ་དེ་ན་སྲིན་དེ་འདྲ་བ་ཕན་ཚུན་འཁོར་ཞིང་འདུག་པ་དེ་མཐོང་ངོ་།
# །
msgctxt "481665ec0d6547ff89564e47b09e5a00"
msgid ""
"སྐྱེ་བོ་ ཕལ་པོ་ དེ་དག་ གིས་ ཕྱི་ས␣འི་ འདམ་ དེ་ ན་ སྲིན་ དེ་ འདྲ་བ་ ཕན་ཚུན་ "
"འཁོར་ ཞིང་ འདུག་པ་ དེ་ མཐོང་ ངོ་ ། །"
msgstr ""

msgctxt "b52017f05b25400da1dd49e25c6566f8"
msgid "མཐོང་ ནས་ དེ་དག་ གིས་ བསམས་པ །"
msgstr ""

msgctxt "a18d503b45d049d9a9ee641fb2fd3da5"
msgid ""
"སེམས་ཅན་ འདི་ ལྟ་བུ་ སྡུག་བསྔལ་ འདི་འདྲ་བ་ མྱོང་བ་ ཅི་ ཡིན་ ཞིག་ གུ་ སྙམ་ མོ"
" ། །"
msgstr ""

msgctxt "c2c979a641d74d82bea69b3bf56d9f8b"
msgid "དེ་ནས་ བཅོམ་ལྡན་འདས་ ཀྱིས་ དགོངས་པ །"
msgstr ""

# བདག་གི་<«གཡུང་»«པེ་»གིས།>གདུལ་བྱ་མཉན་དུ་ཡོད་པ་ན་ཇི་སྙེད་ཡོད་པ་དེ་དག་ཐམས་ཅད་ད་[116a]ལྟར་ཚོགས་སོ་སྙམ་དུ་
msgctxt "4349caabaf214635adacaf3c12b61502"
msgid ""
"བདག་ གི་ གདུལ་བྱ་ མཉན་ དུ་ཡོད་པ་ ན་ ཇི་སྙེད་ ཡོད་པ་ དེ་དག་ ཐམས་ཅད་ ད་ལྟར་ "
"ཚོགས་ སོ་ སྙམ་ དུ་"
msgstr ""

msgctxt "0a05423c797340309047f86d5c894ed9"
msgid "དགོངས་ ནས །"
msgstr ""

msgctxt "590098ea885a416aa2e09355380686fb"
msgid "སྔ་དྲོ་ ཤམ་ཐབས་ དང་ ཆོས་གོས་ གསོལ་ ཏེ་"
msgstr ""

msgctxt "dc5ff391697c48ef9c51b9e1cb675f45"
msgid "ལྷུང་བཟེད་ བསྣམས་ ནས་"
msgstr ""

# དགེ་སློང་གི་ཚོགས་ཀྱིས་:ཞབས་འབྲིང་<«གཡུང་»ཞམ་རིང་།>བྱས་ཏེ།
msgctxt "bbe736de023048058cd663f2aba19499"
msgid "དགེ་སློང་ གི་ ཚོགས་ ཀྱིས་ ཞབས་འབྲིང་ བྱས་ ཏེ །"
msgstr ""

msgctxt "98db2adba7a34b6eb83dba9214862ff4"
msgid "མཉན་ དུ་ཡོད་པ␣ར་ གཤེགས་ ནས་"
msgstr ""

msgctxt "c9985f7fcae943f188479bdb4a4fa8ea"
msgid "ཕྱི་ས␣འི་ འདམ་ དེ་ག་ ལ་བ་ དེ␣ར་ བྱོན་པ་ དང་ །"
msgstr ""

msgctxt "b8cc8ef8e6814328a29fefaf4314e450"
msgid ""
"སྐྱེ་བོ་ ཕལ་པོ་ ཆེ་ དེ་དག་ གིས་ སངས་རྒྱས་ བཅོམ་ལྡན་འདས་ ཐག་ རིང་པོ་ ཞིག་ ནས་"
" མཐོང་ ངོ་ ། །"
msgstr ""

# མཐོང་ནས་ཀྱང་གང་མ་དད་པ་:དེ་དག་<«གཡུང་»ད་དག «པེ་»བདག>ན་རེ།
msgctxt "422ccadeb4414a7fb20728e0134d493a"
msgid "མཐོང་ ནས་ ཀྱང་ གང་ མ་དད་པ་ དེ་དག་ ན་རེ །"
msgstr ""

# དགེ་སྦྱོང་གཽ་ཏ་མ་ལྟད་མོ་འདོད་པ་དང་བྲལ་ལོ་ཞེས་གྲག་<«ཞོལ་»གྲགས།>ན་
msgctxt "e4d468cffb824a46bed00f197e9ccab4"
msgid "དགེ་སྦྱོང་ གཽ་ ཏ་ མ་ ལྟད་མོ་ འདོད་པ་ དང་ བྲལ་ ལོ་ ཞེས་ གྲག་ ན་"
msgstr ""

# དེ་ཡང་ལྟད་མོ་ལ་གནན་<«ཅོ་»མཉན།>པར་གྱུར་ཏེ་
msgctxt "819d993011394f63a7b6222fa74dd5e2"
msgid "དེ་ཡང་ ལྟད་མོ་ ལ་ གནན་པ␣ར་ གྱུར་ ཏེ་"
msgstr ""

msgctxt "0b2c0c5081934c1090496ff61a17d9a5"
msgid "འདི␣ར་ འོངས་ སོ་ ཞེས་"
msgstr ""

# ཟེར་ཏོ<«གཡུང་»«ཁུ་»རོ།>། །
msgctxt "2b2800249fdb4ffba57518cd02b1e0d4"
msgid "ཟེར་ ཏོ ། །"
msgstr ""

msgctxt "1d68c560a22d4708a465c45afc90ff00"
msgid "གང་ དད་པ་ དང་ ལྡན་པ་ དག་ ན་རེ་"
msgstr ""

# བཅོམ་ལྡན་འདས་ནི་སྲིན་འདི་ལས་<«ཞོལ་»ལ།>བརྟེན་ཏེ་ངོ་མཚར་རྨད་དུ་བྱུང་བའི་ཆོས་འགའ་ཞིག་ལུང་སྟོན་པར་བཞེད་པའི་ཕྱིར་འདིར་གཤེགས་གོར་མ་ཆག་སྙམ་ནས།
msgctxt "6112bcf878234492b0341e81643c12a0"
msgid ""
"བཅོམ་ལྡན་འདས་ ནི་ སྲིན་ འདི་ ལ␣ས་ བརྟེན་ ཏེ་ ངོ་མཚར་ རྨད་ དུ་ བྱུང་བ␣འི་ "
"ཆོས་ འགའ་ཞིག་ ལུང་སྟོན་པ␣ར་ བཞེད་པ␣འི་ ཕྱིར་ འདི␣ར་ གཤེགས་ གོར་མ་ཆག་ སྙམ་ ནས"
" །"
msgstr ""

msgctxt "643afc38e49d4d46a1d9fc886e6a2cf1"
msgid "བཅོམ་ལྡན་འདས་ ཀྱི་ གདན་ བཤམས་ ཏེ་"
msgstr ""

msgctxt "e544c3b6607b4969901c1b6e6a8e2c37"
msgid "བཅོམ་ལྡན་འདས་ ལ་ འདི་སྐད་ ཅེས་ གསོལ་ ཏོ ། །"
msgstr ""

msgctxt "e603d81852934dcd9ca82dd4d3ca98e0"
msgid "བཅོམ་ལྡན་འདས་ གཤེགས་པ་ ལེགས་ སོ ། །"
msgstr ""

# བཅོམ་ལྡན་འདས་གདན་བཤམས་པ་ལ་བཞུགས་<«ལི་»གཞུགས།>སུ་གསོལ་ཞེས་
msgctxt "30756894df6947a09aaf3671aed6c7bc"
msgid "བཅོམ་ལྡན་འདས་ གདན་ བཤམས་པ་ ལ་ བཞུགས་ སུ་ གསོལ་ ཞེས་"
msgstr ""

msgctxt "7719a9f930904992a5dda7708eb8f80d"
msgid "བྱས་ ནས །"
