msgctxt "1e7675f3db1e4926b8cf4a4c5850cb94"
msgid ""
"སོང་ ནས་ ཀྱང་ རབ་ཏུ་ སྦྱིན་པ་ ཆེན་པོ་ རྣམ་པ་ བཞི་པོ་ དེ་དག་ མཁན་པོ་ ལ་ ཕུལ་ "
"ལོ ། །"
msgstr ""

msgctxt "20873a0f7677433d9e752937104b2fbb"
msgid "མཁན་པོ␣ས་ ཀྱང་ དེ་ རྣམས་ ཀྱི་ ནང་ ནས་ རྣམ་པ་ གསུམ་ ནི་ བླངས་"
msgstr ""

