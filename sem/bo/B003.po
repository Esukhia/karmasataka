msgctxt "aeeabe0b5e234ccfbdaa6028e5c68419"
msgid "རྒྱལ་པོ་ རྣམས་ ཤིང་ ཁྱེར་ དུ་ གཞུག་ གོ་ སྙམ་པ་ དང་ །"
msgstr ""

# དྲང་སྲོང་རྣམས་དང་། གླང་པོ་ཆེ་དཀར་པོ་རྣམས་དང་། ངང་པ་རྣམས་དང་།
# སེང་གེ་རྣམས་དང་། བྲག་ཆེན་པོ་ལ་བཞོན་པར་བྱའོ་སྙམ་<«གཡུང་»«པེ་»+སྙམ།>སྟེ།
msgctxt "3f7148e1cee7493d8cbb49fec1d9e30b"
msgid ""
"དྲང་སྲོང་ རྣམས་ དང་ །  གླང་པོ་ཆེ་ དཀར་པོ་ རྣམས་ དང་ །  ངང་པ་ རྣམས་ དང་ །  "
"སེང་གེ་ རྣམས་ དང་ །  བྲག་ ཆེན་པོ་ ལ་ བཞོན་པ␣ར་ བྱ␣འོ་ སྙམ་ སྟེ །"
msgstr ""

msgctxt "fc8fdad248674711a9ef5087da99f8ef"
msgid "དེ␣ས་ རྨི་ལམ་ དེ་ རྣམས་ རྨིས་ ནས་"
msgstr ""

msgctxt "b227a9d19f8b42ebb0bf95fe9892a3e5"
msgid "སད་ དོ ། །"
msgstr ""

# སད་<«པེ་»སོང་ངོ་། །སོང་།>ནས་
msgctxt "b58213f8ba6b4512b4015501a53c6513"
msgid "སད་ ནས་"
msgstr ""

msgctxt "0c27edfb4ca442799e4204118a0c9366"
msgid "དེ␣ས་ བསམས་པ །"
msgstr ""

# བདག་གི་རྨི་ལམ་འདི་རྣམས་ལུང་སྟོན་<«གཡུང་»«ལི་»«པེ་»«སྣར་»«ཅོ་»+པ།
# «ཞོལ་»+པར།>ནུས་པ་སུ་ཡོད་སྙམ་མོ། །
msgctxt "d76582db865546f99e3529ee410b8cd9"
msgid "བདག་ གི་ རྨི་ལམ་ འདི་ རྣམས་ ལུང་ སྟོན་ ནུས་པ་ སུ་ ཡོད་ སྙམ་ མོ ། །"
msgstr ""

msgctxt "e218d64ad411440288a5a3e4dfe0a41d"
msgid ""
"དེ་ནས་ ཐག་ མི་ རིང་བ་ ཞིག་ ན་ དྲང་སྲོང་ མངོན་པར་ ཤེས་པ་ ལྔ་ དང་ ལྡན་པ་ ཞིག་ "
"གནས་ སོ ། །"
msgstr ""

msgctxt "3da2dcefbe2e4a5a8eb16d78184b823d"
msgid ""
"དེ་ནས་ བྲམ་ཟེ␣འི་ ཁྱེའུ་ བློ་གྲོས་ བཟང་པོ་ ཐེ་ཚོམ་ གཅད་པ␣འི་ ཕྱིར །  "
"དྲང་སྲོང་ དེ␣འི་ ཐད་ དུ་ སོང་ ནས་"
msgstr ""

msgctxt "659ed9183e3644b98e6637362c775808"
msgid "བློ་གྲོས་ བཟང་པོ␣ས་ དྲང་སྲོང་ དེ་ ལ་ ཀུན་ དུ་ དགའ་བ␣འི་ གཏམ་ བྱས་ ནས་"
msgstr ""

msgctxt "6e8dc97fb8d1430e8008b33c81347dda"
msgid "རྨི་ལམ་ དེ་ རྣམས་ བསྙད་ དེ །"
msgstr ""

msgctxt "7e8140d5cdc94562be92dbbcc3ba7e80"
msgid ""
"ཁྱོད་ ཀྱིས་ རྨི་ལམ་ རྣམ་པ་ བཅུ་པོ་ འདི་ རྣམས་ ཀྱི་ དོན་ ངེས་པར་ གྱིས་ ཤིག་ "
"ཅེས་"
msgstr ""

msgctxt "64a02feb67e44c959bd437c067cdda07"
msgid "བྱས་ སོ ། །"
msgstr ""

msgctxt "417352b45b1341be85f150f0ff5cc84e"
msgid "དེ་ནས་ དྲང་སྲོང་ དེ␣ས་ སྨྲས་པ །"
msgstr ""

# བདག་གིས་རྨི་ལམ་འདི་རྣམས་ལུང་སྟོན་མི་ནུས་ཀྱིས་<«ཞོལ་»ཀྱི།>
msgctxt "9e12ce7793fc44e7b43dab46668640a6"
msgid "བདག་ གིས་ རྨི་ལམ་ འདི་ རྣམས་ ལུང་ སྟོན་ མི་ ནུས་ ཀྱིས་"
msgstr ""

msgctxt "c709a52f7fd7435d8b2600e4fcca5acc"
msgid "རྒྱལ་པོ␣འི་ ཕོ་བྲང་ མར་མེ་ ལྡན་ དུ་ སོང་ ཤིག་ དང་ །"
msgstr ""

msgctxt "b90d68c5c92b4548b4c12ac940b4853b"
msgid ""
"དེ་ ན་ རྒྱལ་པོ་ མར་མེ␣ས་ ཡང་དག་པ␣ར་ རྫོགས་པ␣འི་ སངས་རྒྱས་ མར་མེ་མཛད་ ཞབས་ ས་"
" ལ་ རེག་པ␣ར་ བྱ་བ␣འི་ ཕྱིར །  གྲོང་ཁྱེར་ གྱི་ ནང་ དུ་ སྤྱན་ དྲངས་ ཏེ །"
msgstr ""

msgctxt "99278b54937745afb2034310b7c7877b"
msgid "དེ␣ས་ རྨི་ལམ་ འདི་ རྣམས་ ལུང་སྟོན་པ␣ར་ འགྱུར་ རོ་ ཞེས་"
msgstr ""

msgctxt "eb29a5c159764c23a8b055f6919d88c2"
msgid "བྱས་ སོ ། །"
msgstr ""

msgctxt "8397249d6d7d4328995554c2679dd1c9"
msgid ""
"དེ་ནས་ རྒྱལ་པོ་ ནོར་ ཅན་ གྱི་ བུ་ དེ␣ས་ རྒྱལ་པོ་ མར་མེ␣ས་ སྤྲིང་བ་ བཞིན་ དུ་"
" མཉན་ ཏེ །"
msgstr ""

# བློན་པོ་བརྒྱད་ཁྲིས་:ཞབས་འབྲིང་<«གཡུང་»ཞམ་རིང་།>བྱས་ནས་
msgctxt "52441d995e00441fb1d7b0eae8f7cbfc"
msgid "བློན་པོ་ བརྒྱད་ ཁྲིས་ ཞབས་འབྲིང་ བྱས་ ནས་"
msgstr ""

# རྒྱལ་པོའི་ཕོ་བྲང་མར་མེ་<«གཡུང་»«ལི་»«པེ་»«སྣར་»«ཅོ་»མེས།>ལྡན་དུ་ཕྱིན་ཏོ། །
msgctxt "c489412d9f2d4901b1c8fba4b86d4203"
msgid "རྒྱལ་པོ␣འི་ ཕོ་བྲང་ མར་མེ་ ལྡན་ དུ་ ཕྱིན་ ཏོ ། །"
msgstr ""

msgctxt "1a408010a95842b582e545f1408a0d22"
msgid "རྒྱལ་པོ་ མར་མེ␣ས་ ཀྱང་ ཞག་ བདུན་ ལོན་ ནས་"
msgstr ""

# ཡང་དག་པར་རྫོགས་པའི་སངས་རྒྱས་མར་མེ་མཛད་ཞབས་ས་ལ་རེག་པར་བྱ་བའི་ཕྱིར།
# གྲོང་ཁྱེར་གྱི་ནང་དུ་སྤྱན་དྲང་བར་བྱའོ་སྙམ་སྟེ<«གཡུང་»«པེ་»ཏེ།>།
msgctxt "e6a19304e7d54e01b2ce49acece20a4c"
msgid ""
"ཡང་དག་པ␣ར་ རྫོགས་པ␣འི་ སངས་རྒྱས་ མར་མེ་མཛད་ ཞབས་ ས་ ལ་ རེག་པ␣ར་ བྱ་བ␣འི་ "
"ཕྱིར །  གྲོང་ཁྱེར་ གྱི་ ནང་ དུ་ སྤྱན་ དྲང་བ␣ར་ བྱ␣འོ་ སྙམ་ སྟེ །"
msgstr ""

msgctxt "62611afb3c724621838aefa81e1fee20"
msgid "ཡུལ་ དང་ གྲོང་ཁྱེར་ ཐམས་ཅད་ ནས་ མེ་ཏོག་ ཐམས་ཅད་ བསྡུས་ སོ ། །"
msgstr ""

# དེར་གང་གི་ཉིན་པར་རྒྱལ་པོ་མར་མེས་ཡང་དག་པར་རྫོགས་པའི་སངས་རྒྱས་མར་མེ་མཛད་ཞབས་ས་<«གཡུང་»–ས།>ལ་རེག་པར་བྱ་བའི་ཕྱིར།
# གྲོང་ཁྱེར་གྱི་ནང་དུ་སྤྱན་འདྲེན་པ་དེའི་ཉིན་པར་བློ་གྲོས་བཟང་པོ་ཡང་དེར་འོངས་:ནས།
msgctxt "c2a1b785484c458c9a1473b0b82a03ad"
msgid ""
"དེ␣ར་ གང་ གི་ ཉིན་པ␣ར་ རྒྱལ་པོ་ མར་མེ␣ས་ ཡང་དག་པ␣ར་ རྫོགས་པ␣འི་ སངས་རྒྱས་ "
"མར་མེ་མཛད་ ཞབས་ ས་ ལ་ རེག་པ␣ར་ བྱ་བ␣འི་ ཕྱིར །  གྲོང་ཁྱེར་ གྱི་ ནང་ དུ་ "
"སྤྱན་ འདྲེན་པ་ དེ␣འི་ ཉིན་པ␣ར་ བློ་གྲོས་ བཟང་པོ་ ཡང་ དེ␣ར་ འོངས་ ནས །"
msgstr ""

# དེར་<«གཡུང་»«པེ་»ན། དེར། «ལི་»«སྣར་»«ཅོ་»«ཞོལ་»ན།
# དེས།>རྒྱལ་པོ་དེས་མེ་ཏོག་ཐམས་ཅད་བསྡུས་པར་གྱུར་ཏོ། །
msgctxt "65837cc9ca6d47d3800b0c60c41c9a17"
msgid "དེ␣ར་ རྒྱལ་པོ་ དེ␣ས་ མེ་ཏོག་ ཐམས་ཅད་ བསྡུས་པ␣ར་ གྱུར་ ཏོ ། །"
msgstr ""

msgctxt "086a888b804f4f5e82fcf71b86c49bc2"
msgid ""
"བུ་མོ་ ལྷ␣འི་ རིམ་གྲོ་ བྱེད་པ་ དེ་ཡང་ མེ་ཏོག་ཕྲེང་བ་ རྒྱུད་པ␣འི་ ཐད་ དུ་ "
"སོང་ ནས་"
msgstr ""

# ལྷའི་མཆོད་པ་བྱ་དགོས་ཀྱིས་<«ཞོལ་»ཀྱི།>
msgctxt "7d30e4cdaf6047f9b77a474b18a5dec3"
msgid "ལྷ␣འི་ མཆོད་པ་ བྱ་ དགོས་ ཀྱིས་"
msgstr ""

msgctxt "25c3159755ad4a19b6b4e76b66b052df"
msgid "བདག་ ལ་ ཨུད་པ་ ལ་ སྔོན་པོ་ བྱིན་ ཅིག་ ཅེས་"
msgstr ""

msgctxt "1dc76753d5f34de28e68bbf3f24eae9a"
msgid "བྱས་ སོ ། །"
msgstr ""

msgctxt "8528f27624b349e39e8947d4cf650c26"
msgid "མེ་ཏོག་ཕྲེང་བ་ རྒྱུད་པ␣ས་ སྨྲས་པ །"
msgstr ""

# དེང་རྒྱལ་པོས་<«གཡུང་»«ལི་»«སྣར་»«ཅོ་»«ཞོལ་»པོ།
# «པེ་»པོབ།>སངས་རྒྱས་སྤྱན་དྲང་<«ཁུ་»དྲངས།>བའི་ཕྱིར་མེ་ཏོག་ཐམས་ཅད་བསྡུས་སོ། །
msgctxt "3502492ae8034b21867892b66113c80d"
msgid ""
"དེང་ རྒྱལ་པོ␣ས་ སངས་རྒྱས་ སྤྱན་ དྲང་བ␣འི་ ཕྱིར་ མེ་ཏོག་ ཐམས་ཅད་ བསྡུས་ སོ "
"། །"
msgstr ""

# དེས་<«གཡུང་»«པེ་»དེ།>སྨྲས་པ།
msgctxt "e33d164dcdf1488e9bdf1d50e2fd8c42"
msgid "དེ␣ས་ སྨྲས་པ །"
