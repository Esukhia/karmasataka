msgctxt "e25adbc009244d45a02b347d33658660"
msgid "བགྲངས་ ན་ སྐྲ་ ཉག་མ་ བརྒྱད་ ཁྲི་ བྱུང་ ངོ་ ། །"
msgstr ""

msgctxt "0de1af68ade8441b9f13445db8b3fee1"
msgid "དེ་ནས་ རྒྱལ་པོ་ དེ␣འི་ བློན་པོ་ རྣམས་ ཀྱིས་ གསོལ་པ །"
msgstr ""

msgctxt "30e556e84f44406c97a345f1dec52ec3"
msgid "ལྷ་ བདག་ཅག་ ལ་ སྐྲ་ ཉག་མ་ རེ་རེ་ སྩལ་བ␣ར་ གསོལ །"
msgstr ""

msgctxt "27ac547982424aac9e6b674e73eec89b"
msgid "འོན་ཀྱང་ བདག་ཅག་ གིས་ དེ་ རྣམས་ མཆོད་རྟེན་ དུ་ བགྱི་ བར་ འཚལ་ ལོ ། །"
msgstr ""

msgctxt "eca62ad6bb014ff398a0e6917dd6ef92"
msgid "དེ་ནས་ རྒྱལ་པོ་ དེ␣ས་ བློན་པོ་ དེ་དག་ ལ་ སྐྲ་ ཉག་མ་ རེ་རེ་ བྱིན་ ནས་"
msgstr ""

msgctxt "ff6a88bebe5747a0a13e30991cd8ed44"
msgid "བློན་པོ་ དེ་དག་ རང་རང་ གི་ ཡུལ་ དུ་ སོང་ སྟེ་"
msgstr ""

msgctxt "8c1a1f7d673444ef8b068f3ca81f52a9"
msgid "མཆོད་རྟེན་ བརྩིགས་ སོ ། །"
msgstr ""

# གང་གི་ཚེ་བྲམ་ཟེའི་ཁྱེའུ་བློ་གྲོས་བཟང་པོ་བླ་ན་མེད་པ་ཡང་དག་པར་རྫོགས་པའི་བྱང་ཆུབ་ཏུ་ལུང་བསྟན་པ་དེའི་ཚེ།
# རྒྱལ་པོ་མར་མེ་དང་རྒྱལ་པོ་ནོར་ཅན་གྱི་བུ་དང་གྲོང་ཁྱེར་<«གཡུང་»«ལི་»«པེ་»«སྣར་»«ཅོ་»«ཞོལ་»རྡལ།>གྱི་མི་དང་ཡུལ་གྱི་<«གཡུང་»«ལི་»«པེ་»«སྣར་»«ཅོ་»«ཞོལ་»–གྱི།>མི་དུ་མས་མ་འོངས་པའི་དུས་ཀྱི་ཡོན་ཏན་ལ་བལྟས་ཏེ་
msgctxt "1b6fa326dd6442a194b4cb75e20a1a47"
msgid ""
"གང་ གི་ ཚེ་ བྲམ་ཟེ␣འི་ ཁྱེའུ་ བློ་གྲོས་ བཟང་པོ་ བླ་ ན་ མེད་པ་ ཡང་དག་པ␣ར་ "
"རྫོགས་པ␣འི་ བྱང་ཆུབ་ ཏུ་ ལུང་བསྟན་པ་ དེ␣འི་ ཚེ །  རྒྱལ་པོ་ མར་མེ་ དང་ "
"རྒྱལ་པོ་ ནོར་ ཅན་ གྱི་ བུ་ དང་ གྲོང་ཁྱེར་ གྱི་ མི་ དང་ ཡུལ་ གྱི་ མི་ དུ་མ␣ས་"
" མ་འོངས་པ␣འི་ དུས་ ཀྱི་ ཡོན་ཏན་ ལ་ བལྟས་ ཏེ་"
msgstr ""

msgctxt "0ec59867d60e4bbf8f1d01f2c2d0f9c4"
msgid "ཡོ་བྱད་ ཐམས་ཅད་ བསྟབས་ སོ ། །"
msgstr ""

msgctxt "41b0671438f54569afa2f6a837616653"
msgid "དེ་ནས་ བློ་གྲོས་ བཟང་པོ␣ས་ བྲམ་ཟེ␣འི་ ཁྱེའུ་ བློ་གྲོས་ ལ་ སྨྲས་པ །"
msgstr ""

msgctxt "557defcca99f44da90b7b6b80a71149c"
msgid ""
"ང་ ནི་ བླ་ ན་ མེད་པ␣འི་ བྱང་ཆུབ་ ཏུ་ ལུང་བསྟན་ ན་ ཁྱོད་ ཀྱིས་ གང་ དུ་ "
"སེམས་བསྐྱེད །"
msgstr ""

msgctxt "da655b32b14f4214981558de6751220d"
msgid "དེ␣ས་ སྨྲས་པ །"
msgstr ""

msgctxt "51f31853aa524372a258a390d06f221e"
msgid "བདག་ ནི་ མ་ རུང་ ངོ་ ། །"
msgstr ""

msgctxt "4b2cc4aaf8494f4d8dc80f24117aa3a6"
msgid "བྲམ་ཟེ␣འི་ ཁྱེའུ་ བློ་གྲོས་ བཟང་པོ␣ས་ སྨྲས་པ །"
msgstr ""

# ཁྱོད་ཀྱིས་ཅི་ཞིག་བྱས་<«གཡུང་»«པེ་»བྱ།>ན།
# མ་:རུང་བར་འགྱུར<«གཡུང་»«པེ་»རུངས་པར་འགྱུར། «ཞོལ་»རུང་བར་གྱུར།>།
msgctxt "f671fdbda43446ec8fbc929562b855dc"
msgid "ཁྱོད་ ཀྱིས་ ཅི་ཞིག་ བྱས་ ན །  མ་ རུང་བ␣ར་ འགྱུར །"
msgstr ""

msgctxt "2c6d7766b5b84595aed6d7dab6859386"
