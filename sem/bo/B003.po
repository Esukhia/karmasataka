msgctxt "be1aeda0c42e49799ac08a50d0e9cdd6"
msgid "ཨུད་པ་ ལ་ འོ་ནི་ རྣམས་ ང་ ལ་ བྱིན་ ཅིག  །"
msgstr ""

msgctxt "7ca50fb34ac5472bb92c1fc52cbebe24"
msgid "བུ་མོ་ དེ␣ས་ སྨྲས་པ །"
msgstr ""

# དེའི་ཚེ་ཁྱོད་ཀྱིས་བདག་བཞེས་སུ་མ་གནང་<«གཡུང་»«ལི་»«པེ་»«སྣར་»«ཅོ་»བཏང་།>ན།
# ད་ཨུད་པ་ལ་རྣམས་བཞེད་དམ།
msgctxt "1daf512e8cac4ba4923c376827a4dea2"
msgid ""
"དེ␣འི་ ཚེ་ ཁྱོད་ ཀྱིས་ བདག་ བཞེས་ སུ་ མ་ གནང་ ན །  ད་ ཨུད་པ་ ལ་ རྣམས་ བཞེད་ "
"དམ །"
msgstr ""

# བདག་མི་དབུལ་<«གཡུང་»«ལི་»«པེ་»«སྣར་»«ཅོ་»«ཞོལ་»འབུལ།>ལོ་ཞེས་
msgctxt "89e23944a0404d1b99c11e10214cf14f"
msgid "བདག་ མི་ དབུལ་ ལོ་ ཞེས་"
msgstr ""

msgctxt "25bb5afddab34025a01d9859175df5cf"
msgid "དེ་སྐད་ སྨྲ␣ས་ ནས །"
msgstr ""

msgctxt "bfb1b714cfc04f4d96eea81053ceb2d8"
msgid "ཡང་ བྲམ་ཟེ␣འི་ ཁྱེའུ་ བློ་གྲོས་ བཟང་པོ་ ལ་ སྨྲས་པ །"
msgstr ""

# འདི་རྣམས་ཀྱིས་<«ཅོ་»ཀྱི།>ཅི་ཞིག་བགྱི།
msgctxt "39e1002e7e794885b6e248e2b9a24ca8"
msgid "འདི་ རྣམས་ ཀྱིས་ ཅི་ཞིག་ བགྱི །"
msgstr ""

msgctxt "6bcf3ad8845b4dc796b6bb67c01696a6"
msgid "བློ་གྲོས་ བཟང་པོ␣ས་ སྨྲས་པ །"
msgstr ""

msgctxt "be445a3b113c437492bdf488ac3434ed"
msgid "སངས་རྒྱས་ བཅོམ་ལྡན་འདས་ ལ་ མཆོད་པ␣ར་ བྱ␣འོ ། །"
msgstr ""

msgctxt "86c43bb205f54d5186b3b274b837e3fe"
msgid "དེ␣འི་ འོག་ ཏུ་ བུ་མོ་ དེ␣ས་ སྨྲས་པ །"
msgstr ""

# བདག་ལ་:ཀཱར་ཥཱ་པ་ཎ་<«གཡུང་»ཀར་ཤ་པ་ཎེ། «པེ་»ཀར་ཥ་པ་ཎ།
# «ལི་»«སྣར་»«ཅོ་»ཀར་ཥ་པཱ་ཎ། «ཞོལ་»ཀརྵ་པ་ན།>ཅང་མི་འཚལ་གྱིས་<«ཞོལ་»གྱི།>
msgctxt "fd77f32d4a0a4f778533acf3921c430b"
msgid "བདག་ ལ་ ཀཱར་ ཥཱ་པ་ ཎ་ ཅང་ མི་ འཚལ་ གྱིས་"
msgstr ""

msgctxt "7315659271444937b0562d16e62e88e2"
msgid ""
"གལ་ཏེ་ ཁྱོད་ ལ་ ཨུད་པ་ ལ་ འདི་དག་ ཕུལ་བ␣འི་ འབྲས་བུ␣ས །  ཚེ་རབས་ ཐམས་ཅད་ དུ་"
" བདག་ ཁྱོད་ ཀྱི་ བཙུན་མོ␣ར་ འགྱུར་བ␣ར་ བཞེད་ དེ །"
msgstr ""

# གལ་ཏེ་སྦྱིན་པ་འདི་སྦྱིན་<«སྣར་»«ཞོལ་»བྱིན།>པ་ན་<«གཡུང་»«ལི་»«པེ་»«སྣར་»«ཅོ་»ནི།>ཚེ་རབས་ཐམས་ཅད་དུ་ཁྱོད་བདག་གི་ཆུང་མར་གྱུར་ཅིག་ཅེས་
msgctxt "f3ede2b0c6d046dc830ec5ca6f4e7954"
msgid ""
"གལ་ཏེ་ སྦྱིན་པ་ འདི་ སྦྱིན་པ་ ན་ ཚེ་རབས་ ཐམས་ཅད་ དུ་ ཁྱོད་ བདག་ གི་ ཆུང་མ␣ར་"
" གྱུར་ཅིག་ ཅེས་"
msgstr ""

# དེ་སྐད་དུ་སྨོན་ལམ་འདེབས་ན་ནི། བདག་གིས་ཡེ་<«གཡུང་»«པེ་»ཡོ།
# «ཞོལ་»ཡི།>དབུལ་བར་འཚལ་ལོ། །
msgctxt "b99707da4d604137a91285214c55f1c1"
msgid ""
"དེ་སྐད་ དུ་ སྨོན་ལམ་ འདེབས་ ན་ ནི །  བདག་ གིས་ ཡེ་ དབུལ་བ␣ར་ འཚལ་ ལོ ། །"
msgstr ""

msgctxt "5417319f58b549a3b27c127b5c2f2e38"
msgid "བློ་གྲོས་ བཟང་པོ␣ས་ སྨྲས་པ །"
msgstr ""

msgctxt "9bcff8af2f314b67bf6c3090cc9675f3"
msgid "ང་ ནི་ སྦྱིན་པ་ ལ་ དགའ་བ་ ཡིན་ ཏེ །"
msgstr ""

# བདག་གི་ཆུང་མ་དང་བུ་<«གཡུང་»«པེ་»+མོ།>ཡང་ཡོངས་སུ་གཏོང་ལ།
msgctxt "88a16ff84b8d40fc9605ebc7964b5453"
msgid "བདག་ གི་ ཆུང་མ་ དང་ བུ་ ཡང་ ཡོངས་སུ་ གཏོང་ ལ །"
msgstr ""

msgctxt "0f545467b41745fe99e7189e2e86fbb3"
msgid "བདག་ གི་ ཤ་ ཡང་ ཡོངས་སུ་ གཏོང་བ␣ར་ བྱེད་ དོ ། །"
msgstr ""

msgctxt "b9f5fa7cf8244c35a0f0951164c10f4e"
msgid "བུ་མོ་ དེ␣ས་ སྨྲས་པ །"
msgstr ""

# ཇི་<«གཡུང་»«ལི་»«པེ་»«སྣར་»«ཞོལ་»ཇེ།>སྨོན་ལམ་དེ་ལྟར་ཐོབ་ལ་
msgctxt "c3d43c71c895405fbeb783392ffe9ebe"
msgid "ཇི་ སྨོན་ལམ་ དེ་ལྟར་ ཐོབ་ ལ་"
msgstr ""

msgctxt "6c0b3b6850e0459f8af651a614f25a79"
msgid "སླད་ ཀྱིས་ སུ་ ཅི་ འདོད་པ་ དེ་ ལ་ བདག་ སྩོལ་ ཅིག  །"
msgstr ""

msgctxt "62058e8ff5154e60bb7fd52d98d88498"
msgid "དེ་སྐད་ ཅེས་ སྨྲ␣ས་ ནས །"
msgstr ""

msgctxt "3890b6f4dcd1476e81b1a685a7d308b3"
msgid "བུ་མོ་ དེ␣ས་ བློ་གྲོས་ བཟང་པོ་ ལ་ ཨུད་པ་ ལ་ ལྔ་ བྱིན་ ནོ ། །"
msgstr ""

msgctxt "1e121742699c41778c1f151cdd7e2e94"
msgid "གཉིས་ ནི་ བདག་ཉིད་ ཀྱིས་ ཁྱེར་ ནས་"
msgstr ""

msgctxt "91b04eb5a3f04c139ae6a906178928f5"
msgid "ཚིགས་ སུ་ བཅད་ དེ་ སྨྲས་པ །"
msgstr ""

# ཁྱོད་ཀྱིས་གང་དུ་སྨོན་པ་ཡི།
# །འདྲེན་པ་སངས་རྒྱས་བརྙེས་<«གཡུང་»«ལི་»«པེ་»«སྣར་»«ཅོ་»«ཞོལ་»རྙེད།>པ་དེར།
# རྟག་ཏུ་འཐུན་པར་ཆོས་སྤྱོད་པའི། །ཁྱོད་ཀྱི་བཙུན་མོར་བདག་གྱུར་ཅིག །
msgctxt "19992033902644cdba87638a28b33c4a"
msgid ""
"ཁྱོད་ ཀྱིས་ གང་ དུ་ སྨོན་པ་ ཡི ། ། འདྲེན་པ་ སངས་རྒྱས་ བརྙེས་པ་ དེ␣ར །  རྟག་ "
"ཏུ་ འཐུན་པ␣ར་ ཆོས་སྤྱོད་པ␣འི ། ། ཁྱོད་ ཀྱི་ བཙུན་མོ␣ར་ བདག་ གྱུར་ཅིག  །"
msgstr ""

# དེ་ནས་རྒྱལ་པོ་དེས་གྲོང་ཁྱེར་དེ་དག་ཐམས་ཅད་རྡོ་དང་། གསེག་མ་དང་།
# གྱོ་མོ་མེད་པར་བྱས་ནས། རྒྱལ་མཚན་དང་། བ་དན་དང་།
# རྟ་བབས་རྣམས་<«གཡུང་»«ལི་»«པེ་»«སྣར་»«ཅོ་»«ཞོལ་»–རྣམས།>ནི་བཙུགས།
msgctxt "6f3f07bfdf14449aa2097d3c711d60ba"
msgid ""
"དེ་ནས་ རྒྱལ་པོ་ དེ␣ས་ གྲོང་ཁྱེར་ དེ་དག་ ཐམས་ཅད་ རྡོ་ དང་ །  གསེག་མ་ དང་ །  "
"གྱོ་མོ་ མེད་པ␣ར་ བྱས་ ནས །  རྒྱལ་མཚན་ དང་ །  བ་དན་ དང་ །  རྟ་བབས་ རྣམས་ ནི་ "
"བཙུགས །"
msgstr ""

# དར་གྱི་ལྡ་ལྡི་རྣམས་ནི་བྲེས<«ཁུ་»བྲིས།>།
msgctxt "72f1bdc570264704b5ecebce6cbe5826"
msgid "དར་ གྱི་ ལྡ་ལྡི་ རྣམས་ ནི་ བྲེས །"
msgstr ""

# སྤོས་ཆབ་དང་། ཕྱེ་མ་རྣམས་ཀྱིས་ནི་གཏོར་ཏོ<«གཡུང་»«པེ་»ཏེ།>། །
msgctxt "adf01d878d95491194383eae4f0cf482"
msgid "སྤོས་ ཆབ་ དང་ །  ཕྱེ་མ་ རྣམས་ ཀྱིས་ ནི་ གཏོར་ ཏོ ། །"
msgstr ""

msgctxt "1b221d4c8eae4a6ebbaaf3197891f35a"
msgid ""
"གྲོང་ཁྱེར་ གྱི་ སྒོ་ ནས་ གཙུག་ལག་ཁང་ གི་ བར་ དུ་ ཡང་ རྡོ་ དང་ །  གསེག་མ་ དང་"
" །  གྱོ་མོ་ རྣམས་ མེད་པ␣ར་ བྱས་ ནས །"
msgstr ""

# རྒྱལ་མཚན་དང་། བ་དན་དང་།[88b]རྟ་བབས་ནི་བཙུགས<«གཡུང་»གཙུགས།>།
msgctxt "56cd08f443554d3181fa514a1f4df76a"
msgid "རྒྱལ་མཚན་ དང་ །  བ་དན་ དང་ ། རྟ་བབས་ ནི་ བཙུགས །"
msgstr ""

# དར་གྱི་ལྡ་ལྡི་ནི་བྲེས<«ཁུ་»བྲིས།>།
msgctxt "2782d856cff3401caef1fb8c08bd16fe"
msgid "དར་ གྱི་ ལྡ་ལྡི་ ནི་ བྲེས །"
msgstr ""

# སྤོས་ཆབ་དང་། ཕྱེ་མས་ནི་གཏོར་ཏོ<«གཡུང་»–ཏོ།>། །
msgctxt "7ed506dfc6564f36a5d9e133895de252"
msgid "སྤོས་ ཆབ་ དང་ །  ཕྱེ་མ␣ས་ ནི་ གཏོར་ ཏོ ། །"
msgstr ""

msgctxt "13859b6c153e4180a1ca2cb84b9ebd59"
msgid "རྒྱལ་པོ་ དེ་ཉིད་ ཀྱིས་ ཀྱང་ གདུགས་ རྩིབས་ བརྒྱ་པ་ ཐོགས་ ནས་"
msgstr ""

msgctxt "e998aa882d954ecc9f25cfa632fffc2f"
msgid "ཡང་དག་པ␣ར་ རྫོགས་པ␣འི་ སངས་རྒྱས་ མར་མེ་མཛད་ བསུའོ ། །"
msgstr ""

msgctxt "026cf9ff16f546d7ad2e7524131c3a12"
msgid "དེ་བཞིན་ དུ་ བློན་པོ་ རྣམས་ ཀྱིས་ ཀྱང་ བསུའོ ། །"
msgstr ""

msgctxt "808d9fc2f7444194b988bf82876c3348"
msgid ""
"དེ་བཞིན་ དུ་ རྒྱལ་པོ་ ནོར་ ཅན་ གྱི་ བུ་ བློན་པོ་ དང་ བཅས་པ་ རྣམས་ ཀྱིས་ ཀྱང་"
" བསུའོ ། །"
msgstr ""

# དེ་ནས་རྒྱལ་པོས་སངས་རྒྱས་བཅོམ་ལྡན་འདས་ཀྱི་<«ཞོལ་»ཀྱིས།>ཞབས་ལ་མགོ་བོས་ཕྱག་འཚལ་ཏེ།
msgctxt "0ef17b387d794edfa8df1103a1c1ad96"
msgid ""
"དེ་ནས་ རྒྱལ་པོ␣ས་ སངས་རྒྱས་ བཅོམ་ལྡན་འདས་ ཀྱི་ ཞབས་ ལ་མགོ་ བོས་ ཕྱག་ འཚལ་ ཏེ"
" །"
msgstr ""

msgctxt "a28d650f9a21427b9b5d8ab78995f728"
msgid "བཅོམ་ལྡན་འདས་ གྲོང་ཁྱེར་ དུ་ གཤེགས་ སུ་ གསོལ་ ཞེས་"
msgstr ""

msgctxt "2c289c82f0744f48bd94cdd120339e30"
msgid "གསོལ་བ་ བཏབ་ ནས །"
msgstr ""

# བཅོམ་ལྡན་འདས་དེ་དགེ་སློང་གི་དགེ་འདུན་གྱིས་<«ཅོ་»གྱི།>མདུན་དུ་བདར་ཏེ།
msgctxt "99fea2ad1f3049dca23ba195d7c7f5e0"
msgid "བཅོམ་ལྡན་འདས་ དེ་ དགེ་སློང་ གི་ དགེ་འདུན་ གྱིས་ མདུན་ དུ་ བདར་ ཏེ །"
msgstr ""

msgctxt "287e057627d84ab0ad7867e566e73227"
msgid "གྲོང་ཁྱེར་ ལོགས་ སུ་ ཆ␣ས་ ཏེ་"
msgstr ""

msgctxt "bde676118fd44f03be5722eedefee61e"
msgid "གཤེགས་ སོ ། །"
msgstr ""

msgctxt "5a6470e3ec2247c2afc78609de1de2a7"
msgid ""
"རྒྱལ་པོ་ མར་མེ་ ཡང་ཡང་ དག་པ␣ར་ རྫོགས་པ␣འི་ སངས་རྒྱས་ མར་མེ་མཛད་ ཀྱི་ སྟེང་ "
"དུ་ གདུགས་ རྩིབས་ བརྒྱ་པ་ ཐོགས་ སོ ། །"
msgstr ""

msgctxt "d7ac775440d34b90878fbf67a9f465ab"
msgid ""
"དེ་བཞིན་ དུ་ བློན་པོ་ རྣམས་ དང་ །  རྒྱལ་པོ་ ནོར་ ཅན་ གྱི་ བུ་ བློན་པོ་ དང་ "
"བཅས་པ་ རྣམས་ ཀྱང་ གདུགས་ ཐོགས་ སོ ། །"
msgstr ""

# དེ་ནས་བཅོམ་ལྡན་འདས་ཀྱིས་ཅི་ནས་རེ་རེ་ནས་ད་<«པེ་»«སྣར་»དེ།>བདག་ནི་བཅོམ་ལྡན་འདས་ཀྱི་གདུགས་ཐོགས་སོ།
# །སྙམ་དུ་
msgctxt "218636c371d242029ebf82abd29c4a17"
msgid ""
"དེ་ནས་ བཅོམ་ལྡན་འདས་ ཀྱིས་ ཅི་ ནས་ རེ་རེ་ ནས་ ད་ བདག་ ནི་ བཅོམ་ལྡན་འདས་ ཀྱི་"
" གདུགས་ ཐོགས་ སོ ། ། སྙམ་ དུ་"
msgstr ""

# སེམས་པར་<«གཡུང་»«པེ་»དཔར།>འགྱུར་བ་དེ་ལྟ་བུར་རྫུ་འཕྲུལ་གྱི་<«གཡུང་»«པེ་»«ཞོལ་»གྱིས།>བྱིན་གྱིས་བརླབས་སོ།
# །
msgctxt "cbdba992871f4255a6a93f52d0f01eae"
msgid ""
"སེམས་པ␣ར་ འགྱུར་བ་ དེ་ལྟ་ བུར་ རྫུ་འཕྲུལ་ གྱི་ བྱིན་ གྱིས་ བརླབས་ སོ ། །"
msgstr ""

# དེ་ནས་བཅོམ་ལྡན་འདས་དེ་ལྟ་བུའི་མཛེས་པ་དང་ལྡན་པས་གྲོང་<«ཅོ་»གྲོ།>ཁྱེར་གྱི་སྒོར་གཤེགས་ནས།
msgctxt "33273256259343f595b99100143a7344"
msgid ""
"དེ་ནས་ བཅོམ་ལྡན་འདས་ དེ་ལྟ་ བུ␣འི་ མཛེས་པ་ དང་ ལྡན་པ␣ས་ གྲོང་ཁྱེར་ གྱི་ "
"སྒོར་ གཤེགས་ ནས །"
msgstr ""

msgctxt "89ee0134dbfe402ca4c6fd25e3d24cc1"
msgid "དེ␣ར་ བཅོམ་ལྡན་འདས་ ཀྱིས་ ཆེད་ དུ་ མཛད་ དེ །"
msgstr ""

msgctxt "ae016fd47db94829a76ce5b690a1a623"
msgid "སྒོ␣འི་ ཐེམ་པ་ ལ་ ཞབས་ བཞག་ ནས །"
msgstr ""

msgctxt "4b00b501e0f6435caef577ec334b4027"
msgid ""
"གང་ གི་ ཚེ་ བཅོམ་ལྡན་འདས་ ཀྱིས་ སྒོ␣འི་ ཐེམ་པ་ ལ་ ཞབས་ བཞག་པ་ དེ་ ཁོ་ན␣འི་ "
"མོད་ ལ་ ས་ རྣམ་པ་ དྲུག་ ཏུ་ གཡོས་ ཏེ །"
msgstr ""

# འགུལ་རབ་ཏུ་འགུལ། ཀུན་དུ་རབ་ཏུ་འགུལ། ལྡེག་རབ་<«གཡུང་»ཀུན།>ཏུ་ལྡེག
# །ཀུན་དུ་རབ་ཏུ་ལྡེག་པར་གྱུར་ཏོ། །
msgctxt "11bbf09854324127a439573acbec3c93"
msgid ""
"འགུལ་ རབ་ཏུ་ འགུལ །  ཀུན་ དུ་ རབ་ཏུ་ འགུལ །  ལྡེག་ རབ་ཏུ་ལྡེག  ། ཀུན་ དུ་ "
"རབ་ཏུ་ལྡེག་པ␣ར་ གྱུར་ ཏོ ། །"
msgstr ""

msgctxt "ea80d558a0f24329a6b5b83fad40eec3"
msgid ""
"ཆོས་ཉིད་ ཀྱིས་ ན་ སངས་རྒྱས་ བཅོམ་ལྡན་འདས་ རྣམས་ ཀྱིས་ གང་ གི་ ཚེ་ ཆེད་ དུ་ "
"མཛད་ དེ །"
msgstr ""

msgctxt "03f2284a148a4b90bb086f97a180f35d"
msgid ""
"སྒོ␣འི་ ཐེམ་པ་ ལ་ ཞབས་ འཇོག་པ་ ན་ ངོ་མཚར་ རྨད་ དུ་ བྱུང་བ␣འི་ ཆོས་ རྣམ་པ་ "
"སྣ་ཚོགས་ འབྱུང་བ␣ར་ འགྱུར་ ཏེ །"
msgstr ""

# སྨྱོན་པ་རྣམས་ཀྱིས་ནི་རང་བཞིན་གྱིས་<«ཞོལ་»གྱི།>སེམས་ཐོབ་པར་འགྱུར་རོ། །
msgctxt "d1dfe08011f948ab91a6c94f927a1534"
msgid "སྨྱོན་པ་ རྣམས་ ཀྱིས་ ནི་ རང་བཞིན་ གྱིས་ སེམས་ ཐོབ་པ␣ར་ འགྱུར་ རོ ། །"
msgstr ""

# ལོང་བ་རྣམས་ནི་མིག་གིས་<«གཡུང་»«ལི་»«པེ་»«སྣར་»«ཅོ་»–གིས།>མཐོང་བར་འགྱུར་རོ། །
msgctxt "d3d82ad484d142e2b958dc386007ed38"
msgid "ལོང་བ་ རྣམས་ ནི་ མིག་ གིས་ མཐོང་བ␣ར་ འགྱུར་ རོ ། །"
msgstr ""

# འོན་པ་རྣམས་ནི་རྣ་བས་<«གཡུང་»«པེ་»བ།>ཐོས་པར་འགྱུར་རོ། །
msgctxt "a18e324fc9c74e41819a8e899cd3e482"
msgid "འོན་པ་ རྣམས་ ནི་ རྣ་བ␣ས་ ཐོས་པ␣ར་ འགྱུར་ རོ ། །"
msgstr ""

# ལྐུགས་<«ལི་»«སྣར་»«ཅོ་»«ཞོལ་»ལྐུག>[89a]པ་རྣམས་ནི་སྨྲ་ནུས་པར་འགྱུར་རོ། །
msgctxt "c888668d93c444bfab8cf563a372dab0"
msgid "ལྐུགས་པ་ རྣམས་ ནི་ སྨྲ་ ནུས་པ␣ར་ འགྱུར་ རོ ། །"
msgstr ""

msgctxt "3f33d48a1261413598b11f55da6b98d8"
msgid "འཕྱེ་བོ་ རྣམས་ ནི་ འགྲོ་ ནུས་པ␣ར་ འགྱུར་ རོ ། །"
msgstr ""

# བུད་མེད་བུ་འབྱུང་དུ་མི་བཏུབ་པ་རྣམས་ནི་བདེ་བར་:བཙས་པར་<«ཞོལ་»བཙའ་བར།>འགྱུར་རོ།
# །
msgctxt "4507059588a14bd899cd3a99eeb499e2"
msgid ""
"བུད་མེད་ བུ་ འབྱུང་ དུ་ མི་ བཏུབ་པ་ རྣམས་ ནི་ བདེ་བ␣ར་ བཙས་པ␣ར་ འགྱུར་ རོ "
"། །"
msgstr ""

msgctxt "b8ae45b9390b4719b7c198a6fe26f2d8"
msgid ""
"ཤིང་ མིག་ གི་ གདོས་ དང་ རྐང་ སྒྲིལ་ དང་ །  ཨན་ པན་ བཏགས་པ␣འི་ སེམས་ཅན་ རྣམས་"
" ནི་ བཅིངས་པ་ ལ␣ས་ གྲོལ་བ␣ར་ འགྱུར་ རོ ། །"
msgstr ""

# ཆུང་ངུ་ནས་འཁོན་<«གཡུང་»«པེ་»ཁོན།>བྱུང་བ་རྣམས་ནི་དེ་མ་ཐག་ཏུ་བྱམས་པའི་སེམས་ཐོབ་པར་འགྱུར་རོ།
# །
msgctxt "bd6e83b8595a42b2b6c44c039b8baf1c"
msgid ""
"ཆུང་ངུ་ ནས་ འཁོན་ བྱུང་བ་ རྣམས་ ནི་ དེ་ མ་ ཐག་ ཏུ་ བྱམས་པ␣འི་ སེམས་ ཐོབ་པ␣ར་"
" འགྱུར་ རོ ། །"
msgstr ""

# བེའུ་རྣམས་ནི་བེའུ་གདང་<«གཡུང་»«ལི་»«པེ་»«སྣར་»«ཅོ་»རྡང་།>བཅད་དེ་མའི་ཐད་དུ་<«གཡུང་»–དུ།>འདོང་བར་བྱེད་དོ།
# །
msgctxt "e3b4357fa7814643ba04a92f92abfd87"
msgid ""
"བེའུ་ རྣམས་ ནི་ བེའུ་གདང་ བཅད་ དེ་ མ␣འི་ ཐད་ དུ་ འདོང་བ␣ར་ བྱེད་ དོ ། །"
msgstr ""

msgctxt "5f609c252b1a41838f63f77cf34e76de"
msgid "གླང་པོ་ཆེ་ རྣམས་ ཀྱང་ སྐད་ འབྱིན་ ཏོ ། །"
msgstr ""

msgctxt "b9429817f2d649f788ab7df3ee3430a9"
msgid "རྟ་ རྣམས་ ཀྱང་ སྐད་ འབྱིན་ ཏོ  །"
msgstr ""

# ཁྱུ་མཆོག་རྣམས་ཀྱང་སྐད་<«ལི་»«ཅོ་»–སྐད།>འབྱིན་ཏོ། །
msgctxt "fc1de298405c4d9c85f9a6bfc7daf886"
msgid "ཁྱུ་མཆོག་ རྣམས་ ཀྱང་ སྐད་ འབྱིན་ ཏོ ། །"
msgstr ""

# ནེ་ཙོ་དང་། རི་སྐེགས་<«སྣར་»«ཞོལ་»སྐེག>དང་། ཁུ་བྱུག་དང་།
# ཤང་ཤང་ཏེའུ་རྣམས་ཀྱང་སྐད་སྙན་པ་འབྱིན་ཏོ། །
msgctxt "426db91f90f64cca88fd138254ece69b"
msgid ""
"ནེ་ཙོ་ དང་ །  རི་སྐེགས་ དང་ །  ཁུ་བྱུག་ དང་ །  ཤང་ཤང་ཏེའུ་ རྣམས་ ཀྱང་ སྐད་ "
"སྙན་པ་ འབྱིན་ ཏོ ། །"
msgstr ""

# :རོལ་མོའི་ཆ་བྱད་རྣམས་ཀྱང་མ་བརྡུངས་པར་སྒྲ་སྙན་པ་འབྱིན་ཏོ།
# །<«ལི་»«སྣར་»«ཅོ་»«ཞོལ་»–རོལ་མོའི་ཆ་བྱད་རྣམས་ཀྱང་མ་བརྡུངས་པར་སྒྲ་སྙན་པ་འབྱིན་ཏོ།>
msgctxt "071088ac56284d0aa1da28d3f36bbef0"
msgid "རོལ་མོ␣འི་ ཆ་བྱད་ རྣམས་ ཀྱང་ མ་ བརྡུངས་པ␣ར་ སྒྲ་སྙན་པ་ འབྱིན་ ཏོ ། །"
msgstr ""

msgctxt "13395911d7c040d8a169d97463d71058"
msgid "རྒྱན་ སྣོད་ དུ་ བཅུག་པ་ རྣམས་ ཀྱང་ སྒྲ་སྙན་པ་ འབྱིན་ ཏོ ། །"
msgstr ""

# ས་ཕྱོགས་མཐོ་བ་རྣམས་ནི་དམའ་བར་འགྱུར<«ཞོལ་»འགྱུར་རོ།>།
msgctxt "4f4d97d5ebd341e1a85ca0d4fd7f6b3a"
msgid "ས་ཕྱོགས་ མཐོ་བ་ རྣམས་ ནི་ དམའ་བ␣ར་ འགྱུར །"
msgstr ""

msgctxt "587d950ee3ad4c68a27e210be759851e"
msgid "དམའ་བ་ རྣམས་ ནི་ མཐོ་བ␣ར་ འགྱུར་ རོ ། །"
msgstr ""

# རྡོ་དང་། གསེག་མ་དང་། གྱོ་མོ་རྣམས་ཀྱང་མེད་པར་འགྱུར་རོ<«པེ་»ཏོ།>། །
msgctxt "b16b867aecc545849daaff8c4d13a08e"
msgid "རྡོ་ དང་ །  གསེག་མ་ དང་ །  གྱོ་མོ་ རྣམས་ ཀྱང་ མེད་པ␣ར་ འགྱུར་ རོ ། །"
msgstr ""

# ལྷ་རྣམས་ཀྱང་བར་སྣང་ལས་ལྷའི་མེ་ཏོག་ཨུད་པ་ལ་དང་། པད་མ་དང་།
# ཀུ་མུ་ད་<«གཡུང་»«པེ་»ཏ།>དང་།
# པད་མ་དཀར་པོ་རྣམས་:འཐོར་བར་<«གཡུང་»«པེ་»མཐོར་བར།>བྱེད་དོ། །
msgctxt "f39f8d1bec484cb29e477b6aa9e78c42"
msgid ""
"ལྷ་ རྣམས་ ཀྱང་ བར་སྣང་ ལ␣ས་ ལྷ␣འི་ མེ་ཏོག་ ཨུད་པ་ ལ་ དང་ །  པད་མ་ དང་ །  "
"ཀུ་མུ་ད་ དང་ །  པད་མ་ དཀར་པོ་ རྣམས་ འཐོར་བ␣ར་ བྱེད་ དོ ། །"
msgstr ""

# ཨ་ག་རུའི་ཕྱེ་མ་དང་། རྒྱ་སྤོས་ཀྱི་ཕྱེ་མ་དང་། ཙན་དན་གྱི་ཕྱེ་མ་དང་།
# ཏ་མ་ལའི་ཕྱེ་མ་རྣམས་དང་།
# ལྷའི་མེ་ཏོག་མན་དཱ་ར་བ་<«གཡུང་»«ལི་»«པེ་»«སྣར་»«ཅོ་»–བ།>རྣམས་ཀྱང་འཐོར་བར་བྱེད་དོ།
# །
msgctxt "c778389283d04873b5986bc1181c08ed"
msgid ""
"ཨ་ ག་ རུའི་ ཕྱེ་མ་ དང་ །  རྒྱ་སྤོས་ ཀྱི་ ཕྱེ་མ་ དང་ །  ཙན་དན་ གྱི་ ཕྱེ་མ་ "
"དང་ །  ཏ་ མ་ལ␣འི་ ཕྱེ་མ་ རྣམས་ དང་ །  ལྷ␣འི་ མེ་ཏོག་ མན་ དཱ་ ར་བ་ རྣམས་ ཀྱང་"
" འཐོར་བ␣ར་ བྱེད་ དོ ། །"
msgstr ""

# ཤར་ཕྱོགས་དམའ་ན་ནུབ་ཕྱོགས་མཐོ<«པེ་»འཐོ།>།
msgctxt "edc93a7a2a3a4932ac47292bad397659"
msgid "ཤར་ཕྱོགས་ དམའ་ ན་ ནུབ་ཕྱོགས་ མཐོ །"
msgstr ""

msgctxt "7b96a7a158604624894e518dc3694d82"
msgid "ནུབ་ཕྱོགས་ དམའ་ ན་ ཤར་ཕྱོགས་ མཐོ །"
msgstr ""

msgctxt "6ff1b3a6a500441b81eca95180ac77eb"
msgid "ལྷོ་ཕྱོགས་ དམའ་ ན་ བྱང་ཕྱོགས་ མཐོ །"
msgstr ""

msgctxt "b6c570be75164b4b96f2e9174507a0bc"
msgid "བྱང་ཕྱོགས་ དམའ་ ན་ ལྷོ་ཕྱོགས་ མཐོ །"
msgstr ""

msgctxt "5a8760d12c7d415f825a05a797edd9ec"
msgid "དབུས་ དམའ་ ན་ མཐའ་ མཐོ །"
msgstr ""

msgctxt "8ba4a20dd7bb4553b892d71413476724"
msgid "མཐའ་ དམའ་ ན་ དབུས་ མཐོ་བ␣ར་ འགྱུར་ རོ ། །"
msgstr ""

msgctxt "07cc97803a0e45d5ae1f19df76ebd1f7"
msgid ""
"རྒྱལ་པོ␣འི་ ཕོ་བྲང་ མར་མེ་ ལྡན་ དེ་ ན་ སྲོག་ཆགས་ འབུམ་ཕྲག་ དུ་མ་ མེ་ཏོག་ དང་"
" །  བདུག་པ་ དང་ །  སྤོས་ རྣམས་ ཀྱིས་ བཀུར་སྟི་ བྱེད་ དོ ། །"
msgstr ""

# བློ་གྲོས་བཟང་པོ་དང་། :བློ་གྲོས་དང་།<«ལི་»«སྣར་»«ཅོ་»–བློ་གྲོས་དང་།>
# བུ་མོ་དེ་ཡང་པད་མ་ཁྱེར་ཏེ་
msgctxt "baa388d3e13b4600b6b5b9a18d9c9126"
msgid ""
"བློ་གྲོས་ བཟང་པོ་ དང་ །  བློ་གྲོས་ དང་ །  བུ་མོ་ དེ་ཡང་ པད་མ་ ཁྱེར་ ཏེ་"
msgstr ""

# ཡང་དག་པར་རྫོགས་པའི་སངས་རྒྱས་མར་མེ་མཛད་ག་ལ་བ་དེར་<«ཅོ་»དེ།>དོང་བ་ལས།
msgctxt "99943cd2f0a94108af71e861cc6f6433"
msgid "ཡང་དག་པ␣ར་ རྫོགས་པ␣འི་ སངས་རྒྱས་ མར་མེ་མཛད་ ག་ལ་བ་ དེ␣ར་ དོང་བ་ ལ␣ས །"
msgstr ""

msgctxt "9fbcc338bcd64c9682b519c3cfeae640"
msgid "དེ་དག་ སྐྱེ་བོ་ ཕལ་པོ་ ཆེ་ མཆོད་པ་ བྱེད་པ␣ས་ གཏམས་ ནས་"
msgstr ""

# བཅོམ་ལྡན་འདས་ཀྱི་<«གཡུང་»«པེ་»ཀྱིས།>སྤྱན་སྔར་མ་ཆུད་དོ། །
msgctxt "0bb75f7ecb73435e813e03b600e9f6a8"
msgid "བཅོམ་ལྡན་འདས་ ཀྱི་ སྤྱན་སྔ␣ར་ མ་ ཆུད་ དོ ། །"
msgstr ""

msgctxt "7d7d2e7b654b421c8d4cfb1b2ba2e870"
msgid "དེ་ནས་ བཅོམ་ལྡན་འདས་ ཀྱིས་ དགོངས་པ །"
msgstr ""

# སྐྱེ་བོ་ཕལ་པོ་ཆེ་འདི་རྣམས་པས་<«སྣར་»«ཞོལ་»ལས།>བྲམ་ཟེའི་ཁྱེའུ་བློ་གྲོས་བཟང་པོ་བསོད་ནམས་ཆེས་འཕེལ་བར་འགྱུར་རོ་སྙམ་དུ་
msgctxt "9ae45765b50b4ef6b73c85b43231f016"
msgid ""
"སྐྱེ་བོ་ ཕལ་པོ་ ཆེ་ འདི་ རྣམས་པ␣ས་ བྲམ་ཟེ␣འི་ ཁྱེའུ་ བློ་གྲོས་ བཟང་པོ་ "
"བསོད་ནམས་ ཆེས་ འཕེལ་བ␣ར་ འགྱུར་ རོ་ སྙམ་ དུ་"
msgstr ""

msgctxt "9d88b2138397430d8ec538ee97445914"
msgid "དགོངས་ ནས་"
msgstr ""

msgctxt "1e5e5e1d651541faaa7a66bebc5a42ff"
msgid "རྫི་ཆར་ དྲག་པོ་ ཆེན་པོ་ ཞིག་ སྤྲུལ་ ཏོ ། །"
msgstr ""

msgctxt "cb9cbd7853c84b3793ae75cef1d57d90"
msgid "དེ་ནས་ སྐྱེ་བོ་ ཕལ་པོ་ ཆེ་ དེ་དག་ གིས་ གོ་སྐབས་ ཕྱེ་ ནས །"
msgstr ""

msgctxt "3d68f91811cd4c20b0ec211d43e701da"
msgid "བྲམ་ཟེ␣འི་ ཁྱེའུ་ བློ་གྲོས་ བཟང་པོ␣ས་ སྐབས་ ཐོད་ དེ་"
msgstr ""

# བཅོམ་ལྡན་འདས་:ལ་བལྟ་<«གཡུང་»«ལི་»«པེ་»«སྣར་»«ཅོ་»ལྟ།
# «ཞོལ་»–ལ།>བས་ཆོག་མི་ཤེས་པ་མཐོང་ནས་
msgctxt "400a967b402c4d628ce5a9f6040b31d5"
msgid "བཅོམ་ལྡན་འདས་ ལ་ བལྟ་བ␣ས་ ཆོག་ མི་ ཤེས་པ་ མཐོང་ ནས་"
msgstr ""

msgctxt "059750b0a1254aedb2f52aea0198c78b"
msgid "ཤིན་ཏུ་ དགའ་བ་ སྐྱེས་ སོ ། །"
msgstr ""

msgctxt "afe43256f6c6467cb867c0ba97f9d838"
msgid "དགའ་བ་ སྐྱེས་ ནས་"
msgstr ""

msgctxt "af9620d671d54c2ea608cb4601a2aac2"
msgid "ཨུད་པ་ ལ་ ལྔ་པོ་ དེ་དག་ བཅོམ་ལྡན་འདས་ ལ་ གཏོར་ ཏོ ། །"
msgstr ""

# དེ་ནས་ཡང་དག་པར་རྫོགས་པའི་སངས་རྒྱས་མར་མེ་མཛད་ཀྱིས།
# དེ་དག་ཤིང་རྟའི་<«གཡུང་»«པེ་»ཏའི།>འཕང་ལོ་ཙམ་ཤ་སྟག་ཏུ་བླ་རེར་<«གཡུང་»ར་ར།
# «སྣར་»«ཞོལ་»བྲེར།>ཆགས་ཤིང་འདུག་ལ།
msgctxt "b025544dd0654b9a9d1013531b35e357"
msgid ""
"དེ་ནས་ ཡང་དག་པ␣ར་ རྫོགས་པ␣འི་ སངས་རྒྱས་ མར་མེ་མཛད་ ཀྱིས །  དེ་དག་ ཤིང་རྟ␣འི་"
" འཕང་ལོ་ ཙམ་ ཤ་སྟག་ ཏུ་ བླ་རེ␣ར་ ཆགས་ ཤིང་ འདུག་ ལ །"
msgstr ""

msgctxt "0277013aea5744cca67bfcf92b99e306"
msgid "བཅོམ་ལྡན་འདས་ གཤེགས་ ན་ ནི་ འགྲོ་"
msgstr ""

msgctxt "498cbf210b32467f82fd1a40e59c3447"
msgid "གཞེས་ ན་ ནི་ སྡོད་པ␣ར་ བྱིན་ གྱིས་ བརླབས་ སོ ། །"
msgstr ""

msgctxt "2a048cbfcd63421188063969a08211a0"
msgid "དེ་ནས་ བུ་མོ་ དེ␣ས་ དེ་ལྟ་ བུར་ གྱུར་པ་ མཐོང་ ནས་"
msgstr ""

msgctxt "e10c19caa5d442f8828239df95714007"
msgid "དགའ་བ་ སྐྱེས་ ཏེ །"
msgstr ""

msgctxt "720557f95ea740fd990c3ed313fe2753"
msgid "ཨུད་པ་ ལ་ གཉིས་ བཅོམ་ལྡན་འདས་ ལ་ གཏོར་ ནས་"
msgstr ""

# དེ་གཉིས་ཀྱང་བཅོམ་ལྡན་འདས་ཀྱི་སྙན་གྱི་གོང་དུ་ཤིང་རྟའི་འཕང་ལོ་ཙམ་:གྱིས་བླ་རེར་<«གཡུང་»གྱི་བླ་རེར།
# «སྣར་»གྱིས་བླ་བྲེར། «ཞོལ་»གྱི་བླ་བྲེར།>ཆགས་ཤིང་འདུག་པར་བྱིན་གྱིས་བརླབས་སོ། །
msgctxt "eb9e90d6588047c5b06fb710a604d03d"
msgid ""
"དེ་ གཉིས་ ཀྱང་ བཅོམ་ལྡན་འདས་ ཀྱི་ སྙན་ གྱི་ གོང་ དུ་ ཤིང་རྟ␣འི་ འཕང་ལོ་ ཙམ་ "
"གྱིས་ བླ་རེ␣ར་ ཆགས་ ཤིང་ འདུག་པ␣ར་ བྱིན་ གྱིས་ བརླབས་ སོ ། །"
msgstr ""

msgctxt "ac3e50a07d1240c583798dfe5625f557"
msgid "ས་ཕྱོགས་ དེ་དག་ ཀྱང་ རྫི་ཆར་ དྲག་པོ་ ཆེན་པོ␣ས་ འདམ་རྫབ་ ཏུ་ གྱུར་ ནས །"
msgstr ""

msgctxt "702735fad2374a409ccda24a02f0296a"
msgid "དེ␣འི་ འོག་ ཏུ་ ས་ཕྱོགས་ འདམ་རྫབ་ ཅན་ དེ␣ར་ བཅོམ་ལྡན་འདས་ གཤེགས་པ་ ན །"
msgstr ""

# བྲམ་ཟེའི་ཁྱེའུ་བློ་གྲོས་བཟང་པོས་ས་ཕྱོགས་འདམ་རྫབ་ཅན་དེར་བཅོམ་ལྡན་འདས་ཀྱི་སྤྱན་<«གཡུང་»སྤྱེན།>སྔར་སྐྲ་བཏིང་ནས་
msgctxt "fb912fcc108940d889eccfb70d737722"
msgid ""
"བྲམ་ཟེ␣འི་ ཁྱེའུ་ བློ་གྲོས་ བཟང་པོ␣ས་ ས་ཕྱོགས་ འདམ་རྫབ་ ཅན་ དེ␣ར་ "
"བཅོམ་ལྡན་འདས་ ཀྱི་ སྤྱན་སྔ␣ར་ སྐྲ་ བཏིང་ ནས་"
msgstr ""

msgctxt "6f618084fc8148cc88cfe97381aa725b"
msgid "ཚིགས་ སུ་ བཅད་ དེ་"
msgstr ""

msgctxt "1c1979a5d0d943a1b016d74d569fadcd"
msgid "སྨྲས་པ །"
msgstr ""

msgctxt "884e8693aeaa47678a74f8d31c76b85a"
msgid ""
"བློ་ལྡན་ སད་ མཛད་ གལ་ཏེ་ བདག  ། བྱང་ཆུབ་ ཏུ་ ནི་ འཚང་ རྒྱ་ ན ། ། སྐྱེ་ རྒ་ "
"མེད་ མཛད་ ཞབས་ གཉིས་ ཀྱིས ། ། མྱུར་ དུ་ བདག་ སྐྲ་ བརྫི་བ␣ར་ གསོལ ། །"
msgstr ""

# དེ་ནས་ཡང་དག་པར་རྫོགས་པའི་སངས་རྒྱས་མར་མེ་མཛད་དེས།
# བྲམ་ཟེའི་ཁྱེའུ་བློ་གྲོས་བཟང་པོའི་སྐྲའི་<«པེ་»སྦེའི།>སྟེང་དུ་ཞབས་གཉིས་བཞག་གོ།
# །
msgctxt "8947a49083e942389b2901204ccb9c3b"
msgid ""
"དེ་ནས་ ཡང་དག་པ␣ར་ རྫོགས་པ␣འི་ སངས་རྒྱས་ མར་མེ་མཛད་ དེ␣ས །  བྲམ་ཟེ␣འི་ ཁྱེའུ་"
" བློ་གྲོས་ བཟང་པོ␣འི་ སྐྲ␣འི་ སྟེང་ དུ་ ཞབས་ གཉིས་ བཞག་ གོ ། །"
msgstr ""

msgctxt "b49679fe2e4345058d046fe447a0475f"
msgid ""
"དེ་ནས་ བྲམ་ཟེ␣འི་ ཁྱེའུ་ བློ་གྲོས་ བཟང་པོ␣འི་ ཕྱི་བཞིན་ འབྲང་ ཞིང་ འདུག་པ་ "
"དེ་ ཁྲོ␣ས་ ནས །"
msgstr ""

# ཀྱེ་ལྟོས་དང་ཡང་དག་པར་རྫོགས་པའི་སངས་རྒྱས་མར་མེ་མཛད་འདིས།
# བྲམ་ཟེའི་ཁྱེའུ་བློ་གྲོས་བཟང་པོའི་སྐྲ་ཕྱུགས་ལ་བྱ་བ་བཞིན་དུ་བརྫིས་<«གཡུང་»«པེ་»བརྗེས།
# «ཞོལ་»བརྩིས།>སོ་ཞེས་
msgctxt "57c35ea8377e426bb96a3b80d9502987"
msgid ""
"ཀྱེ་ ལྟོས་ དང་ ཡང་དག་པ␣ར་ རྫོགས་པ␣འི་ སངས་རྒྱས་ མར་མེ་མཛད་ འདི␣ས །  "
"བྲམ་ཟེ␣འི་ ཁྱེའུ་ བློ་གྲོས་ བཟང་པོ␣འི་ སྐྲ་ ཕྱུགས་ ལ་ བྱ་བ་ བཞིན་ དུ་ བརྫིས་"
" སོ་ ཞེས་"
msgstr ""

msgctxt "a6514f56c98b452ab1188ecebed7ccd7"
msgid "ཟེར་ རོ ། །"
msgstr ""

msgctxt "5f36c81b8cf7466dbaf91480f7794c4c"
msgid ""
"དེ་ནས་ དེ␣འི་ འོག་ ཏུ་ ཡང་དག་པ␣ར་ རྫོགས་པ␣འི་ སངས་རྒྱས་ མར་མེ་མཛད་ ཀྱིས་ "
"བྲམ་ཟེ␣འི་ ཁྱེའུ་ བློ་གྲོས་ བཟང་པོ་ ལུང་བསྟན་ ཏེ །"
msgstr ""

# ཁྱོད་ནི་ས་འདིར་འཇིག་རྟེན་རྣམས་ལ་ཕན་པའི་ཕྱིར།
# །སྟོན་པ་མངའ་བདག་འཇིག་རྟེན་གསུམ་ལས་རྣམ་གྲོལ་བ།
# །ཤཱཀྱའི་སྲས་པོ་ཤཱཀྱ་ཐུབ་པ་ཞེས་བྱ་བ།
# །འཇིག་རྟེན་གསུམ་གྱི་སྙིང་པོ་འགྲོ་བའི་སྒྲོན་དུ་འགྱུར<«ལི་»«པེ་»«སྣར་»«ཅོ་»+ཅིག>།
# །ཞེས་
msgctxt "f1881e3c7a914e1bb74f0cd3272f2429"
msgid ""
"ཁྱོད་ ནི་ ས་ འདི␣ར་ འཇིག་རྟེན་ རྣམས་ ལ་ ཕན་པ␣འི་ ཕྱིར ། ། སྟོན་པ་ མངའ་བདག་ "
"འཇིག་རྟེན་ གསུམ་ ལ␣ས་ རྣམ་གྲོལ་བ ། ། ཤཱཀྱའི་ སྲས་པོ་ ཤཱཀྱ་ ཐུབ་པ་ ཞེས་ བྱ་བ "
"། ། འཇིག་རྟེན་ གསུམ་ གྱི་ སྙིང་པོ་ འགྲོ་བ␣འི་ སྒྲོན་ དུ་ འགྱུར ། ། ཞེས་"
msgstr ""

msgctxt "b5b53cb14f77416d8c73674ed0547309"
msgid "གསུངས་ སོ ། །"
msgstr ""

# དེ་ནས་ཡང་དག་པར་རྫོགས་པའི་སངས་རྒྱས་མར་མེ་མཛད་ཀྱིས་བྲམ་ཟེའི་ཁྱེའུ་བློ་གྲོས་བཟང་པོ་ལུང་བསྟན་མ་ཐག་པ་དེའི་མོད་ཁོ་ན་ལ་ནམ་མཁའ་ལ་ཏ་ལ་བདུན་སྲིད་ཙམ་<«གཡུང་»ཙང་།>དུ་འཕགས་སོ།
# །
msgctxt "6ba120deb74244178442f1f61d8033c1"
msgid ""
"དེ་ནས་ ཡང་དག་པ␣ར་ རྫོགས་པ␣འི་ སངས་རྒྱས་ མར་མེ་མཛད་ ཀྱིས་ བྲམ་ཟེ␣འི་ ཁྱེའུ་ "
"བློ་གྲོས་ བཟང་པོ་ ལུང་བསྟན་ མ་ ཐག་པ་ དེ␣འི་ མོད་ ཁོ་ན་ ལ་ ནམ་མཁའ་ ལ་ཏ་ ལ་ "
"བདུན་ སྲིད་ ཙམ་ དུ་ འཕགས་ སོ ། །"
msgstr ""

msgctxt "7dfae9559ad84b6c93cf2a75a3bc5662"
msgid "དེ་ནས་ དེ␣འི་ སྐྲ་ བྱི་ ནས་"
msgstr ""

# སྐྲ་སར་<«ཞོལ་»གསར།>ཁྱད་པར་དུ་འཕགས་པ་བྱུང་ངོ་། །
msgctxt "461511c84a344ca298919cbf45dcfe64"
msgid "སྐྲ་ ས␣ར་ ཁྱད་པར་ དུ་ འཕགས་པ་ བྱུང་ ངོ་ ། །"
msgstr ""

msgctxt "ff56512597a0493488594c1c0ba8f80e"
msgid "དེ་ནས་ དེ་ ནམ་མཁའ་ ལ་ འདུག་པ་ སྐྱེ་བོ་ ཕལ་པོ་ ཆེས་ མཐོང་ ངོ་ ། །"
msgstr ""

msgctxt "81a85633f2034fb39434b5d6d377aa54"
msgid "མཐོང་ ནས་ ཀྱང་ སྨོན་ལམ་ བཏབ་པ །"
msgstr ""

# གང་གི་ཚེ་འདིས་བླ་ན་མེད་པའི་ཡེ་ཤེས་བརྙེས་པར་:འགྱུར་བ་<«ཞོལ་»གྱུར་པ།>དེའི་ཚེ་བདག་ཅག་འདིའི་ཉན་ཐོས་སུ་གྱུར་ཅིག་ཅེས་
msgctxt "566cb38cdc6c4cc2ac55e1d76f359e01"
msgid ""
"གང་ གི་ ཚེ་ འདི␣ས་ བླ་ ན་ མེད་པ␣འི་ ཡེ་ཤེས་ བརྙེས་པ␣ར་ འགྱུར་བ་ དེ␣འི་ ཚེ་ "
"བདག་ཅག་ འདི␣འི་ ཉན་ཐོས་ སུ་ གྱུར་ཅིག་ ཅེས་"
msgstr ""

msgctxt "55d46844dc6c4b3fb6ecf31c496c0d4f"
msgid "བྱས་ སོ ། །"
msgstr ""

# བུ་མོ་དེས་ཀྱང་<«གཡུང་»«ལི་»«པེ་»«སྣར་»«ཅོ་»«ཞོལ་»–ཀྱང་།>སྨོན་ལམ་བཏབ་སྟེ།
msgctxt "bf8b887913c4482598aa0e34dc8c1937"
msgid "བུ་མོ་ དེ␣ས་ ཀྱང་ སྨོན་ལམ་ བཏབ་ སྟེ །"
msgstr ""

msgctxt "a67f787b506740c5bf322f3f02203bf8"
msgid ""
"ཁྱོད་ ཀྱིས་ གང་ དུ་ སྨོན་པ་ ཡི ། ། འདྲེན་པ་ སངས་རྒྱས་ བརྙེས་པ་ དེ␣ར ། ། རྟག་"
" ཏུ་ འཐུན་པ␣ར་ ཆོས་སྤྱོད་པ␣འི ། ། ཁྱོད་ ཀྱི་ བཙུན་མོ␣ར་ བདག་ གྱུར་ཅིག ། །"
msgstr ""

msgctxt "35d660025dc24d9a8aeb973dd41f3a16"
msgid ""
"གང་ ཚེ་ ཁྱོད་ ནི་ རྫོགས་ སངས་རྒྱས ། ། འཇིག་རྟེན་ གཙོ་བོ་ རྣམ་འདྲེན་ འགྱུར "
"། ། དེ་ ཡི་ དུས་བབ་ ཁྱོད་ ཀྱི་ ནི ། ། ཉན་ཐོས་ མར་ ཡང་ བདག་ གྱུར་ཅིག  །"
msgstr ""

# མཁའ་གནས་བྲམ་ཟེའི་ཁྱེའུ་མཐོང་ནས<«གཡུང་»«པེ་»–ནས།>། །
msgctxt "1054c1f4ba124cab8e134ea55d96511a"
msgid "མཁའ་ གནས་ བྲམ་ཟེ␣འི་ ཁྱེའུ་ མཐོང་ ནས ། །"
msgstr ""

# བརྒྱ་ཕྲག་དང་ནི་སྟོང་ཕྲག་རྣམས། །ཐམས་ཅད་མ་འོངས་དུས་དག་<«གཡུང་»ངག «ཞོལ་»རྟག>ཏུ།
# །ཉན་ཐོས་ཉིད་དུ་སྨོན་པར་བྱེད། །
msgctxt "6f8c2bcfb3014068a5c5a45d70ed2545"
msgid ""
"བརྒྱ་ ཕྲག་ དང་ ནི་ སྟོང་ཕྲག་ རྣམས ། ། ཐམས་ཅད་ མ་འོངས་ དུས་ དག་ ཏུ ། ། "
"ཉན་ཐོས་ ཉིད་ དུ་ སྨོན་པ␣ར་ བྱེད ། །"
msgstr ""

# གང་ཚེ་ཁྱོད་ནི་རྫོགས་སངས་རྒྱས། །འཇིག་རྟེན་གཙོ་བོ་རྣམ་འདྲེན་འགྱུར།
# །དེ་<«གཡུང་»ད། «ལི་»དེའ། «ཅོ་»དེའི།>ཡི་དུས་བབ་ཁྱོད་ཀྱི་ནི<«གཡུང་»དི།>།
# །ཉན་ཐོས་བདག་ཅག་:འགྱུར་བར་<«གཡུང་»«ལི་»«པེ་»«སྣར་»«ཅོ་»གྱུར་པར།>ཤོག །ཅེས་
msgctxt "90aaf78b889d4e75972fcab814b78eab"
msgid ""
"གང་ ཚེ་ ཁྱོད་ ནི་ རྫོགས་ སངས་རྒྱས ། ། འཇིག་རྟེན་ གཙོ་བོ་ རྣམ་འདྲེན་ འགྱུར "
"། ། དེ་ ཡི་ དུས་བབ་ ཁྱོད་ ཀྱི་ ནི ། ། ཉན་ཐོས་ བདག་ཅག་ འགྱུར་བ␣ར་ ཤོག  ། ཅེས་"
msgstr ""

msgctxt "26827d945a834d73b6191d081955b48f"
msgid "བྱས་ སོ ། །"
msgstr ""

msgctxt "f0bbc198ad3a4e3bb02a51e53726e0a2"
msgid ""
"དེ་ནས་ གང་ གི་ ཚེ་ ཡང་དག་པ␣ར་ རྫོགས་པ␣འི་ སངས་རྒྱས་ མར་མེ་མཛད་ ཀྱིས་ "
"བྲམ་ཟེ␣འི་ ཁྱེའུ་ བློ་གྲོས་ བཟང་པོ་ ལུང་བསྟན་པ་ དེ␣འི་ ཚེ །  རྒྱལ་པོ་ "
"མར་མེ␣ས་ དེ␣འི་ སྐྲ་ བླངས་ ནས་"
msgstr ""

msgctxt "cda8664ad78a456fbcacb0a7c733e0fa"
msgid "རྒྱལ་པོ་ ནོར་ ཅན་ གྱི་ བུས་ སྨྲས་པ །"
msgstr ""

msgctxt "8531ce00b00f47e29a5bde27b8f2b4f1"
msgid "སྐྲ་ འདི་དག་ བདག་ ལ་ བྱིན་ ཅིག  །"
