msgctxt "e021869fbb34474ca926dc4b450d0387"
msgid ""
"དེ་ནས་ དེ་ གཉིས་ ཀྱིས་ རྒྱལ་པོ་ ནོར་ ཅན་ གྱི་ བུས་ མཆོད་སྦྱིན་ གྱི་ མཇུག་ "
"ཏུ་ རབ་ཏུ་ སྦྱིན་པ་ ཆེན་པོ་ ལྔ་ བཤམས་ ཏེ །"
msgstr ""

msgctxt "dbd09de5a15b431bbf8f6b01fc7e51f2"
msgid "བྲམ་ཟེ་ གང་ ཁ་ཏོན་ རྫོགས་པ␣ར་ ལོབས་པ་ དེ་ དབང་ ངོ་ ཞེས་"
msgstr ""

