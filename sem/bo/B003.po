msgstr ""

# རྒྱ་མཚོ་ཆེན་པོ་བཏུང་<«པེ་»བདུང་།>ངོ་སྙམ་པ་དང་།
msgctxt "914f5ac379ff45b1ad8895e26ffc83df"
msgid "རྒྱ་མཚོ་ ཆེན་པོ་ བཏུང་ ངོ་ སྙམ་པ་ དང་ །"
msgstr ""

msgctxt "823c0565066640fcb950d19f7c0b341a"
msgid "ནམ་མཁའ་ ལ་ འགྲོ␣འོ་ སྙམ་པ་ དང་ །"
