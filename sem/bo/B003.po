msgctxt "40c343225c9540fbb291bf5307587b01"
msgid ""
"ཡང་དག་པ␣ར་ རྫོགས་པ␣འི་ སངས་རྒྱས་ མར་མེ་མཛད་ ཀྱིས །  བྲམ་ཟེ␣འི་ སྐྲ་ ཕྱུགས་ "
"ལ་ བྱ་བ་ བཞིན་ དུ་ རྐང་པ␣ས་ བརྫིས་ སོ་ ཞེས་"
msgstr ""

msgctxt "a01fccd83096431c8b32a44ebfa13c84"
msgid "བྱས་ སོ ། །"
msgstr ""

msgctxt "247bc726442d40ad92e70b7bb4937f33"
msgid "དེ་ནས་ བློ་གྲོས་ བཟང་པོ␣ས་ དེ་ ལ་ སྨྲས་པ །"
msgstr ""

msgctxt "8c6760e64f9d4356953cea1f4e3eca9c"
msgid ""
"ཚུར་ ཤོག་ སངས་རྒྱས་ བཅོམ་ལྡན་འདས་ ཀྱི་ ཐད་ དུ་ རབ་ཏུ་ འབྱུང་བ␣ར་ བྱ␣འོ ། །"
msgstr ""

msgctxt "a0273aabc19b400b860bd91cf04e39d7"
msgid ""
"དེ་ནས་ བློ་གྲོས་ བཟང་པོ་ དང་ བློ་གྲོས་ གཉིས་ ཡང་དག་པ␣ར་ རྫོགས་པ␣འི་ "
"སངས་རྒྱས་ མར་མེ་མཛད་ ཀྱི་ གསུང་རབ་ ལ་ རབ་ཏུ་ བྱུང་ སྟེ །"
msgstr ""

# བློ་གྲོས་བཟང་པོ་ནི་<«གཡུང་»«ལི་»«པེ་»«སྣར་»«ཅོ་»«ཞོལ་»–ནི།>རབ་ཏུ་བྱུང་ནས་
msgctxt "a43622c2628744b293436ee0d3433142"
msgid "བློ་གྲོས་ བཟང་པོ་ ནི་ རབ་ཏུ་ བྱུང་ ནས་"
msgstr ""

# སྡེ་སྣོད་གསུམ་བསླབས་<«གཡུང་»བརླབས།>ཏེ་
msgctxt "a74546e804904bcba3cf98dc139c9d10"
msgid "སྡེ་སྣོད་ གསུམ་ བསླབས་ ཏེ་"
msgstr ""

msgctxt "0f621f219df947e585ef3eba5c15404f"
msgid "ཆོས་བཞིན་ དུ་ འཁོར་ བསྡུས་ སོ ། །"
msgstr ""

msgctxt "714fdcc2535745169b186c0ca26011a7"
msgid "དེ་ནས་ བྲམ་ཟེ␣འི་ ཁྱེའུ་ བློ་གྲོས་ བཟང་པོ་ ཤི་འཕོས་ ཏེ་"
msgstr ""

# དུས་ལས་འདས་<«གཡུང་»སངས།>ནས་
msgctxt "6a6d1129c0504adba71e0dccaa4bd5f6"
msgid "དུས་ ལ␣ས་ འདས་ ནས་"
msgstr ""

msgctxt "b80eba32ea8d47b78912df8076c99998"
msgid "དགའ་ལྡན་ གྱི་ ལྷ␣འི་ རིས་ སུ་ སྐྱེས་ སོ ། །"
msgstr ""

msgctxt "cd67b7ad827742f5b688f4010b10f1b9"
msgid "བྲམ་ཟེ␣འི་ ཁྱེའུ་ བློ་གྲོས་ ནི་ ཤི་འཕོས་ ཏེ་"
msgstr ""

msgctxt "b98d306243b2498591c338606ea5d61e"
msgid "དུས་ ལ␣ས་ འདས་ ནས་"
msgstr ""

msgctxt "8fb66d3489514101ba9354838516a7ab"
msgid "སེམས་ཅན་ དམྱལ་བ␣ར་ སྐྱེས་ སོ ། །"
msgstr ""

msgctxt "62b91462bc2c4e04aa914d3b2c8c6ada"
msgid "དགེ་སློང་ དག་ ཇི་སྙམ་ དུ་ སེམས །"
msgstr ""

# དེའི་ཚེ་དེའི་དུས་ན་བྲམ་ཟེའི་ཁྱེའུ་བློ་གྲོས་བཟང་པོར་གྱུར་པ་བྱང་ཆུབ་སེམས་དཔའི་<«ཅོ་»པའི།>སྤྱོད་པ་ལ་གནས་པ་གང་ཡིན་པ་དེ་ནི་:ང་ཉིད་ཡིན་ཏེ<«གཡུང་»«ལི་»«པེ་»«སྣར་»«ཅོ་»ང་ཡིན་ཏེ།
# «ཞོལ་»–ང་ཉིད་ཡིན་ཏེ།>།
msgctxt "1cedcf69ec5e43238340451b5a97f858"
msgid ""
"དེ␣འི་ ཚེ་ དེ␣འི་ དུས་ ན་ བྲམ་ཟེ␣འི་ ཁྱེའུ་ བློ་གྲོས་ བཟང་པོ␣ར་ གྱུར་པ་ "
"བྱང་ཆུབ་ སེམས་དཔ␣འི་ སྤྱོད་པ་ ལ་ གནས་པ་ གང་ཡིན་པ་ དེ་ ནི་ ང་ཉིད་ ཡིན་ ཏེ །"
msgstr ""

# ངས་དེར་སངས་རྒྱས་མར་མེ་མཛད་ལ་ཨུད་པ་ལ་སྔོན་པོ་དག་གིས་མཆོད་པ་བྱས་པའི་ལས་དེའི་<«ཅོ་»དེས།>རྣམ་པར་སྨིན་པས།
# འཁོར་བ་ན་བདེ་བ་ཆེན་པོ་མྱོང་བར་གྱུར་ཏེ།
msgctxt "656fbda622ef4ab9b457cc6516fe1805"
msgid ""
"ངས་ དེ␣ར་ སངས་རྒྱས་ མར་མེ་མཛད་ ལ་ ཨུད་པ་ ལ་ སྔོན་པོ་ དག་ གིས་ མཆོད་པ་ "
"བྱས་པ␣འི་ ལ␣ས་ དེ␣འི་ རྣམ་པ␣ར་ སྨིན་པ␣ས །  འཁོར་བ་ ན་ བདེ་བ་ ཆེན་པོ་ "
"མྱོང་བ␣ར་ གྱུར་ ཏེ །"
msgstr ""

# དགེ་བའི་རྩ་བ་དེས་ན་<«གཡུང་»«ལི་»«པེ་»«སྣར་»«ཅོ་»«ཞོལ་»ནི།>ངའི་བླ་ན་མེད་པ་ཡང་དག་པར་རྫོགས་པའི་བྱང་ཆུབ་ཀྱི་རྒྱུ་ཙམ་དང་རྐྱེན་ཙམ་དང་ཚོགས་<«པེ་»«ཅོ་»ཆོགས།>ཙམ་དུ་གྱུར་ལ་ད་ལྟའི་བར་དུ་ཡང་ལས་དེའི་རྣམ་པར་སྨིན་པས་མཛའ་སྡེ་བོ་ལྔ་བརྒྱ་པོ་<«གཡུང་»«ལི་»«པེ་»«སྣར་»«ཞོལ་»པ།
# «ཅོ་»–པོ།>རྣམས་ཀྱིས་འདི་ལྟ་བུའི་མཆོད་པ་བྱས་པ་ཡིན་ནོ།། །།
msgctxt "bf6fc194d1464bf4a47b4dfebfee9bee"
msgid ""
"དགེ་བ␣འི་ རྩ་བ་ དེས་ན་ ང␣འི་ བླ་ ན་ མེད་པ་ ཡང་དག་པ␣ར་ རྫོགས་པ␣འི་ བྱང་ཆུབ་ "
"ཀྱི་ རྒྱུ་ ཙམ་ དང་ རྐྱེན་ ཙམ་ དང་ ཚོགས་ ཙམ་ དུ་ གྱུར་ ལ་ད་ ལྟ␣འི་ བར་ དུ་ "
"ཡང་ ལ␣ས་ དེ␣འི་ རྣམ་པ␣ར་ སྨིན་པ␣ས་ མཛའ་ སྡེ་བོ་ ལྔ་ བརྒྱ་པོ་ རྣམས་ ཀྱིས་ "
"འདི་ ལྟ་བུ␣འི་ མཆོད་པ་ བྱས་པ་ ཡིན་ ནོ །། །།"
msgstr ""
