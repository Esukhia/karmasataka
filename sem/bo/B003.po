msgctxt "bc0c5978aca84c2699265cd070bc03af"
msgid "དེ་ནས་ རྒྱལ་པོ་ མར་མེ␣ས་ སྐྲ་ དེ་དག་ དེ་ ལ་ བྱིན་ ནས་"
msgstr ""

msgctxt "5ff7974de2354ad4b5120a884612558d"
