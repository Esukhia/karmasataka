msgctxt "852e7b2f1da74f18bf13ad7c2bce1d65"
msgid "﻿།། མཛའ་ སྡེ་ རྣམས་ ཞེས་ བྱ་བ་ ནི །"
msgstr ""

# གླེང་གཞི་མཉན་དུ་ཡོད་པ་ན་<«གཡུང་»«པེ་»–ན།>བཞུགས་ཏེ།
msgctxt "70028260b3a34afbb507121aff4422ff"
msgid "གླེང་ གཞི་ མཉན་ དུ་ཡོད་པ་ ན་ བཞུགས་ ཏེ །"
msgstr ""

# དེའི་ཚེ་མཉན་དུ་ཡོད་པ་ན་མཛའ་སྡེ་བོ་ལྔ་བརྒྱ་ཙམ་འཁོད་[83a]པ་རྣམས།
# དུས་དུས་སུ་མེ་ཏོག་གང་རྒྱས་པར་དེ་དག་ལོངས་སྤྱད་<«པེ་»སྤྱོད།>པའི་ཕྱིར་སྐྱེད་<«གཡུང་»བསྐྱེད།>མོས་ཚལ་གྱི་ནང་དུ་སོང་ནས་
msgctxt "2ded47db5eae47f28b52886e2aa383dc"
msgid ""
"དེ␣འི་ ཚེ་ མཉན་ དུ་ཡོད་པ་ ན་ མཛའ་ སྡེ་བོ་ ལྔ་ བརྒྱ་ ཙམ་ འཁོད་པ་ རྣམས །  དུས་"
" དུས་ སུ་ མེ་ཏོག་ གང་ རྒྱས་པ␣ར་ དེ་དག་ ལོངས་ སྤྱད་པ␣འི་ ཕྱིར་ སྐྱེད་མོས་ཚལ་ "
"གྱི་ ནང་ དུ་ སོང་ ནས་"
msgstr ""

msgctxt "206600c5d4b94fed8efa3bb22f7d90f9"
msgid "རོལ་མོ་ བྱེད་ དེ །"
msgstr ""

msgctxt "dbe903508ac54e81be1faadcd2c863c8"
msgid "རྩེ་ ཞིང་ དགའ་ ལ་ དགའ་མགུ␣ར་ སྤྱོད་ དོ ། །"
msgstr ""

# དེ་ནས་ཕྱི་ཞིག་ན་མེ་ཏོག་:ཨུད་པ་ལ་<«གཡུང་»ཨུད་བ་མ།>དང་། པད་མ་དང་། ཀུ་མུ་ད་དང་།
# པད་མ་དཀར་པོ་རྣམས་རྒྱས་པར་གྱུར་ནས།
msgctxt "ba6648ae05bb4fc7862ea7850cd3deb0"
msgid ""
"དེ་ནས་ ཕྱི་ ཞིག་ ན་ མེ་ཏོག་ ཨུད་པ་ ལ་ དང་ །  པད་མ་ དང་ །  ཀུ་མུ་ད་ དང་ །  "
"པད་མ་ དཀར་པོ་ རྣམས་ རྒྱས་པ␣ར་ གྱུར་ ནས །"
msgstr ""

# དེ་དག་ལོངས་སྤྱད་<«པེ་»སྤྱོད།>པར་བྱ་བའི་ཕྱིར་དེ་དག་རྒྱན་ཐམས་ཅད་ཀྱིས་བརྒྱན་ཏེ།
# མེ་ཏོག་ཨུད་པ་ལ་དང་། པད་མ་དང་། ཀུ་མུ་ད་<«གཡུང་»«ལི་»«ཅོ་»ཏ།>དང་།
# པད་མ་དཀར་པོ་རྣམས་ཀྱིས་ཕྲེང་བ་དང་། ཐོད་དང་། རྣ་རྒྱན་<«གཡུང་»རྒྱུན།>དང་།
# ཆུན་པོ་རྣམས་རྒྱན་<«ཅོ་»བརྒྱན།>དུ་ཐོགས་ཤིང་ཨུད་པ་ལ་ཆང་<«ཅོ་»ཆངས།>པ་རེ་རེ་ཐོགས་ནས།
msgctxt "84d4c52937ba4601bcf62140bfa3f1ca"
msgid ""
"དེ་དག་ ལོངས་ སྤྱད་པ␣ར་ བྱ་བ␣འི་ ཕྱིར་ དེ་དག་ རྒྱན་ ཐམས་ཅད་ ཀྱིས་ བརྒྱན་ ཏེ "
"།  མེ་ཏོག་ ཨུད་པ་ ལ་ དང་ །  པད་མ་ དང་ །  ཀུ་མུ་ད་ དང་ །  པད་མ་ དཀར་པོ་ རྣམས་"
" ཀྱིས་ ཕྲེང་བ་ དང་ །  ཐོད་ དང་ །  རྣ་རྒྱན་ དང་ །  ཆུན་པོ་ རྣམས་ རྒྱན་ དུ་ "
"ཐོགས་ ཤིང་ ཨུད་པ་ ལ་ ཆང་པ་ རེ་རེ་ ཐོགས་ ནས །"
msgstr ""

msgctxt "66a93e026d0c48fa8604c82e12e25dc5"
msgid "སིལ་སྙན་ སྣ་ཚོགས་ དང་ །  ཕེག་ རྡོབ་ ཅིང་"
msgstr ""

msgctxt "468db6d2959a4059b14d60fba4d9fc20"
msgid "མཉན་ དུ་ཡོད་པ་ ནས་ བྱུང་ སྟེ །"
msgstr ""

# སྐྱེད་<«གཡུང་»བསྐྱེད།>མོས་ཚལ་གྱི་ནང་དུ་ཆས་སོ། །
msgctxt "c1263b3230cd4c5abeb79f3d1e1b5302"
msgid "སྐྱེད་མོས་ཚལ་ གྱི་ ནང་ དུ་ ཆ␣ས་ སོ ། །"
msgstr ""

msgctxt "e47a2d5a07f34cc186cfb0b413bef36c"
msgid ""
"བཅོམ་ལྡན་འདས་ ཀྱང་ དེ་དག་ ལ་ ཐུགས་ བརྩེ་བ␣འི་ ཕྱིར་ སྔ་དྲོ་ ཤམ་ཐབས་ དང་ "
"ཆོས་གོས་ གསོལ་ ཏེ །"
msgstr ""

msgctxt "01741436dea24af0bc58b0ed22ccffca"
msgid "ལྷུང་བཟེད་ བསྣམས་ ནས་"
msgstr ""

msgctxt "2b8c213db45846b5ae21625940c28597"
msgid "མཉན་ དུ་ཡོད་པ␣ར་ བསོད་སྙོམས་ ལ་ གཤེགས་པ་ དང་ །"
msgstr ""

# མཛའ་སྡེ་བོ་དེ་དག་གིས་ཐག་རིང་པོ་ཞིག་ནས།
# སངས་རྒྱས་བཅོམ་ལྡན་འདས་སྐུ་སྐྱེས་བུ་ཆེན་པོའི་མཚན་སུམ་ཅུ་<«གཡུང་»«པེ་»བཅུ།>རྩ་གཉིས་ཀྱིས་ལེགས་པར་བརྒྱན་ཅིང་སྤ་བར་བྱས་པ།
# མེ་ལུས་ཀྱི་དབྱིབས་སུ་འདུག་པ་ལྟ་བུ། མེ་ལྕེ་མར་གྱིས་བཏབ་པ་ལྟ་བུ།
# མར་མེ་གསེར་གྱི་སྣོད་དུ་བཞག་<«ཅོ་»གཞག>པ་ལྟ་བུ།
# མཆོད་སྡོང་རིན་པོ་ཆེ་སྣ་ཚོགས་ཀྱིས་སྤྲས་པ་ལྟ་བུ།
# བློ་གསལ་ཞིང་དྲི་མ་མེད་ལ་རྣམ་པར་<«ཞོལ་»པ།>དག་པ་དང་ལྡན་པའི་སངས་རྒྱས་བཅོམ་ལྡན་འདས་མཐོང་ངོ་།
# །
msgctxt "fce854db320847fe8d11aabd25e85b3b"
msgid ""
"མཛའ་ སྡེ་བོ་ དེ་དག་ གིས་ ཐག་ རིང་པོ་ ཞིག་ ནས །  སངས་རྒྱས་ བཅོམ་ལྡན་འདས་ "
"སྐུ་སྐྱེས་ བུ་ཆེན་པོ␣འི་ མཚན་ སུམ་ཅུ་རྩ་གཉིས་ ཀྱིས་ ལེགས་པ␣ར་ བརྒྱན་ ཅིང་ "
"སྤ་བ␣ར་ བྱས་པ །  མེ་ ལུས་ ཀྱི་ དབྱིབས་ སུ་ འདུག་པ་ ལྟ་བུ །  མེ་ལྕེ་ མར་ "
"གྱིས་ བཏབ་པ་ ལྟ་བུ །  མར་མེ་ གསེར་ གྱི་ སྣོད་ དུ་ བཞག་པ་ ལྟ་བུ །  མཆོད་སྡོང་"
" རིན་པོ་ཆེ་ སྣ་ཚོགས་ ཀྱིས་ སྤྲས་པ་ ལྟ་བུ །  བློ་གསལ་ ཞིང་ དྲི་མ་ མེད་ ལ་ "
"རྣམ་པ␣ར་ དག་པ་ དང་ ལྡན་པ␣འི་ སངས་རྒྱས་ བཅོམ་ལྡན་འདས་ མཐོང་ ངོ་ ། །"
msgstr ""

msgctxt "c225d52552904507a1dced873bbfa364"
msgid "མཐོང་ ནས་ ཀྱང་ དེ་དག་ བཅོམ་ལྡན་འདས་ ལ་ ཤིན་ཏུ་ དགའ་བ་ སྐྱེས་ སོ ། །"
msgstr ""

msgctxt "fe47a3cd6ecc413abc19246e1490ab74"
msgid "དགའ་བ་ སྐྱེས་ ནས་"
msgstr ""

# བཅོམ་ལྡན་འདས་ག་ལ་བ་དེར་དོང་སྟེ་<«གཡུང་»«པེ་»སྡེ།>ལྷགས་ནས།
msgctxt "d80888b96d974bc1a49ce0f46a92455a"
msgid "བཅོམ་ལྡན་འདས་ ག་ལ་བ་ དེ␣ར་ དོང་ སྟེ་ ལྷགས་ ནས །"
msgstr ""

msgctxt "252fc12c50674f649a6bcfd72c6f52aa"
msgid "བཅོམ་ལྡན་འདས་ ཀྱི་ ཞབས་ ལ་མགོ་ བོས་ ཕྱག་ འཚལ་ ཏེ །"
msgstr ""

# བཅོམ་ལྡན་འདས་ལ་ཨུད་པ་ལ་དང་པད་མ་དང་ཀུ་མུ་ད་<«གཡུང་»«ལི་»«ཅོ་»ཏ།>དང་པད་མ་དཀར་པོ་རྣམས་ཀྱིས་གཏོར་ནས།
msgctxt "00cc46db5f6b469493e0867b900088cd"
msgid ""
"བཅོམ་ལྡན་འདས་ ལ་ ཨུད་པ་ ལ་ དང་ པད་མ་ དང་ ཀུ་མུ་ད་ དང་ པད་མ་ དཀར་པོ་ རྣམས་ "
"ཀྱིས་ གཏོར་ ནས །"
msgstr ""

msgctxt "6a660db92c884ca7a71a78beeb5f9fc8"
msgid ""
"སིལ་སྙན་ དང་ །  ཕེག་ རྡོབ་ བཞིན་ དུ་ བཅོམ་ལྡན་འདས་ ལ་ ལན་ གསུམ་ བསྐོར་བ་ "
"བྱས་ ནས་"
msgstr ""

msgctxt "09eb42054f5c4128a36ca1bf0da0d406"
msgid "དོང་ ངོ་ ། །"
msgstr ""

msgctxt "a66feaea743446abb95843b0adf4daea"
msgid "དེ␣འི་ སྐབས་ སུ་ བཅོམ་ལྡན་འདས་ འཛུམ་པ་ མཛད་ དེ །"
msgstr ""

# འདི་ནི་སངས་རྒྱས་བཅོམ་ལྡན་<«ལི་»–ལྡན།>འདས་རྣམས་གང་གི་ཚེ་འཛུམ་པ་མཛད་པའི་ཆོས་ཉིད་དེ།
msgctxt "8fcb3777e42e4d388ca5efd46e319098"
msgid ""
"འདི་ ནི་ སངས་རྒྱས་ བཅོམ་ལྡན་འདས་ རྣམས་ གང་ གི་ ཚེ་ འཛུམ་པ་ མཛད་པ␣འི་ "
"ཆོས་ཉིད་ དེ །"
msgstr ""

msgctxt "ceecc5a13c64469d81d2549921a535d7"
msgid ""
"དེ␣འི་ ཚེ་ ཞལ་ ནས་ འོད་ཟེར་ སྔོན་པོ་ དང་ །  སེར་པོ་ དང་ །  དམར་པོ་ དང་ །  "
"དཀར་པོ་ རྣམས་ བྱུང་ ནས །"
msgstr ""

msgctxt "77a380fd135646ce998fe5f4514dce69"
msgid "ཁ་ཅིག་ ནི་ འོག་ ཏུ་ འགྲོ␣འོ ། །"
msgstr ""

msgctxt "de797f758e884d6592e58385d6bd38f9"
msgid "ཁ་ཅིག་ ནི་ སྟེང་ དུ་ འགྲོ␣འོ ། །"
msgstr ""

# འོག་ཏུ་འགྲོ་བ་གང་ཡིན་པ་དེ་དག་ནི་སེམས་ཅན་དམྱལ་བ་ཡང་སོས་དང་། ཐིག་ནག་དང་།
# བསྡུས་འཇོམས་དང་། ངུ་འབོད་དང་། ངུ་འབོད་ཆེན་པོ་དང་། ཚ་བ་དང་། རབ་ཏུ་ཚ་བ་དང་།
# མནར་མེད་པ་དང་། ཆུ་བུར་ཅན་དང་། ཆུ་བུར་རྡོལ་དང་།
# སོ་:ཐམས་ཐམས་<«སྣར་»«ཞོལ་»ཐམ་ཐམ།>དང་། ཀྱི་ཧུད་ཟེར་བ་<«གཡུང་»–བ།>དང་།
# ཨ་ཆུ་ཟེར་བ་<«གཡུང་»«ལི་»«པེ་»པ།>དང་། ཨུད་པ་<«ཅོ་»ཕ།>ལ་ལྟར་གས་པ་དང་།
# པད་མ་ལྟར་གས་པ་:དང་། པད་མ་ལྟར་གས་པ་<«ཅོ་»–དང་། པད་མ་ལྟར་གས་པ།>ཆེན་པོར་སོང་ནས།
msgctxt "2790b27ffd7d4faf9073386a2f3083e3"
msgid ""
"འོག་ ཏུ་ འགྲོ་བ་ གང་ཡིན་པ་ དེ་དག་ ནི་ སེམས་ཅན་ དམྱལ་བ་ ཡང་སོས་ དང་ །  "
"ཐིག་ནག་ དང་ །  བསྡུས་འཇོམས་ དང་ །  ངུ་འབོད་ དང་ །  ངུ་འབོད་ ཆེན་པོ་ དང་ །  "
"ཚ་བ་ དང་ །  རབ་ཏུ་ ཚ་བ་ དང་ །  མནར་མེད་པ་ དང་ །  ཆུ་བུར་ ཅན་ དང་ །  ཆུ་བུར་ "
"རྡོལ་ དང་ །  སོ་ ཐམས་ ཐམས་ དང་ །  ཀྱི་ཧུད་ ཟེར་བ་ དང་ །  ཨ་ཆུ་ ཟེར་བ་ དང་ ། "
" ཨུད་པ་ ལ་ ལྟར་ གས་པ་ དང་ །  པད་མ་ ལྟར་ གས་པ་ དང་ །  པད་མ་ ལྟར་ གས་པ་ "
"ཆེན་པོ␣ར་ སོང་ ནས །"
msgstr ""

# ཚ་བའི་སེམས་ཅན་དམྱལ་བ་གང་ཡིན་པ་དེ་དག་ཏུ་ནི་<«གཡུང་»–ནི།>བསིལ་བར་གྱུར་ཏེ་
msgctxt "a05b98f47a934f559b7e07fcaab00f40"
msgid "ཚ་བ␣འི་ སེམས་ཅན་ དམྱལ་བ་ གང་ཡིན་པ་ དེ་དག་ ཏུ་ ནི་ བསིལ་བ␣ར་ གྱུར་ ཏེ་"
msgstr ""

msgctxt "89d8b59455214346a091fb5ba62465f9"
msgid "འབབ་བོ ། །"
msgstr ""

# གྲང་བའི་སེམས་ཅན་དམྱལ་བ་གང་ཡིན་པ་དེ་དག་ཏུ་ནི་<«གཡུང་»«ལི་»«པེ་»«སྣར་»«ཅོ་»–ནི།>དྲོ་བར་གྱུར་ཏེ་
msgctxt "8c088b24207e495a9b1e10de3015aa7b"
msgid "གྲང་བ␣འི་ སེམས་ཅན་ དམྱལ་བ་ གང་ཡིན་པ་ དེ་དག་ ཏུ་ ནི་ དྲོ་བ␣ར་ གྱུར་ ཏེ་"
msgstr ""

msgctxt "f10154887d5643d684929452131dfeb0"
msgid "འབབ་བོ ། །"
msgstr ""

# :དེས་ན་<«གཡུང་»«པེ་»«ཁུ་»དེ་ནས།>སེམས་ཅན་དེ་དག་གི་གནོད་པའི་ཁྱད་པར་རྣམས་རྩེ་ཞིལ་བར་:གྱུར་ཏོ<«གཡུང་»«ལི་»«པེ་»«སྣར་»«ཅོ་»འགྱུར་རོ།>།
# །
msgctxt "91f5f66a4798476d8da6767343dfd532"
msgid ""
"དེས་ན་ སེམས་ཅན་ དེ་དག་ གི་ གནོད་པ␣འི་ ཁྱད་པར་ རྣམས་ རྩེ་ ཞིལ་ བར་ གྱུར་ ཏོ "
"། །"
msgstr ""

msgctxt "114fb6f08bb74d579dcd63e0b734efac"
msgid "དེ་ནས་ དེ་དག་ འདི་ སྙམ་ དུ་ སེམས་ ཏེ །"
msgstr ""

msgctxt "10dc53757c1a42999979c14fe4f71328"
msgid "ཀྱེ་ ཅི་ བདག་ཅག་ འདི་ ནས་ ཤི་འཕོས་ སམ་"
msgstr ""

msgctxt "e360b6698d6b4613bca5e0dafb73dc8c"
msgid "འོན་ཏེ་ གཞན་ དུ་ སྐྱེས་ སམ་ སྙམ་ མོ ། །"
msgstr ""

# དེ་ནས་དེ་དག་དད་པ་སྐྱེད་<«ཞོལ་»བསྐྱེད།>པའི་ཕྱིར་བཅོམ་ལྡན་འདས་སྤྲུལ་པ་འགྱེད་པར་མཛད་དེ།
msgctxt "2eb1f16f6a5544c9a2860bf61acccae2"
msgid ""
"དེ་ནས་ དེ་དག་ དད་པ་ སྐྱེད་པ␣འི་ ཕྱིར་ བཅོམ་ལྡན་འདས་ སྤྲུལ་པ་ འགྱེད་པ␣ར་ མཛད་"
" དེ །"
msgstr ""

msgctxt "b6fc063ce1e742cd8dea3d69892f1f76"
msgid "དེ་དག་ གིས་ སྤྲུལ་པ་ མཐོང་ ནས་"
msgstr ""

msgctxt "04d842c60bef4b4e961463c7875a5b28"
msgid "འདི་ སྙམ་ དུ་ སེམས་ ཏེ །"
msgstr ""

msgctxt "a93600a29e6848efb9e0112c133d80d9"
msgid "ཀྱེ་ བདག་ཅག་ ནི་ འདི་ ནས་ ཀྱང་ ཤི་འཕོས་ པར་མ་ གྱུར་ ལ །"
msgstr ""

msgctxt "076c6089a32a41ae9a553346edd5a1bf"
msgid "གཞན་ དུ་ ཡང་ མ་ སྐྱེས་ མོད་ ཀྱི་"
msgstr ""

# འོན་ཀྱང་སྔོན་མ་བྱུང་བའི་སེམས་ཅན་སྣང་བ་འདིའི་མཐུས།
# བདག་ཅག་གི་གནོད་པའི་ཁྱད་པར་རྣམས་རྩེ་ཞིལ་བར་གྱུར་ཏོ་<«གཡུང་»«པེ་»ཏེ།>སྙམ་མོ། །
msgctxt "4a326c5dc9c6460cb0cc3f8c61a1c1ab"
msgid ""
"འོན་ཀྱང་ སྔོན་མ་ བྱུང་བ␣འི་ སེམས་ཅན་ སྣང་བ་ འདི␣འི་ མཐུ␣ས །  བདག་ཅག་ གི་ "
"གནོད་པ␣འི་ ཁྱད་པར་ རྣམས་ རྩེ་ ཞིལ་ བར་ གྱུར་ ཏོ་ སྙམ་ མོ ། །"
msgstr ""

# དེ་ནས་དེ་དག་སྤྲུལ་པ་དེ་<«གཡུང་»«ལི་»«པེ་»«སྣར་»«ཅོ་»«ཞོལ་»–དེ།>ལ་སེམས་<«ཞོལ་»སོམས།>མངོན་པར་དགའ་བར་གྱུར་ནས།
msgctxt "36082bab80384bd2ba30df7b927a857b"
msgid "དེ་ནས་ དེ་དག་ སྤྲུལ་པ་ དེ་ ལ་ སེམས་ མངོན་པར་ དགའ་བ␣ར་ གྱུར་ ནས །"
msgstr ""

# སེམས་ཅན་དམྱལ་བ་མྱོང་བར་:འགྱུར་བའི་<«ཅོ་»གྱུར་པའི།>ལས་དེ་ཟད་དེ་
msgctxt "82c21f655c18442eb1512982525fd92b"
msgid "སེམས་ཅན་ དམྱལ་བ་ མྱོང་བ␣ར་ འགྱུར་བ␣འི་ ལ␣ས་ དེ་ ཟད་ དེ་"
msgstr ""

msgctxt "b2c02a2fb4ba49bdad5d9328268679f2"
msgid ""
"ལྷ་ དང་ །  མི་ རྣམས་ ཀྱི་ ནང་ དུ་ གང་ དུ་ བདེན་པ་ རྣམས་ ཀྱི་ སྣོད་ དུ་ "
"རུང་བ␣ར་ འགྱུར་བ་ དེ␣ར་ ཉིང་མཚམས་ འཛིན་པ␣ར་ འགྱུར་ རོ ། །"
msgstr ""

# སྟེང་དུ་འགྲོ་བ་གང་ཡིན་པ་དེ་དག་ནི་རྒྱལ་ཆེན་བཞིའི་རིས་ཀྱི་ལྷ་རྣམས་དང་།
# སུམ་ཅུ་རྩ་གསུམ་པ་རྣམས་དང་། འཐབ་བྲལ་རྣམས་དང་། དགའ་ལྡན་རྣམས་དང་།
# འཕྲུལ་དགའ་རྣམས་དང་། གཞན་འཕྲུལ་དབང་བྱེད་རྣམས་དང་། ཚངས་རིས་རྣམས་དང་།
# ཚངས་པའི་མདུན་ན་འདོན་པ་རྣམས་དང་། ཚངས་པ་ཆེན་པོ་རྣམས་དང་།
# འོད་ཆུང་རྣམས་[84a]དང་ཚད་མེད་འོད་རྣམས་དང་། འོད་གསལ་རྣམས་དང་།
# དགེ་ཆུང་རྣམས་དང་། ཚད་མེད་དགེ་རྣམས་དང་། དགེ་རྒྱས་རྣམས་དང་།
# སྤྲིན་མེད་རྣམས་དང་། བསོད་ནམས་སྐྱེས་རྣམས་དང་།
# འབྲས་བུ་ཆེ་<«གཡུང་»«པེ་»+བ།>རྣམས་དང་།
# མི་ཆེ་<«གཡུང་»«ལི་»«པེ་»«སྣར་»«ཅོ་»«ཞོལ་»+བ།>རྣམས་དང་། མི་གདུང་རྣམས་དང་།
# གྱ་ནོམ་སྣང་རྣམས་དང་། ཤིན་ཏུ་མཐོང་རྣམས་དང་།
# འོག་མིན་གྱི་ལྷ་རྣམས་ཀྱི་བར་དུ་སོང་ནས་
msgctxt "a674584f76ec43a7a15ebb387f7d61e1"
msgid ""
"སྟེང་ དུ་ འགྲོ་བ་ གང་ཡིན་པ་ དེ་དག་ ནི་ རྒྱལ་ཆེན་ བཞི␣འི་ རིས་ ཀྱི་ ལྷ་ རྣམས་"
" དང་ །  སུམ་ཅུ་རྩ་གསུམ་པ་ རྣམས་ དང་ །  འཐབ་བྲལ་ རྣམས་ དང་ །  དགའ་ལྡན་ རྣམས་ "
"དང་ །  འཕྲུལ་དགའ་ རྣམས་ དང་ །  གཞན་འཕྲུལ་ དབང་ བྱེད་ རྣམས་ དང་ །  ཚངས་རིས་ "
"རྣམས་ དང་ །  ཚངས་པ␣འི་ མདུན་ན་འདོན་པ་ རྣམས་ དང་ །  ཚངས་པ་ ཆེན་པོ་ རྣམས་ དང་ "
"།  འོད་ཆུང་ རྣམས་ དང་ ཚད་ མེད་ འོད་ རྣམས་ དང་ །  འོད་གསལ་ རྣམས་ དང་ །  "
"དགེ་ཆུང་ རྣམས་ དང་ །  ཚད་ མེད་ དགེ་ རྣམས་ དང་ །  དགེ་རྒྱས་ རྣམས་ དང་ །  "
"སྤྲིན་ མེད་ རྣམས་ དང་ །  བསོད་ནམས་ སྐྱེས་ རྣམས་ དང་ །  འབྲས་བུ་ ཆེ་ རྣམས་ "
"དང་ །  མི་ ཆེ་ རྣམས་ དང་ །  མི་ གདུང་ རྣམས་ དང་ །  གྱ་ནོམ་ སྣང་ རྣམས་ དང་ ། "
" ཤིན་ཏུ་ མཐོང་ རྣམས་ དང་ །  འོག་མིན་ གྱི་ ལྷ་ རྣམས་ ཀྱི་ བར་ དུ་ སོང་ ནས་"
msgstr ""

# མི་རྟག་པ་དང་། སྡུག་བསྔལ་བ་དང་། སྟོང་པ་དང་།
# བདག་མེད་པའི་སྒྲ་:སྒྲོགས་སོ<«ཞོལ་»སྒྲོག་གོ།>། །
msgctxt "dff60ff4b2ba4fc599a47b9e9d3847a5"
msgid ""
"མི་རྟག་པ་ དང་ །  སྡུག་བསྔལ་བ་ དང་ །  སྟོང་པ་ དང་ །  བདག་མེད་པ␣འི་ སྒྲ་ "
"སྒྲོགས་ སོ ། །"
msgstr ""

# ཚིགས་སུ་བཅད་<«ལི་»«ཅོ་»གཅད།>པ་གཉིས་ཀྱང་རྗོད་པར་བྱེད་དེ<«གཡུང་»«ལི་»«པེ་»«སྣར་»«ཅོ་»དོ།>།
msgctxt "391e59eb3f5447ce9ba906bed5aa698c"
msgid "ཚིགས་ སུ་ བཅད་པ་ གཉིས་ ཀྱང་ རྗོད་པ␣ར་ བྱེད་ དེ །"
msgstr ""

msgctxt "c034a33673d94d0fad08ec7e20f27cae"
msgid "ངེས་པར་ འབྱུང་བ་ བརྩམ་པ␣ར་ གྱིས ། །"
msgstr ""

msgctxt "ff429ac7f43d49e8a49688a28a30e64c"
msgid "སངས་རྒྱས་ བསྟན་ ལ་ སྦྱོར་བ␣ར་ གྱིས ། །"
msgstr ""

msgctxt "dd35965e580b4b03bba782e5657e6a9e"
msgid ""
"འདམ་བུ␣འི་ ཁྱིམ་ ལ་ གླང་ཆེན་ བཞིན ། ། འཆི་བདག་ སྡེ་ ནི་ གཞོམ་པ␣ར་ གྱིས ། །"
msgstr ""

# གང་ཞིག་ཆོས་འདུལ་འདི་ཉིད་ལ། །བག་ཡོད་:སྤྱོད་པར་<«ཅོ་»སྦྱོར་བར།>བྱེད་པ་ནི།
# །སྐྱེ་བའི་འཁོར་བ་རབ་སྤངས་ནས། །
msgctxt "096843f8d8eb4321bcac40453cff4413"
msgid ""
"གང་ཞིག་ ཆོས་ འདུལ་ འདི་ཉིད་ ལ ། ། བག་ ཡོད་ སྤྱོད་པ␣ར་ བྱེད་པ་ ནི ། ། "
"སྐྱེ་བ␣འི་ འཁོར་བ་ རབ་ སྤངས་ ནས ། །"
msgstr ""

msgctxt "f02658b8447d4852820b0a7278d59116"
msgid "སྡུག་བསྔལ་ ཟད་པར་ བྱེད་པ␣ར་ འགྱུར ། ། ཞེས་"
msgstr ""

# རྗོད་<«གཡུང་»«པེ་»«སྣར་»བརྗོད།>པར་བྱེད་དོ། །
msgctxt "a24dd06efcc54c2aa1e9dec019c4a8de"
msgid "རྗོད་པ␣ར་ བྱེད་ དོ ། །"
msgstr ""

msgctxt "a9b508a8e1a747cd9d16edeb4242903e"
msgid ""
"དེ་ནས་ འོད་ཟེར་ དེ་དག་ གིས་ སྟོང་གསུམ་ གྱི་ སྟོང་ ཆེན་པོ␣འི་ འཇིག་རྟེན་ གྱི་"
" ཁམས་ བསྐོར་ ནས །"
msgstr ""

# བཅོམ་ལྡན་འདས་ཀྱི་<«གཡུང་»–ཀྱི།>ཕྱི་བཞིན་འབྲང་སྟེ།
msgctxt "e4779cf3b0e442cdb1f3dd5e7cc0084a"
msgid "བཅོམ་ལྡན་འདས་ ཀྱི་ ཕྱི་བཞིན་ འབྲང་ སྟེ །"
msgstr ""

msgctxt "b9f5ccce7e544eecabb97ed4ebc3210b"
msgid ""
"གལ་ཏེ་ བཅོམ་ལྡན་འདས་ ཀྱིས་ འདས་པ␣འི་ ལ␣ས་ ལུང་སྟོན་པ␣ར་ བཞེད་ ན་ ནི་ "
"བཅོམ་ལྡན་འདས་ ཀྱི་ སྣམ་ལོགས་ སུ་ མི་སྣང་བ␣ར་ འགྱུར་ རོ ། །"
msgstr ""

# མ་འོངས་པའི་ལས་ལུང་སྟོན་<«གཡུང་»«པེ་»བསྟོན།>པར་བཞེད་ན་ནི་མདུན་ལོགས་སུ་མི་སྣང་བར་འགྱུར་རོ།
# །
msgctxt "ff2fd55c5cea4a7da62e942a60fa239b"
msgid ""
"མ་འོངས་པ␣འི་ ལ␣ས་ ལུང་སྟོན་པ␣ར་ བཞེད་ ན་ ནི་ མདུན་ལོགས་ སུ་ མི་སྣང་བ␣ར་ "
"འགྱུར་ རོ ། །"
msgstr ""

msgctxt "da07d7d27f1c44cab84812d7052b704b"
msgid ""
"སེམས་ཅན་ དམྱལ་བ␣ར་ སྐྱེ་བ␣ར་ ལུང་སྟོན་པ␣ར་ བཞེད་ ན་ ནི་ ཞབས་ ཀྱི་ མཐིལ་ དུ་ "
"མི་སྣང་བ␣ར་ འགྱུར་ རོ ། །"
msgstr ""

msgctxt "d5882b40a30e4ad297fdf7c05962d3af"
msgid ""
"དུད་འགྲོ␣ར་ སྐྱེ་བ␣ར་ ལུང་སྟོན་པ␣ར་ བཞེད་ ན་ ནི་ ཞབས་ ཀྱི་ རྟིང་པ␣ར་ "
"མི་སྣང་བ␣ར་ འགྱུར་ རོ ། །"
msgstr ""

msgctxt "75208735e74649afb9acf2e5fb245bd9"
msgid ""
"ཡི་དགས་ སུ་ སྐྱེ་བ␣ར་ ལུང་སྟོན་པ␣ར་ བཞེད་ ན་ ནི་ ཞབས་ ཀྱི་ མཐེ་བོ␣ར་ "
"མི་སྣང་བ␣ར་ འགྱུར་ རོ ། །"
msgstr ""

# མིར་སྐྱེ་བར་ལུང་སྟོན་པར་བཞེད་ན་<«གཡུང་»«པེ་»ནི།>ནི་པུས་མོ་གཉིས་སུ་མི་སྣང་བར་འགྱུར་རོ།
# །
msgctxt "5371f5fac7f04015a5f8855c0025f46a"
msgid ""
"མི␣ར་ སྐྱེ་བ␣ར་ ལུང་སྟོན་པ␣ར་ བཞེད་ ན་ ནི་ པུས་མོ་ གཉིས་ སུ་ མི་སྣང་བ␣ར་ "
"འགྱུར་ རོ ། །"
msgstr ""

msgctxt "c9d90fa94a92457dacd14fab2f6bd1b2"
msgid ""
"སྟོབས་ ཀྱི་ འཁོར་ལོ␣ས་ སྒྱུར་བ␣འི་ རྒྱལ་པོ␣ར་ ལུང་སྟོན་པ␣ར་ བཞེད་ ན་ ནི་ "
"ཕྱག་ གཡོན་པ␣འི་ མཐིལ་ དུ་ མི་སྣང་བ␣ར་ འགྱུར་ རོ ། །"
msgstr ""

# འཁོར་ལོས་སྒྱུར་བའི་རྒྱལ་པོར་ལུང་སྟོན་པར་བཞེད་ན་ནི་ཕྱག་གཡས་<«གཡུང་»«པེ་»ཡས།>པའི་མཐིལ་དུ་མི་སྣང་བར་འགྱུར་རོ།
# །
msgctxt "15b5e66aea5b4273b3394ce3acca3c26"
msgid ""
"འཁོར་ལོ␣ས་ སྒྱུར་བ␣འི་ རྒྱལ་པོ␣ར་ ལུང་སྟོན་པ␣ར་ བཞེད་ ན་ ནི་ ཕྱག་ གཡས་པ␣འི་ "
"མཐིལ་ དུ་ མི་སྣང་བ␣ར་ འགྱུར་ རོ ། །"
msgstr ""

msgctxt "9b7c16f3c84843c08af6c66cc035a7c9"
msgid ""
"ལྷ␣ར་ སྐྱེ་བ␣ར་ ལུང་སྟོན་པ␣ར་ བཞེད་ ན་ ནི་ ལྟེ་བ␣ར་ མི་སྣང་བ␣ར་ འགྱུར་ རོ "
"། །"
msgstr ""

msgctxt "c1432f750854411094fb340d2e55a967"
msgid ""
"ཉན་ཐོས་ ཀྱི་ བྱང་ཆུབ་ ཏུ་ ལུང་སྟོན་པ␣ར་ བཞེད་ ན་ ནི་ ཞལ་ དུ་ མི་སྣང་བ␣ར་ "
"འགྱུར་ རོ ། །"
msgstr ""

# རང་སངས་རྒྱས་ཀྱི་བྱང་ཆུབ་ཏུ་ལུང་སྟོན་པར་བཞེད་ན་ནི་སྨིན་མཚམས་ཀྱི་<«གཡུང་»«པེ་»–ཀྱི།>མཛོད་སྤུར་མི་སྣང་བར་འགྱུར་རོ།
# །
msgctxt "f8813968a20c4ff4b22594b5af15c677"
msgid ""
"རང་ སངས་རྒྱས་ ཀྱི་ བྱང་ཆུབ་ ཏུ་ ལུང་སྟོན་པ␣ར་ བཞེད་ ན་ ནི་ སྨིན་མཚམས་ ཀྱི་ "
"མཛོད་སྤུ␣ར་ མི་སྣང་བ␣ར་ འགྱུར་ རོ ། །"
msgstr ""

# བླ་ན་མེད་པ་ཡང་དག་པར་རྫོགས་པའི་བྱང་ཆུབ་ཏུ་ལུང་སྟོན་པར་བཞེད་ན་ནི་དབུའི་གཙུག་ཏོར་<«ཅོ་»གཏོར།>དུ་མི་སྣང་བར་འགྱུར་རོ།
# །
msgctxt "31560e74b2fb4b039b7227193a69d658"
msgid ""
"བླ་ ན་ མེད་པ་ ཡང་དག་པ␣ར་ རྫོགས་པ␣འི་ བྱང་ཆུབ་ ཏུ་ ལུང་སྟོན་པ␣ར་ བཞེད་ ན་ ནི་"
" དབུ␣འི་ གཙུག་ཏོར་ དུ་ མི་སྣང་བ␣ར་ འགྱུར་ རོ ། །"
msgstr ""

msgctxt "026b253411bd46429774bf47e050d116"
msgid ""
"དེ་ནས་ འོད་ཟེར་ དེ་དག་ གིས་ བཅོམ་ལྡན་འདས་ ལ་ ལན་ གསུམ་ བསྐོར་བ་ བྱས་ ཏེ །"
msgstr ""

# བཅོམ་ལྡན་འདས་ཀྱི་སྨིན་མཚམས་ཀྱི་མཛོད་<«གཡུང་»འཛོད།>སྤུར་མི་སྣང་བར་:གྱུར་ཏོ<«གཡུང་»«པེ་»འགྱུར་རོ།>།
# །
msgctxt "4c57fe4ff50540ee8e4c2a20099cff99"
msgid ""
"བཅོམ་ལྡན་འདས་ ཀྱི་ སྨིན་མཚམས་ ཀྱི་ མཛོད་སྤུ␣ར་ མི་སྣང་བ␣ར་ གྱུར་ ཏོ ། །"
msgstr ""

msgctxt "1e706f1eaa364f1892d3c16435e52f22"
msgid "དེ་ནས་ ཚེ་ དང་ ལྡན་པ་ ཀུན་དགའ་ བོས་ ཐལ་མོ་ སྦྱར་ ཏེ །"
msgstr ""

msgctxt "5a371b882df74055b7d59d9c71d9dfa4"
msgid "བཅོམ་ལྡན་འདས་ ལ་ ཞུས་པ །"
msgstr ""

# ཚོན་<«གཡུང་»«པེ་»མཚོན།>སྣ་སྟོང་གིས་སྤེལ་བ་སྣ་ཚོགས་ཅན།
# །མང་པོ་ཞལ་གྱི་སྒོ་ནས་བྱུང་བར་གྱུར། །
msgctxt "319fc780b7614dd4989ce0ff2e8489e9"
msgid ""
"ཚོན་ སྣ་ སྟོང་ གིས་ སྤེལ་བ་ སྣ་ཚོགས་ ཅན ། ། མང་པོ་ ཞལ་ གྱི་ སྒོ་ ནས་ "
"བྱུང་བ␣ར་ གྱུར ། །"
msgstr ""

msgctxt "b704890204124844a22ac9d30d94fb95"
msgid "དེ་ ཡིས་ ཕྱོགས་ རྣམས་ ཀུན་ དུ་ སྣང་བ␣ར་ བགྱིས ། །"
msgstr ""

msgctxt "8a54847cdc1c43d0acb45e0f9c33f66e"
msgid "ཉི་མ་ ཤར་བ␣ས་ ཇི་ལྟར་ བགྱིས་པ་ བཞིན ། །"
msgstr ""

msgctxt "f6a797f3157743a0993829b3ce7cf38a"
msgid "ཡང་ ཚིགས་ སུ་ བཅད་ དེ་ གསོལ་པ །"
msgstr ""

msgctxt "b01294ac7ae54c1e8a5cdfb8a2543fb6"
msgid ""
"རྒོད་ དང་ བྲལ་ ཞིང་ རྒྱགས་ དང་ མི་ དགྱེས་ སྤངས་པ་ ཡི ། ། སངས་རྒྱས་ འགྲོ་བ་ "
"དག་ གི་ དམ་པ␣འི་ རྒྱུ␣ར་ གྱུར་པ ། ། རྒྱལ་བ་ དགྲ་ ཐུབ་ རྣམས་ ནི་ རྒྱུ་རྐྱེན་ "
"མ་ མཆིས་པ␣ར ། ། པད་མ␣འི་ རྩ་ དང་ དུང་ མདོག་ ལྟ་བུ␣འི་ འཛུམ་ མི་ སྟོན ། །"
msgstr ""

# བརྟན་པོ་ཉིད་ཀྱི་བློ་ཡིས་དུས་བབ་མཁྱེན་ལགས་ན།
# །དགེ་སྦྱོང་རྒྱལ་དབང་ཉན་པ་སོམ་ཉི་<«གཡུང་»«ལི་»«པེ་»«སྣར་»«ཅོ་»«ཞོལ་»ཉིད།>འཚལ་བ་རྣམས།
# །ཐུབ་པ་ཁྱུ་མཆོག་གི་ནི་གསུང་མཆོག་བརྟན་པ་དང་།
# །བཟང་པོ་དག་གིས་ཐེ་ཚོམ་སྐྱེས་པ་བསལ་བར་གསོལ། །
msgctxt "66c745f01d7a49ee9859277eebd1d356"
msgid ""
"བརྟན་པོ་ ཉིད་ ཀྱི་ བློ་ ཡིས་ དུས་བབ་ མཁྱེན་ ལགས་ ན ། ། དགེ་སྦྱོང་ རྒྱལ་དབང་ "
"ཉན་པ་ སོམ་ཉི་ འཚལ་བ་ རྣམས ། ། ཐུབ་པ་ ཁྱུ་མཆོག་ གི་ ནི་ གསུང་མཆོག་ བརྟན་པ་ "
"དང་ ། ། བཟང་པོ་ དག་ གིས་ ཐེ་ཚོམ་ སྐྱེས་པ་ བསལ་བ␣ར་ གསོལ ། །"
msgstr ""

# རྒྱ་མཚོ་དང་ནི་རི་རྒྱལ་ལྟ་བུར་བརྟན་པ་ཡི།
# །མགོན་པོ་རྫོགས་སངས་རྒྱས་རྣམས་:གྱིན་ད་<«གཡུང་»གྱི་འདའ། «པེ་»གྱིན་དའ།
# «ཅོ་»ཀྱི་ནད། «ཞོལ་»གྱི་ནར།>འཛུམ་མི་སྟོན། །
msgctxt "e3b370d24b844189a02e2c2865220a91"
msgid ""
"རྒྱ་མཚོ་ དང་ ནི་ རི་རྒྱལ་ ལྟ་བུ␣ར་ བརྟན་པ་ ཡི ། ། མགོན་པོ་ རྫོགས་ སངས་རྒྱས་ "
"རྣམས་ གྱིན་ ད་ འཛུམ་ མི་ སྟོན ། །"
msgstr ""

# གང་གི་དོན་དུ་དཔའ་བོ་འཛུམ་པ་སྟོན་མཛད་པ།
# །དེ་ནི་ཁྱོད་ལ་སྐྱེ་བོ་ཕལ་ཆེན་<«གཡུང་»«ལི་»«པེ་»«སྣར་»«ཅོ་»«ཞོལ་»ཆེར།>ཉན་པར་འཚལ།
# །
msgctxt "311fb35c6d99419faef9a59ac9410a45"
msgid ""
"གང་ གི་ དོན་ དུ་ དཔའ་བོ་ འཛུམ་པ་ སྟོན་ མཛད་པ ། ། དེ་ ནི་ ཁྱོད་ ལ་ སྐྱེ་བོ་ "
"ཕལ་ཆེན་ ཉན་པ␣ར་ འཚལ ། །"
msgstr ""

msgctxt "7fb57875a3f24478a6282600d5a148f0"
msgid "བཅོམ་ལྡན་འདས་ ཀྱིས་ བཀའ་ སྩལ་པ །"
msgstr ""

# ཀུན་དགའ་བོ་དེ་ནི་<«ཅོ་»–ནི།>དེ་བཞིན་ནོ། །
msgctxt "d2b6eb27d8c34af3ac22d817ac150480"
msgid "ཀུན་དགའ་བོ་ དེ་ ནི་ དེ་བཞིན་ ནོ ། །"
msgstr ""

# དེ་ནི་དེ་བཞིན་ཏེ། <«གཡུང་»«པེ་»+དེ།>
msgctxt "98059789f7d84c0093aaa816e238f1bb"
msgid "དེ་ ནི་ དེ་བཞིན་ ཏེ །"
msgstr ""

# ཀུན་དགའ་བོ་དེ་བཞིན་གཤེགས་པ་དགྲ་བཅོམ་པ་ཡང་དག་པར་རྫོགས་པའི་<«གཡུང་»«པེ་»–པའི།>སངས་རྒྱས་རྣམས་ནི་རྒྱུ་མེད་རྐྱེན་མེད་པར་འཛུམ་པ་མི་མཛད་དོ།
# །
msgctxt "2d82ad0efb3b49d5b7ed2766bdaabae1"
msgid ""
"ཀུན་དགའ་བོ་ དེ་བཞིན་ གཤེགས་པ་ དགྲ་བཅོམ་པ་ ཡང་དག་པ␣ར་ རྫོགས་པ␣འི་ སངས་རྒྱས་ "
"རྣམས་ ནི་ རྒྱུ་ མེད་ རྐྱེན་ མེད་པ␣ར་ འཛུམ་པ་ མི་ མཛད་ དོ ། །"
msgstr ""

msgctxt "5c03c123a94a4b98b98a5f6dd46d146c"
msgid ""
"ཀུན་དགའ་བོ་ ཁྱོད་ ཀྱིས་ མཛའ་ སྡེ་བོ་ འདི་དག་ གིས་ ང་ ལ་ འདི་ ལྟ་བུ␣འི་ "
"བཀུར་སྟི་ བྱས་པ་ མཐོང་ ངམ །"
msgstr ""

msgctxt "f1d35384dbc9491ab27c3a0c63758881"
msgid "བཙུན་པ་ མཐོང་ ལགས་སོ ། །"
msgstr ""

msgctxt "7b75022122974ce9af574584506c2c26"
msgid ""
"ཀུན་དགའ་བོ་ མཛའ་ སྡེ་བོ་ འདི་དག་ དགེ་བ␣འི་ རྩ་བ་ དང་སེམས་ བསྐྱེད་པ་ དང་ "
"སྦྱིན་པ␣ར་ བྱ་བ␣འི་ ཆོས་ ཡོངས་སུ་ གཏོང་བ་ འདི␣ས །  བསྐལ་པ་ བཅུ་གསུམ་ གྱི་ "
"བར་ དུ་ ལོག་པ␣ར་ ལྟུང་བ␣ར་ མི་ འགྱུར་ ཏེ །"
msgstr ""

msgctxt "e34e7ae75a234a1f9048f74f6d11ff0b"
msgid ""
"བསྐལ་པ་ བཅུ་གསུམ་ གྱི་ བར་ དུ་ ལྷ་ དང་ མི་ རྣམས་ ཀྱི་ ནང་ དུ་ ཉིང་མཚམས་ "
"སྦྱོར་ ཞིང་ འཁོར་བ␣ར་ གྱུར་ ནས །"
msgstr ""

# སྲིད་པ་ཐ་མ་དང་། གནས་ཐ་མ་ལ་<«ཅོ་»ལས།>མིར་སྐྱེ་བ་ཐོབ་ནས།
msgctxt "073eda2b446c4d22a68c722ffafa61c9"
msgid "སྲིད་པ་ ཐ་མ་ དང་ །  གནས་ ཐ་མ་ ལ་ མི␣ར་ སྐྱེ་བ་ ཐོབ་ ནས །"
msgstr ""

msgctxt "f47f463f4b8f47f7941600c9e5980c8a"
msgid "རབ་ཏུ་ བྱུང་ སྟེ་"
msgstr ""

# སློབ་དཔོན་མེད་པ་དང་།
# ལུང་མེད་པར་བྱང་ཆུབ་ཀྱི་ཕྱོགས་དང་འཐུན་པའི་ཆོས་སུམ་ཅུ་<«གཡུང་»«ལི་»«པེ་»བཅུ།>རྩ་བདུན་མངོན་<«གཡུང་»«པེ་»+སུམ།>དུ་བྱས་ནས།
msgctxt "8e89baabe2f247ba96115f29b1465686"
msgid ""
"སློབ་དཔོན་ མེད་པ་ དང་ །  ལུང་ མེད་པ␣ར་ བྱང་ཆུབ་ ཀྱི་ ཕྱོགས་ དང་ འཐུན་པ␣འི་ "
"ཆོས་ སུམ་ཅུ་རྩ་བདུན་ མངོན་དུ་ བྱས་ ནས །"
msgstr ""

msgctxt "dc8ae79f944a43c885d0ff809a3c8593"
msgid "རང་ སངས་རྒྱས་ ཀྱི་ བྱང་ཆུབ་ མངོན་སུམ་ དུ་ བྱེད་པ␣ར་ འགྱུར་ ཏེ །"
msgstr ""

msgctxt "ecf6eb69eb5e438f8635e2fe93e94d1a"
msgid ""
"ཐམས་ཅད་ ཀྱང་ རང་ སངས་རྒྱས་ སྨོན་པ་ མེད་པ་ ཞེས་ བྱ་བ་ ཤ་སྟག་ ཏུ་ འགྱུར་ ཏེ །"
msgstr ""

msgctxt "2db7acd3b0f5440084e8925509b816dc"
msgid "འདི་ ནི་ དེ་དག་ གི་ སྦྱིན་པ␣ར་ བྱ་བ␣འི་ ཆོས་ ཡིན་ ནོ ། །"
msgstr ""

msgctxt "f037e710e8fc4e9885298eba2ac10c06"
msgid "དེ་ནས་ དགེ་སློང་ དག་ གིས་ སངས་རྒྱས་ བཅོམ་ལྡན་འདས་ ལ་ ཞུས་པ །"
msgstr ""

# བཙུན་པ་བཅོམ་ལྡན་འདས་ཀྱིས་ཕྲིན་<«ཞོལ་»འཕྲིན།>ལས་ཅི་མཛད་ན<«ཞོལ་»–ན།>།
# ལས་དེའི་རྣམ་པར་སྨིན་པས་མཛའ་སྡེ་བོ་ལྔ་བརྒྱས་ཨུད་པ་ལ་དང་། པད་མ་དང་།
# ཀུ་མུ་ད་<«ཅོ་»–ད།>དང་། པད་མ་དཀར་པོ་དག་གིས་འདི་ལྟར་བཀུར་སྟི་བགྱིས་ལགས།
msgctxt "6cefe67219a54abe9601f657c4d58046"
msgid ""
"བཙུན་པ་ བཅོམ་ལྡན་འདས་ ཀྱིས་ ཕྲིན་ལས་ ཅི་ མཛད་ ན །  ལ␣ས་ དེ␣འི་ རྣམ་པ␣ར་ "
"སྨིན་པ␣ས་ མཛའ་ སྡེ་བོ་ ལྔ་ བརྒྱ␣ས་ ཨུད་པ་ ལ་ དང་ །  པད་མ་ དང་ །  ཀུ་མུ་ད་ "
"དང་ །  པད་མ་ དཀར་པོ་ དག་ གིས་ འདི་ལྟར་ བཀུར་སྟི་ བགྱིས་ ལགས །"
msgstr ""

msgctxt "34bc15a7ce0d44a2866c3857a0879419"
msgid "བཅོམ་ལྡན་འདས་ ཀྱིས་ བཀའ་ སྩལ་པ །"
msgstr ""

# དགེ་སློང་དག་དེ་བཞིན་གཤེགས་པ་ཉིད་ཀྱིས་<«ཞོལ་»ཀྱི།>ལས་དེ་ལྟ་བུ་བྱས་ཤིང་བསགས་ཏེ།
msgctxt "ac195adab1684ef5bdef93b1f21cde43"
msgid ""
"དགེ་སློང་ དག་ དེ་བཞིན་ གཤེགས་པ་ ཉིད་ ཀྱིས་ ལ␣ས་ དེ་ལྟ་ བུ་ བྱས་ ཤིང་ བསགས་ "
"ཏེ །"
msgstr ""

msgctxt "30727f5e55b34f6bb882e3747d551492"
msgid ""
"དགེ་སློང་ དག་ ལ␣ས་ བྱས་ ཤིང་ བསགས་པ་ རྣམས་ ནི་ ཕྱི་རོལ་ གྱི་ ས␣འི་ ཁམས་ ལ་ "
"རྣམ་པ␣ར་ སྨིན་པ␣ར་ མི་ འགྱུར །"
msgstr ""

msgctxt "7599ef60ff0341e0948e77a95ef59ab2"
msgid ""
"ཆུ␣འི་ ཁམས་ དང་ །  མེ␣འི་ ཁམས་ དང་ །  རླུང་ གི་ ཁམས་ ལ་ རྣམ་པ␣ར་ སྨིན་པ␣ར་ "
"མི་ འགྱུར་ ཏེ །"
msgstr ""

msgctxt "6cf0f41c4b134f059f98648afdfd3c8d"
msgid ""
"ལ␣ས་ དགེ་བ་ དང་ མི་དགེ་བ་ བྱས་ ཤིང་ བསགས་པ་ རྣམས་ ནི་ ཟིན་པ␣འི་ ཕུང་པོ་ དང་ "
"།  ཁམས་ དང་ །  སྐྱེ་མཆེད་ རྣམས་ འབའ་ཞིག་ ལ་ རྣམ་པ␣ར་ སྨིན་པ␣ར་ འགྱུར་ རོ ། །"
msgstr ""

# ལུས་ཅན་དག་གི་<«མི་»«པེ་»«སྣར་»«ཅོ་»གིས།>ལས་རྣམས་ནི།
# །བསྐལ་པ་བརྒྱར་ཡང་ཆུད་མི་ཟ། །
msgctxt "5fef960c5eb44c1f8fd0b4ebd9d02c01"
msgid "ལུས་ཅན་ དག་ གི་ ལ␣ས་ རྣམས་ ནི ། ། བསྐལ་པ་ བརྒྱ␣ར་ ཡང་ ཆུད་ མི་ ཟ ། །"
msgstr ""

msgctxt "f1738a605b6c4605bfa57d61ffa3f9ef"
msgid "ཚོགས་ ཤིང་ དུས་ ལ་ བབ་པ་ ན ། ། འབྲས་བུ་ ཉིད་ དུ་ སྨིན་པ␣ར་ འགྱུར ། །"
msgstr ""

# དགེ་སློང་དག་སྔོན་བྱུང་བ་འདས་པའི་དུས་ན།
# <«གཡུང་»«ལི་»«པེ་»«སྣར་»«ཅོ་»«ཞོལ་»+ང་།>བསྐལ་པ་གྲངས་མེད་པ་གཉིས་ལ་བབ་པ་ན།
# ཡང་དག་པར་རྫོགས་པའི་སངས་རྒྱས་རིག་པ་དང་ཞབས་སུ་ལྡན་པ། བདེ་བར་གཤེགས་པ།
# འཇིག་རྟེན་:མཁྱེན་པ<«གཡུང་»ཁྱེབ་ན།>།
# སྐྱེས་[85b]བུ་:གདུལ་བྱའི་<«གཡུང་»«པེ་»«ཞོལ་»འདུལ་བའི།>ཁ་ལོ་སྒྱུར་བ།
# བླ་ན་<«གཡུང་»«པེ་»+ན།>མེད་པ། ལྷ་དང་མི་རྣམས་ཀྱི་སྟོན་པ།
# སངས་རྒྱས་བཅོམ་ལྡན་འདས་མར་མེ་མཛད་ཅེས་བྱ་བ་འཇིག་རྟེན་དུ་བྱུང་ངོ་། །
msgctxt "ed6ec367f05c4cc78028218bb32e65b6"
msgid ""
"དགེ་སློང་ དག་ སྔོན་བྱུང་བ་ འདས་པ␣འི་ དུས་ ན །  བསྐལ་པ་ གྲངས་མེད་པ་ གཉིས་ ལ་ "
"བབ་པ་ ན །  ཡང་དག་པ␣ར་ རྫོགས་པ␣འི་ སངས་རྒྱས་ རིག་པ་ དང་ ཞབས་ སུ་ ལྡན་པ །  "
"བདེ་བ␣ར་ གཤེགས་པ །  འཇིག་རྟེན་ མཁྱེན་པ །  སྐྱེས་བུ་ གདུལ་བྱ␣འི་ ཁ་ལོ་ "
"སྒྱུར་བ །  བླ་ ན་ མེད་པ །  ལྷ་ དང་ མི་ རྣམས་ ཀྱི་ སྟོན་པ །  སངས་རྒྱས་ "
"བཅོམ་ལྡན་འདས་ མར་མེ་མཛད་ ཅེས་བྱ་བ་ འཇིག་རྟེན་ དུ་ བྱུང་ ངོ་ ། །"
msgstr ""

msgctxt "b98221ee0ca449289cb632ff6f9c3bcd"
msgid ""
"དེ་ནས་ ཡང་དག་པ␣ར་ རྫོགས་པ␣འི་ སངས་རྒྱས་ མར་མེ་མཛད་ ལྗོངས་རྒྱུ་ ཞིང་གཤེགས་པ་ "
"ལ␣ས །"
msgstr ""

# རྒྱལ་པོའི་ཕོ་བྲང་མར་མེ་ལྡན་ཞེས་བྱ་བར་བྱོན་ཏོ<«ཅོ་»ནོ།>། །
msgctxt "6b7510ca7eee4f2780f7327d05309d49"
msgid "རྒྱལ་པོ␣འི་ ཕོ་བྲང་ མར་མེ་ ལྡན་ ཞེས་ བྱ་བ␣ར་ བྱོན་ ཏོ ། །"
msgstr ""

msgctxt "b047bed67c18458297f66f7fde260baf"
msgid ""
"དེ␣འི་ ཚེ་ རྒྱལ་པོ␣འི་ ཕོ་བྲང་ མར་མེ་ ལྡན་ ན་ རྒྱལ་པོ་ མར་མེ་ ཞེས་ བྱ་བ་ "
"རྒྱལ་པོ་ བྱེད་ དེ །"
msgstr ""

# དེའི་རིང་ལ་འབྱོར་པ་དང་། རྒྱས་པ་དང་། བདེ་བ་དང་། ལོ་ལེགས་པ་དང་། སྐྱེ་བོ་དང་།
# མི་མང་པོས་གང་བ་དང་། རྩོད་པ་དང་། འཐབ་པ་ཞི་བ་དང་། འཁྲུག་པ་དང་། ནང་འཁྲུག་དང་།
# ཆོམ་རྐུན་དང་། ནད་དང་། མུ་གེ་མེད་པ་དང་། འབྲས་སཱ་<«གཡུང་»«པེ་»ས།>ལུ་དང་།
# བུ་རམ་ཤིང་དང་། བ་ལང་དང་། མ་ཧེ་དང་ལྡན་པར་འདུག་སྟེ།
msgctxt "f1870fc525ee46b8af4c9e92f2ebbd9e"
msgid ""
"དེ␣འི་ རིང་ ལ་ འབྱོར་པ་ དང་ །  རྒྱས་པ་ དང་ །  བདེ་བ་ དང་ །  ལོ་ལེགས་པ་ དང་ "
"།  སྐྱེ་བོ་ དང་ །  མི་ མང་པོ␣ས་ གང་བ་ དང་ །  རྩོད་པ་ དང་ །  འཐབ་པ་ ཞི་བ་ དང་"
" །  འཁྲུག་པ་ དང་ །  ནང་འཁྲུག་ དང་ །  ཆོམ་རྐུན་ དང་ །  ནད་ དང་ །  མུ་གེ་ "
"མེད་པ་ དང་ །  འབྲས་ སཱ་ ལུ་ དང་ །  བུ་རམ་ཤིང་ དང་ །  བ་ལང་ དང་ །  མ་ཧེ་ དང་ "
"ལྡན་པ␣ར་ འདུག་ སྟེ །"
msgstr ""

msgctxt "760d3ada4f64483cba02c81ec7f1ce29"
msgid "གནོད་པ་ མེད་ ཅིང་"
msgstr ""

msgctxt "cd0b807e0067459695fbd3558cf40594"
msgid "ཚེར་མ་ དཀྲུགས་ ལ་"
msgstr ""

# བུ་གཅིག་པ་སྡུག་པ་ལ་<«གཡུང་»ག>བྱ་བ་བཞིན་དུ་རྒྱལ་སྲིད་བྱེད་དོ། །
msgctxt "687e9c5286a04ab7adf45742e8a17291"
msgid "བུ་ གཅིག་པ་ སྡུག་པ་ ལ་ བྱ་བ་ བཞིན་ དུ་ རྒྱལ་སྲིད་ བྱེད་ དོ ། །"
msgstr ""

msgctxt "cc3bf57255434fb19be312860cfcdefe"
msgid ""
"དེ་ནས་ རྒྱལ་པོ་ མར་མེ␣ས་ ཡང་དག་པ␣ར་ རྫོགས་པ␣འི་ སངས་རྒྱས་ མར་མེ་མཛད་ "
"གྲོང་ཁྱེར་ གྱི་ ནང་ དུ་ ཞབས་ ས་ ལ་ རེག་པ␣ར་ བྱ་བ␣འི་ ཕྱིར་ སྤྱན་ དྲངས་ ནས །"
msgstr ""

# རྒྱལ་པོ་<«གཡུང་»«པེ་»པོའི།>མར་མེའི་འདབ་<«ཞོལ་»འདབས།>ན་རྒྱལ་པོ་ནོར་ཅན་གྱི་བུ་ཞེས་བྱ་བ་འདུག་པ་དེ་ལ་དེས་ཕོ་ཉ་བཏང་སྟེ།
msgctxt "0b55579fdc31438d9abdf509263ac608"
msgid ""
"རྒྱལ་པོ་ མར་མེ␣འི་ འདབ་ ན་ རྒྱལ་པོ་ ནོར་ ཅན་ གྱི་ བུ་ ཞེས་ བྱ་བ་ འདུག་པ་ དེ་"
" ལ་ དེ␣ས་ ཕོ་ཉ་ བཏང་ སྟེ །"
msgstr ""

# ངས་<«གཡུང་»«ཅོ་»ངེས།>ཡང་དག་པར་རྫོགས་པའི་སངས་རྒྱས་མར་མེ་མཛད་གྲོང་ཁྱེར་གྱི་ནང་དུ་ཞབས་ས་ལ་རེག་པར་བྱ་བའི་ཕྱིར་སྤྱན་དྲངས་ཏེ།
msgctxt "2f00deb20f184d8c84f5ae6a7c6db906"
msgid ""
"ངས་ ཡང་དག་པ␣ར་ རྫོགས་པ␣འི་ སངས་རྒྱས་ མར་མེ་མཛད་ གྲོང་ཁྱེར་ གྱི་ ནང་ དུ་ ཞབས་"
" ས་ ལ་ རེག་པ␣ར་ བྱ་བ␣འི་ ཕྱིར་ སྤྱན་ དྲངས་ ཏེ །"
msgstr ""

# དེ་ལ་མཆོད་པ་བྱ་དགོས་ཀྱིས་<«ཞོལ་»ཀྱི།>
msgctxt "ce5832eb02ea4b7fb5722702f3612a87"
msgid "དེ་ ལ་ མཆོད་པ་ བྱ་ དགོས་ ཀྱིས་"
msgstr ""

