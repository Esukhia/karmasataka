# ཁྱིམ་བདག་དེས། དགེ་སློང་དེ་བདག་གི་<«ལི་»«སྣར་»«ཅོ་»གིས།>ཁྱིམ་དུ་ཁྲིད་དེ།
msgctxt "d776a4500052438980a6fe493150e5ac"
msgid "ཁྱིམ་བདག་ དེ␣ས །  དགེ་སློང་ དེ་ བདག་ གི་ ཁྱིམ་ དུ་ ཁྲིད་ དེ །"
msgstr ""

msgctxt "473ce670050845e2af76e2a112549d25"
msgid "སྨན་པ␣འི་ ལུང་ བཞིན་ དུ་ ཡོ་བྱད་ ཐམས་ཅད་ ཀྱིས་ ནད་གཡོག་ བྱས་ སོ ། །"
msgstr ""

msgctxt "26ed16a0ec7743c984ad332753f44635"
msgid ""
"དགེ་སློང་ དེ␣ས་ ཀྱང་ ཁྱིམ་ དེ་ཉིད་ ན་ འདུག་ བཞིན་ དུ་ ནད་ ཀྱིས་ སྐྱོ་བ␣ར་ "
"གྱུར་ ནས །"
msgstr ""

# བརྩོན་པ་དང་བསྒྲུབ་<«ཞོལ་»སྒྲུབ།>པ་དང་འབད་པས་ཉོན་མོངས་པ་ཐམས་ཅད་སྤངས་ནས།
msgctxt "109f20e37639415280990de67c007ce2"
msgid "བརྩོན་པ་ དང་ བསྒྲུབ་པ་ དང་ འབད་པ␣ས་ ཉོན་མོངས་པ་ ཐམས་ཅད་ སྤངས་ ནས །"
msgstr ""

# དགྲ་བཅོམ་པ་ཉིད་མངོན་སུམ་དུ་བྱས་ཏེ<«གཡུང་»«པེ་»སྟེ།>།
msgctxt "4dc43c2559cc481ea09af4d7150cf8d9"
msgid "དགྲ་བཅོམ་པ་ ཉིད་ མངོན་སུམ་ དུ་ བྱས་ ཏེ །"
msgstr ""

msgctxt "d393b7f920284e778dc2a57a54a362fb"
msgid "དེ␣ས་ དགྲ་བཅོམ་པ་ ཐོབ་ ནས་"
msgstr ""

# ཕ་མ་གཉིས་དང་ཁྱིམ་གྱི་མི་རྣམས་ཀྱི་བསམ་པ་དང་བག་ལ་ཉལ་དང་ཁམས་དང་རང་བཞིན་རྟོགས་<«གཡུང་»«པེ་»རྟགས།>ནས་
msgctxt "5bb99dc7221e4a4b87ead0d2cff4de0d"
msgid ""
"ཕ་མ་ གཉིས་ དང་ ཁྱིམ་ གྱི་ མི་ རྣམས་ ཀྱི་ བསམ་པ་ དང་ བག་ ལ་ ཉལ་ དང་ ཁམས་ དང་ "
"རང་བཞིན་ རྟོགས་ ནས་"
msgstr ""

msgctxt "4df5823db77d47aaac7c81c81089c8bc"
msgid "དེ་ དང་ འཐུན་པ␣འི་ ཆོས་ བསྟན་ ཏེ །"
msgstr ""

msgctxt "d39b41a776394e198b3eb516197ae417"
msgid "དེ་ ཐོས་ ནས་"
msgstr ""

# ཁྱིམ་བདག་དེ་འཁོར་དང་བཅས་[15a]པས་<«གཡུང་»–པས།>སྟན་དེ་ཉིད་ལ་འདུག་བཞིན་དུ་འཇིག་ཚོགས་ལ་ལྟ་བའི་རིའི་རྩེ་མོ་མཐོན་པོ་ཉི་ཤུ་ཡེ་ཤེས་རྡོ་རྗེས་<«གཡུང་»«ལི་»«པེ་»རྗེ།>བཅོམ་ནས།
msgctxt "f27670f04d0c4e35b0bfecebddb1fb46"
msgid ""
"ཁྱིམ་བདག་ དེ་ འཁོར་ དང་ བཅས་པ␣ས་ སྟན་ དེ་ཉིད་ ལ་ འདུག་ བཞིན་ དུ་ འཇིག་ཚོགས་ "
"ལ་ ལྟ་བ␣འི་ རི␣འི་ རྩེ་མོ་ མཐོན་པོ་ ཉི་ཤུ་ ཡེ་ཤེས་ རྡོ་རྗེ␣ས་ བཅོམ་ ནས །"
msgstr ""

msgctxt "f352f753a4574577964775848ca31a26"
msgid "རྒྱུན་ དུ་ ཞུགས་པ␣འི་ འབྲས་བུ་ མངོན་སུམ་ དུ་ བྱས་ ཏེ །"
msgstr ""

msgctxt "6beea4b60275478b9012e7adac9bf6bf"
msgid "དེ␣ས་ ཕ་མ་ གཉིས་ བདེན་པ་ རྣམས་ ལ་ བཀོད་ ནས །"
msgstr ""

msgctxt "a5d6814e5a994d82a48fc20b945b6cec"
msgid "བདག་ཉིད་ ཀྱི་ སྔོན་ གྱི་ གནས་ རྗེས་སུ་ དྲན་པ␣ར་ བྱེད་ དེ །"
msgstr ""

# བདག་གང་ནས་འཆི་<«གཡུང་»«པེ་»མཆི།>འཕོས་
msgctxt "cded8f438db94fd68926547981d42012"
msgid "བདག་ གང་ ནས་ འཆི་ འཕོས་"
msgstr ""

# གང་དུ་<«ལི་»རུ།>སྐྱེས་སྙམ་<«གཡུང་»མཉམ།>དུ་
msgctxt "f535f390bcdd45b5bbd7cdb32551110c"
msgid "གང་ དུ་ སྐྱེས་ སྙམ་ དུ་"
msgstr ""

# བསམས་ནས<«གཡུང་»«ལི་»«པེ་»«སྣར་»«ཅོ་»«ཞོལ་»ན།>།
msgctxt "fb6ccc355a514ba38f2b29dfb6632140"
msgid "བསམས་ ནས །"
msgstr ""

msgctxt "63de7067eb5d4d36bfff269b238d057e"
msgid "དེ␣ས་ མི་ལ␣ས་ ཤི་འཕོས་ ཏེ །"
msgstr ""

# མི་ཉིད་དུ་སྐྱེས་ལ་<«གཡུང་»«ལི་»«པེ་»«སྣར་»«ཅོ་»«ཞོལ་»–མི་ཉིད་དུ་སྐྱེས་ལ།>
msgctxt "a6cae8d9ac1a4f9082815ca24e4d50f6"
msgid "མི་ ཉིད་ དུ་ སྐྱེས་ ལ་"
msgstr ""

msgctxt "52a33a0be2c742c7b7b9051559f2e2e9"
msgid "ཐམས་ཅད་ དུ་ ཡང་ ནད་ མང་བ་ དང་ །  ཚེ་ ཐུང་བ␣ར་ གྱུར་པ␣ར་ མཐོང་ ནས །"
msgstr ""

msgctxt "b4397e5c85204263abf369a19da83c52"
msgid "དེ␣ས་ བསམས་པ་"
msgstr ""

