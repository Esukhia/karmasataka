msgctxt "f4a06d6d33d04c31bd65606d45c919ef"
msgid "རུས་པ་ དེ་ རྣམས་ བསྡུས་ ནས་"
msgstr ""

# བུམ་པའི་ནང་དུ་བཅུག་སྟེ་<«ལི་»«ཅོ་»ཏེ།>
msgctxt "0f9a148ed31946c1be64c95526f11cee"
msgid "བུམ་པ␣འི་ ནང་ དུ་ བཅུག་ སྟེ་"
msgstr ""

# ས་ཕྱོགས་དེ་ཉིད་དུ་སྐུ་གདུང་གི་མཆོད་རྟེན་བརྩིགས་<«གཡུང་»«པེ་»ཅིགག>ནས།
msgctxt "0def22651d8c4dbbb4df49687ca6592c"
msgid "ས་ཕྱོགས་ དེ་ཉིད་ དུ་ སྐུ་གདུང་ གི་ མཆོད་རྟེན་ བརྩིགས་ ནས །"
msgstr ""

msgctxt "ca4858ee54a34a099378eb5ecdd360c7"
msgid "མཆོད་རྟེན་ དེ་ ལ་ མཆོད་པ་ ཆེན་པོ་ བྱས་ ཏེ་"
msgstr ""

msgctxt "9efbc019e8f54663a14adb46db666091"
msgid "ཆོས་ མཉན་པ␣འི་ ཕྱིར །  སངས་རྒྱས་ ཀྱི་ སྤྱན་སྔ␣ར་ འཁོད་ ནས །"
msgstr ""

# བཅོམ་ལྡན་འདས་ཀྱིས་འཁོར་བཞི་པོ་རྣམས་ལ་མི་རྟག་པ་དང་ལྡན་པའི་གཏམ་<«གཡུང་»བཏམ།>བཀའ་སྩལ་ནས།
msgctxt "b846062ab76a4f1d8ee730e6e8ec3d3e"
msgid ""
"བཅོམ་ལྡན་འདས་ ཀྱིས་ འཁོར་ བཞི་པོ་ རྣམས་ ལ་ མི་རྟག་པ་ དང་ ལྡན་པ␣འི་ གཏམ་ བཀའ་"
" སྩལ་ ནས །"
msgstr ""

msgctxt "f85cc5c48b084050a19f7a4ef4988f8e"
msgid "གཙུག་ལག་ཁང་ དུ་ གཤེགས་ སོ ། །"
msgstr ""

msgctxt "6948900ef0084555bd788ec74901f9bc"
msgid "དེ་ནས་ དགེ་སློང་ དག་ གིས་ བཅོམ་ལྡན་འདས་ ལ་ ཞུས་པ །"
msgstr ""

msgctxt "d0661b32cc884c07ae9cc85b7dfd571c"
msgid "དགེ་སློང་ རྫོགས་བྱེད་ ཀྱིས་ ལ␣ས་ ཅི་ བགྱིས་ ན །"
msgstr ""

msgctxt "c591140328a6453ca0153b8f1e043511"
msgid ""
"ལ␣ས་ དེ␣འི་ རྣམ་པ␣ར་ སྨིན་པ␣ས་ ཕྱུག་ ཅིང་ ནོར་ མང་ ལ་ ལོངས་སྤྱོད་ ཆེ་བ␣འི་ "
"རིགས་ སུ་ སྐྱེས་ ལ་"
msgstr ""

msgctxt "879e40536e144d119b0d6b6c790fdf7a"
msgid "ནད་ མང་བ␣ར་ གྱུར་ ལགས །"
msgstr ""

msgctxt "2fc60cea81f246f096db27f8b5daaed8"
msgid "བཅོམ་ལྡན་འདས་ ཀྱིས་ བཀའ་ སྩལ་པ །"
msgstr ""

msgctxt "75fae8d69e4b4f59b6af6cbef34d7e21"
msgid "དགེ་སློང་ དག་"
msgstr ""

# རྫོགས་བྱེད་འདི་ཉིད་ཀྱིས་སྔོན་ལས་བྱས་ཤིང་བསགས་ཏེ<«གཡུང་»བསག་སྟེ།>།
msgctxt "cbfed6dd2b3947b596f5f226adbea293"
msgid "རྫོགས་བྱེད་ འདི་ཉིད་ ཀྱིས་ སྔོན་ལས་ བྱས་ ཤིང་ བསགས་ ཏེ །"
msgstr ""

msgctxt "c5c1c5143b584d05a02f5f2ddf1507f7"
msgid "དགེ་སློང་ དག་"
msgstr ""

# ལས་བྱས་ཤིང་བསགས་པ་རྣམས་ནི།
# ཕྱི་རོལ་གྱི་སའི་ཁམས་ལ་རྣམ་པར་<«གཡུང་»«པེ་»–རྣམ་པར།>སྨིན་པར་མི་འགྱུར།
msgctxt "d32fa97457fe49ae97a84b7494be9cf2"
msgid ""
"ལ␣ས་ བྱས་ ཤིང་ བསགས་པ་ རྣམས་ ནི །  ཕྱི་རོལ་ གྱི་ ས␣འི་ ཁམས་ ལ་ རྣམ་པ␣ར་ "
"སྨིན་པ␣ར་ མི་ འགྱུར །"
msgstr ""

msgctxt "c657b7f7574a49019749e5e9f10b9cbf"
msgid ""
"ཆུ␣འི་ ཁམས་ དང་ །  མེ␣འི་ ཁམས་ དང་ །  རླུང་ གི་ ཁམས་ ལ་ རྣམ་པ␣ར་ སྨིན་པ␣ར་ "
"མི་ འགྱུར་ ཏེ །"
msgstr ""

# ལས་དགེ་བ་དང་མི་དགེ་བ་བྱས་ཤིང་བསགས་པ་རྣམས་ནི། ཟིན་པའི་ཕུང་པོ་དང་།
# ཁམས་དང་སྐྱེ་མཆེད་རྣམས་འབའ་ཞིག་ལ་རྣམ་པར་<«གཡུང་»«པེ་»–རྣམ་པར།>སྨིན་པར་འགྱུར་རོ།
# །
msgctxt "5cc6ad3558504a998ef21488e9e747bd"
msgid ""
"ལ␣ས་ དགེ་བ་ དང་ མི་དགེ་བ་ བྱས་ ཤིང་ བསགས་པ་ རྣམས་ ནི །  ཟིན་པ␣འི་ ཕུང་པོ་ "
"དང་ །  ཁམས་ དང་ སྐྱེ་མཆེད་ རྣམས་ འབའ་ཞིག་ ལ་ རྣམ་པ␣ར་ སྨིན་པ␣ར་ འགྱུར་ རོ "
"། །"
msgstr ""

msgctxt "f44b3cf7f26842a78b797e4410416e52"
msgid "ལུས་ཅན་ དག་ གི་ ལ␣ས་ རྣམས་ ནི ། ། བསྐལ་པ་ བརྒྱ␣ར་ ཡང་ ཆུད་ མི་ ཟ ། །"
msgstr ""

# ཚོགས་ཤིང་དུས་ལ་བབ་པ་<«མི་»«སྣར་»«ཅོ་»པས།>ན། །
msgctxt "a46f3911045346fb9d86c56c45b472da"
msgid "ཚོགས་ ཤིང་ དུས་ ལ་ བབ་པ་ ན ། །"
msgstr ""

# འབྲས་བུ་ཉིད་དུ་སྨིན་པར་འགྱུར<«གཡུང་»«པེ་»+རོ།>། །
msgctxt "6b4b4119aeb14758a02afce59968696c"
msgid "འབྲས་བུ་ ཉིད་ དུ་ སྨིན་པ␣ར་ འགྱུར ། །"
msgstr ""

msgctxt "94b94f1ce7da4c3f97c6561fed9d2abc"
msgid "དགེ་སློང་ དག་"
msgstr ""

# སྔོན་བྱུང་བ་འདས་པའི་དུས་ན། བསྐལ་པ་བཟང་པོ་འདི་ཉིད་ལ།
# སྐྱེ་དགུའི་<«ཞོལ་»རྒུའི།>ཚེ་ལོ་བཞི་ཁྲི་ཐུབ་པ་ན། རྒྱལ་པོའི་ཕོ་བྲང་བདེ་ལྡན་ན།
# རྒྱལ་པོ་བདེ་བའི་མདུན་ན་འདོན་པ། བྲམ་ཟེ་མེས་བྱིན་ལ་བུ་གཉིས་ཡོད་པ་ལས་
msgctxt "b814f9467b644ccb9c52640e8dd43f6e"
msgid ""
"སྔོན་བྱུང་བ་ འདས་པ␣འི་ དུས་ ན །  བསྐལ་པ་ བཟང་པོ་ འདི་ཉིད་ ལ །  སྐྱེ་དགུ␣འི་ "
"ཚེ་ལོ་ བཞི་ ཁྲི་ཐུབ་པ་ ན །  རྒྱལ་པོ␣འི་ ཕོ་བྲང་ བདེ་ལྡན་ ན །  རྒྱལ་པོ་ "
"བདེ་བ␣འི་ མདུན་ན་འདོན་པ །  བྲམ་ཟེ་ མེ␣ས་ བྱིན་ ལ་ བུ་ གཉིས་ ཡོད་པ་ ལ␣ས་"
msgstr ""

msgctxt "a95f7fd63c7c46fda782359cb5d2ce0b"
msgid "གཅིག་ གིས་ ནི་ རྒན་པོ་ དང་ །  ནད་པ་ དང་ ཤི་བ་ མཐོང་ ནས་"
msgstr ""

msgctxt "1e1a30472d664c0ba2e67bfd072402ec"
msgid "ནགས་ ཀྱི་ ནང་ དུ་ རྟེན་ བཅས་ ཏེ །"
msgstr ""

msgctxt "1c55b07ae11143fc9ac4cbcd1994f775"
msgid "དེ་དེ␣ར་ སོང་ ནས་"
msgstr ""

# བྱང་ཆུབ་ཀྱི་ཕྱོགས་དང་འཐུན་པའི་ཆོས་སུམ་ཅུ་<«གཡུང་»«ལི་»«པེ་»«ཅོ་»བཅུ།>རྩ་བདུན་དྲན་པར་བྱས་ནས།
msgctxt "0147560b8e3f4265adcf6c518e3ca772"
msgid ""
"བྱང་ཆུབ་ ཀྱི་ ཕྱོགས་ དང་ འཐུན་པ␣འི་ ཆོས་ སུམ་ཅུ་རྩ་བདུན་ དྲན་པ␣ར་ བྱས་ ནས །"
msgstr ""

msgctxt "f87a2c706ef54422b28576f8728ad721"
msgid "བླ་ ན་ མེད་པ་ ཡང་དག་པ␣ར་ རྫོགས་པ␣འི་ བྱང་ཆུབ་ བརྙེས་ ཏེ །"
msgstr ""

msgctxt "b880600526c44ce9ba3eddb5d5375c3c"
msgid "ཆོས་ ཀྱི་ འཁོར་ལོ་ རྣམ་པ་ བཅུ་གཉིས་ ལན་ གསུམ་ དུ་ བསྐོར་ ནས་"
msgstr ""

msgctxt "17c82e92a76c45b8984603648467227c"
msgid "སེམས་ཅན་ ལ་ ཕན་པ་ མཛད་ དེ །"
msgstr ""

msgctxt "af66ca5c50c848c5bd632bae7c874c42"
msgid ""
"ཡང་དག་པ␣ར་ རྫོགས་པ␣འི་ སངས་རྒྱས་ འཁོར་བ་ འཇིག་ ཅེས་བྱ་བ␣ར་ གྱུར་ ཏོ ། །"
msgstr ""

# ཅིག་<«གཡུང་»«ལི་»«པེ་»«སྣར་»«ཅོ་»«ཞོལ་»གཅིག>ཤོས་ནི་འདོད་པ་ལ་ཞུགས་ཏེ་
msgctxt "8724b7b9f63c417bb1f934f022ad9869"
msgid "ཅིག་ཤོས་ ནི་ འདོད་པ་ ལ་ ཞུགས་ ཏེ་"
msgstr ""

msgctxt "4c51996cbff84c47a2d3403cedcb8d6a"
msgid "བག་མེད་པ་ དང་"
msgstr ""

msgctxt "bd2737e0e061475ba6df59c9b819a635"
msgid "གཞན་ གྱི་ ཆུང་མ་ ལ་ བྱི་ བྱེད་པ་ དང་ །"
msgstr ""

msgctxt "e77c87eaf7894f008d1e543e1f110f74"
msgid "སྲོག་གཅོད་པ་ བྱེད་པ␣ར་ གྱུར་ ཏེ །"
msgstr ""

# དེ་རི་དགས་ཤོར་<«ཞོལ་»དྭགས་བཤོར།>དུ་སོང་ནས།
msgctxt "8b7afa743c1b457d89f16742d7d4405c"
msgid "དེ་ རི་ དག␣ས་ ཤོ␣ར་ དུ་ སོང་ ནས །"
msgstr ""

# སེམས་ཅན་སྟོང་ཕྲག་དུ་མ་<«གཡུང་»+ལ།>ཁ་འདོག་པར་བྱེད་དོ<«གཡུང་»–དོ།>། །
msgctxt "06b793cbade94723af3d18636be554b1"
msgid "སེམས་ཅན་ སྟོང་ཕྲག་ དུ་མ་ ཁ་ འདོག་པ␣ར་ བྱེད་ དོ ། །"
msgstr ""

# དེར་གང་གི་ཚེ་ཡང་དག་པར་རྫོགས་<«གཡུང་»–པར་རྫོགས།>པའི་སངས་རྒྱས་འཁོར་བ་འཇིག་ཕོ་བྲང་མཛེས་ལྡན་དུ་གཤེགས་ཏེ།
msgctxt "9e2fea31e3234f9d97499bc220c2a723"
msgid ""
"དེ␣ར་ གང་ གི་ ཚེ་ ཡང་དག་པ␣ར་ རྫོགས་པ␣འི་ སངས་རྒྱས་ འཁོར་བ་ འཇིག་ ཕོ་བྲང་ "
"མཛེས་ལྡན་ དུ་ གཤེགས་ ཏེ །"
msgstr ""

# ཡབ་སྲས་གདན་འཛོམ་<«གཡུང་»«པེ་»འཇོམ།
# «ཞོལ་»འཛོམས།>པ་དེའི་ཚེ་མཛེས་ལྡན་ན་རྟེན་ཅིང་བཞུགས་ཏེ།
msgctxt "78e43636475c48d2ade71f4f2eaf5a6a"
msgid "ཡབ་ སྲས་ གདན་ འཛོམ་པ་ དེ␣འི་ ཚེ་ མཛེས་ལྡན་ ན་ རྟེན་ ཅིང་ བཞུགས་ ཏེ །"
msgstr ""

msgctxt "a185b31561c44f2483256b755c7bc15c"
msgid "སེམས་ཅན་ ལ་ ཕན་པ་ མཛད་ དོ ། །"
msgstr ""

msgctxt "10a6f8d1bae24b93b3a3e8814f152a43"
msgid "དེ་ནས་ དེ␣ས་ གཅུང་ དེ་ སྡིག་པ␣འི་ ལ␣ས་ དེ་ལས་ བཟློག་ ནས །"
msgstr ""

# སྐྱབས་སུ་འགྲོ་བ་དང་། བསླབ་པའི་གཞི་<«གཡུང་»«པེ་»བཞི།>རྣམས་ལ་གནས་པར་མཛད་དོ། །
msgctxt "1da8bf832f1a49e383a9a6f884112369"
msgid ""
"སྐྱབས་ སུ་ འགྲོ་བ་ དང་ །  བསླབ་པ␣འི་ གཞི་ རྣམས་ ལ་ གནས་པ␣ར་ མཛད་ དོ ། །"
msgstr ""

msgctxt "7a6ff1b0a05b4ec49caef11b32c3df79"
msgid "དེ␣ས་ ཀྱང་ རྣམ་པ་ ཐམས་ཅད་ ཡོངས་སུ་ རྫོགས་པ␣འི་ གཙུག་ལག་ཁང་ བྱས་ ནས་"
msgstr ""

msgctxt "17f38720a1b749ef8ed3c0dcf08a55e1"
msgid ""
"ཡང་དག་པ␣ར་ རྫོགས་པ␣འི་ སངས་རྒྱས་ འཁོར་བ་ འཇིག་ ཉན་ཐོས་ ཀྱི་ དགེ་འདུན་ དང་ "
"བཅས་པ་ ལ་ ཕུལ་ ཏེ་"
msgstr ""

msgctxt "750dd5c12f5c4408a2d5a11bfba56afe"
msgid "ཡོ་བྱད་ ཐམས་ཅད་ ཀྱིས་ བསྙེན་བཀུར་ བྱས་ ནས་"
msgstr ""

# འཆི་ཀར་<«ཞོལ་»གར།>སྨོན་ལམ་བཏབ་<«གཡུང་»«པེ་»གཏབ།>པ།
msgctxt "4a4874b1b67547cdbd27032cda180bd7"
msgid "འཆི་ཀ␣ར་ སྨོན་ལམ་ བཏབ་པ །"
msgstr ""

# ཀྱེ་<«གཡུང་»སྐུ།>མ་
msgctxt "1116eee35f7e499ebc50850b3ac814e8"
msgid "ཀྱེ་མ་"
msgstr ""

msgctxt "8cf004f2780c44aa9515a04abb93b647"
msgid ""
"དགེ་བ␣འི་ རྩ་བ་ འདི␣ས་ བདག་ གང་དང་གང་ དུ་ སྐྱེ་བ་ དེ་ དང་ དེ␣ར་ ཕྱུག་ ཅིང་ "
"ནོར་ མང་ ལ་ ལོངས་སྤྱོད་ ཆེ་བ␣འི་ རིགས་ སུ་ སྐྱེ་བ␣ར་ གྱུར་ཅིག  །"
msgstr ""

# སྟོན་པ་འདི་ལྟ་<«ལི་»སྟ།>བུ་མཉེས་པར་བྱེད་པར་གྱུར་ཅིག །
msgctxt "ef67d3a621e74327ae55b7ede9c224a7"
msgid "སྟོན་པ་ འདི་ ལྟ་བུ་ མཉེས་པ␣ར་ བྱེད་པ␣ར་ གྱུར་ཅིག  །"
msgstr ""

msgctxt "28256c32763e4aee8e24c672c9f9ffdf"
msgid "མི་ མཉེས་པ␣ར་ བྱེད་པ␣ར་ མ་ གྱུར་ཅིག  །"
msgstr ""

msgctxt "13375bf5fda2447e936ea4cf2e25b4de"
msgid "དེ་ ཁོ་ན␣འི་ བསྟན་པ་ ལ་ རབ་ཏུ་ བྱུང་ ནས་"
msgstr ""

msgctxt "cc77833752c24d0ea331715473b3ee2d"
msgid "ཉོན་མོངས་པ་ ཐམས་ཅད་ སྤངས་ ཏེ །"
msgstr ""

msgctxt "89182b41491a427e8fef32f451243378"
msgid "དགྲ་བཅོམ་པ་ ཉིད་ མངོན་སུམ་ དུ་ བྱེད་པ␣ར་ གྱུར་ཅིག་ ཅེས་"
msgstr ""

msgctxt "d09f6c6830ca4be8817957746df08808"
msgid "བྱས་ སོ ། །"
msgstr ""

msgctxt "139f89615c5c460fbe28cf547184d9d9"
msgid "དགེ་སློང་ དག་"
msgstr ""

msgctxt "ec08aca257284ebd8430a9ddb20f499d"
msgid "ཅི་སྙམ་ དུ་ སེམས །"
msgstr ""

# དེའི་ཚེ་དེའི་<«གཡུང་»«ལི་»«པེ་»«སྣར་»«ཅོ་»«ཞོལ»–ཚེ་དེའི།>དུས་ན་བྲམ་ཟེའི་བུར་གྱུར་པ་གང་ཡིན་པ་དེ་ནི་རྫོགས་བྱེད་འདི་ཉིད་ཡིན་ཏེ།
msgctxt "85e6aa478e7e4ddcb7aae891d9b02a45"
msgid ""
"དེ␣འི་ ཚེ་ དེ␣འི་ དུས་ ན་ བྲམ་ཟེ␣འི་ བུར་ གྱུར་པ་ གང་ཡིན་པ་ དེ་ ནི་ "
"རྫོགས་བྱེད་ འདི་ཉིད་ ཡིན་ ཏེ །"
msgstr ""

msgctxt "a4f50b22aaf0435c8489fa2ada4444c9"
msgid ""
"དེ␣ས་ དེ␣ར་ སེམས་ཅན་ སྟོང་ཕྲག་ དུ་མ་ ཁ་ བཏག་པ␣འི་ ལ␣ས་ དེ␣འི་ རྣམ་པ␣ར་ "
"སྨིན་པ␣ས་ ནི་ གང་དང་གང་ དུ་ སྐྱེས་པ་ དེ་ དང་ དེ␣ར་ ནད་ མང་བ་ དང་ ཚེ་ "
"ཐུང་བ␣ར་ གྱུར་ ཏོ ། །"
msgstr ""

# དེས་དེར་རྣམ་པ་ཐམས་ཅད་ཡོངས་སུ་རྫོགས་པའི་གཙུག་ལག་ཁང་བྱས་<«གཡུང་»བྱུང་།>ནས།
msgctxt "ea517d566ee64b878c4489740c0ac3a9"
msgid "དེ␣ས་ དེ␣ར་ རྣམ་པ་ ཐམས་ཅད་ ཡོངས་སུ་ རྫོགས་པ␣འི་ གཙུག་ལག་ཁང་ བྱས་ ནས །"
msgstr ""

msgctxt "be458b869a954db39bfcb09eb3badc28"
msgid "སངས་རྒྱས་ ལ་སོགས་པ་ དགེ་སློང་ གི་ དགེ་འདུན་ ལ་ ཕུལ་ ཏེ་"
msgstr ""

msgctxt "9259e08798a844a0a4f4e270de8019aa"
msgid "ཡོ་བྱད་ ཐམས་ཅད་ ཀྱིས་ བསྙེན་བཀུར་ བྱས་ ནས་"
msgstr ""

# འཆི་ཀར་<«ཞོལ་»གར།>སྨོན་ལམ་བཏབ་པ།
msgctxt "2d45d756614943bf99489c6b3da206d4"
msgid "འཆི་ཀ␣ར་ སྨོན་ལམ་ བཏབ་པ །"
msgstr ""

msgctxt "3c7b6f9f632548d58bce63c67ba6dbde"
msgid ""
"བདག་ གང་དང་གང་ དུ་ སྐྱེ་བ་ དེ་ དང་ དེ␣ར་ ཕྱུག་ ཅིང་ ནོར་ མང་ ལ་ ལོངས་སྤྱོད་ "
"ཆེ་བ␣འི་ རིགས་ སུ་ སྐྱེ་བ␣ར་ གྱུར་ཅིག  །"
msgstr ""

msgctxt "b793ebad0de348cfaaa813dd3e9e3167"
msgid "སྟོན་པ་ འདི་ ལྟ་བུ་ མཉེས་པ␣ར་ བྱེད་པ␣ར་ གྱུར་ཅིག  །"
msgstr ""

msgctxt "73af15942d1146d299804d0d1abb734e"
msgid "མི་ མཉེས་པ␣ར་ བྱེད་པ␣ར་ མ་ གྱུར་ཅིག  །"
msgstr ""

# ཡོན་ཏན་འདི་ལྟ་བུ་<«ཁུ་»བུའི།>དག་ཀྱང་ཐོབ་པར་གྱུར་ཅིག ། ཅེས་
msgctxt "6d03480d90234aa0bf31984de469cb1b"
msgid "ཡོན་ཏན་ འདི་ ལྟ་བུ་ དག་ ཀྱང་ ཐོབ་པ␣ར་ གྱུར་ཅིག  །  ཅེས་"
msgstr ""

msgctxt "24f69b04806942f9a9086942cc4d470e"
msgid ""
"བྱས་པ␣འི་ ལ␣ས་ དེ␣འི་ རྣམ་པ␣ར་ སྨིན་པ␣ས་ ནི །  གང་དང་གང་ དུ་ སྐྱེས་པ་ དེ་ "
"དང་ དེ␣ར་ ཕྱུག་ ཅིང་ ནོར་ མང་ ལ་ ལོངས་སྤྱོད་ ཆེ་བ␣འི་ རིགས་ སུ་ སྐྱེས་ ལ །"
msgstr ""

# གཟུགས་བཟང་ཞིང་བལྟ་<«མི་»«སྣར་»«ཅོ་»ལྟ།>ན་སྡུག་ལ་མཛེས་པར་གྱུར་ཏེ<«གཡུང་»«པེ་»སྟེ།>།
msgctxt "43fe1b56580c436c8a3a3a031e401316"
msgid "གཟུགས་བཟང་ ཞིང་ བལྟ་ ན་ སྡུག་ ལ་ མཛེས་པ␣ར་ གྱུར་ ཏེ །"
msgstr ""

msgctxt "b4cd0840cdf94c9ca611209487c6d0b8"
msgid "དགེ་སློང་ དག་"
msgstr ""

msgctxt "bbd457930332426ab6891e59acafbde6"
msgid ""
"ང་ ཡང་དག་པ␣ར་ རྫོགས་པ␣འི་ སངས་རྒྱས་ འཁོར་བ་ འཇིག་ དང་ ཤིན་ཏུ་ མཉམ་པ་ དང་ "
"ཤུགས་ མཉམ་པ་ དང་ མཛད་པ་ མཉམ་པ་ དང་ ཐབས་ མཉམ་པ་ བརྙེས་པ␣ར་ གྱུར་ ནས །"
msgstr ""

msgctxt "dc8d7236cd7341d3bd5f53f05024101b"
msgid "འདི␣ས་ ང་ མཉེས་པ␣ར་ བྱས་ ཏེ །"
msgstr ""

msgctxt "4fa339afb89542d7a1486e1c5797fb60"
msgid "མི་ མཉེས་པ␣ར་ མ་ བྱས་ ལ་"
msgstr ""

# ང་<«པེ་»དེ།>ཉིད་ཀྱི་བསྟན་<«གཡུང་»སྟན།>པ་ལ་རབ་ཏུ་བྱུང་ནས།
msgctxt "2389288880584b11a6cbb67ecaaad5f2"
msgid "ང་ཉིད་ ཀྱི་ བསྟན་པ་ ལ་ རབ་ཏུ་ བྱུང་ ནས །"
msgstr ""

msgctxt "eec6ecb9a791492abc351904f5e19593"
msgid "ཉོན་མོངས་པ་ ཐམས་ཅད་ སྤངས་ ཏེ་"
msgstr ""

# དགྲ་བཅོམ་པ་ཉིད་མངོན་སུམ་དུ་བྱས་སོ<10,ལ>། །
msgctxt "db8d3ecdfa8b41e09c27812b10cfc052"
msgid "དགྲ་བཅོམ་པ་ ཉིད་ མངོན་སུམ་ དུ་ བྱས་ སོ ། །"
msgstr ""

msgctxt "e46677d4e73540f38cd62b6ad091de1d"
msgid "ལ␣ས་ ཅི་ བགྱིས་ ན་"
msgstr ""

# ལས་དེའི་རྣམ་པར་སྨིན་པས་ཡོངས་སུ་མྱ་ངན་ལས་འདས་པ་ན་འཁོར་བཞི་པོ་རྣམས་མཆོད་པ་བགྱིད་པར་འགྱུར་<«གཡུང་»«ལི་»«པེ་»«སྣར་»«ཅོ་»གྱུར།>ལགས།
msgctxt "8ccc6bcfa8d94bef927a44c46291b56c"
msgid ""
"ལ␣ས་ དེ␣འི་ རྣམ་པ␣ར་ སྨིན་པ␣ས་ ཡོངས་སུ་ མྱ་ངན་ ལ␣ས་ འདས་པ་ ན་ འཁོར་ བཞི་པོ་ "
"རྣམས་ མཆོད་པ་ བགྱིད་པ␣ར་ འགྱུར་ ལགས །"
msgstr ""

msgctxt "af1fe543d7c74406811aee8fe84819b4"
msgid "བཅོམ་ལྡན་འདས་ ཀྱིས་ བཀའ་ སྩལ་པ །"
msgstr ""

msgctxt "3aaf2506aa1549b98a93b2410dd6fdf7"
msgid "སྨོན་ལམ་ བཏབ་པ␣འི་ དབང་ གིས་ གྱུར་ ཏོ ། །"
msgstr ""

msgctxt "c156192f8b6247b69c5d9acd4a74a659"
msgid "འདི␣ས་ གང་ དུ་ སྨོན་ལམ་ བཏབ་པ་ ལགས །"
msgstr ""

msgctxt "20409a5e6d8b4deb9c83453fc7a0c4e2"
msgid "བཅོམ་ལྡན་འདས་ ཀྱིས་ བཀའ་ སྩལ་པ །"
msgstr ""

msgctxt "5f95cc6c838b4f88bd8337eed5679416"
msgid "དགེ་སློང་ དག་"
msgstr ""

# སྔོན་བྱུང་བ་འདས་པའི་དུས་ན།
# བསྐལ་པ་བཟང་པོ་འདི་ཉིད་ལ་སྐྱེ་དགུའི་<«ཞོལ་»རྒུའི།>ཚེ་ལོ་ཉི་ཁྲི་ཐུབ་པ་ན།
# ཡང་དག་པར་རྫོགས་པའི་སངས་རྒྱས་རིག་<«གཡུང་»«པེ་»རིགས།>པ་དང་ཞབས་སུ་ལྡན་པ།
# བདེ་བར་གཤེགས་པ་འཇིག་རྟེན་མཁྱེན་པ།
# སྐྱེས་བུ་འདུལ་བ་<«ཞོལ་»བྱའི>ཁ་ལོ་སྒྱུར་བ་བླ་ན་མེད་པ་ལྷ་དང་མི་རྣམས་ཀྱི་སྟོན་པ།
# སངས་རྒྱས་བཅོམ་ལྡན་འདས་འོད་སྲུང་ཞེས་བྱ་བ་འཇིག་རྟེན་དུ་བྱུང་སྟེ།
msgctxt "23ba9847d36a41d9b52ebeee112adb12"
msgid ""
"སྔོན་བྱུང་བ་ འདས་པ␣འི་ དུས་ ན །  བསྐལ་པ་ བཟང་པོ་ འདི་ཉིད་ ལ་ སྐྱེ་དགུ␣འི་ "
"ཚེ་ལོ་ ཉི་ཁྲི་ ཐུབ་པ་ ན །  ཡང་དག་པ␣ར་ རྫོགས་པ␣འི་ སངས་རྒྱས་ རིག་པ་ དང་ ཞབས་ "
"སུ་ ལྡན་པ །  བདེ་བ␣ར་ གཤེགས་པ་ འཇིག་རྟེན་ མཁྱེན་པ །  སྐྱེས་བུ་ འདུལ་བ་ ཁ་ལོ་"
" སྒྱུར་བ་ བླ་ ན་ མེད་པ་ ལྷ་ དང་ མི་ རྣམས་ ཀྱི་ སྟོན་པ །  སངས་རྒྱས་ "
"བཅོམ་ལྡན་འདས་ འོད་སྲུང་ ཞེས་ བྱ་བ་ འཇིག་རྟེན་ དུ་ བྱུང་ སྟེ །"
msgstr ""

msgctxt "e21c57bdd95e4552b8983e7d1101ceff"
msgid "དེ␣འི་ ཚེ་ གྲོང་ཁྱེར་ བཱ་རཱ་ཎ་སཱི་ ན་ ཁྱིམ་བདག་ ཅིག་ གནས་པ་ ལ␣ས་"
msgstr ""

msgctxt "7c6a0bd9d6984ea5ad9b9c13305d7897"
msgid "ཕྱི་ ཞིག་ ན །  དེ␣འི་ ཆུང་མ་ ལ་ བུ་ ཆགས་ ནས །"
msgstr ""

msgctxt "5312a9f1ff4b42369a4151b2d7df5d14"
msgid "དེ་ ཟླ་བ་ དགུ␣འམ་ བཅུ་ ལོན་པ་ དང་"
msgstr ""

# བུ་<«གཡུང་»«པེ་»–བུ།>གཟུགས་བཟང་ཞིང་བལྟ་<«ལི་»«སྣར་»«ཅོ་»ལྟ།>ན་སྡུག་<«གཡུང་»བདུག>ལ་མཛེས་པ་ཞིག་བཙས་ནས།
msgctxt "69181ad27e914798b26c22ef92bba8f7"
msgid "བུ་ གཟུགས་བཟང་ ཞིང་ བལྟ་ ན་ སྡུག་ ལ་ མཛེས་པ་ ཞིག་ བཙས་ ནས །"
msgstr ""

msgctxt "1d3e326ad04e4e759d0b0f1882c3e978"
msgid ""
"གང་ གི་ ཚེ་ དེ་ ཆེ␣ར་ སྐྱེས་པ་ དེ␣འི་ ཚེ །  དེ␣ས་ ཡང་དག་པ␣ར་ རྫོགས་པ␣འི་ "
"སངས་རྒྱས་ འོད་སྲུང་ གི་ བསྟན་པ་ ལ་ དད་པ་ རྙེད་ ནས་"
msgstr ""

msgctxt "c3179c6873e34734b5c06191a3e31145"
msgid "དེ␣ས་ ཕ་མ་ ལ་ གསོལ་ ཏེ །"
msgstr ""

# ཡང་དག་པར་རྫོགས་པའི་སངས་རྒྱས་འོད་སྲུང་<«ལི་»«ཅོ་»བསྲུང་།>གི་གསུང་རབ་<«གཡུང་»བར།>ལ་རབ་ཏུ་བྱུང་ནས།
msgctxt "ae9d323e64ff41648c504750639ab6c0"
msgid ""
"ཡང་དག་པ␣ར་ རྫོགས་པ␣འི་ སངས་རྒྱས་ འོད་སྲུང་ གི་ གསུང་རབ་ ལ་ རབ་ཏུ་ བྱུང་ ནས །"
msgstr ""

# བརྩོན་པ་དང་བསྒྲུབ་པ་<«གཡུང་»བསྒྱུ་མ།
# «ཞོལ་»སྒྲུབ་པ།>དང་འབད་པས་ཉོན་མོངས་པ་ཐམས་ཅད་སྤངས་ནས་
msgctxt "9d14ae8893e845d0be4491c0f169694a"
msgid "བརྩོན་པ་ དང་ བསྒྲུབ་པ་ དང་ འབད་པ␣ས་ ཉོན་མོངས་པ་ ཐམས་ཅད་ སྤངས་ ནས་"
msgstr ""

# དགྲ་བཅོམ་པ་ཉིད་མངོན་སུམ་དུ་བྱས་ཏེ<«གཡུང་»«པེ་»སྟེ།>།
msgctxt "296383f220044b0a87488f7fabae17d3"
msgid "དགྲ་བཅོམ་པ་ ཉིད་ མངོན་སུམ་ དུ་ བྱས་ ཏེ །"
msgstr ""

msgctxt "7103a48d983444b28edf2bf7604ea285"
msgid "དེ␣ས་ དགྲ་བཅོམ་པ་ ཉིད་ ཐོབ་ ནས་"
msgstr ""

msgctxt "f86bfdc5ba924197b600f0a00fd59e44"
msgid "བསམས་པ །"
msgstr ""

# བདག་གིས་འཁོར་བའི་ལུས་འདིས་བྱ་བ་བྱས་ཟིན་གྱིས<«ཞོལ་»གྱི།>།
msgctxt "24b35fd589f442168febf83dc9495895"
msgid "བདག་ གིས་ འཁོར་བ␣འི་ ལུས་ འདི␣ས་ བྱ་བ་ བྱས་ ཟིན་ གྱིས །"
msgstr ""

msgctxt "b26ee5d3fae14a479c90ab95ba1b1e8d"
msgid "མ་ལ་"
msgstr ""

# ད་<«པེ་»དེ།>ནི་བདག་ཞི་བའི་དབྱིངས་སུ་འཇུག་པར་བྱའོ་སྙམ་དུ་
msgctxt "d0b1f5862ce84a54b1c47e1461789a46"
msgid "ད་ནི་ བདག་ ཞི་བ␣འི་ དབྱིངས་ སུ་ འཇུག་པ␣ར་ བྱ␣འོ་ སྙམ་ དུ་"
msgstr ""

msgctxt "148f854ffb8948ab8bc3d5d71262cd9e"
msgid "བསམས་ ནས །"
msgstr ""

msgctxt "a5a5c9fa03c54b95b3abbe03e3daac36"
msgid ""
"འབར་བ་ དང་ ལམ་ མེ␣ར་ འདུག་པ་ དང་ ཆ␣ར་ འབབ་པ་ དང་ གློག་ འགྱུ་བ␣འི་ ཆོ་འཕྲུལ་ "
"རྣམས་ བསྟན་ ཏེ །"
msgstr ""

# ཕུང་པོ་ལྷག་མ་མེད་པའི་མྱ་ངན་ལས་འདས་པའི་དབྱིངས་<«གཡུང་»བྱིངས།>སུ་ཡོངས་སུ་<«གཡུང་»«ལི་»«པེ་»«སྣར་»«ཅོ་»«ཞོལ་»–ཡོངསསུ།>མྱ་ངན་ལས་འདས་ནས།
msgctxt "6cd3c6e7dd0245249a9c0ce9a0b51c3b"
msgid ""
"ཕུང་པོ་ ལྷག་མ་ མེད་པ␣འི་ མྱ་ངན་ ལ␣ས་ འདས་པ␣འི་ དབྱིངས་ སུ་ ཡོངས་སུ་ མྱ་ངན་ "
"ལ␣ས་ འདས་ ནས །"
msgstr ""

# མཁན་པོས་དེའི་ཕ་མ་ལ་<«གཡུང་»«ལི་»«པེ་»«སྣར་»«ཅོ་»–ལ།>སྦྲན་ཏེ།
msgctxt "117fd1fa124a45e6b9b2a79ae9a24c23"
msgid "མཁན་པོ␣ས་ དེ␣འི་ ཕ་མ་ ལ་ སྦྲན་ ཏེ །"
msgstr ""

msgctxt "56cb9fd94eb0415baacf3579181e95c2"
msgid "དེ␣འི་ ཕ་མ་ གཉིས་ དང་ མཁན་པོ␣ས་ སྐུ་གདུང་ གི་ མཆོད་པ་ ཆེན་པོ་ བྱས་ ནས་"
msgstr ""

msgctxt "ce41ebf991224a8cb1e89e33fb053007"
msgid "མཁན་པོ␣ས་ སྨོན་ལམ་ བཏབ་པ །"
msgstr ""

# ཀྱེ་མ་དགེ་བའི་རྩ་བ་འདིས་བདག་གང་དང་གང་དུ་སྐྱེ་བ་དེ་<«གཡུང་»«པེ་»–དེ།>དང་དེར་ཕྱུག་ཅིང་ནོར་མང་ལ་ལོངས་སྤྱོད་ཆེ་བའི་རིགས་སུ་སྐྱེ་བར་གྱུར་ཅིག
# །
msgctxt "31498c5287df4808a206321c86ced4c8"
msgid ""
"ཀྱེ་མ་ དགེ་བ␣འི་ རྩ་བ་ འདི␣ས་ བདག་ གང་དང་གང་ དུ་ སྐྱེ་བ་ དེ་ དང་ དེ␣ར་ ཕྱུག་"
" ཅིང་ ནོར་ མང་ ལ་ ལོངས་སྤྱོད་ ཆེ་བ␣འི་ རིགས་ སུ་ སྐྱེ་བ␣ར་ གྱུར་ཅིག  །"
msgstr ""

msgctxt "135aa70ab7e343009811fa3f774c861e"
msgid ""
"ཡང་དག་པ␣ར་ རྫོགས་པ␣འི་ སངས་རྒྱས་ འོད་སྲུང་ གིས་ བྲམ་ཟེ␣འི་ ཁྱེའུ་ བླ་མ་ "
"ལུང་བསྟན་པ་ གང་ཡིན་པ␣འི་ སངས་རྒྱས་ བཅོམ་ལྡན་འདས་ དེ་ མཉེས་པ␣ར་ བྱེད་པ␣ར་ "
"གྱུར་ཅིག  །"
msgstr ""

msgctxt "d4abeae211ec473898763505d860712f"
msgid "མི་ མཉེས་པ␣ར་ བྱེད་པ␣ར་ མ་ གྱུར་ཅིག  །"
msgstr ""

msgctxt "8cdec84c9e404584bff82665322a24ca"
msgid "ཡོན་ཏན་ འདི་ ལྟ་བུ་ དག་ ཀྱང་ ཐོབ་པ␣ར་ གྱུར་ཅིག  །"
msgstr ""

msgctxt "e95c6c77b679473a953e8d3e50cfdd17"
msgid "བདག་ ཡོངས་སུ་ མྱ་ངན་ ལ␣ས་ འདས་པ་ ན་ ཡང་"
msgstr ""

# བཅོམ་ལྡན་འདས་འཁོར་བཞི་པོ་དང་བཅས་པ་རྣམས་ཀྱིས་<«གཡུང་»«པེ་»ཀྱི།>མཆོད་པའི་ལས་མཛད་པར་ཤོག་ཅིག་ཅེས་
msgctxt "49fed70af41049a3a9958486c729b66a"
msgid ""
"བཅོམ་ལྡན་འདས་ འཁོར་ བཞི་པོ་ དང་ བཅས་པ་ རྣམས་ ཀྱིས་ མཆོད་པ␣འི་ ལ␣ས་ མཛད་པ␣ར་ "
"ཤོག་ ཅིག་ ཅེས་"
msgstr ""

msgctxt "13f2b9ee5b5f41aa8dcb03b60934fa3c"
msgid "བྱས་ སོ ། །"
msgstr ""

msgctxt "b6f5196b772a4918ac9beddf9c4692d3"
msgid "དགེ་སློང་ དག་"
msgstr ""

# འདི་ཇི་<«ཞོལ་»ཅི།>སྙམ་དུ་སེམས།
msgctxt "a6a09f0a05a34a32acaeeb98dd93638d"
msgid "འདི་ ཇི་སྙམ་ དུ་ སེམས །"
msgstr ""

msgctxt "5e6113647c194f1f8a4fa6457b347724"
msgid ""
"དེ␣འི་ ཚེ་ དགེ་སློང་ དུ་ གྱུར་པ་ གང་ཡིན་པ་ དེ་ ནི་ རྫོགས་བྱེད་ འདི་ཉིད་ ཡིན་"
" ཏེ །"
msgstr ""

msgctxt "22a8db3136684f89a3b5128704879349"
msgid "དེ␣ས་ དགྲ་བཅོམ་པ་ དེ་ ལ་ མཆོད་པ་ བྱས་ ནས་"
msgstr ""

msgctxt "a5a7fe1ef6354db88d2462449d92173e"
msgid ""
"སྨོན་ལམ་ བཏབ་པ␣འི་ ལ␣ས་ དེ␣འི་ རྣམ་པ␣ར་ སྨིན་པ␣ས་ གང་དང་གང་ དུ་ སྐྱེས་པ་ དེ་"
" དང་ དེ␣ར་ ཕྱུག་ ཅིང་ ནོར་ མང་ ལ་ ལོངས་སྤྱོད་ ཆེ་བ␣འི་ རིགས་ སུ་ སྐྱེས་ ཏེ །"
msgstr ""

msgctxt "16359f7beb1844998012c14b34bdb69c"
msgid "དགེ་སློང་ དག་"
msgstr ""

msgctxt "c5d792ff2f9d4267a7c27a031c4e570d"
msgid ""
"ང་ ཡང་དག་པ␣ར་ རྫོགས་པ␣འི་ སངས་རྒྱས་ འོད་སྲུང་ དང་ ཤིན་ཏུ་ མཉམ་པ་ དང་ ཤུགས་ "
"མཉམ་པ་ དང་ མཛད་པ་ མཉམ་པ་ དང་ ཐབས་ མཉམ་པ་ བརྙེས་པ␣ར་ གྱུར་ ནས །"
msgstr ""

# འདིས་ང་མཉེས་པར་བྱས་ཏེ་<«གཡུང་»པ་བྱེད་དེ།
# «མི་»«པེ་»«སྣར་»«ཅོ་»«ཞོལ་»པར་བྱེད་དེ།>
msgctxt "adbfac8a2f6b4cdfb4d131a201508c15"
msgid "འདི␣ས་ ང་ མཉེས་པ␣ར་ བྱས་ ཏེ་"
msgstr ""

msgctxt "654590302bc848a6a04c19633e50f814"
msgid "མི་ མཉེས་པ␣ར་ མ་ བྱས་ ལ །"
msgstr ""

msgctxt "45be958fb3af4e8f8158e55d82535989"
msgid "ང་ཉིད་ ཀྱི་ བསྟན་པ་ ལ་ རབ་ཏུ་ བྱུང་ ནས །"
msgstr ""

# ཉོན་མོངས་པ་ཐམས་ཅད་སྤངས་ཏེ<«གཡུང་»«པེ་»སྟེ།>།
msgctxt "5cc373773f4f4959b113ba709ce58a9f"
msgid "ཉོན་མོངས་པ་ ཐམས་ཅད་ སྤངས་ ཏེ །"
msgstr ""

# དགྲ་བཅོམ་པ་ཉིད་མངོན་སུམ་དུ་བྱས་ནས།<«གཡུང་»«པེ་»+ད།>
msgctxt "81eb8bd97b4d498b80a43f57c618042f"
msgid "དགྲ་བཅོམ་པ་ ཉིད་ མངོན་སུམ་ དུ་ བྱས་ ནས །"
msgstr ""

msgctxt "149e798ed0624258a9aa85d453d914c4"
msgid "ཡོངས་སུ་ མྱ་ངན་ ལ␣ས་ འདས་པ་ ན་ ཡང་"
msgstr ""

msgctxt "863456ca0c6f471c8bc09c9bb11d6ccd"
msgid "འཁོར་ བཞི་པོ་ རྣམས་ ཀྱིས་ མཆོད་པ␣འི་ ལ␣ས་ བྱས་ སོ །། །།"
msgstr ""
