msgctxt "d3e03522841347a68d752553529d50ac"
msgid "བཅོམ་ལྡན་འདས་ འོ་བརྒྱལ་ མི་ འཚལ་ ལོ ། །"
msgstr ""

msgctxt "335aeb832f484f48819e5d626f96cb9a"
msgid ""
"དེ་ནས་ བཅོམ་ལྡན་འདས་ ཀྱིས །  ཁྱིམ་བདག་ མགོན་མེད་ ཟས་ སྦྱིན་ ལ་ ཅང་ མི་ "
"གསུང་བ␣ས་ གནང་ ངོ་ ། །"
msgstr ""

# དེ་ནས་ཁྱིམ་བདག་མགོན་མེད་ཟས་སྦྱིན་དང་།
# དགེ་བསྙེན་གཞན་དག་གིས་དགེ་སློང་དེའི་ཁྱོགས་<«གཡུང་»«པེ་»དེའི་ཕྱོགས།
# «ལི་»«སྣར་»«ཅོ་»དེ་ཡི་ཁྱོགས།>ཁྱེར་ནས།
msgctxt "cd6f9d6840234a52bc6da0df242a27d2"
msgid ""
"དེ་ནས་ ཁྱིམ་བདག་ མགོན་མེད་ ཟས་ སྦྱིན་ དང་ །  དགེ་བསྙེན་ གཞན་དག་ གིས་ "
"དགེ་སློང་ དེ␣འི་ ཁྱོགས་ ཁྱེར་ ནས །"
msgstr ""

msgctxt "a7d0b2adcf3a4f75a7d53b26ec27c99c"
msgid "དུར་ཁྲོད་ དུ་ ཆ␣ས་ ཏེ །"
msgstr ""

# དེའི་<«ལི་»«སྣར་»«ཅོ་»དེ་ཡི།>ཕྱི་བཞིན་དུ་བཅོམ་ལྡན་འདས་དང་དགེ་སློང་གཞན་དག་དང་མཆོད་པ་དང་བཀུར་སྟི་བྱེད་པའི་དགེ་བསྙེན་དང་དགེ་བསྙེན་མ་རྣམས་ཀྱང་ཕྱི་བཞིན་དུ་འབྲང་སྟེ།
msgctxt "20b3f575f1f84835b853d0a5d635140a"
msgid ""
"དེ␣འི་ ཕྱི་བཞིན་ དུ་ བཅོམ་ལྡན་འདས་ དང་ དགེ་སློང་ གཞན་དག་ དང་ མཆོད་པ་ དང་ "
"བཀུར་སྟི་ བྱེད་པ␣འི་ དགེ་བསྙེན་ དང་ དགེ་བསྙེན་མ་ རྣམས་ ཀྱང་ ཕྱི་བཞིན་ དུ་ "
"འབྲང་ སྟེ །"
msgstr ""

# དེ་དག་གིས་དུར་<«གཡུང་»«པེ་»དུ།>ཁྲོད་དུ་<«པེ་»དུར།>ཁྱེར་ནས།
msgctxt "59c7b4ab1cdf40fb9bd32a165cff65fa"
msgid "དེ་དག་ གིས་ དུར་ཁྲོད་ དུ་ ཁྱེར་ ནས །"
msgstr ""

# སྤོས་ཀྱི་ཤིང་སྣ་ཚོགས་བཙེར་<«གཡུང་»«པེ་»«ཅོ་»བཅོར། «མི་»བཙེར།>ཏེ་
msgctxt "9f023d8330be4c54a8dbf1657c982e25"
msgid "སྤོས་ ཀྱི་ ཤིང་ སྣ་ཚོགས་ བཙེར་ ཏེ་"
msgstr ""

# མེ་<«གཡུང་»«ལི་»«པེ་»«སྣར་»«ཅོ་»«ཞོལ་»–མ>སྦར་ནས།
msgctxt "fa8d81b21db14386895c4373349dca0f"
msgid "མེ་ སྦ␣ར་ ནས །"
