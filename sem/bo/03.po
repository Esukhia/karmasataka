msgctxt "8128f53d3c8a459383641db963fec0e2"
msgid "ཐོས་ ནས་"
msgstr ""

msgctxt "5d3656a88c184a73b7703342d548901c"
msgid "དེ་དག་ ཀྱང་ འཁོར་ དང་ བཅས་པ␣ར་ དེ␣ར་ དོང་ སྟེ །"
