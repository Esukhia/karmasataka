msgctxt "28fdd4dcfd614fc6b7b735a938cbf2b3"
msgid "དེ་དག་ གིས་ བཅོམ་ལྡན་འདས་ ཀྱི་ ཞབས་ ལ་ ཕྱག་ འཚལ་ ནས་"
msgstr ""

msgctxt "33e61ed903eb4b47a1565f5f4b8f461c"
msgid "གསོལ་པ །"
msgstr ""

# དགེ་སློང་འདི་ཡི་<«ཞོལ་»འདིའི།>སྐུ་གདུང་གི་མཆོད་པ་བདག་ཅག་བགྱིད་ཀྱིས<«ཞོལ་»ཀྱི།>།
msgctxt "768599ae25f240f2af245b6bbfa03ea3"
msgid "དགེ་སློང་ འདི་ ཡི་ སྐུ་གདུང་ གི་ མཆོད་པ་ བདག་ཅག་ བགྱིད་ ཀྱིས །"
