msgctxt "89e3fb033c7c4a07b4a7f983912d63df"
msgid "བཅོམ་ལྡན་འདས་ ཀྱི་ ཕྱི་བཞིན་ འབྲང་ སྟེ །"
msgstr ""

msgctxt "6c000b5fc2af4fa9b34db9bf71221b90"
msgid "གལ་ཏེ་ བཅོམ་ལྡན་འདས་ ཀྱིས་ འདས་པ␣འི་ ལ␣ས་ ལུང་སྟོན་པ␣ར་ བཞེད་ ན་ ནི །"
msgstr ""

msgctxt "4826b0e17ff74e80bbe432577e0574a9"
