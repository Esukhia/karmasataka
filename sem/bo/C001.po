msgctxt "c2144348a6894250a2caebd871c5cda4"
msgid "བཅོམ་ལྡན་འདས་ ཀྱིས་ བཀའ་ ཅི་ སྩལ །"
msgstr ""

msgctxt "fd0c23e489e84734a4037b1b37e7d5ec"
msgid "བཅོམ་ལྡན་འདས་ ཀྱིས་ བཀའ་ སྩལ་པ །"
msgstr ""

# སྡིག་པའི་ལས་འདི་ཐོང་<«ཅོ་»ཐོངས།>ཤིག །
msgctxt "fe0ea35e2bab489abb3230fa0f0e87e0"
msgid "སྡིག་པ␣འི་ ལ␣ས་ འདི་ ཐོང་ ཤིག  །"
msgstr ""

msgctxt "97c2eb0202cb44f08a348cd2361e64b2"
msgid "གནོད་སྦྱིན་ གྱིས་ གསོལ་པ །"
msgstr ""

# བཅོམ་ལྡན་འདས་ཀྱིས་<«གཡུང་»«པེ་»-ཀྱིས།>བདག་ལ་བཀའ་ཅི་སྩལ་པ་བཞིན་དུ་འཚལ་ལོ། །
msgctxt "c46cb4702cf443809377c124ffe436fa"
msgid "བཅོམ་ལྡན་འདས་ ཀྱིས་ བདག་ ལ་ བཀའ་ ཅི་ སྩལ་པ་ བཞིན་ དུ་ འཚལ་ ལོ ། །"
msgstr ""

msgctxt "e9db8e2543d8478899d626295bde44d2"
msgid "དེ་སྐད་ ཅེས་ གསོལ་ :␣ཏེ །"
msgstr ""

# དེས་<«ལི་»«ཅོ་»ཏོ། །དེ།>སྐྱབས་སུ་འགྲོ་བ་དང་། བསླབ་པའི་གཞི་རྣམས་བླངས་ནས་
msgctxt "a2a58c5ef3cc4f00ada50754fc1fd335"
msgid "དེ␣ས་ སྐྱབས་ སུ་ འགྲོ་བ་ དང་ །  བསླབ་པ␣འི་ གཞི་ རྣམས་ བླངས་ ནས་"
msgstr ""

msgctxt "757cd63fa194432bba6ca279e2fd1849"
msgid "བཅོམ་ལྡན་འདས་ ལ་ གསོལ་པ །"
msgstr ""

# བཙུན་པ་བདག་གིས་སྡིག་པའི་ལས་བཏང་:ལགས་ཏེ<«ལི་»«ཅོ་»སྟེ།>།
msgctxt "435f143de76d46758dd1605231fdd12d"
msgid "བཙུན་པ་ བདག་ གིས་ སྡིག་པ␣འི་ ལ␣ས་ བཏང་ ལགས་ ཏེ །"
msgstr ""

# བཙུན་པ་:དེང་སླན་ཅད་<«གཡུང་»«པེ་»དེ་སླད་ཅད།
# «སྣར་»«ཞོལ་»དེང་སླན་ཆད།>མཉན་དུ་ཡོད་པ་ན་<«གཡུང་»«པེ་»+གནས་པ་ན།>གནས་པའི་སྐྱེ་བོ་མང་པོ་རྣམས་ཀྱི་སྲུང་<«གཡུང་»«པེ་»སྲུངས།
# «ལི་»«ཅོ་»བསྲུང༌།>མ་དང་།
# :སྐྱབས་རྣམས་<«གཡུང་»«པེ་»«སྣར་»«ཞོལ་»སྐྱོང་བ།>ནི་བདག་ལགས་སོ། །
msgctxt "9ce357cf493b4fd081d1dd592483a3be"
msgid ""
"བཙུན་པ་ དེང་ སླན་ ཅད་ མཉན་ དུ་ཡོད་པ་ ན་ གནས་པ␣འི་ སྐྱེ་བོ་ མང་པོ་ རྣམས་ ཀྱི་"
" སྲུང་མ་ དང་ །  སྐྱབས་ རྣམས་ ནི་ བདག་ ལགས་སོ ། །"
msgstr ""

msgctxt "429612dd5dbc4cb88453cf2103082e90"
msgid "དེ་ནས་ དགེ་སློང་ རྣམས་ ཀྱིས་ སངས་རྒྱས་ བཅོམ་ལྡན་འདས་ ལ་ གསོལ་པ །"
msgstr ""

# བཙུན་པ་བཅོམ་ལྡན་འདས་ཀྱིས་གནོད་སྦྱིན་མ་རུངས་པ་དེ་<«གཡུང་»«པེ་»འདི།>བཏུལ་ནས།
msgctxt "4a1262cd281040788b6c41e5b1ebc80e"
msgid "བཙུན་པ་ བཅོམ་ལྡན་འདས་ ཀྱིས་ གནོད་སྦྱིན་ མ་རུངས་པ་ དེ་ བཏུལ་ ནས །"
msgstr ""

# སྐྱེ་བོ་ཕལ་པོ་ཆེ་དེ་<«ལི་»«ཅོ་»-དེ།>དག་འཇིགས་པ་མེད་པའི་གནས་སུ་བཀོད་པ་ལ་གཟིགས།
msgctxt "604be1b9a26146da93b00a13c028f3ce"
msgid ""
"སྐྱེ་བོ་ ཕལ་པོ་ ཆེ་ དེ་དག་ འཇིགས་པ་ མེད་པ␣འི་ གནས་ སུ་ བཀོད་པ་ ལ་ གཟིགས །"
msgstr ""

msgctxt "87fe307188a3471882d1301d514eafeb"
msgid "བཅོམ་ལྡན་འདས་ ཀྱིས་ བཀའ་ སྩལ་པ །"
msgstr ""

msgctxt "6b2a4b4538554c55a888a275eabf1063"
msgid "ད་ལྟར་ འབའ་ཞིག་ མ་ ཡིན་ ཏེ །"
msgstr ""

# འདས་པའི་དུས་ན་ཡང་ངས་གནོད་སྦྱིན་མ་རུངས་པ་འདི་བཏུལ་<«གཡུང་»«པེ་»བརྟུལ།>ནས།
msgctxt "83949792612b486ebd8e6b3fcdb846e8"
msgid "འདས་པ␣འི་ དུས་ ན་ ཡང་ ངས་ གནོད་སྦྱིན་ མ་རུངས་པ་ འདི་ བཏུལ་ ནས །"
msgstr ""

# སྐྱེ་བོ་ཕལ་པོ་ཆེ་ཇི་ལྟར་འཇིགས་པ་མེད་པའི་གནས་སུ་བཀོད་པ་དེ་ཉོན་ཅིག<«ལི་»«ཅོ་»གཅིག>།
msgctxt "8ec53525abf74af08dd9473e30b8612e"
msgid ""
"སྐྱེ་བོ་ ཕལ་པོ་ ཆེ་ ཇི་ལྟར་ འཇིགས་པ་ མེད་པ␣འི་ གནས་ སུ་ བཀོད་པ་ དེ་ ཉོན་ ཅིག"
" །"
msgstr ""

# དགེ་སློང་དག་སྔོན་བྱུང་བ་འདས་པའི་དུས་ན་གྲོང་ཁྱེར་བཱ་རཱ་ཎ་སཱི་ན་རྒྱལ་པོ་ཚངས་པས་བྱིན་རྒྱལ་པོ་བྱེད་དེ<«ལི་»«ཅོ་»དོ།>།
msgctxt "e77b79dad9c94b5ca70eda0d43b52da6"
msgid ""
"དགེ་སློང་ དག་ སྔོན་བྱུང་བ་ འདས་པ␣འི་ དུས་ ན་ གྲོང་ཁྱེར་ བཱ་རཱ་ཎ་སཱི་ ན་ "
"རྒྱལ་པོ་ ཚངས་པ␣ས་ བྱིན་ རྒྱལ་པོ་ བྱེད་ དེ །"
msgstr ""

# དེའི་ཚེ་ཡུལ་ཀ་ཤིར་<«ལི་»«ཅོ་»ཤི་ཀའི།>ཀླུ་གདུག་པ་དུག་<«ཁུ་»+བ།>མི་བཟད་པ་ཞིག་བྱུང་སྟེ<«ལི་»ཏེ།>།
msgctxt "559a788d17d448a39dfbe43f7252fb4c"
msgid "དེ␣འི་ ཚེ་ ཡུལ་ ཀ་ ཤི␣ར་ ཀླུ་ གདུག་པ་ དུག་ མི་བཟད་པ་ ཞིག་ བྱུང་ སྟེ །"
msgstr ""

# དེས་གྲོང་ཡང་སྟོངས་<«སྣར་»«ཞོལ་»སྟོང་།>པར་བྱེད<«གཡུང་»«པེ་»+དེ༑>།
msgctxt "e5e6f515d0b548149d04d91710d14e47"
msgid "དེ␣ས་ གྲོང་ ཡང་ སྟོངས་པ␣ར་ བྱེད །"
msgstr ""

# ལྗོངས་ཀྱང་སྟོངས་<«སྣར་»«ཞོལ་»སྟོང་།>པར་བྱེད་ཅིང་།
msgctxt "912e5c5ce22e4636a87faf1e0f743847"
msgid "ལྗོངས་ ཀྱང་ སྟོངས་པ␣ར་ བྱེད་ ཅིང་ །"
msgstr ""

# བཱ་རཱ་ཎ་སཱིའི་<«ཞོལ་»སིའི།>ཕྱོགས་སུ་ཆས་<«ལི་»«ཅོ་»ཆགས།>པ་ལས་
msgctxt "39cb47db7af7429ab30a1106667bd0c0"
msgid "བཱ་ སཱིའི་ ཕྱོགས་ སུ་ ཆས་པ་ ལ␣ས་"
msgstr ""

msgctxt "9331630f9a124aa2babe438531a757e8"
msgid "རྒྱལ་པོ་ ཚངས་པ␣ས་ བྱིན་ གྱིས་ ཐོས་ ནས །"
msgstr ""

# དེ་ཐོས་མ་ཐག་ཏུ་འཇིགས་པ་ཆེན་<«ལི་»ཆེས།>པོ་སྐྱེས་ཏེ།
msgctxt "e44e94c31624486ca8a871a80e291caf"
msgid "དེ་ ཐོས་ མ་ ཐག་ ཏུ་ འཇིགས་པ་ ཆེན་པོ་ སྐྱེས་ ཏེ །"
msgstr ""

msgctxt "84dfe659e64847a7803519146c3592e5"
msgid "དེ␣ས་ དཔུང་ གི་ ཚོགས་ ཡན་ལག་ བཞི་ གོ་ བསྐོན་ ཏེ་"
msgstr ""

msgctxt "2a73661b6831459ca76886bd89dd8dd2"
msgid "བཏང་ ནས །"
msgstr ""

# ཁྱེད་<«ཅོ་»ཁྱོད།>དེང་ལ་ཀླུ་གདུག་པ་དེ་སོད་ཅིག་<«ལི་»གཅིག>ཅེས་
msgctxt "f529b1239be340d3bfb5f9032c4b7714"
msgid "ཁྱེད་ དེང་ ལ་ ཀླུ་ གདུག་པ་ དེ་ སོད་ ཅིག་ ཅེས་"
msgstr ""

msgctxt "f4c6a7feccda4466ace1be62848220a5"
msgid "བསྒོའོ ། །"
msgstr ""

# དེ་ནས་དེ་དག་གིས་དེའི་<«གཡུང་»«སྣར་»«ཞོལ་»-དེའི།>རྐང་པ་ལ་ཕྱག་འཚལ་ཏེ།
msgctxt "5df731fd2b11468ba7f1c3b4adab3104"
msgid "དེ་ནས་ དེ་དག་ གིས་ དེ␣འི་ རྐང་པ་ ལ་ ཕྱག་ འཚལ་ ཏེ །"
msgstr ""

msgctxt "7062937526f5481aa359df351e8ed6b2"
msgid "གསོལ་པ །"
msgstr ""

# ལྷ་དེ་ནི་དཔུང་གི་ཚོགས་ཀྱིས་དགུམ་<«ལི་»«ཅོ་»གུམ།>པར་མི་ནུས་ཀྱིས<«གཡུང་»«པེ་»«ཞོལ་»ཀྱི།>།
msgctxt "3233161dcd8a44de92171cf28651b742"
msgid "ལྷ་ དེ་ ནི་ དཔུང་ གི་ ཚོགས་ ཀྱིས་ དགུམ་པ␣ར་ མི་ ནུས་ ཀྱིས །"
msgstr ""

# སྔགས་སམ་སྨན་གྱིས་:དེ་དགུམ་<«ལི་»«ཅོ་»འགུམ། «ཁུ་»དཀུམ།>པར་ནུས་ལགས་སོ། །
msgctxt "91c18efcd8424efa848d99f43da10d76"
msgid "སྔགས་ སམ་ སྨན་ གྱིས་ དེ་ དགུམ་པ␣ར་ ནུས་ ལགས་སོ ། །"
msgstr ""

# ཡང་ན་དགེ་བའི་ཚུལ་གྱིས་བཀུར་བའི་དགེ་སྦྱོང་<«ལི་»སློང་།>དང་།
# བྲམ་[119a]ཟེ་རྣམས་ཀྱིས་གདུལ་བར་ནུས་ལགས་སོ་ཞེས་
msgctxt "f3be1046f5034c61bb6302eee845b189"
msgid ""
"ཡང་ན་ དགེ་བ␣འི་ ཚུལ་ གྱིས་ བཀུར་བ␣འི་ དགེ་སྦྱོང་ དང་ །  བྲམ་ཟེ་ རྣམས་ ཀྱིས་ "
"གདུལ་བ␣ར་ ནུས་ ལགས་སོ་ ཞེས་"
msgstr ""

msgctxt "0b522167ac7a40f89bd1a622665130ee"
msgid "གསོལ་ ཏོ ། །"
msgstr ""

# དེ་ནས་དེའི་ཚེ་བཱ་རཱ་ཎ་སཱི་དེ་ཉིད་ན་གདོལ་པའི་ཁྱེའུ་བྱམས་པའི་རང་བཞིན་ཅན།
# སྙིང་རྗེ་ཆེ་བ་སེམས་ཅན་ལ་བྱམས་པ་ཞིག་གནས་པ་དེས།
# སྐྱེ་བོ་མང་པོ་དེ་<«ལི་»«ཅོ་»-དེ།>དག་ཅི་བྱ་གཏོལ་མེད་པར་གྱུར་པ་མཐོང་ནས།
msgctxt "b517b34c1b48484dbbfe4a495d7076bc"
msgid ""
"དེ་ནས་ དེ␣འི་ ཚེ་ བཱ་རཱ་ཎ་སཱི་ དེ་ཉིད་ ན་ གདོལ་པ␣འི་ ཁྱེའུ་ བྱམས་པ␣འི་ "
"རང་བཞིན་ ཅན །  སྙིང་རྗེ་ ཆེ་བ་ སེམས་ཅན་ ལ་ བྱམས་པ་ ཞིག་ གནས་པ་ དེ␣ས །  "
"སྐྱེ་བོ་ མང་པོ་ དེ་དག་ ཅི་ བྱ་ གཏོལ་མེད་པ␣ར་ གྱུར་པ་ མཐོང་ ནས །"
msgstr ""

# མཐོང་མ་ཐག་ཏུ་དེ་<«གཡུང་»«ལི་»«པེ་»«ཅོ་»-དེ།>སྙིང་རྗེ་སྐྱེས་ནས་
msgctxt "72107e18840b401c933c9e825e72e121"
msgid "མཐོང་ མ་ ཐག་ ཏུ་ དེ་ སྙིང་རྗེ་ སྐྱེས་ ནས་"
msgstr ""

# དེས་སྐྱེ་<«གཡུང་»སྐྱེས།>བོ་མང་པོ་རྣམས་ལ་སྨྲས་པ།
msgctxt "db0330aa2f0f4439a01663e4f80af905"
msgid "དེ␣ས་ སྐྱེ་བོ་ མང་པོ་ རྣམས་ ལ་ སྨྲས་པ །"
msgstr ""

# ཁྱོད་<«གཡུང་»«པེ་»«སྣར་»«ཞོལ་»ཁྱེད།>ཅི་བྱ་གཏོལ་མེད་པར་བྱ་མི་དགོས་ཀྱིས<«ཞོལ་»ཀྱི།>།
msgctxt "4c0fc5b1762c4eb5936cd93dcb981fa9"
msgid "ཁྱོད་ ཅི་ བྱ་ གཏོལ་མེད་པ␣ར་ བྱ་ མི་ དགོས་ ཀྱིས །"
msgstr ""

# བདག་གིས་<«གཡུང་»«པེ་»གི།>ཀླུ་གདུག་པ་དེ་གདུལ་བར་བྱའོ་ཞེས་
msgctxt "ee20e0e320a244a892933f2f5e685ecc"
msgid "བདག་ གིས་ ཀླུ་ གདུག་པ་ དེ་ གདུལ་བ␣ར་ བྱ␣འོ་ ཞེས་"
msgstr ""

msgctxt "df3f1804c6e946c0b182cbd307e8aa49"
msgid "བྱས་ སོ ། །"
msgstr ""

msgctxt "9eb3a4139d764818ae9acccbbb770984"
msgid "དེ་ནས་ དེ␣འི་ ཕ་མ་ གཉིས་ ན་རེ །"
msgstr ""

msgctxt "e868eb2bcb9b4dfcaa11279ca9a1b5d2"
msgid "དེ་ ནི་ དབུགས་ གདུག་པ་ ཡིན་ ཏེ་"
msgstr ""

# ཁྱོད་ཀྱིས་དེ་གདུལ་<«སྣར་»«ཞོལ་»འདུལ།>མི་ནུས་སོ། །
msgctxt "13069f02be2f4db2983f75332e36a3e5"
msgid "ཁྱོད་ ཀྱིས་ དེ་ གདུལ་ མི་ ནུས་ སོ ། །"
