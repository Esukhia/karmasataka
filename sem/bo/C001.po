msgctxt "6dbdeaac9b50435884262c11fd6620ab"
msgid "བཅོམ་ལྡན་འདས་ ཀྱི་ ཐད་ ནས་ སོང་ ངོ་ ། །"
msgstr ""

# དེ་ནས་བཅོམ་ལྡན་འདས་ཀྱི་<«ལི་»«ཅོ་»«ཁུ་»ཀྱིས།>རྫུ་འཕྲུལ་གྱིས།
# གནོད་སྦྱིན་དེ་<«ལི་»«ཅོ་»+དག>:རྒྱལ་བུ་<«ཞོལ་»པོ།>རྒྱལ་བྱེད་ཀྱི་ཚལ་གྱི་:འདབ་ཏུ་<«ཞོལ་»འདབས་སུ།>བཞག་ནས།
msgctxt "1f27096785954fb6bddaebdcbe77c7ca"
msgid ""
"དེ་ནས་ བཅོམ་ལྡན་འདས་ ཀྱི་ རྫུ་འཕྲུལ་ གྱིས །  གནོད་སྦྱིན་ དེ་ རྒྱལ་བུ་ "
"རྒྱལ་བྱེད་ ཀྱི་ ཚལ་ གྱི་ འདབ་ ཏུ་ བཞག་ ནས །"
msgstr ""

# བཅོམ་ལྡན་འདས་:ཀྱིས་དེ་བཏུལ་<«གཡུང་»«པེ་»དེ་གདུལ།
# «སྣར་»«ཞོལ་»ཀྱིས་དེ་གདུལ།>བའི་ཕྱིར་མེ་ཆེན་པོ་སྤྲུལ་ཏེ།
msgctxt "7918f97b1c324d3b8357e6a313c6d723"
msgid "བཅོམ་ལྡན་འདས་ ཀྱིས་ དེ་ བཏུལ་བ␣འི་ ཕྱིར་ མེ་ ཆེན་པོ་ སྤྲུལ་ ཏེ །"
msgstr ""

# དེའི་:ཁོ་ར་<«ལི་»འཁོར།>ཁོར་ཡུག་ནས་བདག་ཉིད་མེ་ཆེན་པོས་འཁོར་བར་མཐོང་སྟེ།
msgctxt "650599a45ab049478faaa9afbe1351cb"
msgid "དེ␣འི་ ཁོ་ར་ ཁོར་ཡུག་ ནས་ བདག་ཉིད་ མེ་ ཆེན་པོ␣ས་ འཁོར་བ␣ར་ མཐོང་ སྟེ །"
msgstr ""

# བཅོམ་ལྡན་འདས་ཀྱི་<«གཡུང་»«པེ་»ཀྱིས།>ཞབས་ཀྱི་དྲུང་འབའ་ཞིག་ཞི་བར་མཐོང་ངོ་། །
msgctxt "d0d7be40f81649d0888dbe2c87d3e9cc"
msgid "བཅོམ་ལྡན་འདས་ ཀྱི་ ཞབས་ ཀྱི་ དྲུང་ འབའ་ཞིག་ ཞི་བ␣ར་ མཐོང་ ངོ་ ། །"
msgstr ""

msgctxt "fd6e000f74254ecb9b32c8a3c051356a"
msgid "དེ་ནས་ དེ་ འཇིགས་ ཤིང་ སྐྲག་ ནས་"
msgstr ""

# བཅོམ་ལྡན་འདས་ག་ལ་བ་དེར་སོང་སྟེ་<«ལི་»«ཅོ་»ཏེ།>ཕྱིན་ནས།
msgctxt "bb3157cdc9ed4f038b84f69e99e951a2"
msgid "བཅོམ་ལྡན་འདས་ ག་ལ་བ་ དེ␣ར་ སོང་ སྟེ་ ཕྱིན་ ནས །"
msgstr ""

# བཅོམ་ལྡན་འདས་ཀྱིས་གནོད་སྦྱིན་མ་རུངས་པ་དེ་<«ལི་»«ཅོ་»+དག>ལ་བཀའ་སྩལ་པ།
msgctxt "b3d552d9e9da4379b8fd3ddeacaeb385"
msgid "བཅོམ་ལྡན་འདས་ ཀྱིས་ གནོད་སྦྱིན་ མ་རུངས་པ་ དེ་ ལ་ བཀའ་ སྩལ་པ །"
msgstr ""

# :བཞིན་བཟངས་<«ལི་»«ཅོ་»-བཞིན་བཟངས།«ཞོལ་»བཞིན་བཟང༌།>ཁྱོད་ཀྱིས་སྔོན་<«གཡུང་»«པེ་»+གྱི།>མི་དགེ་བའི་ལས་བྱས་པས།
# རྒྱལ་པོ་གསལ་རྒྱལ་གྱིས་<«ལི་»གྱི།>[118b]བསད་<«གཡུང་»«པེ་»གསད།>པ་ཡིན་ན།
msgctxt "5d84d0dac0a64d31b50418e241a7174b"
msgid ""
"བཞིན་ བཟངས་ ཁྱོད་ ཀྱིས་ སྔོན་ མི་དགེ་བ␣འི་ ལ␣ས་ བྱས་པ␣ས །  རྒྱལ་པོ་ "
"གསལ་རྒྱལ་ གྱིས་ བསད་པ་ ཡིན་ ན །"
msgstr ""

# ད་ཡང་ཁྱོད་འདིར་སྐྱེས་ནས་སྐྱེ་བོ་མང་པོ་ཁ་འདོགས་<«གཡུང་»«པེ་»«སྣར་»«ཞོལ་»འདོག>པར་བྱེད་ན།
msgctxt "4d4861f3c7a949fbab248ee91ce249cb"
msgid "ད་ ཡང་ ཁྱོད་ འདི␣ར་ སྐྱེས་ ནས་ སྐྱེ་བོ་ མང་པོ་ ཁ་ འདོགས་པ␣ར་ བྱེད་ ན །"
msgstr ""

# འདི་ནས་<«གཡུང་»«པེ་»ལས།>ཤི་འཕོས་པའི་འོག་ཏུ་ཁྱོད་ཀྱི་<«གཡུང་»«པེ་»ཀྱིས།>འགྲོ་བ་ནི་གང་ཡིན།
msgctxt "94c684433356418e86532988bd58b24f"
msgid "འདི་ ནས་ ཤི་འཕོས་པ␣འི་ འོག་ ཏུ་ ཁྱོད་ ཀྱི་ འགྲོ་བ་ ནི་ གང་ཡིན །"
msgstr ""

msgctxt "ed0a15177e414e338c1f632a8677afd3"
msgid "སྐྱེ་གནས་ ནི་ གང་ཡིན །"
