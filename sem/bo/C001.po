msgctxt "35052f5275d54b2d86d62c11d7f66897"
msgid "﻿ཀླུ་ ཞེས་ བྱ་བ་ ནི །"
msgstr ""

msgctxt "d1f4028a95964fcba4ffcdf36cf70510"
msgid "གླེང་ གཞི་ མཉན་ དུ་ཡོད་པ་ ན་ བཞུགས་ ཏེ །"
msgstr ""

# དེའི་ཚེ་<«ཞོལ་»-ཚེ།>རྒྱལ་པོ་གསལ་རྒྱལ་གྱི་བློན་པོ་:ཆེན་པོ་<«ལི་»«ཅོ་»-ཆེན་པོ།>ཞིག་གྱོད་ཅིག་ལ་བཏགས་ཏེ།
msgctxt "8516b66c38684331a116e5ad41a1b25a"
msgid ""
"དེ␣འི་ ཚེ་ རྒྱལ་པོ་ གསལ་རྒྱལ་ གྱི་ བློན་པོ་ ཆེན་པོ་ ཞིག་ གྱོད་ ཅིག་ ལ་བཏགས་ "
"ཏེ །"
msgstr ""

# ཅི་:བདོག་པ་ཕྲོགས་<«ལི་»«ཅོ་»བདོགས་པ་ཕྲོགས།«ཞོལ་»བདོག་པ་འཕྲོག>ནས།
msgctxt "0c47dcc866094e5cb5276718e07bf43d"
msgid "ཅི་ བདོག་པ་ ཕྲོགས་ ནས །"
msgstr ""

msgctxt "48312a3ff2294cc998cef5a6f865428a"
msgid "ཁྲི་མོན་ དུ་ བཅུག་པ་ ལ␣ས །"
msgstr ""

# དེ་<«ལི་»«ཅོ་»-དེ།>དེར་ཟས་དང་སྐོམ་གྱིས་<«ལི་»གྱི།>མནར་ཏེ་
msgctxt "c938a005cc074970ba75486cc390443b"
msgid "དེ་དེ␣ར་ ཟས་ དང་ སྐོམ་ གྱིས་ མན␣ར་ ཏེ་"
msgstr ""

msgctxt "65ad221fb520478d80cfa1a0d078b1fe"
msgid "ཤི␣འོ ། །"
msgstr ""

msgctxt "a3c10dee03b546dc8d3a70ff71aab178"
msgid "དེ་ནས་ དེ␣ས་ སྨོན་ལམ་ ལོག་པ␣ར་ བཏབ་ ནས །"
msgstr ""

msgctxt "e52ac20b595b4328a8201d4f14df00e6"
msgid "ཤི་འཕོས་ ཏེ་"
msgstr ""

msgctxt "2b2cf0dfc0714760bccdc2d2c46c30fd"
msgid ""
"དུས་ ལ␣ས་ འདས་པ␣འི་ འོག་ ཏུ །  གནོད་སྦྱིན་ མ་རུངས་པ་ ཞིག་ ཏུ་ སྐྱེས་ ནས །"
msgstr ""

# དེས་ཡུལ་ཀོ་ས་ལ་དེ་ཐམས་ཅད་དུ་མི་ནད་བཏང་སྟེ<«ལི་»ཏེ།>།
msgctxt "e670cbfe918b4940b78ebf08d6bce48a"
msgid "དེ␣ས་ ཡུལ་ ཀོ་ས་ལ་ དེ་ ཐམས་ཅད་ དུ་ མི་ ནད་ བཏང་ སྟེ །"
msgstr ""

# སྐྱེ་བོ་ཕལ་པོ་ཆེ་དེ་<«གཡུང་»«པེ་»«སྣར་»«ཞོལ་»-དེ།>དག་ཁ་དག་<«གཡུང་»«པེ་»«སྣར་»«ཅོ་»«ཞོལ་»བཏག
# །>པར་གྱུར་ནས།
msgctxt "bde897cfba3042d3a2f5e30449b7648c"
msgid "སྐྱེ་བོ་ ཕལ་པོ་ ཆེ་ དེ་དག་ ཁ་དག་པ␣ར་ གྱུར་ ནས །"
msgstr ""

msgctxt "90c4b313429e444f8ff616c8877ec438"
msgid "ལྟས་མཁན་ དག་ གིས་ སྨྲས་པ །"
msgstr ""

# བློན་པོ་ཆེན་པོ་དེས་འཆི་ཀར་<«ཞོལ་»ཁར།>སྨོན་ལམ་ལོག་པར་བཏབ་ནས།
msgctxt "b893086ea5954608b54609ccd67fccfa"
msgid "བློན་པོ་ ཆེན་པོ་ དེ␣ས་ འཆི་ཀ␣ར་ སྨོན་ལམ་ ལོག་པ␣ར་ བཏབ་ ནས །"
msgstr ""

msgctxt "acf481dfc03a44c0acb81956a1c8423a"
msgid "གནོད་སྦྱིན་ མ་རུངས་པ␣ར་ སྐྱེས་ ཏེ །"
msgstr ""

# དེས་མི་ནད་འདི་དག་བཏང་<«ལི་»«སྣར་»«ཅོ་»གཏང༌།>ངོ་ཞེས་
msgctxt "a92a604cdb5f49b89c1c7de24a67cb62"
msgid "དེ␣ས་ མི་ ནད་ འདི་དག་ བཏང་ ངོ་ ཞེས་"
msgstr ""

msgctxt "cdc84ce99fd54fd1a12ec3c057734906"
msgid "བྱས་ སོ ། །"
msgstr ""

# དེ་ནས་དེ་ཐོས་མ་ཐག་ཏུ་སྐྱེ་བོ་མང་པོ་དེ་<«གཡུང་»«ལི་»«པེ་»«ཅོ་»-དེ།>དག་ཅི་བྱ་གཏོལ་མེད་པར་གྱུར་ཏོ།
# །
msgctxt "33913489fe73410db19d4fa31ef71913"
msgid ""
"དེ་ནས་ དེ་ ཐོས་ མ་ ཐག་ ཏུ་ སྐྱེ་བོ་ མང་པོ་ དེ་དག་ ཅི་ བྱ་ གཏོལ་མེད་པ␣ར་ "
"གྱུར་ ཏོ ། །"
msgstr ""

msgctxt "5eb8cc49ad504e4fbf227dfaf5d37193"
msgid "དེ་ནས་ རྒྱལ་པོ་ གསལ་རྒྱལ་ གྱིས་ བསམས་པ །"
msgstr ""

# འདི་ནི་སུས་ཀྱང་འདུལ་<«གཡུང་»«པེ་»«སྣར་»«ཞོལ་»གདུལ།>བར་མི་ནུས་ཀྱིས<«ཞོལ་»ཀྱི།>།
msgctxt "395e448b8bd04701b63804dfca5180be"
msgid "འདི་ ནི་ སུ␣ས་ ཀྱང་ འདུལ་བ␣ར་ མི་ ནུས་ ཀྱིས །"
msgstr ""

# ད་<«སྣར་»-ད། «ཅོ་»དེ།>འདིའི་ཕྱིར་བཅོམ་ལྡན་འདས་ལ་གསོལ་བ་གདབ་དགོས་སོ་སྙམ་སྟེ།
msgctxt "d2b7ecceb2c04f67b1e7367642c42ff9"
msgid "ད་ འདི␣འི་ ཕྱིར་ བཅོམ་ལྡན་འདས་ ལ་ གསོལ་བ་ གདབ་ དགོས་ སོ་ སྙམ་ སྟེ །"
msgstr ""

# བཅོམ་ལྡན་འདས་ག་ལ་བ་དེར་སོང་སྟེ་<«ལི་»«ཅོ་»ཏེ།>
msgctxt "8a49db2fd0264bb28540e4feac74eee3"
msgid "བཅོམ་ལྡན་འདས་ ག་ལ་བ་ དེ␣ར་ སོང་ སྟེ་"
msgstr ""

msgctxt "4d32e8c86aaf41d4940f3eff06ae6200"
msgid "ཕྱིན་ ནས །"
msgstr ""

# བཅོམ་ལྡན་འདས་ཀྱི་<«གཡུང་»«པེ་»ཀྱིས།>ཞབས་ལ་མགོ་བོས་ཕྱག་འཚལ་ཏེ།
msgctxt "b8978171ea9b450d92e85e3ec1758b45"
msgid "བཅོམ་ལྡན་འདས་ ཀྱི་ ཞབས་ ལ་མགོ་ བོས་ ཕྱག་ འཚལ་ ཏེ །"
msgstr ""

msgctxt "93b658fe3876410fbf0b26d6c0da8306"
msgid "ཕྱོགས་ གཅིག་ ཏུ་ འདུག་ གོ ། །"
msgstr ""

msgctxt "47cbf92d929d409cab8d40831cde2c7a"
msgid "ཕྱོགས་ གཅིག་ ཏུ་ འདུག་ ནས་"
msgstr ""

# རྒྱལ་པོ་གསལ་རྒྱལ་གྱིས་བཅོམ་ལྡན་འདས་ལ་<«ཅོ་»-ལ།>འདི་སྐད་ཅེས་གསོལ་ཏོ། །
msgctxt "b826026f00e24e959a7f10478f337571"
msgid "རྒྱལ་པོ་ གསལ་རྒྱལ་ གྱིས་ བཅོམ་ལྡན་འདས་ ལ་ འདི་སྐད་ ཅེས་ གསོལ་ ཏོ ། །"
msgstr ""

# བཙུན་པ་བཅོམ་ལྡན་འདས་ཀྱིས་ནི་<«ལི་»«ཅོ་»-ནི།>ཀླུ་མ་རུངས་པ་དགའ་བོ་དང་།
# ཉེ་དགའ་བོ་ལ་སོགས་པ་:གཞན་དང་<«ལི་»«ཅོ་»-གཞན་དང་།>གཞན་དག་ཀྱང་བཏུལ།
msgctxt "34d231c9feb347eb9f67a396cf62a858"
msgid ""
"བཙུན་པ་ བཅོམ་ལྡན་འདས་ ཀྱིས་ ནི་ ཀླུ་ མ་རུངས་པ་ དགའ་བོ་ དང་ །  ཉེ་དགའ་བོ་ "
"ལ་སོགས་པ་ གཞན་ དང་ གཞན་དག་ ཀྱང་ བཏུལ །"
msgstr ""

# གནོད་སྦྱིན་མ་རུངས་པ་འབྲོག་<«ལི་»«ཅོ་»འབྲོགས།>གནས་ལ་སོགས་པ་གཞན་དང་གཞན་དག་ཀྱང་བཏུལ་ན<«སྣར་»-ན།>།
msgctxt "6b4f2e849c0b4659aa4d5607bca67e86"
msgid ""
"གནོད་སྦྱིན་ མ་རུངས་པ་ འབྲོག་ གནས་ ལ་སོགས་པ་ གཞན་ དང་ གཞན་དག་ ཀྱང་ བཏུལ་ ན །"
msgstr ""

# བཙུན་པ་བློན་པོ་:ཆེན་པོ་<«གཡུང་»«ལི་»«པེ་»«ཅོ་»-ཆེན་པོ།>ཆེ་གེ་མོ་ཞིག་གིས་སྨོན་ལམ་ལོག་པར་བཏབ་སྟེ<«གཡུང་»«པེ་»པས།«ལི་»ཏེ།>།
msgctxt "1e530d8bbf6445e58e0d2532900daf15"
msgid ""
"བཙུན་པ་ བློན་པོ་ ཆེན་པོ་ ཆེ་གེ་མོ་ ཞིག་ གིས་ སྨོན་ལམ་ ལོག་པ␣ར་ བཏབ་ སྟེ །"
msgstr ""

msgctxt "4b58d26991c84dc9b6a6507bc3c68d6b"
msgid "གནོད་སྦྱིན་ མ་རུངས་པ␣ར་ སྐྱེས་ ནས །"
msgstr ""

msgctxt "6f491653e26046a089ad2a7fe602dc11"
msgid "དེ␣ས་ བདག་ གི་ ཡུལ་ དུ་ མི་ ནད་ བཏང་ ན །"
msgstr ""

# བཅོམ་ལྡན་འདས་ཀྱིས་ཐུགས་བརྩེ་<«གཡུང་»«པེ་»རྩེ།>བའི་སླད་དུ།
# གནོད་སྦྱིན་མ་རུངས་པ་དེ་གདུལ་<«ཅོ་»གཏུལ།>བར་གསོལ།
msgctxt "e8632282e09d4f1094fd0f75761301ab"
msgid ""
"བཅོམ་ལྡན་འདས་ ཀྱིས་ ཐུགས་ བརྩེ་བ␣འི་ སླད་ དུ །  གནོད་སྦྱིན་ མ་རུངས་པ་ དེ་ "
"གདུལ་བ␣ར་ གསོལ །"
msgstr ""

# དེ་ནས་བཅོམ་ལྡན་འདས་ཀྱིས་རྒྱལ་པོ་གསལ་རྒྱལ་ལ་ཅང་མི་གསུང་བས་<«ལི་»«ཅོ་»བར།
# «ཁུ་»བའི།>གནང་ངོ་། །
msgctxt "613f8e60afde47d6b760a10afb486904"
msgid ""
"དེ་ནས་ བཅོམ་ལྡན་འདས་ ཀྱིས་ རྒྱལ་པོ་ གསལ་རྒྱལ་ ལ་ ཅང་ མི་ གསུང་བ␣ས་ གནང་ ངོ་ "
"། །"
msgstr ""

msgctxt "eb81ee1ff9d24fa09f738a2ac6508daf"
msgid ""
"དེ་ནས་ རྒྱལ་པོ་ གསལ་རྒྱལ་ བཅོམ་ལྡན་འདས་ ཀྱིས་ གསུངས་པ་ ལ་ མངོན་པར་ བསྟོད་ "
"དེ་"
msgstr ""

# རྗེས་སུ་:ཡི་རངས་<«གཡུང་»«པེ་»ཡིད་རང༌། «ཞོལ་»ཡི་རང་།>ནས།
msgctxt "d7ec7df308674a948bcd645c2f0159cb"
msgid "རྗེས་སུ་ ཡི་ རངས་ ནས །"
msgstr ""

# བཅོམ་ལྡན་འདས་ཀྱི་<«ཁུ་»ཀྱིས།>ཞབས་ལ་མགོ་བོས་ཕྱག་འཚལ་ཏེ།
msgctxt "5db16f30f66d4718a2c7e1e6c6105923"
msgid "བཅོམ་ལྡན་འདས་ ཀྱི་ ཞབས་ ལ་མགོ་ བོས་ ཕྱག་ འཚལ་ ཏེ །"
