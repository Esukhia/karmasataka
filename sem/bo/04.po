msgstr ""

msgctxt "df2673c30dd94a038586dfe4ccc8ecaf"
msgid "དེ་ ནི་ མགོ་ བྲེགས་"
msgstr ""

# ལུས་ལ་སྣམ་<«གཡུང་»«པེ་»བསྣམ།>སྦྱར་གྱོན། །
msgctxt "b3dd55c2f31f4013930100fdf29feff8"
msgid "ལུས་ ལ་ སྣམ་སྦྱར་ གྱོན ། །"
msgstr ""

# སངས་རྒྱས་ཀྱིས་<«གཡུང་»«ལི་»«པེ་»«སྣར་»«ཅོ་»«ཞོལ་»ཀྱི།>ནི་དགོངས་པས་གོས་བགོས་ནས།
# །
msgctxt "0e69408b47dc407ca314edf8ee2a3ccd"
msgid "སངས་རྒྱས་ ཀྱིས་ ནི་ དགོངས་པ␣ས་ གོས་ བགོས་ ནས ། །"
msgstr ""

msgctxt "6533978a85d141b3a8f92cd8727eca2c"
msgid "མོད་ ལ་ དབང་པོ་ རབ་ཏུ་ ཞི་བ␣ར་ གནས ། །"
msgstr ""

msgctxt "3c5acb328342421fac4544f64cabec9e"
msgid "དེ་ནས་ བཅོམ་ལྡན་འདས་ ཀྱིས་ དེ་ ལ་ལུང་ ཕོག་ ནས་"
msgstr ""

# དེས་ཀྱང་བརྩོན་པ་དང་།
# བསྒྲུབ་<«ཞོལ་»སྒྲུབ།>པ་དང་འབད་པས་ཉོན་མོངས་པ་ཐམས་ཅད་སྤངས་ནས།
msgctxt "d9e50468353e4f22862505bc664e5b38"
msgid ""
"དེ␣ས་ ཀྱང་ བརྩོན་པ་ དང་ །  བསྒྲུབ་པ་ དང་ འབད་པ␣ས་ ཉོན་མོངས་པ་ ཐམས་ཅད་ སྤངས་ "
"ནས །"
msgstr ""

# དགྲ་བཅོམ་པ་<«གཡུང་»ལ།>ཉིད་མངོན་སུམ་དུ་བྱས་ཏེ།
msgctxt "b31bae1526f94217af2a99808f9462c5"
msgid "དགྲ་བཅོམ་པ་ ཉིད་ མངོན་སུམ་ དུ་ བྱས་ ཏེ །"
msgstr ""

msgctxt "40858b90890b4669853d91c3dbbd794b"
msgid "དེ་ དགྲ་བཅོམ་པ་ ཁམས་ གསུམ་པ␣འི་ འདོད་ཆགས་ དང་ བྲལ་བ␣ར་ གྱུར་ ལ །"
msgstr ""

# གསེར་དང་བོང་བར་མཉམ་པ<«གཡུང་»ང་།>།
msgctxt "9cd19e4d404941d3b16715e3b9ee05f8"
msgid "གསེར་ དང་ བོང་བ␣ར་ མཉམ་པ །"
msgstr ""

# ནམ་མཁའ་དང་ལག་མཐིལ་<«གཡུང་»ཐིལ།>དུ་འདྲ་བའི་སེམས་དང་ལྡན་པ།
msgctxt "0b7d2a8364df4bd991c2b381015eb211"
msgid "ནམ་མཁའ་ དང་ ལག་མཐིལ་ དུ་ འདྲ་བ␣འི་ སེམས་ དང་ ལྡན་པ །"
msgstr ""

msgctxt "131e39c765d84ee3adcf51a533a43d1f"
msgid "ཙན་དན་ སྦངས་པ་ ལྟ་བུ␣ར་ བསིལ་བ␣ར་ གྱུར་པ །"
msgstr ""

msgctxt "62cdcb03129b448f8eeed6667e2dd4a8"
msgid "རིག་པ␣ས་ སྒོ་ང␣འི་ སྦུབས་ བཅོམ་པ །"
msgstr ""

msgctxt "368ee4f2c0e94469a7a2c9e407dfff61"
msgid "རིག་པ་ དང་ མངོན་པར་ ཤེས་པ་ དང་ །  སོ་སོ་ ཡང་དག་པ␣ར་ རིག་པ་ ཐོབ་པ །"
msgstr ""

# སྲིད་པའི་རྙེད་པ་དང་ཆགས་པ་དང་བཀུར་སྟི་ལ་རྒྱབ་ཀྱིས་ཕྱོགས་པ་དང་<«གཡུང་»«ཞོལ་»–དང་།>
msgctxt "6a76b5fca70b4840ae49615bac9c1a77"
msgid ""
"སྲིད་པ␣འི་ རྙེད་པ་ དང་ ཆགས་པ་ དང་ བཀུར་སྟི་ ལ་ རྒྱབ་ ཀྱིས་ ཕྱོགས་པ་ དང་"
msgstr ""

# དབང་པོ་དང་ཉེ་དབང་དང་བཅས་པའི་ལྷ་རྣམས་ཀྱིས་མཆོད་ཅིང་རྗེད་<«གཡུང་»«པེ་»བརྗེད།>པ་དང་།
# གུས་པར་སྨྲ་བའི་གནས་སུ་གྱུར་ཏོ། །
msgctxt "d7835a8efa5b46daa0d552bd896f5fce"
msgid ""
"དབང་པོ་ དང་ ཉེ་དབང་ དང་ བཅས་པ␣འི་ ལྷ་ རྣམས་ ཀྱིས་ མཆོད་ ཅིང་ རྗེད་པ་ དང་ །  "
"གུས་པ␣ར་ སྨྲ་བ␣འི་ གནས་ སུ་ གྱུར་ ཏོ ། །"
msgstr ""

msgctxt "5ee50ceae0db43fe933bc439f2b2a6d8"
msgid "དེ་ནས་ དགེ་སློང་ དག་ གིས་ བཅོམ་ལྡན་འདས་ ལ་ ཞུས་པ །"
msgstr ""

msgctxt "7eed0d48bea640c1a86b6d67fe25d8b0"
msgid "བཙུན་པ་"
msgstr ""

# དགེ་སློང་འདིས་ལས་ཅི་བགྱིས་ན།
# ལས་<«གཡུང་»པེས།>དེའི་རྣམ་པར་སྨིན་པས་ཕྱུག་ཅིང་ནོར་མང་ལ་ལོངས་སྤྱོད་ཆེ་བའི་རིགས་སུ་སྐྱེས་ལགས།
msgctxt "96a6946f45314cf1a1b5a3796d4a425f"
msgid ""
"དགེ་སློང་ འདི␣ས་ ལ␣ས་ ཅི་ བགྱིས་ ན །  ལ␣ས་ དེ␣འི་ རྣམ་པ␣ར་ སྨིན་པ␣ས་ ཕྱུག་ "
"ཅིང་ ནོར་ མང་ ལ་ ལོངས་སྤྱོད་ ཆེ་བ␣འི་ རིགས་ སུ་ སྐྱེས་ ལགས །"
msgstr ""

# ལས་ཅི་བགྱིས་ན།
# ལས་དེའི་རྣམ་པར་སྨིན་པས་རྒྱབ་སྒུར་<«གཡུང་»«པེ་»རྒུར།>བུར་གྱུར་ལ།
msgctxt "7cb0cc80f2ad44d6af876df07e44ebda"
msgid ""
"ལ␣ས་ ཅི་ བགྱིས་ ན །  ལ␣ས་ དེ␣འི་ རྣམ་པ␣ར་ སྨིན་པ␣ས་ རྒྱབ་ སྒུར་ བུར་ གྱུར་ ལ"
" །"
msgstr ""

# བཅོམ་ལྡན་འདས་ལ་བརྟེན་<«གཡུང་»«པེ་»རྟེན།>ཏེ།
msgctxt "3f36745401eb420ebaf1c12e70e6bd0b"
msgid "བཅོམ་ལྡན་འདས་ ལ་ བརྟེན་ ཏེ །"
msgstr ""

# སྔ་མཁོ་<«གཡུང་»ཁོ།>བཞིན་དུ་རངས་པར་གྱུར་ནས།
msgctxt "e88c032b1b004a8d9a997afdd714f771"
msgid "སྔ་ མཁོ་ བཞིན་ དུ་ རངས་པ␣ར་ གྱུར་ ནས །"
msgstr ""

# འདིས་<«གཡུང་»«པེ་»འདི།>བཅོམ་ལྡན་འདས་མཉེས་པར་བགྱིས་ཏེ།
msgctxt "d8f13068ea584d49bfdfa3961f458dd9"
msgid "འདི␣ས་ བཅོམ་ལྡན་འདས་ མཉེས་པ␣ར་ བགྱིས་ ཏེ །"
msgstr ""

# མི་<«གཡུང་»–མཉེས་པར་བགྱིས་ཏེ། མི།
# «ལི་»«པེ་»«སྣར་»«ཅོ་»«ཞོལ་»མཉེས་པར་བགྱིས་ཏེ། མ།>མཉེས་པར་མ་བགྱིས་ན<«ཁུ་»ལ།>།
msgctxt "b5e7adc2fdf442c5b0b752b391ac54e8"
msgid "མི་ མཉེས་པ␣ར་ མ་ བགྱིས་ ན །"
msgstr ""

# བཅོམ་ལྡན་འདས་ཀྱི་<«གཡུང་»«ལི་»«པེ་»«སྣར་»«ཅོ་»«ཞོལ་»ཀྱིས།>བསྟན་པ་ལ་རབ་ཏུ་བྱུང་སྟེ།
msgctxt "e64c12664a8d400ba54276d85f4173f0"
msgid "བཅོམ་ལྡན་འདས་ ཀྱི་ བསྟན་པ་ ལ་ རབ་ཏུ་ བྱུང་ སྟེ །"
msgstr ""

msgctxt "594cffb40d8847a89ae088be32045543"
msgid "ཉོན་མོངས་པ་ ཐམས་ཅད་ སྤངས་ ནས །"
msgstr ""

msgctxt "bccc8c65332d4f1ab6a5faa7f9a1224c"
msgid "དགྲ་བཅོམ་པ་ ཉིད་ མངོན་སུམ་ དུ་ བགྱིས་ ལགས །"
msgstr ""

msgctxt "2a7f1d1e968e43208e7de8561fdf15ce"
msgid "བཅོམ་ལྡན་འདས་ ཀྱིས་ བཀའ་ སྩལ་པ །"
msgstr ""

msgctxt "225eee4d13ed484da88503341832da06"
msgid "དགེ་སློང་ དག་"
msgstr ""

msgctxt "7fac6d961a8e48659c8f5f61bc0c5654"
msgid "དགེ་སློང་ འདི␣ས་ ལ␣ས་ བྱས་ ཤིང་ བསགས་ ཏེ །"
msgstr ""

msgctxt "5b8be0eed4324f74997ec141fb14c9ed"
msgid "དགེ་སློང་ དག་"
msgstr ""

# སྔོན་བྱུང་བ་འདས་པའི་དུས་ན།
# བསྐལ་པ་བཟང་པོ་འདི་ཉིད་ལ་སྐྱེ་དགུའི་<«ཞོལ»རྒུའི།>ཚེ་ལོ་ཉི་ཁྲི་ཐུབ་པ་ན།
# ཡང་དག་པར་རྫོགས་པའི་སངས་རྒྱས་རིག་པ་དང་ཞབས་སུ་ལྡན་པ་བདེ་བར་གཤེགས་པ་འཇིག་རྟེན་མཁྱེན་པ་སྐྱེས་བུ་འདུལ་བའི་ཁ་ལོ་སྒྱུར་བ།
# བླ་ན་མེད་པ་ལྷ་དང་མི་རྣམས་ཀྱི་སྟོན་པ།
# སངས་རྒྱས་བཅོམ་ལྡན་འདས་འོད་སྲུང་ཞེས་བྱ་བ་འཇིག་རྟེན་དུ་བྱུང་སྟེ།
msgctxt "db4d717272d6411ca65c510bc4ff9739"
msgid ""
"སྔོན་བྱུང་བ་ འདས་པ␣འི་ དུས་ ན །  བསྐལ་པ་ བཟང་པོ་ འདི་ཉིད་ ལ་ སྐྱེ་དགུ␣འི་ "
"ཚེ་ལོ་ ཉི་ཁྲི་ ཐུབ་པ་ ན །  ཡང་དག་པ␣ར་ རྫོགས་པ␣འི་ སངས་རྒྱས་ རིག་པ་ དང་ ཞབས་ "
"སུ་ ལྡན་པ་ བདེ་བ␣ར་ གཤེགས་པ་ འཇིག་རྟེན་ མཁྱེན་པ་ སྐྱེས་བུ་ འདུལ་བ␣འི་ ཁ་ལོ་ "
"སྒྱུར་བ །  བླ་ ན་ མེད་པ་ ལྷ་ དང་ མི་ རྣམས་ ཀྱི་ སྟོན་པ །  སངས་རྒྱས་ "
