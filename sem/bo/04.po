msgctxt "ae938a2c84d64bd995c194fe61dbb021"
msgid ""
"ཁྱིམ་བདག་ དེ་ འཁོར་ དང་ བཅས་པ་ དང་ ཁྱེའུ་ དེ␣ས་ འཇིག་ཚོགས་ ལ་ ལྟ་བ␣འི་ "
"རི␣འི་ རྩེ་མོ་ མཐོན་པོ་ ཉི་ཤུ་ ཡེ་ཤེས་ རྡོ་རྗེ␣ས་ བཅོམ་ ནས །"
msgstr ""

# རྒྱུན་དུ་ཞུགས་པའི་འབྲས་བུ་མངོན་སུམ་<«ཅོ་»–སུམ།>དུ་བྱས་སོ། །
msgctxt "409c94e371b4435eb89a25be772273cb"
msgid "རྒྱུན་ དུ་ ཞུགས་པ␣འི་ འབྲས་བུ་ མངོན་སུམ་ དུ་ བྱས་ སོ ། །"
msgstr ""

msgctxt "35dd14a6a317474b9570dc795ade8fd1"
msgid "དེ་ནས་ བཅོམ་ལྡན་འདས་ ཀྱིས་ དེ་དག་ བདེན་པ་ རྣམས་ ལ་ བཀོད་ ནས །"
msgstr ""

msgctxt "a66795a1f09c48848c4215275d4a8268"
msgid "ཆོས་ ཀྱི་ གཏམ་ གྱིས་ ཡང་དག་པ␣ར་ བསྟན །"
msgstr ""

msgctxt "b3d8cafb11d9494a86038b121a092b7b"
