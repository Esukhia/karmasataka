msgctxt "0ce329aed7ed47ef98d8853d5d0e315d"
msgid "བླ་གོས་ ཕྲག་པ་ གཅིག་ ཏུ་ གཟར་ ནས །"
msgstr ""

# བཅོམ་ལྡན་འདས་ག་ལ་བ་དེ་<«གཡུང་»«པེ་»དེར།
# «ལི་»«སྣར་»«ཅོ་»བ་དེར།>ལོགས་སུ་ཐལ་མོ་སྦྱར་བ་བཏུད་དེ་
msgctxt "a3534235dc8b4ca1bf2eb98593293413"
msgid "བཅོམ་ལྡན་འདས་ ག་ལ་བ་ དེ་ ལོགས་ སུ་ ཐལ་མོ་ སྦྱར་བ་ བཏུད་ དེ་"
msgstr ""

msgctxt "a21821ae919e4455928d95502c6eeaa5"
msgid "བཅོམ་ལྡན་འདས་ ལ་ འདི་སྐད་ ཅེས་ གསོལ་ ཏོ ། །"
msgstr ""

msgctxt "f1babfb6147c4252b75d3a42da811247"
msgid "བཙུན་པ་"
msgstr ""

