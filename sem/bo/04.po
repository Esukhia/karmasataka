# [17b]ལས་བརྒྱ་ཐམ་པ<«གཡུང་»བ།>། བམ་པོ་གཉིས་པ། 
msgctxt "line 1, 04.txt"
msgid "ལ␣ས་ བརྒྱ་ ཐམ་པ །  བམ་པོ་ གཉིས་པ །"
msgstr ""

msgctxt "line 2, 04.txt"
msgid "སྒུར་ བུ་ གཉིས་ ཞེས་ བྱ་བ་ དེ་ ལ །"
msgstr ""

# སྒུར་བུ་<«གཡུང་»སྐུ་བུ། «ཅོ་»«ཞོལ་»སྒྱུར་པོ།>དང་པོ་ནི། 
msgctxt "line 3, 04.txt"
msgid "སྒུར་ བུ་ དང་པོ་ ནི །"
msgstr ""

# གླེང་གཞི་<«གཡུང་»«པེ་»«ཁུ་»བཞི།>མཉན་དུ་ཡོད་པ་ན་བཞུགས་ཏེ། 
msgctxt "line 4, 04.txt"
msgid "གླེང་ གཞི་ མཉན་ དུ་ཡོད་པ་ ན་ བཞུགས་ ཏེ །"
msgstr ""

# དེའི་ཚེ་མཉན་དུ་ཡོད་པ་ན་ཁྱིམ་བདག་ཕྱུག་ཅིང་<«གཡུང་»ཅིག>ནོར་མང་ལ་ལོངས་སྤྱོད་ཆེ་བ་ཡོངས་སུ་འཛིན་པ་ཡངས་ཤིང་རྒྱ་ཆེ་བ།
# རྣམ་ཐོས་ཀྱི་བུའི་ནོར་དང་ལྡན་པ<«གཡུང་»«པེ་»–རྣམ་ཐོས་ཀྱི་བུའི་ནོར་དང་ལྡན་པ།>།
# རྣམ་ཐོས་ཀྱི་བུའི་ནོར་དང་འགྲན་པ་ཞིག་གནས་པ་དེས་ཐབས་ཟླར་བབ་པ་ལས་<«གཡུང་»«ལི་»«པེ་»«སྣར་»«ཅོ་»«ཞོལ་»ལ།>ཆུང་མ་བླངས་ནས།
msgctxt "line 5, 04.txt"
msgid ""
"དེ␣འི་ ཚེ་ མཉན་ དུ་ཡོད་པ་ ན་ ཁྱིམ་བདག་ ཕྱུག་ ཅིང་ ནོར་ མང་ ལ་ ལོངས་སྤྱོད་ "
"ཆེ་བ་ ཡོངས་སུ་ འཛིན་པ་ ཡངས་ ཤིང་ རྒྱ་ཆེ་བ །  རྣམ་ཐོས་ ཀྱི་ བུ␣འི་ ནོར་ དང་ "
"ལྡན་པ །  རྣམ་ཐོས་ ཀྱི་ བུ␣འི་ ནོར་ དང་ འགྲན་པ་ ཞིག་ གནས་པ་ དེ␣ས་ ཐབས་ ཟླ␣ར་ "
"བབ་པ་ ལ␣ས་ ཆུང་མ་ བླངས་ ནས །"
msgstr ""

# དེ་དེ་དང་ལྷན་ཅིག་ཏུ་རྩེ་<«གཡུང་»«པེ་»+ཅིག>ཞིང་དགའ་ལ་དགའ་མགུར་[18a]སྤྱོད་པ་ལས་
msgctxt "line 6, 04.txt"
msgid "དེ་དེ་ དང་ ལྷན་ཅིག་ ཏུ་ རྩེ་ ཞིང་ དགའ་ ལ་ དགའ་མགུ␣ར་ སྤྱོད་པ་ ལ␣ས་"
msgstr ""

msgctxt "line 7, 04.txt"
msgid "ཕྱི་ ཞིག་ ན་ དེ␣འི་ ཆུང་མ་ ལ་ བུ་ ཆགས་ ནས །"
msgstr ""

msgctxt "line 8, 04.txt"
msgid "དེ་ ཟླ་བ་ དགུ␣འམ་ བཅུ་ ལོན་པ་ དང་ །"
msgstr ""

msgctxt "line 9, 04.txt"
msgid "ཁྱེའུ་ གཟུགས་བཟང་ ཞིང་"
msgstr ""

# བལྟ་<«གཡུང་»«ལི་»«སྣར་»«ཅོ་»ལྟ།
# «པེ་»ཞི་བལྟ།>ན་སྡུག་<«གཡུང་»«པེ་»+པ།>ལ་མཛེས་པ་ཞིག་བཙས་ཏེ།
msgctxt "line 10, 04.txt"
msgid "བལྟ་ ན་ སྡུག་ ལ་ མཛེས་པ་ ཞིག་ བཙས་ ཏེ །"
msgstr ""

msgctxt "line 11, 04.txt"
msgid "དེ␣འི་ བཙས་ སྟོན་ རྒྱས་པ␣ར་ བྱས་ ནས །"
msgstr ""

msgctxt "line 12, 04.txt"
msgid "རིགས་ དང་ འཐུན་པ␣ར་ མིང་ བཏགས་ ཏེ །"
msgstr ""

# འོ་མ་དང་ཞོ་དང་མར་དང་ཞུན་མར་དང་མར་གྱི་ཉིང་གུས་<«གཡུང་»«པེ་»སྙིང་ཀུས།
# «ལི་»«སྣར་»སྙིང་གུས། «ཅོ་»«ཞོལ་»སྙིང་ཁུས།>བསྐྱེད་བསྲིངས་ཏེ།
msgctxt "line 13, 04.txt"
msgid ""
"འོ་མ་ དང་ ཞོ་ དང་ མར་ དང་ ཞུན་མར་ དང་ མར་ གྱི་ ཉིང་ གུས་ བསྐྱེད་ བསྲིངས་ ཏེ "
"།"
msgstr ""

# དེ་གང་གི་ཚེ་ཆེར་སྐྱེས་པ་དེའི་ཚེ་ཡི་གེ་དང་ཁོང་རྩིས་དང་ཤོད་དགོད་པ་དང་།
# ལག་རྩིས་དང་དབྱུང་བ་དང་སྦ་བ་དང་དགྲམ་པ་དང་གོས་བརྟག་པ་དང་ནོར་བུ་བརྟག་པ་དང་རིན་པོ་ཆེ་བརྟག་<«ཁུ་»བཏག>པ་དང་སྤོས་བརྟག་པ་དང་སྨན་བརྟག་པ་དང་གླང་པོ་ཆེ་བརྟག་པ་དང་རྟ་བརྟག་པ་དང་གོ་མཚོན་བརྟག་པ་རྣམས་བསླབས་ཏེ།<«གཡུང་»+ད།
# «ཅོ»+དེ།>
msgctxt "line 14, 04.txt"
msgid ""
"དེ་ གང་ གི་ ཚེ་ ཆེ␣ར་ སྐྱེས་པ་ དེ␣འི་ ཚེ་ ཡི་གེ་ དང་ ཁོང་ རྩིས་ དང་ ཤོད་ "
"དགོད་པ་ དང་ །  ལག་རྩིས་ དང་ དབྱུང་བ་ དང་ སྦ་བ་ དང་ དགྲམ་པ་ དང་ གོས་ བརྟག་པ་ "
"དང་ ནོར་བུ་ བརྟག་པ་ དང་ རིན་པོ་ཆེ་ བརྟག་པ་ དང་ སྤོས་ བརྟག་པ་ དང་ སྨན་ "
"བརྟག་པ་ དང་ གླང་པོ་ཆེ་ བརྟག་པ་ དང་ རྟ་ བརྟག་པ་ དང་ གོ་མཚོན་ བརྟག་པ་ རྣམས་ "
"བསླབས་ ཏེ །"
msgstr ""

# ཡི་གེ་མཁས་པ་དང་ཀློག་མཁས་པ་དང་མཛངས་<«གཡུང་»«ལི་»«པེ་»«སྣར་»«ཅོ་»འཛངས།>པ་དང་བྱ་བ་ལ་གསལ་བ་དང་ཡིད་གཞུངས་<«གཡུང་»«པེ་»བཞུངས།>པ་དང་བརྟག་པ་རྣམ་པ་བརྒྱད་ལ་བྱང་བར་གྱུར་ཏོ།
# །
msgctxt "line 15, 04.txt"
msgid ""
"ཡི་གེ་ མཁས་པ་ དང་ ཀློག་ མཁས་པ་ དང་ མཛངས་པ་ དང་ བྱ་བ་ ལ་ གསལ་བ་ དང་ "
"ཡིད་གཞུངས་པ་ དང་ བརྟག་པ་ རྣམ་པ་ བརྒྱད་ ལ་ བྱང་བ␣ར་ གྱུར་ ཏོ ། །"
msgstr ""

# དེ་ནས་<«གཡུང་»«པེ་»དེས།>ཕྱི་ཞིག་ན་ཁྱེའུ་དེ་ནད་ཀྱིས་ཐེབས་ནས་
msgctxt "line 16, 04.txt"
msgid "དེ་ནས་ ཕྱི་ ཞིག་ ན་ ཁྱེའུ་ དེ་ ནད་ ཀྱིས་ ཐེབས་ ནས་"
msgstr ""

# དེའི་<«གཡུང་»«ལི་»«པེ་»«སྣར་»«ཅོ་»«ཞོལ་»–དེའི།>རྒྱབ་རླུང་གིས་སྒུར་བུར་བྱས་སོ།
# །
msgctxt "line 17, 04.txt"
msgid "དེ␣འི་ རྒྱབ་ རླུང་ གིས་ སྒུར་ བུར་ བྱས་ སོ ། །"
msgstr ""

msgctxt "line 18, 04.txt"
msgid ""
"དེ་ནས་ དེ་ མཐོང་ མ་ ཐག་ ཏུ་ ཕ་མ་ གཉིས་ ཤིན་ཏུ་ སྡུག་བསྔལ་ བར་ གྱུར་ ནས །"
msgstr ""

msgctxt "line 19, 04.txt"
msgid "དེ་ ལ་སྨན་པ་ ཐམས་ཅད་ དང་ ཡོ་བྱད་ ཐམས་ཅད་ བསོགས་ ཏེ་"
msgstr ""

