msgctxt "94de377bb7374a309c4ef10e97613dd7"
msgid "ཆོས་ མཉན་པ␣འི་ ཕྱིར་ བཅོམ་ལྡན་འདས་ ཀྱི་ སྤྱན་སྔ␣ར་ འདུག་ གོ ། །"
msgstr ""

# དེ་ནས་བཅོམ་ལྡན་འདས་ཀྱིས་ཁྱིམ་བདག་དེ་འཁོར་དང་བཅས་པའི་བསམ་པ་དང་བག་ལ་ཉལ་དང་ཁམས་དང་<«གཡུང་»«ལི་»«པེ་»«སྣར་»«ཅོ་»«ཞོལ་»–ཁམས་དང་།>རང་བཞིན་ཐུགས་སུ་ཆུད་ནས་
msgctxt "0932619e60be40f7a10de7814a9e7d68"
msgid ""
"དེ་ནས་ བཅོམ་ལྡན་འདས་ ཀྱིས་ ཁྱིམ་བདག་ དེ་ འཁོར་ དང་ བཅས་པ␣འི་ བསམ་པ་ དང་ བག་ "
"ལ་ ཉལ་ དང་ ཁམས་ དང་ རང་བཞིན་ ཐུགས་ སུ་ ཆུད་ ནས་"
msgstr ""

msgctxt "b7b007269eb0452fbc8c1796fbd4bd7c"
msgid "དེ་ དང་ འཐུན་པ␣འི་ ཆོས་ བསྟན་ ཏེ །"
msgstr ""

msgctxt "b156828c36094f3cb70cd9867d4b3101"
msgid "དེ་ ཐོས་ ནས་"
msgstr ""

