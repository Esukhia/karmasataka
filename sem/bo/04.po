msgstr ""

# སོས་པར་བྱེད་མ་<«ཞོལ་»མི།>ནུས་སོ། །
msgctxt "line 21, 04.txt"
msgid "སོས་པ␣ར་ བྱེད་ མ་ནུ␣ས་ སོ ། །"
msgstr ""

msgctxt "line 22, 04.txt"
msgid "དེ་ནས་ ཕྱི་ ཞིག་ ན་ ཁྱིམ་བདག་ དེ␣འི་ བློ་ སྐྱེས་པ་"
msgstr ""

# ཁྱེའུ་འདི་སྨན་པ་ཐམས་ཅད་ཀྱིས་གསོ་མ་ནུས་ཀྱིས<«ཞོལ་»ཀྱི།>། 
msgctxt "line 23, 04.txt"
msgid "ཁྱེའུ་ འདི་ སྨན་པ་ ཐམས་ཅད་ ཀྱིས་ གསོ་མ་ ནུས་ ཀྱིས །"
msgstr ""

msgctxt "line 24, 04.txt"
msgid "ད་ནི་ ཡོན་གནས་ རྣམས་ ལ་ གསོལ་བ་ བཏབ་ ན །"
msgstr ""

# དེ་དག་ནི་<«གཡུང་»«པེ་»གི།>རྫུ་འཕྲུལ་ཆེ་ཞིང་མཐུ་ཆེ་བ་ཡིན་པས།
# དེ་དག་གིས་བདག་གི་བུའི་རྒྱབ་སྔ་མཁོ་བཞིན་དུ་རངས་པར་བྱེད་ནུས་ཀྱང་སྲིད་དོ་སྙམ་དུ་
msgctxt "line 25, 04.txt"
msgid ""
"དེ་དག་ ནི་ རྫུ་འཕྲུལ་ ཆེ་ ཞིང་ མཐུ་ ཆེ་བ་ ཡིན་པ␣ས །  དེ་དག་ གིས་ བདག་ གི་ "
"བུ␣འི་ རྒྱབ་ སྔ་ མཁོ་ བཞིན་ དུ་ རངས་པ␣ར་ བྱེད་ ནུས་ ཀྱང་ སྲིད་ དོ་ སྙམ་ དུ་"
msgstr ""

