msgid "དེ་ ཁྲོ␣ས་ ནས་"
msgstr ""

msgctxt "beab8ad8ebd149909d90e4c97e13d8dc"
msgid ""
"ཕུ་བོ␣འི་ རྐེད་པ་ བཅག་པ␣འི་ ལ␣ས་ དེ␣འི་ རྣམ་པ␣ར་ སྨིན་པ␣ས་ ནི་ གང་དང་གང་ དུ་"
" སྐྱེས་པ་ དེ་ དང་ དེ␣ར་ རྐེད་པ་ ཆག་པ␣ར་ གྱུར་ ཏོ ། །"
msgstr ""

msgctxt "a5ba9747186f4f768182959ee0a243c8"
msgid "དེ␣ས་ དེ␣ར་ སྨོན་ལམ་ བཏབ་ སྟེ །"
msgstr ""

# ཇི་<«ཞོལ་»ཅི།>སྟེ་བདག་ལ་ལས་དེའི་<«པེ་»དེ།>འབྲས་<«གཡུང་»འབྲུས།>བུ་རྣམ་<«གཡུང་»རྣམས།>པར་སྨིན་<«གཡུང་»སྨིམ།>པར་གྱུར་ན་ཡང་<«གཡུང་»+ད།>བདག་གི་ལུས་དང་སེམས་ཀྱི་གནོད་པ་དེ་བཅོམ་ལྡན་འདས་ཀྱིས་བསལ་བར་གྱུར་ཅིག་ཅེས་
msgctxt "65373ccfd99b4b1aafd80ee58d0e0d03"
msgid ""
"ཇི་སྟེ་ བདག་ ལ་ལ␣ས་ དེ␣འི་ འབྲས་བུ་ རྣམ་པ␣ར་ སྨིན་པ␣ར་ གྱུར་ ན་ ཡང་ བདག་ གི་"
" ལུས་ དང་སེམས་ ཀྱི་ གནོད་པ་ དེ་ བཅོམ་ལྡན་འདས་ ཀྱིས་ བསལ་བ␣ར་ གྱུར་ཅིག་ ཅེས་"
msgstr ""

# བྱས་པ་དེས་ན་ད་ལྟར་ངས་འདི་གནོད་པ་གཉི་ག་<«གཡུང་»«པེ་»གཉིས་ལས་ག
# «སྣར་»«ཞོལ་»གཉིས་ཀ>ལས་ཡོངས་སུ་བསྐྱབས་སོ། །
msgctxt "9465510bee374e04b615b4140c1c4317"
msgid ""
"བྱས་པ་ དེས་ན་ ད་ལྟར་ ངས་ འདི་ གནོད་པ་ གཉི་ག་ ལ␣ས་ ཡོངས་སུ་ བསྐྱབས་ སོ ། །"
msgstr ""

msgctxt "9f2a3955c7dc49a0aac53d397cd86113"
msgid "དེ␣ས་ དེ␣ར་ སྨོན་ལམ་ བཏབ་ སྟེ །"
msgstr ""

msgctxt "21c0eedde83a41408375d222469d8b01"
msgid ""
"བདག་ གང་དང་གང་ དུ་ སྐྱེ་བ་ དེ་ དང་ དེ␣ར་ ཕྱུག་ ཅིང་ ནོར་ མང་ ལ་ ལོངས་སྤྱོད་ "
"ཆེ་བ␣འི་ རིགས་ སུ་ སྐྱེ་བ␣ར་ གྱུར་ཅིག  །"
msgstr ""

# ཡང་དག་པར་རྫོགས་པའི་སངས་རྒྱས་འོད་སྲུང་གིས་བྲམ་ཟེའི་ཁྱེའུ་བླ་མ་ལུང་བསྟན་པ་གང་ཡིན་པའི་སངས་རྒྱས་བཅོམ་ལྡན་འདས་དེ་བདག་གིས་མཉེས་པར་བྱེད་པར་<«གཡུང་»«པེ་»–བྱེད་པར།>གྱུར་ཅིག
# །
msgctxt "a42656382fe044e7a33affb8a58124e2"
msgid ""
"ཡང་དག་པ␣ར་ རྫོགས་པ␣འི་ སངས་རྒྱས་ འོད་སྲུང་ གིས་ བྲམ་ཟེ␣འི་ ཁྱེའུ་ བླ་མ་ "
"ལུང་བསྟན་པ་ གང་ཡིན་པ␣འི་ སངས་རྒྱས་ བཅོམ་ལྡན་འདས་ དེ་ བདག་ གིས་ མཉེས་པ␣ར་ "
"བྱེད་པ␣ར་ གྱུར་ཅིག  །"
msgstr ""

# མི་མཉེས་པར་<«གཡུང་»«ལི་»«པེ་»«ཅོ་»པ།>བྱེད་པར་མ་གྱུར་ཅིག །
msgctxt "a76a01eaa1d94ab3a1a8802b2dcd7593"
msgid "མི་ མཉེས་པ␣ར་ བྱེད་པ␣ར་ མ་ གྱུར་ཅིག  །"
msgstr ""

msgctxt "c42b54f751fb47faaaf23dd6076e2fb0"
msgid "དེ་ ཁོ་ན␣འི་ བསྟན་པ་ ལ་ རབ་ཏུ་ བྱུང་ ནས །"
msgstr ""

msgctxt "0b24ed3db4fa4e258bc4ea68d894176f"
msgid "ཉོན་མོངས་པ་ ཐམས་ཅད་ སྤངས་ ཏེ །"
msgstr ""

# དགྲ་བཅོམ་པ་ཉིད་མངོན་སུམ་དུ་<«གཡུང་»«པེ་»–དུ།>བྱེད་པར་གྱུར་ཅིག་ཅེས་
msgctxt "c63b15dde75649d8a736239b489cb88a"
msgid "དགྲ་བཅོམ་པ་ ཉིད་ མངོན་སུམ་ དུ་ བྱེད་པ␣ར་ གྱུར་ཅིག་ ཅེས་"
msgstr ""

msgctxt "e3ee963c83aa4414bde6e408a82eaa7e"
msgid ""
"བྱས་པ␣འི་ ལ␣ས་ དེ␣འི་ རྣམ་པ␣ར་ སྨིན་པ␣ས་ ནི །  འདི་ གང་དང་གང་ དུ་ སྐྱེས་པ་ "
"དེ་ དང་ དེ␣ར་ ཕྱུག་ ཅིང་ ནོར་ མང་ ལ་ ལོངས་སྤྱོད་ ཆེ་བ␣འི་ རིགས་ སུ་ སྐྱེས་ "
"ཏེ །"
msgstr ""

msgctxt "c102a25d76af400cb1ab8dcd3d02d760"
msgid ""
"དགེ་སློང་ དག་ ང་ ཡང་དག་པ␣ར་ རྫོགས་པ␣འི་ སངས་རྒྱས་ འོད་སྲུང་ དང་ ཤིན་ཏུ་ "
"མཉམ་པ་ དང་ ཤུགས་ མཉམ་པ་ དང་ མཛད་པ་ མཉམ་པ་ དང་ ཐབས་ མཉམ་པ་ བརྙེས་པ␣ར་ གྱུར་ "
"ནས །"
msgstr ""

msgctxt "2d8fba5ae258456c9dbd0dcd246352ec"
msgid "འདི␣ས་ ང་ མཉེས་པ␣ར་ བྱས་ ཏེ །"
msgstr ""

msgctxt "794cee38d6df4cf1b66dc102ca4c9f1f"
