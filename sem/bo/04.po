msgctxt "8d6587bb86cc42c7a7df10d36519f881"
msgid "དེ་ནས་ ཕྱི་ ཞིག་ ན་ ཁྱེའུ་ དེ␣འི་ བློ་ སྐྱེས་པ །"
msgstr ""

# བཅོམ་ལྡན་འདས་ཀྱིས་བདག་གི་སྡུག་བསྔལ་དང་ཡིད་མི་བདེ་བ་རྣམ་པ་དུ་མ་<«གཡུང་»«པེ་»–མ།>ནི་བསལ<«གཡུང་»«པེ་»གསལ།>།
msgctxt "b9c312e3cb4e496ea2366645c257d9f6"
msgid ""
"བཅོམ་ལྡན་འདས་ ཀྱིས་ བདག་ གི་ སྡུག་བསྔལ་ དང་ ཡིད་ མི་ བདེ་བ་ རྣམ་པ་ དུ་མ་ ནི་"
" བསལ །"
msgstr ""

# བདེ་བ་དང་ཡིད་བདེ་བ་<«གཡུང་»–བ།>རྣམ་པ་དུ་མ་ནི་བསྒྲུབས།
msgctxt "eac0b6723cc641e18711924c5795b9b7"
msgid "བདེ་བ་ དང་ ཡིད་ བདེ་བ་ རྣམ་པ་ དུ་མ་ ནི་ བསྒྲུབས །"
msgstr ""

# སྡིག་པ་མི་དགེ་བའི་ཆོས་རྣམ་པ་དུ་མ་ནི་བསལ<«གཡུང་»«པེ་»གསལ།>།
msgctxt "c06846b89ccc4e008293838cf415faa8"
msgid "སྡིག་པ་ མི་དགེ་བ␣འི་ ཆོས་ རྣམ་པ་ དུ་མ་ ནི་ བསལ །"
msgstr ""

# དགེ་བའི་ཆོས་རྣམ་པ་དུ་མ་ནི་བསྒྲུབས་ཀྱིས་<«ཞོལ་»ཀྱི།>
msgctxt "1f8de7480ca8407698eeadee5e49a565"
msgid "དགེ་བ␣འི་ ཆོས་ རྣམ་པ་ དུ་མ་ ནི་ བསྒྲུབས་ ཀྱིས་"
msgstr ""

msgctxt "549afc9baa034c888067bbafdbffbd82"
msgid "བདག་ གིས་ ཁྱིམ་ ན་ གནས་པ་ བཏང་ ལ་"
msgstr ""

msgctxt "c6aeb82b248b43cb979f8fa8474d8604"
msgid ""
"བཅོམ་ལྡན་འདས་ ཀྱི་ ཐད་ དུ་ ཚངས་པ␣ར་ སྤྱོད་པ་ སྤྱད་ དུ་ འགྲོ␣འོ་ སྙམ་ དུ་"
msgstr ""

# བསམས་ཏེ་<«པེ་»བསམས་སྟ། «ཞོལ་»བསམ་སྟེ།>
msgctxt "e3524f6fbd9547cea02192be989f5dbd"
