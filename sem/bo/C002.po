msgctxt "f658216f85654d028a64fa788ab6c58e"
msgid "དེ་ནས་ དེ་ ཐོས་ མ་ ཐག་ ཏུ་ ཚོང་པ་ ལྔ་ བརྒྱ་ འདུས་ ནས་"
msgstr ""

# དེ་དག་གིས་རྔ་མོ་དང་བ་ལང་དང་བོང་བུ་དག་གིས་ཟོང་བསྐྱས་<«ཅོ་»བསྐྱེས།>ཏེ།
msgctxt "f88498b5a42345a8b85d9eab8c5f314b"
msgid "དེ་དག་ གིས་ རྔ་མོ་ དང་བ་ ལང་ དང་ བོང་བུ་ དག་ གིས་ ཟོང་ བསྐྱས་ ཏེ །"
msgstr ""

# གྲོང་:དང་གྲོང་<«གཡུང་»ད་བགྲོ།
# «པེ་»ང་བགྲོ།>རྡལ་དང་གྲོང་ཁྱེར་དང་ནགས་ཁྲོད་ཀྱི་གྲོང་<«སྣར་»«ཞོལ་»ཁྲོད།>དང་ཡུལ་པོ་ཆེ་རྣམས་སུ་རྒྱུས་ནས་
msgctxt "242461fd8a15473ab34a102a1351838d"
msgid ""
"གྲོང་ དང་ གྲོང་རྡལ་ དང་ གྲོང་ཁྱེར་ དང་ ནགས་ཁྲོད་ ཀྱི་ གྲོང་ དང་ ཡུལ་པོ་ ཆེ་ "
"རྣམས་ སུ་ རྒྱུས་ ནས་"
msgstr ""

# རྒྱ་མཚོ་ཆེན་པོའི་འགྲམ་<«གཡུང་»འགྲེམ།>དུ་ཕྱིན་ཏེ།
msgctxt "88cb756347db472184982ec62e9ee296"
msgid "རྒྱ་མཚོ་ ཆེན་པོ␣འི་ འགྲམ་ དུ་ ཕྱིན་ ཏེ །"
msgstr ""

# :ཀཱར་ཥཱ་པ་ཎ་<«གཡུང་»ཀར་ཤ་པ་ཎེ། «མི་»«པེ་»«སྣར་»«ཅོ་»ཀར་ཥ་པ་ཎ།
# «ཞོལ་»ཀརྟ་པ་ན།>ལྔ་བརྒྱས་གྲུའི་གླ་<«ཅོ་»བླ།>བྱས་ནས་
msgctxt "9af84870848a4de893be0c397f5229a2"
msgid "ཀཱར་ ཥཱ་པ་ ཎ་ ལྔ་ བརྒྱ␣ས་ གྲུ␣འི་ གླ་ བྱས་ ནས་"
msgstr ""

# ཞོ་ཤས་འཚོ་བ་རྣམ་པ་ལྔ་སྟེ།
# འཚོ་བ་སྦྱོར་བ་<«གཡུང་»-བ།>དང་ནན་ཐུར་<«ཞོལ་»ཏུར།>བྱེད་པ་དང་ཞལ་ཏ་བྱེད་པ་དང་འཕྱག་པ་དང་དྲོད་པ་རྣམས་ཀྱང་ཁྲིད་དོ།
# །
msgctxt "8cf83d445b1e4dbdbd1c8e38d1c6d483"
msgid ""
"ཞོ་ཤས་འཚོ་བ་ རྣམ་པ་ ལྔ་ སྟེ །  འཚོ་བ་ སྦྱོར་བ་ དང་ ནན་ ཐུར་ བྱེད་པ་ དང་ "
"ཞལ་ཏ་ བྱེད་པ་ དང་ འཕྱག་པ་ དང་ དྲོད་པ་ རྣམས་ ཀྱང་ ཁྲིད་ དོ ། །"
msgstr ""

msgctxt "fa1a52df44894805a7ce6fdbdbd92c50"
msgid "དེ་ ལ་ འཚོ་བ་ སྦྱོར་བ་ གང་ཞེ་ན །"
msgstr ""

# དེའི་ནང་དུ་ཟས་དང་སྐོམ་:སྦྱོར་བ་<«གཡུང་»སྦྱོང་བ། «པེ་»སྤྱོད་པ།>གང་ཡིན་པའོ། །
msgctxt "52f6c52029a4430ba8784f517270faad"
msgid "དེ␣འི་ ནང་ དུ་ ཟས་ དང་ སྐོམ་ སྦྱོར་བ་ གང་ཡིན་པ␣འོ ། །"
