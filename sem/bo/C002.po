msgctxt "7eeec01892104e28b73821350319c5e2"
msgid "རླུང་ མཐུ་ཆེན་པོ␣ས་ ནི་ ཁྱེར་ ནས་"
msgstr ""

msgctxt "8fc6f372e2984d399ca8e6e5d8aa0d76"
msgid "སྤྲིན་ཆེན་པོ་ བཞིན་ དུ་ ཆ␣ས་ ཏེ །"
msgstr ""

# རླུང་དྲང་<«གཡུང་»«པེ་»དྲག>པོར་འགྲོ་བས་རིན་པོ་ཆེའི་གླིང་དུ་[103a]ཕྱིན་པར་བྱས་སོ།
# །
msgctxt "ef3df80747924d90abfcecff6862d8d6"
msgid ""
"རླུང་ དྲང་པོ␣ར་ འགྲོ་བ␣ས་ རིན་པོ་ཆེ␣འི་ གླིང་ དུ་ ཕྱིན་པ␣ར་ བྱས་ སོ ། །"
msgstr ""

msgctxt "7269c10c250d4f09a5b7fe4092d8cfa5"
msgid "དེ་ནས་ དྲོད་པ␣ས་ སྒྲ་ བསྒྲགས་པ །"
msgstr ""

# ཀྱེ་:འཛམ་བུའི་<«སྣར་»«ཞོལ་»ཛམྦུའི།>གླིང་པའི་ཚོང་པ་རྣམས་རིན་པོ་ཆེའི་གླིང་འདི་ན་<«གཡུང་»«པེ་»ནི།>མཆིང་བུ་ནོར་བུ་<«གཡུང་»-ནོར་བུ།>རིན་པོ་ཆེ་འདྲ་བ་དག་ཡོད་དེ།
msgctxt "30eba7fbc67e4855a965838603cc5277"
msgid ""
"ཀྱེ་ འཛམ་བུ␣འི་ གླིང་པ␣འི་ ཚོང་པ་ རྣམས་ རིན་པོ་ཆེ␣འི་ གླིང་ འདི་ ན་ མཆིང་བུ་"
" ནོར་བུ་ རིན་པོ་ཆེ་ འདྲ་བ་ དག་ ཡོད་ དེ །"
msgstr ""

msgctxt "e21068db6b5a47cf91696b9662aed2a4"
msgid "དེ་དག་ ཁྱེད་ ཀྱིས་ བརྟག་ ཅིང་ བརྟག་ ཅིང་ བླང་བ␣ར་ གྱིས་ ཤིག  །"
msgstr ""

# ཁྱེད་:འཛམ་བུའི་<«སྣར་»«ཞོལ་»ཛམྦུའི།>གླིང་དུ་སོན་པའི་འོག་ཏུ་འགྱོད་པར་གྱུར་ཏ་<«གཡུང་»ཏེ།>རེ།
msgctxt "cca8af7fd00a42f8ac67ce9e0755abf6"
msgid "ཁྱེད་ འཛམ་བུ␣འི་ གླིང་ དུ་ སོན་པ␣འི་ འོག་ ཏུ་ འགྱོད་པ␣ར་ གྱུར་ ཏ་ རེ །"
msgstr ""

# རིན་པོ་ཆེའི་གླིང་འདི་ཉིད་ན་སྲིན་<«ཅོ་»གྲིན།>མོ་ཁྲུང་ཁྲུང་གཞོན་ནུ་མ་ཞེས་བྱ་བ་དག་ཀྱང་གནས་ཏེ།
msgctxt "e6edba0813c1429d8493102544323228"
msgid ""
"རིན་པོ་ཆེ␣འི་ གླིང་ འདི་ཉིད་ ན་ སྲིན་མོ་ ཁྲུང་ ཁྲུང་ གཞོན་ནུ་ མ་ ཞེས་ བྱ་བ་ "
"དག་ ཀྱང་ གནས་ ཏེ །"
msgstr ""

msgctxt "e90c848ddd984fa8bbb17869a03731b5"
msgid "ཐབས་ ཇི་ལྟ་བུ␣ས་ ཀྱང་ སྐྱེས་པ་ བྲིད་ ནས་"
msgstr ""

# འདི་<«གཡུང་»འདྲི།>ཉིད་དུ་མ་:རུངས་པར་<«གཡུང་»མ་རུང་།
# «ལི་»«པེ་»«སྣར་»«ཅོ་»མི་རུང་བར། «ཞོལ་»མ་རུང་བར།>བྱེད་ཅིང་
msgctxt "9c275daf2c4a4d5baa3fc8bc3713cf5a"
msgid "འདི་ཉིད་ དུ་མ་ རུངས་པ␣ར་ བྱེད་ ཅིང་"
msgstr ""

msgctxt "eec09bd1140d41349e1e4d16a85f61fc"
msgid "སྡུག་བསྔལ་ བར་ བྱེད་ དོ ། །"
msgstr ""

# རིན་པོ་ཆེའི་གླིང་འདི་ཉིད་ན་མྱོས་པར་བྱེད་པའི་ཤིང་ཏོག་<«གཡུང་»རྟོག>དག་ཡོད་དེ།
msgctxt "8e82060e9c444f2fabe0f45d9c7baa65"
msgid ""
"རིན་པོ་ཆེ␣འི་ གླིང་ འདི་ཉིད་ ན་ མྱོས་པ␣ར་ བྱེད་པ␣འི་ ཤིང་ཏོག་ དག་ ཡོད་ དེ །"
msgstr ""

msgctxt "14ae383ac6384722a953feb0573336e1"
msgid "དེ་དག་ སུ␣ས་ ཟོས་པ་ དེ་ཞག་ བདུན་ གྱི་ བར་ དུ་ བརྒྱལ་ ཏེ་"
msgstr ""

# འདུག་པར་འགྱུར་གྱིས་<«ཞོལ་»གྱི།>
msgctxt "06af3884d3ab4c029bc2d64c088d0ad3"
msgid "འདུག་པ␣ར་ འགྱུར་ གྱིས་"
msgstr ""

msgctxt "3bd069877de74eef804ea3632cd56105"
msgid "དེ་དག་ ཁྱེད་ མ་ ཟ་ ཞིག  །"
msgstr ""

# རིན་པོ་ཆེའི་གླིང་འདི་ཉིད་:ན་མི་མ་<«གཡུང་»ནམ།>ཡིན་པ་དག་ཀྱང་གནས་ཏེ།
msgctxt "977e768f23ee42869513be08f7719552"
msgid "རིན་པོ་ཆེ␣འི་ གླིང་ འདི་ཉིད་ ན་ མི་མ་ཡིན་པ་ དག་ ཀྱང་ གནས་ ཏེ །"
msgstr ""

msgctxt "572c63d7e11549de8117ecb3ff66d792"
msgid "དེ་དག་ ནི་ གཞན་ ཞག་ བདུན་ གྱི་ བར་ དུ་ འདུག་པ␣ར་ ནི་ བཟོད་ ཀྱི །"
msgstr ""

msgctxt "766df5fe326149269d0e2ed1c8e2cf09"
msgid "ཞག་ བདུན་ ལ␣ས་ འདས་ ནས་"
msgstr ""

# དོན་གང་བསྒྲུབས་<«གཡུང་»«པེ་»བསྒྲུབ།>པ་དེ་དག་མ་ཟིན་པར་རླུང་གང་གིས་གྲུ་ཁྱེར་བར་བྱེད་པ་དེ་ལྟ་བུའི་རླུང་གཏོང་བར་བྱེད་དོ་ཞེས་
msgctxt "1765818c65b54bc2bfdeb32d80e1bc09"
msgid ""
"དོན་ གང་ བསྒྲུབས་པ་ དེ་དག་ མ་ཟིན་པ␣ར་ རླུང་ གང་ གིས་ གྲུ་ ཁྱེར་བ␣ར་ བྱེད་པ་ "
"དེ་ལྟ་ བུ␣འི་ རླུང་ གཏོང་བ␣ར་ བྱེད་ དོ་ ཞེས་"
msgstr ""

# བསྒོ་<«ཞོལ་»བསྒོས།>ནས།
msgctxt "bd403bbe7ed3413e9ed8f3240e6d111c"
msgid "བསྒོ་ ནས །"
msgstr ""

# དེ་ཐོས་པས་ཚོང་པ་རྣམས་ཀྱིས་བསྒྲིམས་<«གཡུང་»བསྒྲིབས། «པེ་»མསྒྲིམས།>ཏེ་
msgctxt "e5d5ddfeb6794e3eb5b0e9e287bdc4ef"
msgid "དེ་ ཐོས་པ␣ས་ ཚོང་པ་ རྣམས་ ཀྱིས་ བསྒྲིམས་ ཏེ་"
msgstr ""

msgctxt "2d694ab8f3a34c638ad305699b17c9a8"
msgid "བག་ ཡོད་པ␣ར་ འདུག་ ནས །"
msgstr ""

msgctxt "5a7f121da340478eb03c7799cae9649a"
msgid "དེ་དག་ གིས་ རིན་པོ་ཆེ་ རྣམས་ བརྟག་ ཅིང་ བརྟག་ ཅིང་ གྲུ་ དེ་ བཀང་ སྟེ །"
msgstr ""

msgctxt "8e2e4d421eba479b86a8faf29577cd3e"
msgid "དཔེར་ན །"
msgstr ""

msgctxt "9ee396a96f394a4ab38734f4f60f4c3d"
msgid ""
"ཏིལ་ དང་ །  འབྲས་ དང་ །  བ་ ད་ར␣འི་ འབྲས་བུ་ དང་ རྒྱ་ སྲན་ གྱིས་ གང་བ་ བཞིན་"
" དུ་ བྱས་ ཏེ །"
msgstr ""

# དེ་དག་རླུང་དྲང་པོས་:འཛམ་བུའི་<«སྣར་»«ཞོལ་»ཛམྦུའི།>གླིང་དུ་ཕྱིན་པར་བྱས་སོ། །
msgctxt "8450747cf2ee412ba0ddfa0f7c5f3227"
msgid "དེ་དག་ རླུང་ དྲང་པོ␣ས་ འཛམ་བུ␣འི་ གླིང་ དུ་ ཕྱིན་པ␣ར་ བྱས་ སོ ། །"
msgstr ""

msgctxt "f268b715f0ca4171b45b747f5f59831a"
msgid ""
"དེ␣ས་ ཐབས་ དེ་ལྟ་ བུར་ རྒྱ་མཚོ␣འི་ གྲུ␣འི་ དོན་ ལན་ དྲུག་ གི་ བར་ དུ་ "
"བསྒྲུབས་ ཏེ །"
msgstr ""

msgctxt "6553b9e133d34376adfebb3733e863a9"
msgid "བྰ་ རཱ་ ཎ་ སཱིར་ འོངས་ ནས་"
msgstr ""

# སྦྱིན་པ་དག་སྦྱིན་<«སྣར་»«ཞོལ་»བྱིན།>པ་དང་བསོད་ནམས་དག་བྱེད་པ་ལ་<«པེ་»-ལ།>ཞུགས་སོ།
# །
msgctxt "befb2ebca4e040c38568755fd964be44"
msgid "སྦྱིན་པ་ དག་ སྦྱིན་པ་ དང་ བསོད་ནམས་ དག་བྱེད་པ་ ལ་ ཞུགས་ སོ ། །"
msgstr ""

msgctxt "bbd2d8ef59614753816c77ab15457228"
msgid "དེ␣འི་ ཚེ་ དེ་ མ་ཡིན་པ␣འི་ གྲོང་ཁྱེར་ ལྔ་ ན་ དེད་དཔོན་ ལྔ་ གནས་ ཏེ །"
msgstr ""

msgctxt "e07a5ca2936f4be4ad850b906933f676"
msgid "དེ་དག་ ནི་ སོང་ སྟེ་ བོར་ ཡང་"
msgstr ""

# གྲུ་རྣམས་མ་:རུངས་པར་<«གཡུང་»རླུང་བར།
# «ལི་»«པེ་»«སྣར་»«ཅོ་»«ཞོལ་»རུང་བར།>གྱུར་ནས་
msgctxt "9e0919d9ae7a44878047731d78e5e4ce"
msgid "གྲུ་ རྣམས་ མ་རུངས་པ␣ར་ གྱུར་ ནས་"
msgstr ""

# དེ་དག་གིས་<«ཅོ་»གི།>བསམས་པ།
msgctxt "c1cb6dffb78b44c8909722dd81a1e60c"
msgid "དེ་དག་ གིས་ བསམས་པ །"
msgstr ""

# སེམས་ཅན་བསོད<«གཡུང་»«པེ་»བསོན།>་ནམས་ཀྱིས་ཆེ་བར་གྲགས་པ་གཅིག་གི་མཐུས་སེམས་ཅན་མང་པོ་བདེ་བར་འགྱུར་ལ།
msgctxt "11d55a28c36f44b0be6eac85d86b1ec7"
msgid ""
"སེམས་ཅན་ བསོད་ནམས་ ཀྱིས་ ཆེ་བ␣ར་ གྲགས་པ་ གཅིག་ གི་ མཐུ␣ས་ སེམས་ཅན་ མང་པོ་ "
"བདེ་བ␣ར་ འགྱུར་ ལ །"
msgstr ""

msgctxt "a8ef93417eff4f2bb5c05bbe8cd70dc1"
msgid ""
"དེད་དཔོན་ ཆེན་པོ་ དབྱིག་ དགའ་ དེ་ ནི །  བསོད་ནམས་ ཀྱིས་ ཆེ་བ␣ར་ གྲགས་པ་ ཡིན་"
" ཏེ །"
msgstr ""

# དེས་རྒྱ་མཚོའི་:གྲུའི་དོན་<«གཡུང་»«པེ་»གྲུལ་ན། «ཅོ་»གྲུའི་གྲུ།
# «ལི་»«སྣར་»«ཅོ་»གྲུ།>ལན་དྲུག་གི་བར་དུ་བསྒྲུབས་ཀྱིས<«ཞོལ་»ཀྱི།>།
msgctxt "70d8dc45c10247bcaaf63f0f5550f555"
msgid "དེ␣ས་ རྒྱ་མཚོ␣འི་ གྲུ␣འི་ དོན་ ལན་ དྲུག་ གི་ བར་ དུ་ བསྒྲུབས་ ཀྱིས །"
msgstr ""

# གལ་ཏེ་བདག་ཅག་གིས་དེ་སྤྲོ་བ་བསྐྱེད་པར་ནུས་ན་ནི།
# དེའི་མཐུས་བདག་ཅག་གིས་ནོར་བསྒྲུབ་<«གཡུང་»«པེ་»བསྒྲུབས།
# «ཞོལ་»སྒྲུབ།>པར་ནུས་པར་འགྱུར་རོ་སྙམ་སྟེ།
msgctxt "60d8cd465a9440ce92b9ec1b7625cb32"
msgid ""
"གལ་ཏེ་ བདག་ཅག་ གིས་ དེ་ སྤྲོ་བ་ བསྐྱེད་པ␣ར་ ནུས་ ན་ ནི །  དེ␣འི་ མཐུ␣ས་ "
"བདག་ཅག་ གིས་ ནོར་ བསྒྲུབ་པ␣ར་ ནུས་པ␣ར་ འགྱུར་ རོ་ སྙམ་ སྟེ །"
msgstr ""

msgctxt "523381e95132422884b27325b713f9ba"
msgid "ལྷན་ཅིག་ ཏུ་ འདུས་ ནས་"
msgstr ""

msgctxt "e0eec7b666f348b19aafad2a4aa398c5"
msgid "བཱ་ སཱིར་ སོང་ སྟེ་"
msgstr ""

msgctxt "d9e7cb25ebd04c12b70c4f3de276d3a9"
msgid "དེད་དཔོན་ དབྱིག་ དག␣འི་ ཐད་ དུ་ སོང་ ནས་"
msgstr ""

msgctxt "292aaafb7d4f41dc878dc4c18d0dd8d8"
msgid "སྨྲས་པ །"
msgstr ""

msgctxt "7f7a70aa1c1444a49ba40c7278cfa12f"
msgid "དེད་དཔོན་ བདག་ཅག་ སོང་ སྟེ་ བོར་ ཡང་"
msgstr ""

# ནོར་གྱི་གྲུ་རྣམས་མ་:རུངས་པར་<«གཡུང་»«པེ་»རུང་པར།
# «ལི་»«སྣར་»«ཅོ་»«ཞོལ་»རུང་བར།>གྱུར་ཏེ།
msgctxt "5a19cfcda12c41899c88337fefd90261"
msgid "ནོར་ གྱི་ གྲུ་ རྣམས་ མ་རུངས་པ␣ར་ གྱུར་ ཏེ །"
msgstr ""

# ཁྱོད་ནི་བསོད་ནམས་ཀྱིས་ཆེ་བར་གྲགས་པ་ལགས་ཀྱིས<«ཞོལ་»ཀྱི།>།
msgctxt "cc3187e2f8a445909a0543c20a54ba2f"
msgid "ཁྱོད་ ནི་ བསོད་ནམས་ ཀྱིས་ ཆེ་བ␣ར་ གྲགས་པ་ ལགས་ ཀྱིས །"
msgstr ""

msgctxt "f1397a53d02949d4b129b44c31d3ab20"
msgid ""
"བདག་ཅག་ ལ་ ཐུགས་ བརྩེ་བ␣འི་ སླད་ དུ །  རྒྱ་མཚོ་ ཆེན་པོ␣འི་ ནང་ དུ་ བཞུད་ དང་"
msgstr ""

# ཁྱོད་ཀྱི་<«ཅོ་»ཀྱིས།>མཐུས་བདག་ཅག་གིས་ནོར་བསྒྲུབ་<«ཅོ་»བསྒྲུབས།
# «ཞོལ་»སྒྲུབ།>ནུས་པར་འགྱུར་རོ། །
msgctxt "911023023ad64bdabca646645deef21b"
msgid "ཁྱོད་ ཀྱི་ མཐུ␣ས་ བདག་ཅག་ གིས་ ནོར་ བསྒྲུབ་ ནུས་པ␣ར་ འགྱུར་ རོ ། །"
msgstr ""

msgctxt "bc27f92618624c8093eb0b3ea2f78ae6"
msgid "དེ␣ས་ སྨྲས་པ །"
msgstr ""

# ཀྱེ་རྒྱ་མཚོ་ཆེན་པོའི་ཆོས་ཀྱིས་ནི། གྲུ་བདུན་པ་མ་:རུངས་པར་<«གཡུང་»«པེ་»རུང་པར།
# «ལི་»«སྣར་»«ཅོ་»«ཞོལ་»རུང་བར།>འགྱུར་ཏེ།
msgctxt "eaef161302fb46fea179fb4d91fc4ea8"
msgid ""
"ཀྱེ་ རྒྱ་མཚོ་ ཆེན་པོ␣འི་ ཆོས་ ཀྱིས་ ནི །  གྲུ་ བདུན་པ་ མ་རུངས་པ␣ར་ འགྱུར་ ཏེ"
" །"
msgstr ""

# སྔན་<«ཁུ་»«ཞོལ་»སྔོན།>ཆད་གང་གིས་ཀྱང་རྒྱ་མཚོའི་<«གཡུང་»+ན།>གྲུའི་དོན་ལན་བདུན་གྲུབ་པ་སུ་ཡང་མེད་དོ།
# །
msgctxt "69447fef9e87424eb43a24c8987a8b6b"
msgid ""
"སྔན་ ཆད་ གང་ གིས་ ཀྱང་ རྒྱ་མཚོ␣འི་ གྲུ␣འི་ དོན་ ལན་ བདུན་ གྲུབ་པ་ སུ་ ཡང་ "
"མེད་ དོ ། །"
msgstr ""

# འོན་ཀྱང་ཁྱེད་<«ཅོ་»ཁྱོད།>ཀྱིས་བསམས་པ་ཡོངས་སུ་བསྐང་བར་:བྱ་སྟེ<«གཡུང་»«ལི་»«པེ་»«སྣར་»«ཅོ་»བྱས་ཏེ།>།
msgctxt "e81432904af7444193066a6c891ba381"
msgid "འོན་ཀྱང་ ཁྱེད་ ཀྱིས་ བསམས་པ་ ཡོངས་སུ་ བསྐང་བ␣ར་ བྱ་ སྟེ །"
msgstr ""

# ཁྱེད་ཁོ་ན་ལ་སྙིང་བརྩེ་བའི་ཕྱིར།
# ལན་བདུན་དུ་<«གཡུང་»«ལི་»«པེ་»«སྣར་»«ཅོ་»«ཞོལ་»-དུ།>རྒྱ་མཚོའི་<«གཡུང་»«པེ་»མཚོ།>ནང་དུ་འགྲོ་བར་བྱའོ་ཞེས་
msgctxt "abd046c949054bf4b411888fa33ac9f5"
msgid ""
"ཁྱེད་ ཁོ་ན་ ལ་ སྙིང་བརྩེ་བ␣འི་ ཕྱིར །  ལན་ བདུན་ དུ་ རྒྱ་མཚོ␣འི་ ནང་ དུ་ "
"འགྲོ་བ␣ར་ བྱ␣འོ་ ཞེས་"
msgstr ""

msgctxt "f4588d07afea40938ca5597c4d9d2dad"
msgid "སྨྲ␣ས་ ནས །"
msgstr ""

# དེད་དཔོན་ཆེན་པོ་དབྱིག་དགས་དེད་དཔོན་<«ཅོ་»པོན།>ལྔ་པོ་དེ་དག་ཁྲིད་དེ་
msgctxt "53cb91f260cf45f2b4c8db7e623c0207"
msgid "དེད་དཔོན་ ཆེན་པོ་ དབྱིག་ དག␣ས་ དེད་དཔོན་ ལྔ་པོ་ དེ་དག་ ཁྲིད་ དེ་"
msgstr ""

msgctxt "e24695ad42184a4f9149f3abdfb5803b"
msgid "རྒྱ་མཚོ་ ཆེན་པོ␣འི་ ནང་ དུ་ སོང་བ་ ལ␣ས་"
msgstr ""

msgctxt "29126b5ce56e400da291628806834cc6"
msgid "དེ་ རིན་པོ་ཆེ␣འི་ གླིང་ དུ་ ཕྱིན་ ནས་"
msgstr ""

msgctxt "2e7c39455061425d9366c010dbf14b4f"
msgid "རིན་པོ་ཆེ་ བསྡུས་ ཏེ་"
msgstr ""

# ཡང་:འཛམ་བུའི་<«སྣར་»«ཞོལ་ཛམྦུའི།>གླིང་ལོགས་སུ་གྲུ་བཏང་ངོ་། །
msgctxt "e94130c0ce094bf2be173e622dc5d4ba"
msgid "ཡང་ འཛམ་བུ␣འི་ གླིང་ ལོགས་ སུ་ གྲུ་ བཏང་ ངོ་ ། །"
msgstr ""

# དེ་ནས་དེད་དཔོན་ཆེན་པོ་དབྱིག་དགའ<«གཡུང་»«ལི་»«པེ་»«སྣར་»«ཅོ་»«ཞོལ་»དགས།>་རྒྱ་མཚོ་ཆེན་པོའི་ནང་ན་འདུག་བཞིན་དུ་འདི་སྙམ་དུ་སེམས་ཏེ།
msgctxt "cf54c2b67df146648a4e5ab4afaa7a5c"
msgid ""
"དེ་ནས་ དེད་དཔོན་ ཆེན་པོ་ དབྱིག་ དགའ་ རྒྱ་མཚོ་ ཆེན་པོ␣འི་ ནང་ ན་ འདུག་ བཞིན་ "
"དུ་ འདི་ སྙམ་ དུ་ སེམས་ ཏེ །"
msgstr ""

# བདག་ཅག་རྒྱ་མཚོ་ཆེན་པོའི་ནང་ན་འཁོད་པ་རྣམས་ཉེ་ཞོ་མེད་པ་དང་བདེ་བླག་ཏུ་ཕ་རོལ་ཏུ་སོན་པར་འགྱུར་དུ་མངོན་:པ་མེད་ཀྱིས<«གཡུང་»«པེ་»པར་མེད་ཀྱིས།
# «ཞོལ་»པ་མེད་ཀྱི།>།
msgctxt "4051870650884135a8cbbeab73b482c4"
msgid ""
"བདག་ཅག་ རྒྱ་མཚོ་ ཆེན་པོ␣འི་ ནང་ ན་ འཁོད་པ་ རྣམས་ ཉེ་ཞོ་ མེད་པ་ དང་ བདེ་བླག་ "
"ཏུ་ ཕ་རོལ་ ཏུ་ སོན་པ␣ར་ འགྱུར་ དུ་ མངོན་པ་ མེད་ ཀྱིས །"
msgstr ""

msgctxt "a128e8171799453e874e763b61ab9fe7"
msgid "མ་ལ་"
msgstr ""

msgctxt "e02ab714427248f693b00c3d98ae0538"
msgid "བདག་ གིས་ རིན་པོ་ཆེ་ རིན་ཐང་ ཆེན་པོ་ རྣམས་ ཀྱིས་ སྒྱེའུ་ བཀང་ སྟེ །"
msgstr ""

# རྐེད་པ་ལ་:བཅིང་བར་<«ཅོ་»བཅིངས་པར།>བྱའོ་སྙམ་ནས།
msgctxt "6303f0c8c53d49dcb9bcbd7a8d47d1b3"
msgid "རྐེད་པ་ ལ་ བཅིང་བ␣ར་ བྱ␣འོ་ སྙམ་ ནས །"
msgstr ""

msgctxt "1a3293137e214c3a8c2bf0646e99c391"
msgid "དེ␣ས་ རིན་པོ་ཆེ་ རིན་ཐང་ ཆེན་པོ་ རྣམས་ ཀྱིས་ སྒྱེའུ་ བཀང་ སྟེ །"
msgstr ""

# རྐེད་<«གཡུང་»«པེ་»སྐེད།>པ་ལ་བཅིངས་ནས་
msgctxt "d7d6147bc1004e808db1bec57be922c4"
msgid "རྐེད་པ་ ལ་ བཅིངས་ ནས་"
msgstr ""

msgctxt "0a93df1a30f748c6a2f1f8699ff6b8cf"
msgid "ཚོང་པ་ དེ་དག་ ལ་ སྨྲས་པ །"
msgstr ""

msgctxt "2085a5c948b248fa884c5d3c350c4c29"
msgid "ཀྱེ་"
msgstr ""

# གལ་ཏེ་བརྒྱ་ལ་གྲུ་མ་:རུངས་པར་<«གཡུང་»«ལི་»«པེ་»«སྣར་»«ཅོ་»«ཞོལ་»རུང་བར།>གྱུར་ན།
msgctxt "7d224195476745b582f385eb5aac98c7"
msgid "གལ་ཏེ་ བརྒྱ་ལ་ གྲུ་ མ་རུངས་པ␣ར་ གྱུར་ ན །"
msgstr ""

msgctxt "ce3ed9046137433a976ed15991c60d21"
msgid "ཁྱེད་ ང␣འི་ ལུས་ ལ་ འཇུས་ ཤིག  །"
msgstr ""

# དེ་<«གཡུང་»«པེ་»དེས།>ནས་ཕྱི་ཞིག་ན་ཆུའི་ནང་ན་འདུག་པའི་རིས་དེ་དག་གི་<«གཡུང་»«ལི་»«པེ་»«སྣར་»«ཅོ་»«ཞོལ་»གིས།>གྲུ་བཤིག་ནས།
msgctxt "993651ee309041ada2c986fca82038f2"
msgid ""
"དེ་ནས་ ཕྱི་ ཞིག་ ན་ ཆུ␣འི་ ནང་ ན་ འདུག་པ␣འི་ རིས་ དེ་དག་ གི་ གྲུ་ བཤིག་ ནས །"
msgstr ""

# དེད་དཔོན་ལྔ་པོ་དེ་<«གཡུང་»«ལི་»«པེ་»«སྣར་»«ཞོལ་»-དེ༑>དག་དེད་<«ཅོ་»དེ།>དཔོན་དབྱིག་དགའི་ལུས་ལ་འཇུས་སོ།
# །
msgctxt "6354a11f012f40558fadf9602fa367a4"
msgid "དེད་དཔོན་ ལྔ་པོ་ དེ་དག་ དེད་དཔོན་ དབྱིག་ དག␣འི་ ལུས་ ལ་ འཇུས་ སོ ། །"
msgstr ""

msgctxt "2636c566aabe4ccf8864d6be7ffb6f22"
msgid "དེ་ནས་ དེད་དཔོན་ དབྱིག་ དག␣ས་ བསམས་པ །"
msgstr ""

msgctxt "a3cb964eb3fa4254a0e6892f4161e499"
msgid ""
"བདག་ གསོན་པོ␣ར་ འདུག་ བཞིན་ དུ་ ནི་ འདི་ རྣམས་ རྒྱ་མཚོ་ ཆེན་པོ་ ལ␣ས་ "
"བསྒྲལ་བ␣ར་ མི་ ནུས་ ལ །"
msgstr ""

# རྒྱ་མཚོ་ཆེན་པོ་ནི་ཤི་བའི་རོ་དང་:ལྷན་ཅིག་<«ཅོ་»ལྡན་ཅིག
# «ཞོལ་»ལྷན་གཅིག>གནས་པ་མི་སྲིད་ཅིང་
msgctxt "cfe3a94f7aa848ebbb33f6d3c4d48859"
msgid "རྒྱ་མཚོ་ ཆེན་པོ་ ནི་ ཤི་བ␣འི་ རོ་ དང་ ལྷན་ཅིག་ གནས་པ་ མི་ སྲིད་ ཅིང་"
msgstr ""

msgctxt "6ef774fe901743f0a8a6d726c0fbfc34"
msgid "སྐབས་མེད་ དེ །"
msgstr ""

# དེ་<«གཡུང་»«པེ་»འདི།>ནི་གནས་མ་ཡིན་པས་ན།
msgctxt "1bd8331c40fe4478898a495f25e9dd12"
msgid "དེ་ ནི་ གནས་མ་ ཡིན་པ␣ས་ ན །"
msgstr ""

# བདག་ཤི་བ་དང་།
# རྒྱ་མཚོ་ཆེན་པོ་ལས་མྱུར་དུ་རྒལ་<«གཡུང་»«པེ་»རྒྱལ།>བར་འགྱུར་རོ་སྙམ་ནས།
msgctxt "89e48a7eb1a442c393ce6d804ea1e692"
msgid ""
"བདག་ ཤི་བ་ དང་ །  རྒྱ་མཚོ་ ཆེན་པོ་ ལ␣ས་ མྱུར་ དུ་ རྒལ་བ␣ར་ འགྱུར་ རོ་ སྙམ་ "
"ནས །"
msgstr ""

msgctxt "5de86f4e0fd246cebd5d79f7339d6d20"
msgid "དེད་དཔོན་ རྣམས་ ལ་ སྨྲས་པ །"
msgstr ""

msgctxt "2e3a4559999b4e28b9609017035a5895"
msgid "ཀྱེ་"
msgstr ""

# ཁྱེད་རྣམས་བརྟན་ཞིང་:སྤྲོ་བ་<«གཡུང་»སྤྲོས་པ།
# «པེ་»སྤྱོས་པ།>ཆེར་སྐྱེད་<«གཡུང་»«ལི་»«པེ་»«སྣར་»«ཅོ་»བསྐྱེད།>ཅིག་དང་།
msgctxt "3a497137f5b24120966c4d6a5101de28"
msgid "ཁྱེད་ རྣམས་ བརྟན་ ཞིང་ སྤྲོ་བ་ ཆེ␣ར་ སྐྱེད་ ཅིག་ དང་ །"
msgstr ""

# ངས་ཁྱེད་ཅི་<«ཅོ་»ཅིས།>ནས་ཀྱང་ཉེ་ཞོ་མེད་པར་བདེ་བླག་<«གཡུང་»བྲག>ཏུ་རྒྱ་མཚོ་ཆེན་པོ་ལས་བརྒལ་<«གཡུང་»«པེ་»རྒལ།>བར་བྱའོ།
# །
msgctxt "1547492ac504454d829b47b54da26f94"
msgid ""
"ངས་ ཁྱེད་ ཅི་ ནས་ ཀྱང་ ཉེ་ཞོ་ མེད་པ␣ར་ བདེ་བླག་ ཏུ་ རྒྱ་མཚོ་ ཆེན་པོ་ ལ␣ས་ "
"བརྒལ་བ␣ར་ བྱ␣འོ ། །"
msgstr ""

# བརྒལ་<«གཡུང་»«པེ་»རྒལ།>ཟིན་ནས་ཀྱང་
msgctxt "85c177855d2541fd8ee5f7f3569dd5c5"
msgid "བརྒལ་ ཟིན་ ནས་ ཀྱང་"
msgstr ""

# ངའི་ལུས་མ་དོར་བར་ངས་རིན་པོ་ཆེ་<«གཡུང་»«ལི་»«པེ་»«སྣར་»«ཅོ་»«ཞོལ་»ཆེའི།>རིན་ཐང་ཆེན་པོ་རྣམས་ཀྱིས་སྒྱེའུ་བཀང་སྟེ།
# རྐེད་པ་ལ་བཅིངས་པ་དེ་ཁྱེད་<«གཡུང་»ཁྱོད།>ཀྱིས་བགོ་བཤའ་གྱིས་ལ་
msgctxt "262db4923e5c4d5bbf6a9bf7fd6a4096"
msgid ""
"ང␣འི་ ལུས་ མ་ དོར་བ␣ར་ ངས་ རིན་པོ་ཆེ་ རིན་ཐང་ ཆེན་པོ་ རྣམས་ ཀྱིས་ སྒྱེའུ་ "
"བཀང་ སྟེ །  རྐེད་པ་ ལ་ བཅིངས་པ་ དེ་ ཁྱེད་ ཀྱིས་ བགོ་བཤའ་ གྱིས་ ལ་"
msgstr ""

# ཁྱེར་<«གཡུང་»གྱེར། «ཅོ་»ཁྱེར་ཁྱེར།>ཅིག་དང་།
msgctxt "34ca2a8c6b264d81bc2d19590fc7e5b0"
msgid "ཁྱེར་ ཅིག་ དང་ །"
msgstr ""

# དེས་<«གཡུང་»«ལི་»«པེ་»«སྣར་»«ཅོ་»དེ།>ཁྱེད་ཀྱི་:གཉི་གའི་<«སྣར་«ཞོལ་»གཉིས་ཀའི།>ཕྱོགས་ཀྱི་བདུན་རྒྱུད་འཚོ་བར་འགྱུར་རོ་ཞེས་
msgctxt "5a6ad498ec8d4431ba0d9be18e61cb0b"
msgid ""
"དེ␣ས་ ཁྱེད་ ཀྱི་ གཉི་ག␣འི་ ཕྱོགས་ ཀྱི་ བདུན་ རྒྱུད་ འཚོ་བ␣ར་ འགྱུར་ རོ་ ཞེས་"
msgstr ""

msgctxt "39847c6448be432aa35af09e5022045a"
msgid "སྨྲས་པ་ དང་ །"
msgstr ""

# དེས་བླ་ན་<«གཡུང་»-ན།>མེད་པ་ཡང་དག་པར་རྫོགས་པའི་བྱང་ཆུབ་ཏུ་སྨོན་ལམ་བཏབ་པ།
msgctxt "1d80ba3f777b4c48a30e1613fda8c8c1"
msgid ""
"དེ␣ས་ བླ་ ན་ མེད་པ་ ཡང་དག་པ␣ར་ རྫོགས་པ␣འི་ བྱང་ཆུབ་ ཏུ་ སྨོན་ལམ་ བཏབ་པ །"
msgstr ""

msgctxt "b9b9f0d621594ab0ab412f41d4f7ee2f"
msgid "ཀྱེ་མ་"
msgstr ""

# དགེ་བའི་རྩ་བ་འདིས་ན་<«གཡུང་»«པེ་»ནི།>བདག་འཇིག་རྟེན་ལོང་བ་འདྲེན་པ་མེད་ཅིང་<«གཡུང་»-ཅིང་།>ཡོངས་སུ་འདྲེན་པ་མེད་པ་དང་སྟོན་པ་མེད་པར་དེ་བཞིན་གཤེགས་པ་དགྲ་བཅོམ་པ་ཡང་དག་པར་རྫོགས་པའི་སངས་རྒྱས་རིག་པ་དང་ཞབས་སུ་ལྡན་པ།
# བདེ་བར་གཤེགས་པ། འཇིག་རྟེན་མཁྱེན་པ། སྐྱེས་བུ་:གདུལ་བྱའི་<«གཡུང་»གདུལ་བའི།
# «ཞོལ་»འདུལ་བའི།>ཁ་ལོ་སྒྱུར་<«གཡུང་»རྒྱུར། །>བ། བླ་ན་མེད་པ།
# ལྷ་དང་མི་རྣམས་ཀྱི་སྟོན་པ། སངས་རྒྱས་བཅོམ་ལྡན་འདས་སུ་གྱུར་ཅིག་ཅེས་
msgctxt "4fdd44e39ada4b72b2e20df8fa055461"
msgid ""
"དགེ་བ␣འི་ རྩ་བ་ འདི␣ས་ ན་ བདག་ འཇིག་རྟེན་ ལོང་བ་ འདྲེན་པ་ མེད་ ཅིང་ ཡོངས་སུ་"
" འདྲེན་པ་ མེད་པ་ དང་ སྟོན་པ་ མེད་པ␣ར་ དེ་བཞིན་ གཤེགས་པ་ དགྲ་བཅོམ་པ་ "
"ཡང་དག་པ␣ར་ རྫོགས་པ␣འི་ སངས་རྒྱས་ རིག་པ་ དང་ ཞབས་ སུ་ ལྡན་པ །  བདེ་བ␣ར་ "
"གཤེགས་པ །  འཇིག་རྟེན་ མཁྱེན་པ །  སྐྱེས་བུ་ གདུལ་བྱ␣འི་ ཁ་ལོ་ སྒྱུར་བ །  བླ་ "
"ན་ མེད་པ །  ལྷ་ དང་ མི་ རྣམས་ ཀྱི་ སྟོན་པ །  སངས་རྒྱས་ བཅོམ་ལྡན་འདས་ སུ་ "
"གྱུར་ཅིག་ ཅེས་"
msgstr ""

msgctxt "cad766b0c4b7499a9161019878c7f64b"
msgid "བྱས་ སོ ། །"
msgstr ""

msgctxt "f6177a3b4d47495faf7d57a9beaa24e3"
msgid "བླ་ ན་ མེད་པ་ ཡང་དག་པ␣ར་ རྫོགས་པ␣འི་ བྱང་ཆུབ་ ཏུ་ སྨོན་ལམ་ བཏབ་ ནས །"
msgstr ""

msgctxt "539d7e2fa9474bc099d3f899262af8db"
msgid "མཚོན་ རྣོན་པོ་ བླངས་ ཏེ་"
msgstr ""

msgctxt "a404830859644d259add4a44a277fc4c"
msgid "མགུལ་པ་ བཅད་ དོ ། །"
msgstr ""

# རྒྱ་མཚོ་ཆེན་པོ་ནི་<«ཅོ་»-ནི།>ཤི་བའི་རོ་དང་ལྷན་ཅིག་གནས་མི་སྲིད་ཅིང་
msgctxt "9e50060c04784b4f9a4dc0ef91ad5759"
