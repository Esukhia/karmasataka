msgctxt "dd3c3c33681743b38ca8910d29a7ac76"
msgid "﻿དབྱིག་ དགའ་ ཞེས་ བྱ་བ་ ནི །"
msgstr ""

# བཅོམ་ལྡན་འདས་ཡུལ་:ཀཱ་ཤིའི་<«གཡུང་»«པེ་»ཀ་ཤའི། «སྣར་»ཀཱ་ཤེའི།
# «ཅོ་»ཀཱ་ཤའི།>ཕྱོགས་སུ་ལྗོངས་རྒྱུ་ཞིང་གཤེགས་པ་ལས་
msgctxt "9cda7b2b515148fa8857e285509204b0"
msgid "བཅོམ་ལྡན་འདས་ ཡུལ་ ཀཱ་ ཤི␣འི་ ཕྱོགས་ སུ་ ལྗོངས་རྒྱུ་ ཞིང་གཤེགས་པ་ ལ␣ས་"
msgstr ""

# བ་རཱ་ཎ་སཱིར་བྱོན་<«ཅོ་»བྱིན།>ཏེ།
msgctxt "8887759515024c3f96c337441a5891da"
msgid "བ་ རཱ་ ཎ་ སཱིར་ བྱོན་ ཏེ །"
msgstr ""

# དེའི་ཚེ་བཱ་རཱ་ཎ་སཱི་ན་དྲང་སྲོང་སྨྲ་བ་རི་དགས་ཀྱི་ནགས་<«གཡུང་»«ལི་»«པེ་»«སྣར་»«ཅོ་»«ཞོལ་»གནས།>ན་དགེ་སློང་ལྔ་སྡེ་རྣམས་གནས་སོ།
# །
msgctxt "4938979002e146bd91cd2410bbf8182a"
msgid ""
"དེ␣འི་ ཚེ་ བཱ་རཱ་ཎ་སཱི་ ན་ དྲང་སྲོང་ སྨྲ་བ་ རི་ དག␣ས་ ཀྱི་ ནགས་ ན་ དགེ་སློང་"
" ལྔ་ སྡེ་ རྣམས་ གནས་ སོ ། །"
msgstr ""

# དེ་ནས་དགེ་སློང་ལྔ་སྡེ་རྣམས་ཀྱིས་<«གཡུང་»«པེ་»-ཀྱིས།>ཐག་རིང་པོ་ཞིག་ནས་བཅོམ་ལྡན་འདས་མཐོང་ངོ་།
# །
msgctxt "8361adfe4d304778acfaf5d5c3cc9b8f"
msgid ""
"དེ་ནས་ དགེ་སློང་ ལྔ་ སྡེ་ རྣམས་ ཀྱིས་ ཐག་ རིང་པོ་ ཞིག་ ནས་ བཅོམ་ལྡན་འདས་ "
"མཐོང་ ངོ་ ། །"
msgstr ""

msgctxt "8442c9807ef84e7983bd7c1577265bfe"
msgid "མཐོང་ ནས་ ཀྱང་ ཕན་ཚུན་ ནས་ འདུས་ ཏེ་"
msgstr ""

msgctxt "756148f6389f46149b61f7779d6c2a15"
msgid "ནང་ཁྲིམས་ བཅས་ ནས་"
msgstr ""

msgctxt "5c64d44d31384df089db13bca070809a"
msgid "འདི་སྐད་ དུ །"
msgstr ""

# ཀྱེ་དགེ་སྦྱོང་<«གཡུང་»སློང་།>གཽ་ཏ་མ་དེ་ནི་གཡེལ་བ་མང་དུ་སོགས་<«གཡུང་»«ལི་»«པེ་»«སྣར་»«ཅོ་»སོག
# «ཞོལ་»གསོག>པ་མང་དུ་:སོགས་ཤིང་<«གཡུང་»«ལི་»«པེ་»«སྣར་»«ཅོ་»སོག་ཅིང་།
# «ཞོལ་»གསོག་ཅིང་།>འཚོ་བ་སྤོང་བ་ལས་ཉམས་པ་སྟེ།
msgctxt "16e714de25164e21b175b82bc3e9db3d"
msgid ""
"ཀྱེ་ དགེ་སྦྱོང་ གཽ་ ཏ་ མ་ དེ་ ནི་ གཡེལ་བ་ མང་ དུ་ སོགས་པ་ མང་ དུ་ སོགས་ ཤིང་"
" འཚོ་བ་ སྤོང་བ་ ལ␣ས་ ཉམས་པ་ སྟེ །"
msgstr ""

# དེ་:ནི་ད་<«པེ་»ནི་ང་།
# «ཅོ་»ཉིད།>ཁ་ཟས་མང་དུ་ཟ་ཞིང་འབྲས་ཅན་དང་ཙང་<«གཡུང་»«པེ་»ཙངས།
# «ཅོ་»གཙང་།>ཕྱེ་ཟ་ལ་ཞུན་མར་དང་ཏིལ་མར་གྱིས་ལུས་རྣམས་སྐུད་པར་བྱེད་ཅིང་
msgctxt "ca2ce75914d04d84ad6a6cf57d1abc4e"
msgid ""
"དེ་ ནི་ ད་ ཁ་ཟས་ མང་ དུ་ ཟ་ ཞིང་ འབྲས་ ཅན་ དང་ ཙང་ ཕྱེ་ ཟ་ ལ་ ཞུན་མར་ དང་ "
"ཏིལ་མར་ གྱིས་ ལུས་ རྣམས་ སྐུད་པ␣ར་ བྱེད་ ཅིང་"
msgstr ""

# ཆུ་དྲོན་པོས་ལུས་འཁྲུ་བར་བྱེད་དེ། <«གཡུང་»«ཁུ་»དོ།>
msgctxt "9c2750033be647e08c45f9bc61d98988"
msgid "ཆུ་ དྲོན་པོ␣ས་ ལུས་ འཁྲུ་བ␣ར་ བྱེད་ དེ །"
msgstr ""

msgctxt "f8f35d0ce8fb488789290180334cdf2e"
msgid "དེ་ འུ་བུ་ཅག་ གི་ ཐད་ དུ་ འོང་ :␣གིས །"
msgstr ""

# ཀྱེ་<«ཞོལ་»གི། ཀྱཻ།>གཽ་ཏ་མ་སྟན་དག་ཡོད་ཀྱིས་<«ཞོལ་»ཀྱི།>
msgctxt "7f4737751a74415681032fb5b9e4be5b"
msgid "ཀྱེ་ གཽ་ ཏ་ མ་ སྟན་ དག་ ཡོད་ ཀྱིས་"
msgstr ""

msgctxt "adcff413b54c40b78c9318cae5f7a9e4"
msgid "གལ་ཏེ་ འདོད་པ་ ལྟ་ ན་ འདུག་ ཅིག་ ཅེས་"
msgstr ""

# དེ་སྐད་ཙམ་དག་སྨྲ་བར་བྱའི<«གཡུང་»«པེ་»«ཅོ་»བྱ་བའི།>་
msgctxt "5bc7dd340ffc4145ba3f478a7ad7fb29"
msgid "དེ་སྐད་ ཙམ་ དག་ སྨྲ་བ␣ར་ བྱ␣འི་"
msgstr ""

msgctxt "55ed22b03cd24e3ab865c57aed0ec4b8"
msgid "གུས་པ␣ར་ སྨྲ་བ␣ར་ མི་ བྱ␣འོ ། །"
msgstr ""

msgctxt "12e34e4544854acfacd23d6a85891a89"
msgid "ཕྱག་ མི་ བྱ␣འོ ། །"
msgstr ""

msgctxt "79496615ec1449db9e554d2bf4ee7632"
msgid "ལྡང་བ␣ར་ མི་ བྱ␣འོ ། །"
msgstr ""

msgctxt "8166daa5f28442cabebb99dae4de557c"
msgid "ལངས་ ཏེ་"
msgstr ""

# སྟན་བསྟབ་<«གཡུང་»«པེ་»བསྟབས།>པར་མི་བྱའོ་ཞེས་
msgctxt "6d3fb8e5c9494576985841e080202935"
msgid "སྟན་ བསྟབ་པ␣ར་ མི་ བྱ␣འོ་ ཞེས་"
msgstr ""

msgctxt "7724663314274ffeb079aad5e7e29f1c"
msgid "ཟེར་ རོ ། །"
msgstr ""

# དེ་ནས་བཅོམ་ལྡན་འདས་དགེ་སློང་ལྔ་སྡེ་རྣམས་དང་ཇི་<«གཡུང་»«ལི་»«པེ་»«སྣར་»«ཅོ་»ཅི།>ཙམ་དུ་ཉེ་བར་བྱོན་པ།
# དེ་<«གཡུང་»-དེ།>ཙམ་དུ་དགེ་སློང་ལྔ་སྡེ་རྣམས་བཅོམ་ལྡན་འདས་ཀྱི་དཔལ་དང་གཟི་བརྗིད་མ་<«གཡུང་»«ལི་»«པེ་»«སྣར་»«ཅོ་»མི།>བཟོད་ནས་
msgctxt "dd00203208b14a8faff558e23d7c2fe7"
msgid ""
"དེ་ནས་ བཅོམ་ལྡན་འདས་ དགེ་སློང་ ལྔ་ སྡེ་ རྣམས་ དང་ ཇི་ཙམ་ དུ་ ཉེ་བར་ བྱོན་པ "
"།  དེ་ཙམ་ དུ་ དགེ་སློང་ ལྔ་ སྡེ་ རྣམས་ བཅོམ་ལྡན་འདས་ ཀྱི་ དཔལ་ དང་ "
"གཟི་བརྗིད་ མ་ བཟོད་ ནས་"
msgstr ""

msgctxt "54f3c0083e2b47078dc2a5d47cbb7a0a"
msgid "ཞེ་ས་ མི་ བྱ་བ␣འི་ ཐུགས་ མ་ ཐུབ་ སྟེ །"
msgstr ""

msgctxt "d299e309e91240fe9385107570834459"
msgid "ཁ་ཅིག་ ནི་ སྟན་ ལ␣ས་ ལངས་ ནས་"
msgstr ""

# བཅོམ་ལྡན་འདས་ཀྱི་<«གཡུང་»«པེ་»ཀྱིས།>གདན་ཤོམ་མོ། །
msgctxt "4c91d9b5a1ac40f18a0bcd822f773953"
msgid "བཅོམ་ལྡན་འདས་ ཀྱི་ གདན་ ཤོམ་ མོ ། །"
msgstr ""

# ཁ་ཅིག་ནི་ཞབས་བསིལ་བའི་ཆུ་དང་ཞབས་རྟེན་ཕྱོགས་གཅིག་<«གཡུང་»«པེ་»ཅིག
# །>ཏུ་འཇོག་པར་བྱེད་དོ། །
msgctxt "e67bb15a0ca348749c1d9adbc2447fbc"
msgid ""
"ཁ་ཅིག་ ནི་ ཞབས་བསིལ་བ␣འི་ ཆུ་ དང་ ཞབས་རྟེན་ ཕྱོགས་ གཅིག་ ཏུ་ འཇོག་པ␣ར་ བྱེད་"
" དོ ། །"
msgstr ""

msgctxt "cb3451972ed74dcdb1c0be51be70f643"
msgid "ཁ་ཅིག་ ནི་ ལངས་ ནས་"
msgstr ""

msgctxt "516fc9acd73b4cb4a622db7f5c01cbc0"
msgid "ཆོས་གོས་ རྣམས་ འཛིན་པ␣ར་ བྱེད་ དེ །"
msgstr ""

msgctxt "84e4239f55d54c4f9fd951401382bbd7"
msgid "འདི་སྐད་ དུ་"
msgstr ""

# ཀྱེ་<«ཞོལ་»ཀྱཻ།>གཽ་ཏ་མ་ཚུར་སྤྱོན།
msgctxt "a39c277f7c0a476e953c2242a3379c15"
msgid "ཀྱེ་ གཽ་ ཏ་ མ་ ཚུར་སྤྱོན །"
msgstr ""

# ཀྱེ་<«ཞོལ་»ཀྱཻ།>གཽ་ཏ་མ་ཁྱོད་གཤེགས་པ་ལེགས་སོ། །
msgctxt "57ac1f95d39647f391540852950bf895"
msgid "ཀྱེ་ གཽ་ ཏ་ མ་ ཁྱོད་ གཤེགས་པ་ ལེགས་ སོ ། །"
msgstr ""

# ཀྱེ་གཽ་ཏ་མ་ཁྱོད་གདན་བཤམས་པ་ལ་བཞུགས་ཞེས་<«གཡུང་»«ལི་»«པེ་»«ཅོ་»ཤེས།>
msgctxt "c5a6dcb30a3649e89635507fa3e20c94"
msgid "ཀྱེ་ གཽ་ ཏ་ མ་ ཁྱོད་ གདན་ བཤམས་པ་ ལ་ བཞུགས་ ཞེས་"
msgstr ""

msgctxt "5c2513c4687c448487ed0e4ba7767068"
msgid "ཟེར་ རོ ། །"
msgstr ""

msgctxt "764f1fcac0e244d4acbd641707a22718"
msgid "དེ་ནས་ བཅོམ་ལྡན་འདས་ འདི་ སྙམ་ དུ་ དགོངས་ ཏེ །"
msgstr ""

msgctxt "ac0d82a87d4343548c9244ebbf2d6c15"
msgid ""
"ཀྱེ་མ་ མི་ བླུན་པོ་ འདི་ རྣམས་ ནི་ རང་ གི་ ནང་ཁྲིམས་ ལ␣ས་ ཉམས་ སོ་ སྙམ་ དུ་"
msgstr ""

msgctxt "837c26029a3849828fd4d509b7bec57e"
msgid "དགོངས་ ནས་"
msgstr ""

msgctxt "33710de00d9e4ec6917285f9f772abab"
msgid "གདན་ བཤམས་པ་ དེ་ཉིད་ ལ་ བཞུགས་ སོ ། །"
msgstr ""

# དེ་ཙམ་ན་དགེ་སློང་ལྔ་སྡེ་རྣམས་བཅོམ་ལྡན་འདས་ལ་མིང་གི་ཚིག་དང་རུས་ཀྱི་ཚིག་དང་ཚེ་དང་ལྡན་པ་ཞེས་བྱ་བའི་ཚིག་འབའ་ཞིག་རྗོད་<«གཡུང་»«པེ་»+གི།>པར་བྱེད་དོ།
# །
msgctxt "da350843a8b54a4183c39af10ab77a2f"
msgid ""
"དེ་ཙམ་ ན་ དགེ་སློང་ ལྔ་ སྡེ་ རྣམས་ བཅོམ་ལྡན་འདས་ ལ་ མིང་ གི་ ཚིག་ དང་རུས་ "
"ཀྱི་ ཚིག་ དང་ ཚེ་ དང་ ལྡན་པ་ ཞེས་ བྱ་བ␣འི་ ཚིག་ འབའ་ཞིག་ རྗོད་པ␣ར་ བྱེད་ དོ "
"། །"
