msgctxt "98fb6fa2fee542b0a415a23eae8726e0"
msgid "བསམས་ ཏེ །"
msgstr ""

msgctxt "4845b5207ef7486694942f647fb79a2d"
msgid ""
"ཐམས་ཅད་ ཀྱིས་ ཉི་མ་ གཅིག་ ཏུ་ དཔུང་ གི་ ཚོགས་ ཡན་ལག་ བཞི་ གོ་ བསྐོན་ ནས་"
msgstr ""

msgctxt "3463c44e7cf249c6bc794dd689d50fb8"
msgid "གྲོང་ཁྱེར་ བཱ་ སཱིར་ དོང་ སྟེ །"
msgstr ""

msgctxt "60bbea266f474f4e8be25e27ee0578de"
msgid "ཁོ་ར་ ཁོར་ཡུག་ ནས་ བསྐོར་ ཏོ ། །"
msgstr ""

msgctxt "73c0f25f9e4c4dc9adfcbe800d3b1345"
msgid ""
"དེ་ནས་ རྒྱལ་པོ་ ཚངས་པ␣ས་ བྱིན་ གྱིས་ བདག་ཉིད་ ཕ་རོལ་ གྱི་ དམག་ཚོགས་ ཀྱིས་ "
"གཙེས་པ␣ར་ རིག་ ནས་"
msgstr ""

# ཁང་བཟངས་ཀྱི་སྟེང་དུ་སོང་སྟེ<«གཡུང་»«ལི་»«པེ་»«སྣར་»«ཅོ་»བཟངས་ཀྱི་སྟེང་ན།
# «ཞོལ་»བཟང་གི་སྟེང་དུ་སོང་ནས།>།
msgctxt "6985ecb7ee56462d8f579f93987cda3e"
msgid "ཁང་ བཟངས་ ཀྱི་ སྟེང་ དུ་ སོང་ སྟེ །"
msgstr ""

# ལག་པ་ལ་རྐོམ་<«གཡུང་»«པེ་»«ཁུ་»སྐོམ།>ཚུགས་བཅས་ནས།
msgctxt "85d19d25f04b49739402c04dc49ccb3f"
msgid "ལག་པ་ ལ་ རྐོམ་ཚུགས་ བཅས་ ནས །"
msgstr ""

# ད་<«པེ་»«སྣར་»«ཞོལ་»དེ།>བདག་གིས་ཇི་ལྟར་བྱ་སྙམ་སྟེ་
msgctxt "4dc18db8f37b407cbd5a2bf854589cbc"
msgid "ད་ བདག་ གིས་ ཇི་ལྟར་ བྱ་ སྙམ་ སྟེ་"
msgstr ""

# སེམས་ཁོངས་སུ་<«ཞོལ་»ཁོང་དུ།>ཆུད་ཅིང་འདུག་གོ། །
msgctxt "104bea90d88d4a019bb73bc1c6a6ea5f"
msgid "སེམས་ ཁོངས་ སུ་ ཆུད་ ཅིང་ འདུག་ གོ ། །"
msgstr ""

