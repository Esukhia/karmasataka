msgctxt "7963b347143545e090ca603fa9e98c5b"
msgid "ཕ་ལ་ འདི་སྐད་ ཅེས་ སྨྲ␣ས་ སོ ། །"
msgstr ""

msgctxt "57a0587309b44d4e9bb94f6417abb815"
msgid "ཡབ་"
msgstr ""

# ཅིའི་སླད་དུ་ཁང་བཟངས་ཀྱི་<«ཞོལ་»བཟང་གི།>སྟེང་དུ་གཤེགས་ནས།
msgctxt "7d2e1ed97388404a999864d92593a630"
msgid "ཅིའི་སླད་དུ་ ཁང་ བཟངས་ ཀྱི་ སྟེང་ དུ་ གཤེགས་ ནས །"
msgstr ""

# ཕྱག་ལ་རྐོམ་<«གཡུང་»«པེ་»«ཁུ་»སྐོམ།>ཚུགས་བཅས་ཏེ་
msgctxt "b63c87b51a3d4d078123f11011b90a76"
msgid "ཕྱག་ ལ་ རྐོམ་ཚུགས་ བཅས་ ཏེ་"
msgstr ""

# སེམས་ཁོངས་སུ་<«ཞོལ་»ཐུགས་ཁོང་དུ།>ཆུད་ཅིང་བཞུགས།
msgctxt "9276f7051c114ece880c52db3d957d4c"
msgid "སེམས་ ཁོངས་ སུ་ ཆུད་ ཅིང་ བཞུགས །"
