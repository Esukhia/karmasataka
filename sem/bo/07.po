msgctxt "f68dc20a01334abe8e493c368cbbba36"
msgid "འདི␣ས་ སྨོན་ལམ་ གང་ དུ་ བཏབ་ ལགས །"
msgstr ""

msgctxt "6e6b9e31949449cd96033a8344d69b05"
msgid "བཅོམ་ལྡན་འདས་ ཀྱིས་ བཀའ་ སྩལ་པ །"
msgstr ""

msgctxt "ce0fa2951f2946b68ccb0626f6756c47"
msgid "དགེ་སློང་ དག་"
msgstr ""

# སྔོན་བྱུང་བ་འདས་པའི་དུས་ན་བསྐལ་པ་བཟང་པོ་འདི་ཉིད་ལ་སྐྱེ་དགུའི་<«ཞོལ་»རྒུའི།>ཚེ་ལོ་ཉི་ཁྲི་ཐུབ་པ་ན།
# ཡང་དག་པར་རྫོགས་པའི་སངས་རྒྱས་རིག་<«གཡུང་»«སྣར་»རིགས།>པ་དང་ཞབས་སུ་ལྡན་པ་བདེ་བར་གཤེགས་པ་འཇིག་རྟེན་མཁྱེན་<«གཡུང་»ཁྱེན།>པ་སྐྱེས་བུ་གདུལ་བྱའི་ཁ་ལོ་སྒྱུར་བ་བླ་ན་མེད་པ་ལྷ་དང་མི་རྣམས་ཀྱི་སྟོན་པ་སངས་རྒྱས་བཅོམ་ལྡན་འདས་འོད་སྲུང་ཞེས་བྱ་བ་འཇིག་རྟེན་དུ་བྱུང་སྟེ།
msgctxt "113dc3efe0474ec898ec3030383d9c5c"
msgid ""
"སྔོན་བྱུང་བ་ འདས་པ␣འི་ དུས་ ན་ བསྐལ་པ་ བཟང་པོ་ འདི་ཉིད་ ལ་ སྐྱེ་དགུ␣འི་ "
"ཚེ་ལོ་ ཉི་ཁྲི་ ཐུབ་པ་ ན །  ཡང་དག་པ␣ར་ རྫོགས་པ␣འི་ སངས་རྒྱས་ རིག་པ་ དང་ ཞབས་ "
"སུ་ ལྡན་པ་ བདེ་བ␣ར་ གཤེགས་པ་ འཇིག་རྟེན་ མཁྱེན་པ་ སྐྱེས་བུ་ གདུལ་བྱ␣འི་ ཁ་ལོ་"
" སྒྱུར་བ་ བླ་ ན་ མེད་པ་ ལྷ་ དང་ མི་ རྣམས་ ཀྱི་ སྟོན་པ་ སངས་རྒྱས་ "
"བཅོམ་ལྡན་འདས་ འོད་སྲུང་ ཞེས་ བྱ་བ་ འཇིག་རྟེན་ དུ་ བྱུང་ སྟེ །"
msgstr ""

msgctxt "05f4a946a1e54bd9a5affdfaa7f75c5f"
msgid ""
"དེ␣འི་ ཚེ་ གྲོང་ཁྱེར་ བཱ་རཱ་ཎ་སཱི་ ན་ ཁྱིམ་བདག་ ཕྱུག་ ཅིང་ ནོར་ མང་ ལ་ "
"ལོངས་སྤྱོད་ ཆེ་བ་ ཡོངས་སུ་ འཛིན་པ་ ཡངས་ ཤིང་ རྒྱ་ཆེ་བ་ རྣམ་ཐོས་ ཀྱི་ བུ␣འི་ "
"ནོར་ དང་ ལྡན་པ །  རྣམ་ཐོས་ ཀྱི་ བུ␣འི་ ནོར་ དང་ འགྲན་པ་ ཞིག་ གནས་པ་"
msgstr ""

msgctxt "3458dd15e9304b6382eda03f796955fd"
msgid "དེ␣ས །  ཐབས་ ཟླ␣ར་ བབ་པ་ ལ␣ས་ ཆུང་མ་ བླངས་ ནས །"
msgstr ""

msgctxt "cf8ae6851f5347019212d62f997c0810"
msgid "དེ་དེ་ དང་ ལྷན་ཅིག་ ཏུ་ རྩེ་ ཞིང་ དགའ་ ལ་ དགའ་མགུ␣ར་ སྤྱོད་པ་ ལ␣ས །"
msgstr ""

# ཕྱི་ཞིག་ན་དེའི་ཆུང་མ་ལ་བུ་<«གཡུང་»ལུ། «པེ་»བ།>ཆགས་ནས་
msgctxt "41809b55dad742cbb1acd526a5d664f7"
msgid "ཕྱི་ ཞིག་ ན་ དེ␣འི་ ཆུང་མ་ ལ་ བུ་ ཆགས་ ནས་"
msgstr ""

msgctxt "19132abdfa324961b9da8d5b9ab3b041"
msgid "དེ་ ཟླ་བ་ དགུ␣འམ་ བཅུ་ ལོན་པ་ དང་"
msgstr ""

# བུ་མོ་གཟུགས་བཟང་ཞིང་བལྟ་<«ལི་»«སྣར་»«ཅོ་»ལྟ།>ན་སྡུག་ལ་མཛེས་པ་ཞིག་བཙས་ཏེ།
msgctxt "e8e000b6838d4ab1b6c182ee20c5cd8a"
msgid "བུ་མོ་ གཟུགས་བཟང་ ཞིང་ བལྟ་ ན་ སྡུག་ ལ་ མཛེས་པ་ ཞིག་ བཙས་ ཏེ །"
msgstr ""

msgctxt "be6add7053214564a73d307d079a00ec"
msgid "དེ␣འི་ བཙས་ སྟོན་ རྒྱས་པ␣ར་ བྱས་ ནས་"
msgstr ""

msgctxt "8a35cbe3528b43f688e0bc006f82bf4e"
msgid "རིགས་ དང་ འཐུན་པ␣ར་ མིང་ བཏགས་ ཏེ་"
msgstr ""

# འོ་མ་དང་ཞོ་དང་མར་དང་ཞུན་མར་དང་མར་གྱི་ཉིང་གུས་<«གཡུང་»«པེ་»གྱི་སྙིང་ཀུས།
# «ལི་»«སྣར་»«ཅོ་»དང་མར་གྱི་སྙིང་གུས།
# «ཞོལ་»དང་མར་གྱི་སྙིང་ཁུས།>བསྐྱེད་བསྲིངས་ཏེ།
msgctxt "c2812d8d815a483daf886560e332ec15"
msgid ""
"འོ་མ་ དང་ ཞོ་ དང་ མར་ དང་ ཞུན་མར་ དང་ མར་ གྱི་ ཉིང་ གུས་ བསྐྱེད་ བསྲིངས་ ཏེ "
"།"
msgstr ""

# མཚོའི་ནང་གི་<«གཡུང་»གིའི།>པད་མ་བཞིན་དུ་སྐྱེ་<«གཡུང་»སྐྱེད།>དྲག་པར་གྱུར་ཏོ། །
msgctxt "288fc80465a9475d9d884f6b5de1c042"
msgid "མཚོ␣འི་ ནང་ གི་ པད་མ་ བཞིན་ དུ་ སྐྱེ་ དྲག་པ␣ར་ གྱུར་ ཏོ ། །"
msgstr ""

# དེ་ནས་དེ་གང་གི་ཚེ་<«གཡུང་»«ལི་»«པེ་»«སྣར་»«ཅོ་»«ཞོལ་»+ན།>ཆེན་མོར་གྱུར་པ་དེའི་ཚེ་ཡང་དག་པར་རྫོགས་པའི་སངས་རྒྱས་འོད་སྲུང་གི་བསྟན་པ་ལ་དད་པ་རྙེད་ནས་
msgctxt "dcad1706451446dcb4e51b286bd40fcf"
msgid ""
"དེ་ནས་ དེ་ གང་ གི་ ཚེ་ ཆེན་ མོ␣ར་ གྱུར་པ་ དེ␣འི་ ཚེ་ ཡང་དག་པ␣ར་ རྫོགས་པ␣འི་ "
"སངས་རྒྱས་ འོད་སྲུང་ གི་ བསྟན་པ་ ལ་ དད་པ་ རྙེད་ ནས་"
msgstr ""

msgctxt "7b07c431130a4b9ea5a7c4de598c8f5a"
msgid "དེ␣ས་ ཕ་མ་ ལ་ གསོལ་ ཏེ །"
msgstr ""

msgctxt "6bc8e9ee39d24e30aa8b7b2636542636"
msgid "རྣམ་པ་ ཐམས་ཅད་ ཡོངས་སུ་ རྫོགས་པ␣འི་ གཙུག་ལག་ཁང་ བརྩིགས་ ནས །"
msgstr ""

msgctxt "f3642dbd1118457c87d46bc6c75480e2"
msgid ""
"སངས་རྒྱས་ འོད་སྲུང་ ལ་སོགས་པ་ དགེ་སློང་ གི་ དགེ་འདུན་ རྣམས་ ལ་ ཕུལ་ ཏེ །"
msgstr ""

