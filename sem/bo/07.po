msgctxt "99182ea3dddd405cbfd6b53c88090d37"
msgid ""
"དེ་དག་ ཐམས་ཅད་ ཀྱིས་ ཀཱ་ ཤི␣འི་ རྒྱལ་པོ་ ཚངས་པ␣ས་ བྱིན་ ལ་ ཕོ་ཉ་ བཏང་ སྟེ །"
msgstr ""

msgctxt "04c7a776474e4719adac0b971589f8f0"
msgid "དེ་དག་ རེ་རེ་ ནས་ བུ་མོ་ དེ་ ང་ ལ་ བྱིན་ ཅིག་ ཅེས་"
