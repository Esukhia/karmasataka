msgctxt "a188dfeb9b174c4fab3c3e66ad992786"
msgid "བུ་མོ␣ས་ སྨྲས་པ །"
msgstr ""

msgctxt "6efa69e593f94435bb7dcb9f0574418d"
msgid "ཡབ་"
msgstr ""

msgctxt "eeb6feaf3c944cf79fbed3b209ad0b60"
msgid "བདག་ བྱད་བཞིན་ ངན་ ལགས་ སམ་"
msgstr ""

# ཅི་ན་བདག་གི་སླད་དུ་ཡབ་སེམས་ཁོངས་སུ་<«ཞོལ་»ཐུགས་ཁོང་དུ།>ཆུད་ཅིང་བཞུགས།
msgctxt "bcfec5be2ccf40f8acf04b1ba8a5f1b9"
msgid "ཅི་ ན་ བདག་ གི་ སླད་ དུ་ ཡབ་ སེམས་ ཁོངས་ སུ་ ཆུད་ ཅིང་ བཞུགས །"
