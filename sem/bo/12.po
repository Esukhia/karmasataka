" །"
msgstr ""

# ཁྱོད་ཀྱང་བདག་ཅག་གཉིས་ཀྱི་<«གཡུང་»«ལི་»«པེ་»ཀྱིས། «སྣར་»གིས།>བུར་གྱུར་ཅིག །
msgctxt "efabc72412964301acaf9c59fdf63b47"
msgid "ཁྱོད་ ཀྱང་ བདག་ཅག་ གཉིས་ ཀྱི་ བུར་ གྱུར་ཅིག  །"
msgstr ""

# ཁྱོད་ཁོ་ན་ལ་བརྟེན་<«གཡུང་»«པེ་»རྟེན།>ཏེ་
msgctxt "2b62fc39f95441ea850a58297532009d"
msgid "ཁྱོད་ ཁོ་ན་ ལ་ བརྟེན་ ཏེ་"
msgstr ""

msgctxt "25453318b57f42dfac88e5ed597118fc"
msgid ""
"བདག་ཅག་ གཉིས་ ཀྱིས་ སངས་རྒྱས་ བཅོམ་ལྡན་འདས་ མཉེས་པ␣ར་ བྱེད་པ␣ར་ གྱུར་ཅིག  །"
msgstr ""

msgctxt "c7ca31c4a416420db3fdd50aa0882401"
msgid "མི་ མཉེས་པ␣ར་ བྱེད་པ␣ར་ མ་ གྱུར་ཅིག  །"
msgstr ""

msgctxt "8e00823583474b96aef526e63983cfc2"
msgid "དེ་ ཁོ་ན␣འི་ བསྟན་པ་ ལ་ རབ་ཏུ་ བྱུང་ ནས་"
msgstr ""

msgctxt "7cd0e8a41ed841b29de8020afcdfe1a8"
msgid "ཉོན་མོངས་པ་ ཐམས་ཅད་ སྤངས་ ཏེ །"
msgstr ""

msgctxt "f9ff9a5bc7f844cd9b05d661021338a9"
msgid "དགྲ་བཅོམ་པ་ ཉིད་ མངོན་སུམ་ དུ་ བྱེད་པ␣ར་ གྱུར་ཅིག་ ཅེས་"
msgstr ""

msgctxt "51a45bdaa9ba478abbd90b97f0c474d9"
msgid "བྱས་ སོ ། །"
msgstr ""

msgctxt "33c090d457664319b1eade0ac6c7d921"
msgid "དགེ་སློང་ དག་"
msgstr ""

# ཇི་<«གཡུང་»«ལི་»«པེ་»«སྣར་»«ཅོ་»ཅི།>སྙམ་དུ་སེམས།
msgctxt "cd31f44fa20249fc9aa08b66da14e5af"
msgid "ཇི་སྙམ་ དུ་ སེམས །"
msgstr ""

msgctxt "101ff68c063344aba5cf68f2da350a02"
msgid ""
"དེ␣འི་ ཚེ་ ཡང་དག་པ␣ར་ རྫོགས་པ␣འི་ སངས་རྒྱས་ འོད་སྲུང་ གི་ བསྟན་པ་ ལ་ རབ་ཏུ་ "
"བྱུང་བ␣འི་ དགེ་སློང་ དུ་ གྱུར་པ །  གང་ཡིན་པ་ དེ་ ནི་ འདུས་པ␣འི་ ཀཱ་ ཏྱཱ་ ཡ་ "
"ན་ འདི་ཉིད་ ཡིན་ ནོ ། །"
msgstr ""

# དེའི་ཚེ་དེའི་ཕ་མར་གྱུར་པ་གཉིས་ནི་ཕ་མ་འདི་གཉིས་<«སྣར་»«ཞོལ་»ཉིད།>ཁོ་ན་ཡིན་ནོ།
# །
msgctxt "4239f3bf146f4ced8ec9f29e858fa515"
msgid ""
"དེ␣འི་ ཚེ་ དེ␣འི་ ཕ་མ␣ར་ གྱུར་པ་ གཉིས་ ནི་ ཕ་མ་ འདི་ གཉིས་ ཁོ་ན་ ཡིན་ ནོ ། །"
msgstr ""

msgctxt "d9b7d17381cd440981486b90efef6962"
msgid "དེ␣ས་ དེ␣ར་ སྦྱིན་པ་ དག་ བྱིན་ ཅིང་"
msgstr ""

msgctxt "d7ca6db067b648398e183c57647640ff"
msgid "བསོད་ནམས་ དག་ བྱས་ ནས་"
msgstr ""

msgctxt "732357899d6a45da83c2781a9b2e2b57"
msgid ""
"སྨོན་ལམ་ བཏབ་པ␣འི་ ལ␣ས་ དེ␣འི་ རྣམ་པ␣ར་ སྨིན་པ␣ས །  གང་དང་གང་ དུ་ སྐྱེས་པ་ "
"དེ་ དང་ དེ␣ར་ ཕྱུག་ ཅིང་ ནོར་ མང་ ལ་ ལོངས་སྤྱོད་ ཆེ་བ␣འི་ རིགས་ སུ་ སྐྱེས་ "
"ཏེ །"
msgstr ""

# གཟུགས་བཟང་ཞིང་བལྟ་<«ལི་»«སྣར་»«ཅོ་»ལྟ།>ན་སྡུག་ལ་མཛེས་པ་དང་།
msgctxt "58a354f960cf4de18682e40d826ba9b0"
msgid "གཟུགས་བཟང་ ཞིང་ བལྟ་ ན་ སྡུག་ ལ་ མཛེས་པ་ དང་ །"
msgstr ""

msgctxt "8beff39500174ab5b4a6d9af2d23f14a"
msgid "བསྟན་བཅོས་ ཐམས་ཅད་ ཀྱང་ རྟོགས་ ལ །"
msgstr ""

msgctxt "109af539cec74c41a079f2d708bd9979"
msgid "གཞུང་ སྨྲ་བ་ ཐམས་ཅད་ ཀྱང་ ཚར་ བཅད་ དེ །"
msgstr ""

msgctxt "5ea8674c8f1c4a90b56469cb0b6d7ce1"
msgid "དགེ་སློང་ དག་"
msgstr ""

msgctxt "7b5581fe9c7e412187af5e43434907f2"
msgid ""
"ང་ ཡང་དག་པ␣ར་ རྫོགས་པ␣འི་ སངས་རྒྱས་ འོད་སྲུང་ དང་ ཤིན་ཏུ་ མཉམ་པ་ དང་ ཤུགས་ "
"མཉམ་པ་ དང་ མཛད་པ་ མཉམ་པ་ དང་ ཐབས་ མཉམ་པ་ བརྙེས་པ␣ར་ གྱུར་ ནས །"
