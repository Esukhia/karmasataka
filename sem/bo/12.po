# དང་པོ་སངས་རྒྱས་མཐོང་བའི་ཉམས་བདེ་བ་ཅི་འདྲ་བ་དེ་ལྟ་བུ་<«གཡུང་»«པེ་»བ།>ནི་ལོ་བཅུ་གཉིས་ཀྱི་བར་དུ་ཞི་གནས་ཀྱི་སེམས་ལ་གོམས་པར་བྱས་པ་ལ་ཡང་ཉམས་བདེ་བ་དེ་ལྟ་བུ་སྐྱེ་བར་མི་འགྱུར་རོ།
# །
msgctxt "51d305ffd22f4f018ae5a755f1818782"
msgid ""
"དགེ་བ␣འི་ རྩ་བ་ བསགས་པ␣འི་ རྒྱུ་ལ␣ས་ བྱུང་བ␣འི་ སེམས་ཅན་ གྱིས །  དང་པོ་ "
"སངས་རྒྱས་ མཐོང་བ␣འི་ ཉམས་ བདེ་བ་ ཅི་འདྲ་བ་ དེ་ལྟ་ བུ་ ནི་ ལོ་ བཅུ་གཉིས་ ཀྱི་"
" བར་ དུ་ ཞི་གནས་ ཀྱི་ སེམས་ ལ་ གོམས་པ␣ར་ བྱས་པ་ ལ་ ཡང་ ཉམས་ བདེ་བ་ དེ་ལྟ་ "
"བུ་ སྐྱེ་བ␣ར་ མི་ འགྱུར་ རོ ། །"
msgstr ""

msgctxt "c9730539604f42c982512899f5a14815"
msgid "དེ་ལྟར་ མཐོང་ ནས་ ཀྱང་"
msgstr ""

msgctxt "84f10b5a0e434629bdfca2f6fac1f73c"
msgid "མཆོག་ ཏུ་ དགའ་བ་ སྐྱེས་ སོ ། །"
msgstr ""

msgctxt "4c7d6fbd219749d7b6f2ae87ec053775"
msgid "དགའ་བ་ སྐྱེས་ ནས་"
msgstr ""

# བཅོམ་ལྡན་འདས་ག་ལ་བ་<«གཡུང་»–བ།>དེར་དོང་སྟེ་
msgctxt "f95e603ac1524352b44998334ca41289"
msgid "བཅོམ་ལྡན་འདས་ ག་ལ་བ་ དེ␣ར་ དོང་ སྟེ་"
msgstr ""

msgctxt "1aafade243dc4795a9b04948affb11e5"
msgid "ལྷགས་ ནས །"
msgstr ""

msgctxt "125f8364b1134dc2ba51ce982b7004ff"
msgid "བཅོམ་ལྡན་འདས་ ཀྱི་ ཞབས་ ལ་མགོ་ བོས་ ཕྱག་ འཚལ་ ཏེ །"
msgstr ""

msgctxt "265dcc6e219e4cdc982c0f6bfc457ca2"
msgid "ཆོས་ མཉན་པ␣འི་ ཕྱིར་ བཅོམ་ལྡན་འདས་ ཀྱི་ སྤྱན་སྔ␣ར་ འཁོད་ དོ ། །"
msgstr ""

# དེ་ནས་བཅོམ་ལྡན་འདས་ཀྱིས།
# དེ་རྣམས་ཀྱི་བསམ་པ་དང་བག་ལ་ཉལ་<«གཡུང་»«ལི་»«པེ་»«སྣར་»«ཅོ་»«ཞོལ་»+བ།>དང་ཁམས་དང་རང་བཞིན་ཐུགས་སུ་ཆུད་ནས་
msgctxt "fc6ec8301b24473583d5104f1b7a48f3"
msgid ""
"དེ་ནས་ བཅོམ་ལྡན་འདས་ ཀྱིས །  དེ་ རྣམས་ ཀྱི་ བསམ་པ་ དང་ བག་ ལ་ ཉལ་ དང་ ཁམས་ "
"དང་ རང་བཞིན་ ཐུགས་ སུ་ ཆུད་ ནས་"
msgstr ""

# དེ་དང་འཐུན་པའི་ཆོས་བསྟན་ཏེ<«གཡུང་»«པེ་»སྟེ། «ཁུ་»དེ།>།
msgctxt "b85cd65c0304496c8e5aff568bf62d20"
msgid "དེ་ དང་ འཐུན་པ␣འི་ ཆོས་ བསྟན་ ཏེ །"
msgstr ""

msgctxt "2f60db59bdc6440ea41c90b96a3e65ac"
msgid ""
"དེ་ རྣམས་ ཀྱིས་ སྟན་ དེ་ཉིད་ ལ་ འཁོད་ བཞིན་ དུ་ འཇིག་ཚོགས་ ལ་ ལྟ་བ␣འི་ "
"རི␣འི་ རྩེ་མོ་ མཐོན་པོ་ ཉི་ཤུ་ ཡེ་ཤེས་ རྡོ་རྗེ␣ས་ བཅོམ་ ནས་"
msgstr ""

msgctxt "4757d537ec464df58540ac089bd35aa6"
msgid "རྒྱུན་ དུ་ ཞུགས་པ␣འི་ འབྲས་བུ་ མངོན་སུམ་ དུ་ བྱས་ སོ ། །"
msgstr ""

msgctxt "8d6e192e4c9a415c93bd9b55d3f789a5"
msgid "དེ་ རྣམས་ ཀྱིས་ བདེན་པ་ མཐོང་ ནས་"
msgstr ""

msgctxt "8fca061d5cbe477397db86fdb68dd0b2"
msgid "སྟན་ ལ␣ས་ ལངས་ ཏེ་"
msgstr ""

# བླ་གོས་ཕྲག་པ་གཅིག་ཏུ་གཟར་<«གཡུང་»«པེ་»བཟར།>ནས་
msgctxt "fae3593bddf54ba387eeb3ae8c51c007"
msgid "བླ་གོས་ ཕྲག་པ་ གཅིག་ ཏུ་ གཟར་ ནས་"
msgstr ""

# བཅོམ་ལྡན་འདས་ག་ལ་བ་དེ་<«གཡུང་»«པེ་»དེར།>ལོགས་སུ་ཐལ་མོ་སྦྱར་བ་བཏུད་དེ།
msgctxt "468f55ef36454bffac63de30acd3d2ee"
msgid "བཅོམ་ལྡན་འདས་ ག་ལ་བ་ དེ་ ལོགས་ སུ་ ཐལ་མོ་ སྦྱར་བ་ བཏུད་ དེ །"
msgstr ""

msgctxt "3f138bae7f924475bb2c240664f7e9e5"
msgid "བཅོམ་ལྡན་འདས་ ལ་ འདི་སྐད་ ཅེས་ གསོལ་ ཏོ ། །"
msgstr ""

msgctxt "dfc310b9a6744a21b4a831c8b4963e67"
msgid "བཙུན་པ་"
msgstr ""

# བདག་ཅག་ལེགས་པར་གསུངས་པའི་ཆོས་འདུལ་བ་ལ་རབ་ཏུ་བྱུང་བ་དང་བསྙེན་<«གཡུང་»བརྙེན།>པར་རྫོགས་ཤིང་དགེ་སློང་གི་དངོས་པོ་འཐོབ་ཏུ་རུང་ན།
msgctxt "8ebef9a38bf74e68bac0ed3cfd142635"
msgid ""
"བདག་ཅག་ ལེགས་པ␣ར་ གསུངས་པ␣འི་ ཆོས་ འདུལ་བ་ ལ་ རབ་ཏུ་ བྱུང་བ་ དང་ བསྙེན་པ␣ར་ "
"རྫོགས་ ཤིང་ དགེ་སློང་ གི་ དངོས་པོ་ འཐོབ་ ཏུ་ རུང་ ན །"
msgstr ""

msgctxt "e051243c86e545fea2b8b5b44594d449"
msgid ""
"བདག་ཅག་ ཀྱང་ བཅོམ་ལྡན་འདས་ ཀྱི་ ཐད་ དུ་ ཚངས་པ␣ར་ སྤྱོད་པ་ སྤྱད་པ␣ར་ འཚལ་ ལོ "
"། །"
msgstr ""

# དེ་ནས་བཅོམ་ལྡན་འདས་ཀྱིས་དེ་གཉིས་<«གཡུང་»དེ་སྤང་གཉིས། «པེ་»དེས་སྤང་གཉིས།
# «ཅོ་»དེ་གཉིས་ལ།>དགེ་སློང་ཚུར་ཤོག་ཅེས་བྱ་བས་རབ་ཏུ་ཕྱུང་<«སྣར་»«ཞོལ་»བྱུང་།>སྟེ་
msgctxt "9cc1a3f8802b449ca078183b7019fefa"
msgid ""
"དེ་ནས་ བཅོམ་ལྡན་འདས་ ཀྱིས་ དེ་ གཉིས་ དགེ་སློང་ ཚུར་ ཤོག་ ཅེས་བྱ་བ␣ས་ རབ་ཏུ་ "
"ཕྱུང་ སྟེ་"
msgstr ""

msgctxt "dd3818a9711c4d07aeb2e9e821741fdf"
msgid "བསྙེན་པ␣ར་ རྫོགས་པ␣ར་ མཛད་ དོ ། །"
msgstr ""

msgctxt "66f239da32214e7ab354b484e109e128"
msgid "དེ་ གཉིས་ ལ་ལུང་ ཡང་ ཕོག་ སྟེ །"
msgstr ""

# དེ་གཉིས་ཀྱིས་ཀྱང་བརྩོན་པ་དང་། བསྒྲུབ་<«ཞོལ་»སྒྲུབ།>པ་དང་།
# འབད་པས་ཉོན་མོངས་པ་ཐམས་ཅད་སྤངས་ནས་
msgctxt "42ef4c6ba4d64cefa9e0a1da085f8273"
msgid ""
"དེ་ གཉིས་ ཀྱིས་ ཀྱང་ བརྩོན་པ་ དང་ །  བསྒྲུབ་པ་ དང་ །  འབད་པ␣ས་ ཉོན་མོངས་པ་ "
"ཐམས་ཅད་ སྤངས་ ནས་"
msgstr ""

msgctxt "c516c676059c459c8d473abd8df8e326"
msgid "དགྲ་བཅོམ་པ་ ཉིད་ མངོན་སུམ་ དུ་ བྱས་ ཏེ །"
msgstr ""

msgctxt "3f52cf5b13c740e8a760ccad03708eb0"
msgid "དགྲ་བཅོམ་པ་ ཁམས་ གསུམ་པ␣འི་ འདོད་ཆགས་ དང་ བྲལ་བ␣ར་ གྱུར་ ནས །"
msgstr ""

# གསེར་དང་བོང་བར་<«གཡུང་»མར།>མཉམ་པ།
msgctxt "83599900a8e24fae89b3a134eb773228"
msgid "གསེར་ དང་ བོང་བ␣ར་ མཉམ་པ །"
msgstr ""

# ནམ་མཁའ་དང་ལག་མཐིལ་<«གཡུང་»«པེ་»ཐིལ།>དུ་འདྲ་བའི་སེམས་དང་ལྡན་པ།
msgctxt "8676fff164bb4f46b088912ded7ae566"
msgid "ནམ་མཁའ་ དང་ ལག་མཐིལ་ དུ་ འདྲ་བ␣འི་ སེམས་ དང་ ལྡན་པ །"
msgstr ""

msgctxt "63390cdb2b184f05ba275b8117dc197f"
msgid "ཙན་དན་ སྦངས་པ་ ལྟ་བུ␣ར་ བསིལ་བ␣ར་ གྱུར་པ །"
msgstr ""

msgctxt "04a8f2b0706e42528f75c615712c249f"
msgid "རིག་པ␣ས་ སྒོ་ང␣འི་ སྦུབས་ བཅོམ་པ་"
msgstr ""

msgctxt "5bd52fc9164643e2bc5887501600c322"
msgid "རིག་པ་ དང་ མངོན་པར་ ཤེས་པ་ དང་ སོ་སོ་ ཡང་དག་པ␣ར་ རིག་པ་ ཐོབ་པ །"
msgstr ""

msgctxt "36577431932643af84220212ab700f87"
msgid "སྲིད་པ␣འི་ རྙེད་པ་ དང་ ཆགས་པ་ དང་ བཀུར་སྟི་ ལ་ རྒྱབ་ ཀྱིས་ ཕྱོགས་པ །"
msgstr ""

# དབང་པོ་དང་ཉེ་དབང་དང་བཅས་པའི་ལྷ་རྣམས་ཀྱིས་མཆོད་ཅིང་རྗེད་<«གཡུང་»«པེ་»«སྣར་»བརྗེད།>པ་དང་གུས་པར་སྨྲ་བའི་གནས་སུ་གྱུར་ཏོ།
# །
msgctxt "2e0f13d50df040a0b0cd0fbedb3a8a08"
msgid ""
"དབང་པོ་ དང་ ཉེ་དབང་ དང་ བཅས་པ␣འི་ ལྷ་ རྣམས་ ཀྱིས་ མཆོད་ ཅིང་ རྗེད་པ་ དང་ "
"གུས་པ␣ར་ སྨྲ་བ␣འི་ གནས་ སུ་ གྱུར་ ཏོ ། །"
msgstr ""

# དེ་ནས་འདུས་མོ་ཡང་བཅོམ་ལྡན་འདས་ཀྱིས་སྐྱེ་དགུའི་<«ཞོལ»རྒུའི།>བདག་མོ་ཆེན་མོ་<«གཡུང་»པོ།>གཽ་ཏ་མཱི་ལ་གཏད་<«གཡུང་»མཱི་ལ་བཏད།
# «ཞོལ་»མི་ལ་གཏད།>ནས།
msgctxt "4d7303c698b748198fc8d2d40dc460e7"
msgid ""
"དེ་ནས་ འདུས་ མོ་ ཡང་ བཅོམ་ལྡན་འདས་ ཀྱིས་ སྐྱེ་དགུ␣འི་ བདག་མོ་ ཆེན་ མོ་ གཽ་ "
"ཏ་ མཱི་ ལ་ གཏད་ ནས །"
msgstr ""

msgctxt "8af2a666719547d9a37120ff6efc8cf4"
msgid "དེ་ཡང་ དེ␣ས་ རབ་ཏུ་ ཕྱུང་ སྟེ །"
msgstr ""

# བསྙེན་<«གཡུང་»བྱུང་སྟེ་བརྙེན།
# «པེ་»«ཅོ་»«ཞོལ་»བྱུང་སྟེ་བསྙེན།>པར་རྫོགས་པར་བྱས་ནས་
msgctxt "be4f82ec2f6442e19b303b71a6dfbcb5"
msgid "བསྙེན་པ␣ར་ རྫོགས་པ␣ར་ བྱས་ ནས་"
msgstr ""

msgctxt "0d353cef848e4d2ba99a701426220d58"
msgid "དེ་ ལ་ལུང་ ཡང་ ཕོག་ སྟེ །"
msgstr ""

# དེས་ཀྱང་བརྩོན་པ་དང་བསྒྲུབ་<«ཞོལ་»སྒྲུབ།>པ་དང་འབད་པས་ཉོན་མོངས་པ་ཐམས་ཅད་སྤངས་ནས་
msgctxt "e8117431b3244daca4b17d7897b6b0fa"
msgid ""
"དེ␣ས་ ཀྱང་ བརྩོན་པ་ དང་ བསྒྲུབ་པ་ དང་ འབད་པ␣ས་ ཉོན་མོངས་པ་ ཐམས་ཅད་ སྤངས་ ནས་"
msgstr ""

# དགྲ་བཅོམ་པ་ཉིད་<«གཡུང་»«ལི་»«པེ་»«སྣར་»«ཅོ་»–ཉིད།>མངོན་སུམ་དུ་བྱས་སོ། །
msgctxt "374d06f7937c4e1dabbedcb24eee2c8e"
msgid "དགྲ་བཅོམ་པ་ ཉིད་ མངོན་སུམ་ དུ་ བྱས་ སོ ། །"
msgstr ""

msgctxt "d6c259c3f4844bee85923d132158165a"
msgid "དེ་ནས་ དགེ་སློང་ དག་ གིས་ བཅོམ་ལྡན་འདས་ ལ་ ཞུས་པ །"
msgstr ""

msgctxt "42ccf3070d1d488a867a6015c04fdf5c"
msgid "བཙུན་པ་"
msgstr ""

msgctxt "68f6e41596324fb19f36cec3d7c6c402"
msgid "འདུས་པ␣འི་ ཀཱ་ ཏྱཱ་ ཡ་ ནས་ ལ␣ས་ ཅི་ བགྱིས་ ན །"
msgstr ""

# ལས་དེའི་<«གཡུང་»དའི།
# «པེ་»དིའི།>རྣམ་པར་སྨིན་པས་ཕྱུག་ཅིང་ནོར་མང་ལ་ལོངས་སྤྱོད་ཆེ་བའི་རིགས་སུ་སྐྱེས་ཏེ།
msgctxt "ecf72971283f4db09d0052ec503d6e83"
msgid ""
"ལ␣ས་ དེ␣འི་ རྣམ་པ␣ར་ སྨིན་པ␣ས་ ཕྱུག་ ཅིང་ ནོར་ མང་ ལ་ ལོངས་སྤྱོད་ ཆེ་བ␣འི་ "
"རིགས་ སུ་ སྐྱེས་ ཏེ །"
msgstr ""

# གཟུགས་བཟང་ཞིང་བལྟ་ན་<«གཡུང་»–ན། «ཅོ་»«ལི་»«སྣར་»ལྟ་ན།>སྡུག་ལ་མཛེས་པ་དང་
msgctxt "6fc532f4848e4fa7acb344154bdcc10d"
msgid "གཟུགས་བཟང་ ཞིང་ བལྟ་ ན་ སྡུག་ ལ་ མཛེས་པ་ དང་"
msgstr ""

msgctxt "bc18757da6ba4266b4f17a36e06936ba"
msgid "བསྟན་བཅོས་ ཐམས་ཅད་ རྟོགས་ ཏེ་"
msgstr ""

msgctxt "27078d394d874687912af0dd47faea4e"
msgid "གཞུང་ སྨྲ་བ་ ཐམས་ཅད་ ཚར་ བཅད་ ལ །"
msgstr ""

msgctxt "bbe8a14435cb456da529545fc3d73ea1"
msgid "འདི␣ས་ བཅོམ་ལྡན་འདས་ ཀྱང་ མཉེས་པ␣ར་ བགྱིས་ ཏེ་"
msgstr ""

# མི་<«གཡུང་»«པེ་»མ།>མཉེས་པར་མ་བགྱིས་པ་དང་།
msgctxt "d832043092d84def988f3b335b739336"
msgid "མི་ མཉེས་པ␣ར་ མ་ བགྱིས་པ་ དང་ །"
msgstr ""

msgctxt "4491734836b84f668a202ab0dde326a8"
msgid "བཅོམ་ལྡན་འདས་ ཀྱི་ བསྟན་པ་ ལ་ རབ་ཏུ་ བྱུང་ ནས་"
msgstr ""

msgctxt "86dd75b4d7144fec90233c631e721ae8"
msgid "ཉོན་མོངས་པ་ ཐམས་ཅད་ སྤངས་ ཏེ་"
msgstr ""

msgctxt "88adca81e93a40f5a47a3485012e3631"
msgid "དགྲ་བཅོམ་པ་ ཉིད་ མངོན་སུམ་ དུ་ བགྱིས་པ་ དང་ །"
msgstr ""

msgctxt "11d3f8a9119040a88064fda70f9b2e6e"
msgid "དེ␣འི་ ཕ་མ་ གཉིས་ ཀྱང་ གཞུང་ སྨྲ་བ␣འི་ མཆོག་ ཏུ་ གྱུར་ ཏེ །"
msgstr ""

msgctxt "fd98eb3696fa46b38943d0cc79e8ec0c"
msgid "འདི་ཉིད་ ལ་ བརྟེན་ ནས་"
msgstr ""

msgctxt "41d9b2a5be7048baa4044797c696a8f7"
msgid "བཅོམ་ལྡན་འདས་ མཉེས་པ␣ར་ བགྱིས་ ཏེ་"
msgstr ""

msgctxt "4abd1e7b884a4f35a19851cc1a1793d3"
msgid "མི་ མཉེས་པ␣ར་ མ་ བགྱིས་ ལ །"
msgstr ""

msgctxt "7fa28758793746fb8d526789dbfd2bf0"
msgid "བཅོམ་ལྡན་འདས་ ཀྱི་ བསྟན་པ་ ལ་ རབ་ཏུ་ བྱུང་ ནས་"
msgstr ""

msgctxt "11c107a484d0460185cf22c201ab5fe8"
msgid "ཉོན་མོངས་པ་ ཐམས་ཅད་ སྤངས་ ཏེ །"
msgstr ""

msgctxt "c10310eaa4764391bc73f23bd58e29ed"
msgid "དགྲ་བཅོམ་པ་ ཉིད་ མངོན་སུམ་ དུ་ བགྱིས་ ལགས །"
msgstr ""

msgctxt "ef553a7a591b4a5b87acebdb66212a83"
msgid "བཅོམ་ལྡན་འདས་ ཀྱིས་ བཀའ་ སྩལ་པ །"
msgstr ""

msgctxt "aaeaa5a8d1ed478f991ee612c98fb515"
msgid "སྨོན་ལམ་ གྱི་ དབང་ ལ␣ས་ གྱུར་ ཏོ ། །"
msgstr ""

msgctxt "11dcd2f3b919436c8464b9a16f249379"
msgid "བཙུན་པ་"
msgstr ""

msgctxt "58f37c31c63d42af94f4386f54c755f4"
msgid "འདི་ རྣམས་ ཀྱིས་ གང་ དུ་ སྨོན་ལམ་ བཏབ་ ལགས །"
msgstr ""

msgctxt "38b8b39ef6db4871a766ed678fad28fe"
msgid "བཅོམ་ལྡན་འདས་ ཀྱིས་ བཀའ་ སྩལ་པ །"
msgstr ""

msgctxt "c5fb8229069c41e39cbeadef499aa947"
msgid "དགེ་སློང་ དག་"
msgstr ""

# སྔོན་བྱུང་བ་འདས་པའི་དུས་ན་བསྐལ་<«གཡུང་»བཀལ།>པ་བཟང་པོ་འདི་ཉིད་ལ་སྐྱེ་དགུའི་<«ཞོལ་»རྒུའི།>ཚེ་ལོ་ཉི་ཁྲི་ཐུབ་པ་ན།
# ཡང་དག་པར་རྫོགས་པའི་སངས་རྒྱས་རིག་པ་དང་ཞབས་སུ་ལྡན་པ། བདེ་བར་གཤེགས་པ།
# འཇིག་རྟེན་མཁྱེན་པ།
# སྐྱེས་བུ་གདུལ་བྱའི་ཁ་ལོ་སྒྱུར་<«གཡུང་»«ལི་»«པེ་»«ཅོ་»བསྒྱུར།>བ།
# བླ་ན་མེད་པ་ལྷ་དང་མི་རྣམས་ཀྱི་སྟོན་པ།
# སངས་རྒྱས་བཅོམ་ལྡན་འདས་འོད་སྲུང་ཞེས་བྱ་བ་འཇིག་རྟེན་དུ་བྱུང་སྟེ།
msgctxt "61cef1de732a465eb841dca7ba8167aa"
msgid ""
"སྔོན་བྱུང་བ་ འདས་པ␣འི་ དུས་ ན་ བསྐལ་པ་ བཟང་པོ་ འདི་ཉིད་ ལ་ སྐྱེ་དགུ␣འི་ "
"ཚེ་ལོ་ ཉི་ཁྲི་ ཐུབ་པ་ ན །  ཡང་དག་པ␣ར་ རྫོགས་པ␣འི་ སངས་རྒྱས་ རིག་པ་ དང་ ཞབས་ "
"སུ་ ལྡན་པ །  བདེ་བ␣ར་ གཤེགས་པ །  འཇིག་རྟེན་ མཁྱེན་པ །  སྐྱེས་བུ་ གདུལ་བྱ␣འི་"
" ཁ་ལོ་ སྒྱུར་བ །  བླ་ ན་ མེད་པ་ ལྷ་ དང་ མི་ རྣམས་ ཀྱི་ སྟོན་པ །  སངས་རྒྱས་ "
"བཅོམ་ལྡན་འདས་ འོད་སྲུང་ ཞེས་ བྱ་བ་ འཇིག་རྟེན་ དུ་ བྱུང་ སྟེ །"
msgstr ""

msgctxt "598a162199c24b11abf6c44a69d9786c"
msgid ""
"དེ␣འི་ ཚེ་ བཱ་རཱ་ཎ་སཱི་ ན་ བྲམ་ཟེ་ ཞིག་ གནས་པ་ དེ␣ས་ ཐབས་ ཟླ␣ར་ བབ་པ་ ལ␣ས་ "
"ཆུང་མ་ བླངས་པ་ ལ␣ས །"
msgstr ""

msgctxt "8b0a9fb6bbaa40b0b599669cb8bb2865"
msgid "ཕྱི་ ཞིག་ ན་ དེ␣འི་ ཆུང་མ་ ལ་ བུ་ ཆགས་ ནས་"
msgstr ""

# དེ་ཟླ་བ་དགུའམ་བཅུ་ལོན་པ་དང་ཁྱེའུ་གཟུགས་བཟང་ཞིང་བལྟ་<«ལི་»«སྣར་»«ཅོ་»ལྟ།>ན་སྡུག་ལ་མཛེས་པ་ཞིག་བཙས་ནས།
msgctxt "380aa5dd1fe84788a99e836d65909b5f"
msgid ""
"དེ་ ཟླ་བ་ དགུ␣འམ་ བཅུ་ ལོན་པ་ དང་ ཁྱེའུ་ གཟུགས་བཟང་ ཞིང་ བལྟ་ ན་ སྡུག་ ལ་ "
"མཛེས་པ་ ཞིག་ བཙས་ ནས །"
msgstr ""

# འོ་མ་དང་ཞོ་དང་། མར་དང་ཞུན་མར་དང་། མར་གྱི་ཉིང་གུས་<«གཡུང་»«པེ་»སྙིང་ཀུས།
# «ལི་»«སྣར་»«ཅོ་»སྙིང་གུས། «ཞོལ་»སྙིང་ཁུས།>བསྐྱེད་བསྲིངས་ཏེ
msgctxt "896590d4e64e48ae8d2616c9dcff592e"
msgid ""
"འོ་མ་ དང་ ཞོ་ དང་ །  མར་ དང་ ཞུན་མར་ དང་ །  མར་ གྱི་ ཉིང་ གུས་ བསྐྱེད་ "
"བསྲིངས་ ཏེ"
