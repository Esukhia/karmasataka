msgctxt "238e3d6eb51e48729cec41ee0ba4fb54"
msgid "ལ␣ས་ བརྒྱ་ ཐམ་པ །"
msgstr ""

msgctxt "ede540f20b894a59a16eb9512f74168d"
msgid "བམ་པོ་ བཞི་པ །"
msgstr ""

msgctxt "2f1e0a6483de4eeeb78a399ef5fa7fca"
msgid "འདུས་ མོ་ ཞེས་ བྱ་བ་ ནི །"
msgstr ""

# གླེང་གཞི་<«གཡུང་»བཞི།>རྒྱལ་པོའི་ཁབ་ན་བཞུགས་ཏེ།
msgctxt "97951e774673471d9358a3f46f3295d0"
msgid "གླེང་ གཞི་ རྒྱལ་པོ␣འི་ ཁབ་ ན་ བཞུགས་ ཏེ །"
msgstr ""

# དེའི་ཚེ་ཡུལ་ཏཀྵ་ཤཱི་<«གཡུང་»«པེ་»«སྣར་»«ཅོ་»ཏག་ཤ་ཤི། «ལི་»ཏག་ཤ་ཤ
# «ཞོལ་»ཏ་ཀྵེ་ཤཱ།>ལ་ན། རྒྱལ་པོ་པད་མའི་སྙིང་པོ་ཅན་ཞེས་བྱ་བ་རྒྱལ་པོ་བྱེད་དེ།
msgctxt "5a50833c01aa44c0aacaac67d573ace8"
msgid ""
"དེ␣འི་ ཚེ་ ཡུལ་ ཏཀྵ་ ཤཱི་ ལ་ ན །  རྒྱལ་པོ་ པད་མ␣འི་ སྙིང་པོ་ ཅན་ ཞེས་ བྱ་བ་ "
"རྒྱལ་པོ་ བྱེད་ དེ །"
msgstr ""

# དེའི་རིང་ལ་འབྱོར་པ་དང་། རྒྱས་པ་དང་། བདེ་བ་དང་། ལོ་ལེགས་པ་དང་། སྐྱེ་བོ་དང་།
# མི་མང་པོས་གང་བ་དང་། རྩོད་<«གཡུང་»བཙོད།>པ་དང་། འཐབ་པ་ཞི་བ་དང་། འཁྲུག་པ་དང་།
# ནང་འཁྲུག་<«གཡུང་»ཁྲུག>དང་། ཆོམ་རྐུན་དང་། མུ་གེ་དང་། ནད་རྣམས་མེད་པ་དང་།
# འབྲས་སཱ་<«གཡུང་»«པེ་»ས།>ལུ་དང་། བུ་རམ་ཤིང་དང་། བ་ལང་དང་།
# མ་ཧེ་དང་ལྡན་པར་འདུག་སྟེ
msgctxt "9d6c9d17ea7243e1912d2a09ad649378"
msgid ""
"དེ␣འི་ རིང་ ལ་ འབྱོར་པ་ དང་ །  རྒྱས་པ་ དང་ །  བདེ་བ་ དང་ །  ལོ་ལེགས་པ་ དང་ "
"།  སྐྱེ་བོ་ དང་ །  མི་ མང་པོ␣ས་ གང་བ་ དང་ །  རྩོད་པ་ དང་ །  འཐབ་པ་ ཞི་བ་ དང་"
" །  འཁྲུག་པ་ དང་ །  ནང་འཁྲུག་ དང་ །  ཆོམ་རྐུན་ དང་ །  མུ་གེ་ དང་ །  ནད་ "
"རྣམས་ མེད་པ་ དང་ །  འབྲས་ སཱ་ ལུ་ དང་ །  བུ་རམ་ཤིང་ དང་ །  བ་ལང་ དང་ །  "
"མ་ཧེ་ དང་ ལྡན་པ␣ར་ འདུག་ སྟེ"
msgstr ""

msgctxt "741009afebd24ccb92e408675e784dee"
msgid ""
"བུ་ གཅིག་པ་ སྡུག་པ་ ལ་ བྱ་བ་ བཞིན་ དུ་ ཆོས་བཞིན་ དུ་ རྒྱལ་སྲིད་ བྱེད་ དོ ། །"
msgstr ""

msgctxt "010e5dc7bb94401184ee9fdbad5acaa3"
msgid ""
"དེ་ནས་ དེ་ བཙུན་མོ་ དང་ ལྷན་ཅིག་ ཏུ་ རྩེ་ ཞིང་ དགའ་ ལ་ དགའ་མགུ␣ར་ སྤྱོད་པ་ "
"ལ␣ས །"
msgstr ""

msgctxt "57438ccd9e064277915f9b1b718f1fe5"
msgid "བཙུན་མོ་ ལ་ བུ་ ཆགས་ ནས །"
msgstr ""

msgctxt "347a2b330b334d01a892b53ddef401ce"
msgid ""
"དེ␣འི་ ཡིད་ ལ་ གཞུང་ སྨྲ་བ་ རྣམས་ དང་ ལྷན་ཅིག་ ཏུ་ རྩོད་ ཀྱང་ ཅི་མ་རུང་ སྙམ་"
" དུ་ ཡིད་ ལ་ འདོད་པ །  དེ་ལྟ་ བུ་ ཞིག་ སྐྱེས་ སོ ། །"
msgstr ""

# དེ་ནས་རྒྱལ་པོས་དེ་ཐོས་མ་ཐག་ཏུ་ལྟས་མཁན་རྣམས་<«པེ་»«སྣར་»«ཁུ་»«ཞོལ་»རྣམ།>ལ་བསྟན་ཏེ།
msgctxt "e62ec59750864378bc6be8df5ad77ad7"
msgid "དེ་ནས་ རྒྱལ་པོ␣ས་ དེ་ ཐོས་ མ་ ཐག་ ཏུ་ ལྟས་མཁན་ རྣམས་ ལ་ བསྟན་ ཏེ །"
msgstr ""

msgctxt "d21b1fe6e6964d58ae9657a3108e5f83"
msgid "ལྟས་མཁན་ རྣམས་ ན་རེ་"
msgstr ""

msgctxt "554b5fe815b94ca4abccac26fd8c38fc"
msgid "ལྷ་"
msgstr ""

msgctxt "893671fd2403463e81a1a348b61bb6c3"
msgid "འདི་ ནི་ ལྟོ་ ན་ མཆིས་པ␣འི་ སེམས་ཅན་ དེ␣འི་ མཐུ་ ལགས་ ཏེ །"
msgstr ""

msgctxt "3298d0bbc29446a98fe768da6a4da14d"
msgid ""
"དེ␣ས་ ནི་ བསྟན་བཅོས་ ཐམས་ཅད་ རྟོགས་པ␣ར་ འགྱུར་ ལ་ གཞུང་ སྨྲ་བ་ ཐམས་ཅད་ "
"ཚར་གཅོད་པ␣ར་ འགྱུར་ ཏེ །"
msgstr ""

msgctxt "d8b6c84b74f648fab9fefaab67914709"
msgid ""
"དེ␣འི་ སླད་ དུ་ འདི␣འི་ ཡིད་ ལ་ འདོད་པ །  དེ་ལྟ་ བུ་ སྐྱེས་པ་ ལགས་སོ་ ཞེས་"
msgstr ""

msgctxt "2ef4fc5522df44e5b450e77512a62451"
msgid "ཟེར་ རོ ། །"
msgstr ""

msgctxt "ec92fd6fad504c1cafa94391adc32fd8"
msgid "རྒྱལ་པོ␣ས་ སྨྲས་པ །"
msgstr ""

msgctxt "87c88d83cbf64179bec145ba196930be"
msgid "ཤེས་ལྡན་ དག་"
msgstr ""

msgctxt "4c9354e523d842e997c1b288caf4f9e3"
msgid ""
"གལ་ཏེ །  བཙུན་མོ␣འི་ ཡིད་ ལ་ འདོད་པ་ སྐྱེས་པ་ དེ་ མ་ བསལ་ ན །  དེ་ ཅིར་ "
"འགྱུར་"
msgstr ""

msgctxt "0f2b0c3ab01e45cbb67521d2e074f88e"
msgid "ལྟས་མཁན་ རྣམས་ ཀྱིས་ སྨྲས་པ །"
msgstr ""

msgctxt "6773509d5c8845a7bb38644e2a68cc78"
msgid "སྲས་ དེ་ཡན་ ལག་ མ་ཚང་ བར་ སྐྱེ་བ␣ར་ འགྱུར་ ལགས་སོ ། །"
msgstr ""

msgctxt "5bb9f9d42de44b9a9f56bb7189bec027"
msgid "དེ་ནས་ རྒྱལ་པོ␣ས་ བསམས་པ །"
msgstr ""

# བདག་གི་བུ་ཡིན་ལ་ཡན་ལག་མ་ཚང་བར་འགྱུར་<«ཞོལ་»གྱུར།>ན་མི་རུང་ངོ་སྙམ་ནས་
msgctxt "b8d0b55d98354e5faef0528f4bcc0b19"
msgid "བདག་ གི་ བུ་ ཡིན་ ལ་ ཡན་ལག་ མ་ཚང་ བར་ འགྱུར་ ན་ མི་ རུང་ ངོ་ སྙམ་ ནས་"
msgstr ""

# བཙུན་མོ་ཡོལ་བའི་ནང་དུ་བཞག་<«གཡུང་»བཞུག>སྟེ།
msgctxt "bfc4e433316b439d906b6667c7db5912"
msgid "བཙུན་མོ་ ཡོལ་བ␣འི་ ནང་ དུ་ བཞག་ སྟེ །"
msgstr ""

# གཞུང་སྨྲ་བ་ཐམས་ཅད་བསགས་<«གཡུང་»«ལི་»«པེ་»«སྣར་»«ཅོ་»བསོགས།>ནས་
msgctxt "4fb07377712347cea9ca61c1b7154189"
msgid "གཞུང་ སྨྲ་བ་ ཐམས་ཅད་ བསགས་ ནས་"
msgstr ""

# གཟུ་བོ་<«གཡུང་»མོ།>རྣམས་ཀྱང་བསྐོས་<«གཡུང་»«པེ་»«ཅོ་»བསྒོས།>ཏེ།
msgctxt "0e3e03e5f2cd43c880a59e81638f4c8a"
msgid "གཟུ་བོ་ རྣམས་ ཀྱང་ བསྐོས་ ཏེ །"
msgstr ""

# གཞུང་གིས་རྩོད་<«གཡུང་»«པེ་»བརྩོད།>དུ་བཅུག་གོ། །
msgctxt "32938299ee5842f8a862d85baf29618a"
msgid "གཞུང་ གིས་ རྩོད་ དུ་ བཅུག་ གོ ། །"
msgstr ""

msgctxt "928d16686a984f07883afa8fbe59dcad"
msgid "དེ་ནས་ དེ␣ས་ གཞུང་ སྨྲ་བ་ ཐམས་ཅད་ ཚར་ བཅད་ ནས །"
msgstr ""

# དེའི་ཡིད་ལ་<«གཡུང་»–ལ།>འདོད་པ་སྐྱེས་<«གཡུང་»«པེ་»ལ་བསྐྱེས།>པ་དེ་མེད་པར་གྱུར་ཏོ།
# །
msgctxt "eb5eb4ef151a44798159d15870a39945"
msgid "དེ␣འི་ ཡིད་ ལ་ འདོད་པ་ སྐྱེས་པ་ དེ་ མེད་པ␣ར་ གྱུར་ ཏོ ། །"
msgstr ""

# དེ་ནས་དེ་ཟླ་བ་དགུའམ་བཅུ་ལོན་པ་དང་།
# བུ་མོ་གཟུགས་བཟང་ཞིང་བལྟ་<«ལི་»«སྣར་»«ཅོ་»ལྟ།>ན་སྡུག་ལ་མཛེས་པ་ཞིག་བཙས་ནས།
msgctxt "bb9ff1a96b2a42cda2313fc8229021ac"
msgid ""
"དེ་ནས་ དེ་ ཟླ་བ་ དགུ␣འམ་ བཅུ་ ལོན་པ་ དང་ །  བུ་མོ་ གཟུགས་བཟང་ ཞིང་ བལྟ་ ན་ "
"སྡུག་ ལ་ མཛེས་པ་ ཞིག་ བཙས་ ནས །"
msgstr ""

msgctxt "c3cb8ea30b6a42fcb1c45140b920e810"
msgid "དེ␣འི་ བཙས་ སྟོན་ རྒྱས་པ␣ར་ བྱས་ ཏེ །"
msgstr ""

# བུ་མོ་འདིའི་མིང་ཅི་སྐད་གདགས་ཞེས་<«གཡུང་»«ལི་»«པེ་»«ཅོ་»ཤེས།>
msgctxt "8a77490c9e0e4c1d84e3426e5e86954f"
msgid "བུ་མོ་ འདི␣འི་ མིང་ ཅི་ སྐད་ གདགས་ ཞེས་"
msgstr ""

msgctxt "3b6f52f07fb64cc0887164467407c4e5"
msgid "མིང་འདོགས་པ␣ར་ བྱེད་ དེ །"
msgstr ""

# བུ་མོ་འདིའི་<«ཞོལ་»འདིས།>མས་འདུས་པའི་ནང་དུ་གཞུང་སྨྲ་བ་ཐམས་ཅད་ཚར་བཅད་པས་ན་བུ་མོ་འདིའི་མིང་འདུས་མོ་ཞེས་གདགས་སོ་ཞེས་
msgctxt "c4b3e283f8544ab3b0f88e6dc1f72912"
msgid ""
"བུ་མོ་ འདི␣འི་ མས་ འདུས་པ␣འི་ ནང་ དུ་ གཞུང་ སྨྲ་བ་ ཐམས་ཅད་ ཚར་ བཅད་པ␣ས་ ན་ "
"བུ་མོ་ འདི␣འི་ མིང་ འདུས་ མོ་ ཞེས་ གདགས་ སོ་ ཞེས་"
msgstr ""

msgctxt "c4038a86572b4df48d3fe5b39ea08812"
msgid "ཟེར་ རོ ། །"
msgstr ""

# དེ་ནས་བུ་མོ་འདུས་མོ་པང་ན་འཚོ་བའི་མ་མ་གཉིས་དང་།
# ནུ་མ་སྣུན་<«གཡུང་»«པེ་»བསྣུན།>པའི་མ་མ་གཉིས་དང་། དྲི་མ་འཁྲུ་བའི་མ་མ་གཉིས་དང་།
# རྩེ་བའི་མ་མ་གཉིས་ཏེ<«གཡུང་»«ལི་»«པེ་»«ཅོ་»སྟེ།>།
# མ་མ་བརྒྱད་ལ་གཏད་<«གཡུང་»བཏད།>ནས།
msgctxt "e3b27c4ef07b4d0eb5e72769701ba28d"
msgid ""
"དེ་ནས་ བུ་མོ་ འདུས་ མོ་ པང་ ན་ འཚོ་བ␣འི་ མ་མ་ གཉིས་ དང་ །  ནུ་མ་ སྣུན་པ␣འི་ "
"མ་མ་ གཉིས་ དང་ །  དྲི་མ་ འཁྲུ་བ␣འི་ མ་མ་ གཉིས་ དང་ །  རྩེ་བ␣འི་ མ་མ་ གཉིས་ "
"ཏེ །  མ་མ་ བརྒྱད་ ལ་ གཏད་ ནས །"
msgstr ""

# དེ་སྲེད་མེད་ཀྱི་བུའི་ལག་ན་ཐོགས་པའི་རྨ་བྱའི་མདོངས་དང་བསྲུང་<«ཞོལ་»སྲུང་།>བའི་སྲད་<«གཡུང་»སྲས།>བུ་དང་།
# འོ་མ་དང་། ཞོ་དང་། མར་དང་། ཞུན་མར་དང་<«གཡུང་»–མར་དང་།>།
# མར་གྱི་ཉིང་གུས་<«གཡུང་»«པེ་»སྙིང་ཀུས། «ལི་»«སྣར་»«ཅོ་»སྙིང་གུས།
# «ཞོལ་»སྙིང་ཁུས།>བསྐྱེད་བསྲིངས་ཏེ
msgctxt "068f775f425645138bad248fa62a7214"
msgid ""
"དེ་ སྲེད་ མེད་ ཀྱི་ བུ␣འི་ ལག་ ན་ ཐོགས་པ␣འི་ རྨ་བྱ␣འི་ མདོངས་ དང་ "
"བསྲུང་བ␣འི་ སྲད་བུ་ དང་ །  འོ་མ་ དང་ །  ཞོ་ དང་ །  མར་ དང་ །  ཞུན་མར་ དང་ ། "
" མར་ གྱི་ ཉིང་ གུས་ བསྐྱེད་ བསྲིངས་ ཏེ"
msgstr ""

msgctxt "ea8ef16b0a3046fb8514f6297a4fcf36"
msgid "མཚོ␣འི་ ནང་ གི་ པད་མ་ བཞིན་ དུ་ སྐྱེ་ དྲག་པ␣ར་ གྱུར་ ཏོ ། །"
msgstr ""

# དེ་ནས་དེ་གང་གི་ཚེ་<«གཡུང་»«ལི་»«པེ་»«སྣར་»«ཅོ་»«ཞོལ་»+དེ།>ཆེན་མོར་གྱུར་པ་དེའི་ཚེ།
# དེ་རྒྱལ་པོའི་ཕོ་བྲང་དེ་ཉིད་དུ་ཡོལ་བའི་ནང་དུ་བཞག་<«གཡུང་»«པེ་»གཞགའཀྱེ>ནས་
msgctxt "b5f15b32274e442bb1d0e17254eab97b"
msgid ""
"དེ་ནས་ དེ་ གང་ གི་ ཚེ་ ཆེན་ མོ␣ར་ གྱུར་པ་ དེ␣འི་ ཚེ །  དེ་ རྒྱལ་པོ␣འི་ "
"ཕོ་བྲང་ དེ་ཉིད་ དུ་ ཡོལ་བ␣འི་ ནང་ དུ་ བཞག་ ནས་"
msgstr ""

# ཡི་གེ་བསླབས་སོ<«གཡུང་»«ལི་»«པེ་»«སྣར་»«ཅོ་»དེའི།>། །
msgctxt "48937620462f4fe7bd2e96270c8744ac"
msgid "ཡི་གེ་ བསླབས་ སོ ། །"
msgstr ""

# དེ་<«གཡུང་»སེམས་ཞིང་།>ཡི་གེ་བྱང་ནས་
msgctxt "cdd5d6d1ab9f460c8817b95ba9ed5a5e"
msgid "དེ་ ཡི་གེ་ བྱང་ ནས་"
msgstr ""

# དེས་ཡོལ་བའི་ནང་འབའ་ཞིག་ཏུ་བསྟན་བཅོས་ཐམས་ཅད་<«གཡུང་»«ལི་»«པེ་»«སྣར་»«ཅོ་»ཏག་ཤ་ཤི།
# «ཞོལ་»ཏཁཱ་ཤཱི།>བསླབས་ནས་
msgctxt "06d96e27239c47829d971f018750fec6"
msgid "དེ␣ས་ ཡོལ་བ␣འི་ ནང་ འབའ་ཞིག་ ཏུ་ བསྟན་བཅོས་ ཐམས་ཅད་ བསླབས་ ནས་"
msgstr ""

# ཡུལ་ཏཀྵ་<24,ྲ>ཤཱི་<25,2,༢>ལ་དེར་གཞུང་སྨྲ་བ་ཐམས་ཅད་ཚར་བཅད་དོ། །
msgctxt "01de7950154d46a4b04b94b235391472"
msgid "ཡུལ་ ཏཀྵ་ ཤཱི་ ལ་ དེ␣ར་ གཞུང་ སྨྲ་བ་ ཐམས་ཅད་ ཚར་ བཅད་ དོ ། །"
msgstr ""

msgctxt "2fce0f50005a4017ab380da8e8c0cb0e"
msgid "དེ་ནས་ ཕྱི་ ཞིག་ ན་ རྒྱལ་པོ␣ས་ བུ་མོ་ དེ་ ལ་ དྲིས་པ །"
msgstr ""

msgctxt "bb10ab1fe4734fbc831a100e864f9387"
msgid "ཁྱོད་ ངས་ སུ་ ལ་ སྦྱིན་པ␣ར་ བྱ །"
msgstr ""

msgctxt "365332923726453db614dbac2149935f"
msgid "བུ་མོ␣ས་ སྨྲས་པ །"
msgstr ""

# སུས་གཞུང་བརྩད་<«གཡུང་»བཙད།>པས་བདག་ཐུབ་པར་ནུས་པ་<«ཞོལ་»ལ།>དེ་ལ་སྩལ་བར་གསོལ།
msgctxt "1a1b453973be438495de498b8a79f96c"
msgid "སུ␣ས་ གཞུང་ བརྩད་པ␣ས་ བདག་ ཐུབ་པ␣ར་ ནུས་པ་ དེ་ ལ་ སྩལ་བ␣ར་ གསོལ །"
msgstr ""

msgctxt "684c33d50b054eb3a9c7c8744da28088"
msgid "དེ་ནས་ རྒྱལ་པོ␣ས་ དེ་ ཐོས་ ནས་"
msgstr ""

# འདིས་<«གཡུང་»«པེ་»འདིས།>འདི་ལྟ་བུའི་ཚིག་སྨྲས་པ་ལས་ན།
msgctxt "a0b9b96972fe4a64b8fbddf7080a66dd"
msgid "འདི␣ས་ འདི་ ལྟ་བུ␣འི་ ཚིག་ སྨྲས་པ་ ལ␣ས་ ན །"
msgstr ""

msgctxt "b191dd93a9cd4fe1a52384d017c377a8"
msgid ""
"བུ་མོ་ འདི་ ནི་ ངས་ གཞུང་ སྨྲ་བ་ གང་ གིས་ འདི་ ཐུབ་པ་ དེ་ འབའ་ཞིག་ ལ་ "
"སྦྱིན་པ␣ར་ བྱ␣འི་"
msgstr ""

msgctxt "bd23e8f4a4d54cf08b93c5fa76b56697"
msgid "བྱད་གཟུགས་ བཟང་བ་ ལ་ ཡང་ མི་ སྦྱིན །"
msgstr ""

msgctxt "fa4cf9f9cfbd4550aca1dbb4b6584e84"
msgid "རིགས་ མཐོ་བ་ ལ་ ཡང་ མི་ སྦྱིན །"
msgstr ""

msgctxt "4657cb42059c45d988f433df6165830b"
msgid "དབང་ཕྱུག་ དང་ ལྡན་པ་ ལ་ ཡང་ མི་ སྦྱིན་ ནོ་ ཞེས་"
msgstr ""

msgctxt "6fb191ff94604d3f8fdf4f3c165f98a7"
msgid "ཟེར་ རོ ། །"
msgstr ""

# དེ་ནས་དེའི་<«ཁུ་»དེས།>སྒྲ་དེ་ཀུན་ཏུ་<«གཡུང་»«པེ་»བུ།>གྲགས་ནས།
msgctxt "346356e689304fb5ad02bcd2cad5a45d"
msgid "དེ་ནས་ དེ␣འི་ སྒྲ་ དེ་ ཀུན་ ཏུ་ གྲགས་ ནས །"
msgstr ""

# དེ་ཐོས་མ་ཐག་ཏུ་ཕྱོགས་ཀུན་ནས་གཞུང་སྨྲ་བ་རྣམས་ལྷགས་<«ལི་»«སྣར་»«ཅོ་»«ཞོལ་»ལྷག>པ་ལས་
msgctxt "709baa72846647f080b56b93e6bdb2d9"
msgid "དེ་ ཐོས་ མ་ ཐག་ ཏུ་ ཕྱོགས་ ཀུན་ ནས་ གཞུང་ སྨྲ་བ་ རྣམས་ ལྷགས་པ་ ལ␣ས་"
msgstr ""

msgctxt "1ad8006b54194b6b8d9979fb584a7deb"
msgid "བུ་མོ་ དེ␣ས་ ཐམས་ཅད་ ཚར་ བཅད་ དོ ། །"
msgstr ""

# དེ་ནས་དེའི་ཚེ་ལྷོ་ཕྱོགས་ན་གཞུང་སྨྲ་བ་རིའུ་<«གཡུང་»«པེ་»རི་བུ།>ཞེས་བྱ་བ་
msgctxt "ba614410b1904205aa749a51a450893e"
msgid "དེ་ནས་ དེ␣འི་ ཚེ་ ལྷོ་ཕྱོགས་ ན་ གཞུང་ སྨྲ་བ་ རིའུ་ ཞེས་ བྱ་བ་"
msgstr ""

# བསྟན་<«གཡུང་»སྟན།>བཅོས་ཐམས་ཅད་ལ་འཇིགས་པ་མེད་པ་
msgctxt "58a7c1d1098d408389c6e0d53ee2a77e"
msgid "བསྟན་བཅོས་ ཐམས་ཅད་ ལ་ འཇིགས་པ་ མེད་པ་"
msgstr ""

msgctxt "30bde30f6d74476dbab4845fd72c38bf"
msgid "གཙུག་ལག་ ཆེ་བ །"
msgstr ""

# གཙུག་ལག་ལ་འབྱམ་<«ཞོལ་»འབྱམས་པ།>པ་
msgctxt "68952c9e93214d60b3abf2a48552f337"
msgid "གཙུག་ལག་ ལ་ འབྱམ་པ་"
msgstr ""

# གཟུགས་བཟང་ཞིང་བལྟ་<«ལི་»«སྣར་»«ཅོ་»ལྟ།>ན་སྡུག་ལ་མཛེས་པ།
msgctxt "bbc41cff06d541d3bef8e67552e66265"
msgid "གཟུགས་བཟང་ ཞིང་ བལྟ་ ན་ སྡུག་ ལ་ མཛེས་པ །"
msgstr ""

# ཁ་དོག་ཤིན་ཏུ་རྒྱས་པ་དང་ལྡན་པ་ཞིག་གནས་པ་དེས་ཡུལ་ཏཀྵ་ཤཱི་<«གཡུང་»«ལི་»«པེ་»«སྣར་»«ཅོ་»ཏག་ཤ་ཤི།>ལའི་རྒྱལ་པོ་པད་མའི་སྙིང་པོའི་སྲས་མོས་གཞུང་སྨྲ་<«ཞོལ་»སྒྲ།>བ་ཐམས་ཅད་ཚར་བཅད་ལ་
msgctxt "62260bf6ab4a4c53bfddd11bfdce4012"
