msgctxt "b38db65b1c244306adfc4164d778b17d"
msgid "ཤིང་རྟ་ བཞི་པ་ ནི །"
msgstr ""

# གླེང་གཞི་མཉན་<«གཡུང་»བཞི་གཉན།>དུ་ཡོད་པ་ན་བཞུགས་ཏེ།
msgctxt "ee88e0c963f94d5f890b617abdf90767"
msgid "གླེང་ གཞི་ མཉན་ དུ་ཡོད་པ་ ན་ བཞུགས་ ཏེ །"
msgstr ""

# དེའི་ཚེ་བཅོམ་ལྡན་འདས་མཉན་<«གཡུང་»གཉན།
# «ཁུ་»མཉེན།>དུ་ཡོད་པ་ནས་རྒྱལ་པོའི་ཁབ་ཏུ་གཤེགས་སོ། །
msgctxt "8eea873bb70a402c84a7fa0fb29145c4"
msgid ""
"དེ␣འི་ ཚེ་ བཅོམ་ལྡན་འདས་ མཉན་ དུ་ཡོད་པ་ ནས་ རྒྱལ་པོ␣འི་ ཁབ་ ཏུ་ གཤེགས་ སོ "
"། །"
msgstr ""

msgctxt "4f9cfaeb3874444384fc74e62cf5f4b1"
msgid "བྲམ་ཟེ་ ཞིག་ ཀྱང་ ཤིང་རྟ་ ལ་ ཞོན་ ཏེ་"
msgstr ""

msgctxt "d9f85b4895c9493dbabf568254f82b04"
msgid "ལམ་ དུ་ སྣོལ་མ␣ར་ འགྲོ་བ་ ལ␣ས །"
msgstr ""

msgctxt "d139896e64344d12862aafa4b22d9792"
msgid "བྲམ་ཟེ་ དེ␣ས་ བཅོམ་ལྡན་འདས་ མཐོང་ ནས་"
msgstr ""

msgctxt "4403ce1c903f41e485b23cea8f21d2da"
msgid "ཤིང་རྟ་ ལ␣ས་ བབས་ ཏེ་"
msgstr ""

msgctxt "d0a5ba05c5814d7ea0190d7f40f2d9fa"
msgid "བཅོམ་ལྡན་འདས་ ལ་ ཤིང་རྟ་ བསྟབས་ སོ ། །"
msgstr ""

# བཅོམ་ལྡན་འདས་ཀྱིས་ཀྱང་དེ་ཉིད་ལ་ཐུགས་བརྩེ་བའི་ཕྱིར།
# ཤིང་རྟའི་སྟེང་<«གཡུང་»སྟའི།>གི་ནམ་མཁའ་ལ་བཞུགས་སོ། །
msgctxt "fe63cfda9c1f4fe291d387f457b05879"
msgid ""
"བཅོམ་ལྡན་འདས་ ཀྱིས་ ཀྱང་ དེ་ཉིད་ ལ་ ཐུགས་ བརྩེ་བ␣འི་ ཕྱིར །  ཤིང་རྟ␣འི་ "
"སྟེང་ གི་ ནམ་མཁའ་ ལ་ བཞུགས་ སོ ། །"
msgstr ""

# དེ་ནས་བྲམ་ཟེ་དེ་<«གཡུང་»«པེ་»–དེ།>རྒྱུ་དེ་ལས་དགའ་བ་ཆེན་པོ་སྐྱེས་སོ། །
msgctxt "55933435e11443bf9b2f9f47b46e0c02"
msgid "དེ་ནས་ བྲམ་ཟེ་ དེ་ རྒྱུ་ དེ་ལས་ དགའ་བ་ ཆེན་པོ་ སྐྱེས་ སོ ། །"
msgstr ""

msgctxt "3bca7841cc8647b6bbb4390e3c1b5745"
msgid "དགའ་བ་ ཆེན་པོ་ སྐྱེས་ ནས་"
msgstr ""

msgctxt "1dd8df5e58944b60b880e0e927bc8841"
msgid "བཅོམ་ལྡན་འདས་ ཀྱི་ ཐད་ ནས་ སོང་ ངོ་ ། །"
msgstr ""

# དེའི་སྐབས་སུ་བཅོམ་ལྡན་འདས་ཀྱིས་འཛུམ་པ་མཛད་དོ<«གཡུང་»«པེ་»དེ།>། །
msgctxt "9a69a9af1e774c16a34866c046000fb2"
msgid "དེ␣འི་ སྐབས་ སུ་ བཅོམ་ལྡན་འདས་ ཀྱིས་ འཛུམ་པ་ མཛད་ དོ ། །"
msgstr ""

msgctxt "6218437a0f294c9ab3ce80d69401d984"
msgid ""
"འདི་ ནི་ སངས་རྒྱས་ བཅོམ་ལྡན་འདས་ རྣམས་ གང་ གི་ ཚེ །  འཛུམ་པ་ མཛད་པ␣འི་ "
"ཆོས་ཉིད་ དེ །"
msgstr ""

msgctxt "111a058a66a24fac94655768dfa27760"
msgid ""
"དེ␣འི་ ཚེ་ ཞལ་ ནས་ འོད་ཟེར་ སྔོན་པོ་ དང་ །  སེར་པོ་ དང་ །  དམར་པོ་ དང་ །  "
"དཀར་པོ་ རྣམས་ བྱུང་ ནས་"
msgstr ""

msgctxt "893ae838717846ea97a1f6b6260fcccc"
msgid "ཁ་ཅིག་ ནི་ འོག་ ཏུ་ འགྲོ␣འོ ། །"
msgstr ""

msgctxt "b472103a422c4746b64369108f4db89a"
msgid "ཁ་ཅིག་ ནི་ སྟེང་ དུ་ འགྲོ␣འོ ། །"
msgstr ""

# འོག་ཏུ་འགྲོ་བ་གང་ཡིན་པ་དེ་དག་ནི། སེམས་ཅན་དམྱལ་བ་ཡང་སོས་དང་། ཐིག་ནག་དང་།
# བསྡུས་འཇོམས་དང་། ངུ་འབོད་དང་། ངུ་འབོད་ཆེན་པོ་དང་། ཚ་བ་དང་། རབ་ཏུ་ཚ་བ་དང་།
# མནར་མེད་པ་དང་། ཆུ་བུར་ཅན་དང་། ཆུ་བུར་རྡོལ་བ་<«གཡུང་»«པེ་»–བ།>དང་།
# སོ་ཐམས་ཐམས་<«སྣར་»«ཞོལ་»ཐམ་ཐམ།>དང་། ཀྱི་ཧུད་ཟེར་བ་<«གཡུང་»–བ།>དང་།
# ཨ་ཆུ་ཟེར་བ་དང་། ཨུད་པལ་ལྟར་གས་པ་དང་། པད་མ་ལྟར་གས་པ་དང་།
# པད་མ་ལྟར་གས་པ་ཆེན་པོར་སོང་ནས།
msgctxt "8da2242c7cb745af9f7d3195b993c425"
msgid ""
"འོག་ ཏུ་ འགྲོ་བ་ གང་ཡིན་པ་ དེ་དག་ ནི །  སེམས་ཅན་ དམྱལ་བ་ ཡང་སོས་ དང་ །  "
"ཐིག་ནག་ དང་ །  བསྡུས་འཇོམས་ དང་ །  ངུ་འབོད་ དང་ །  ངུ་འབོད་ ཆེན་པོ་ དང་ །  "
"ཚ་བ་ དང་ །  རབ་ཏུ་ ཚ་བ་ དང་ །  མནར་མེད་པ་ དང་ །  ཆུ་བུར་ ཅན་ དང་ །  ཆུ་བུར་ "
"རྡོལ་བ་ དང་ །  སོ་ ཐམས་ ཐམས་ དང་ །  ཀྱི་ཧུད་ ཟེར་བ་ དང་ །  ཨ་ཆུ་ ཟེར་བ་ དང་ "
"།  ཨུད་ པལ་ ལྟར་ གས་པ་ དང་ །  པད་མ་ ལྟར་ གས་པ་ དང་ །  པད་མ་ ལྟར་ གས་པ་ "
"ཆེན་པོ␣ར་ སོང་ ནས །"
msgstr ""

# ཚ་བའི་སེམས་ཅན་དམྱལ་བ་གང་ཡིན་པ་དེ་དག་ཏུ་<«གཡུང་»«པེ་»–ཏུ།
# «ལི»«ཅོ་»–དུ།>ནི་བསིལ་བར་གྱུར་ཏེ་
msgctxt "778511dc3dab403a88ab27f41af32501"
msgid "ཚ་བ␣འི་ སེམས་ཅན་ དམྱལ་བ་ གང་ཡིན་པ་ དེ་དག་ ཏུ་ ནི་ བསིལ་བ␣ར་ གྱུར་ ཏེ་"
msgstr ""

msgctxt "91eb356a540747c18bc09950c0ed4b5a"
msgid "འབབ་བོ ། །"
msgstr ""

# གྲང་བའི་སེམས་ཅན་དམྱལ་བ་གང་ཡིན་པ་དེ་དག་ཏུ་<«གཡུང་»«པེ་»–ཏུ།>ནི་དྲོ་བར་གྱུར་ཏེ་
msgctxt "fea066f94f22469980b682427d407271"
msgid "གྲང་བ␣འི་ སེམས་ཅན་ དམྱལ་བ་ གང་ཡིན་པ་ དེ་དག་ ཏུ་ ནི་ དྲོ་བ␣ར་ གྱུར་ ཏེ་"
msgstr ""

msgctxt "6644d33ad7724d86b75381776dd3f928"
msgid "འབབ་བོ ། །"
msgstr ""

# དེས་ན་སེམས་ཅན་དེ་དག་གི་གནོད་པའི་ཁྱད་པར་རྣམས་རྩེ་ཞིལ་བར་གྱུར་ཏོ<«གཡུང་»«ལི་»«པེ་»«སྣར་»«ཅོ་»འགྱུར་རོ།>།
# །
msgctxt "9481e08c0fbc4acfa77802c6e8b8f220"
msgid ""
"དེས་ན་ སེམས་ཅན་ དེ་དག་ གི་ གནོད་པ␣འི་ ཁྱད་པར་ རྣམས་ རྩེ་ ཞིལ་ བར་ གྱུར་ ཏོ "
"། །"
msgstr ""

# དེ་ནས་དེ་དག་འདི་སྙམ་དུ་<«གཡུང་»«པེ་»–དུ།>སེམས་ཏེ།
msgctxt "dbdcc6c635134fa0b2cd09c055d129f0"
msgid "དེ་ནས་ དེ་དག་ འདི་ སྙམ་ དུ་ སེམས་ ཏེ །"
msgstr ""

msgctxt "0789e5f8e14f4de194e1071ed25bd3b1"
msgid "ཀྱེ་"
msgstr ""

# ཅི་བདག་ཅག་<«གཡུང་»«ལི་»«པེ་»«སྣར་»«ཅོ་»«ཞོལ་»–ཅག>འདི་ནས་ཤི་འཕོས་སམ་
msgctxt "6e1ddfe307b7403a8d323181d0a9e8e8"
msgid "ཅི་ བདག་ཅག་ འདི་ ནས་ ཤི་འཕོས་ སམ་"
msgstr ""

msgctxt "c9f08a9c3d0d479683d3bb95eb1508eb"
msgid "འོན་ཏེ་ གཞན་ དུ་ སྐྱེས་ སམ་ སྙམ་ མོ ། །"
msgstr ""

# དེ་ནས་དེ་དག་དད་པ་བསྐྱེད་པའི་ཕྱིར་བཅོམ་ལྡན་འདས་སྤྲུལ་པ་འགྱེད་པར་མཛད་<«པེ་»འགྱིད་པར་མཛད།
# «ཅོ་»ཛད།>དེ།
msgctxt "0279de9c8fb54c68b9eb4f524e58944f"
msgid ""
"དེ་ནས་ དེ་དག་ དད་པ་ བསྐྱེད་པ␣འི་ ཕྱིར་ བཅོམ་ལྡན་འདས་ སྤྲུལ་པ་ འགྱེད་པ␣ར་ "
"མཛད་ དེ །"
msgstr ""

msgctxt "6dee5f087a764c768247c0c697483006"
msgid "དེ་དག་ གིས་ སྤྲུལ་པ་ མཐོང་ ནས་"
msgstr ""

msgctxt "44cd16ff7a2c420dbe66e1ed0d8058d4"
msgid "འདི་ སྙམ་ དུ་ སེམས་ ཏེ །"
msgstr ""

msgctxt "e99f831b7feb449a874a628cff37ef62"
msgid "ཀྱེ་"
msgstr ""

# བདག་ཅག་ནི་འདི་ནས་ཀྱང་<«གཡུང་»«པེ་»–ཀྱང་།>ཤི་འཕོས་པར་མ་གྱུར་ལ་
msgctxt "9ee0cfd97bfd4610ab71c184d453b0aa"
msgid "བདག་ཅག་ ནི་ འདི་ ནས་ ཀྱང་ ཤི་འཕོས་ པར་མ་ གྱུར་ ལ་"
msgstr ""

msgctxt "5dd6f792496c4f85a1a19e61b0e86608"
msgid "གཞན་ དུ་ ཡང་ མ་ སྐྱེས་ མོད་ ཀྱི །"
msgstr ""

msgctxt "4b87e3e7bca947d69825f9e730580f19"
msgid ""
"འོན་ཀྱང་ སྔོན་མ་ བྱུང་བ␣འི་ སེམས་ཅན་ སྣང་བ་ འདི␣འི་ མཐུ␣ས་ བདག་ཅག་ གི་ "
"གནོད་པ␣འི་ ཁྱད་པར་ རྣམས་ རྩེ་ ཞིལ་ བར་ གྱུར་ ཏོ་ སྙམ་ མོ ། །"
msgstr ""

msgctxt "a5bfd312fe9b4616a0422a9c8d617cc8"
msgid "དེ་ནས་ དེ་དག་ སྤྲུལ་པ་ ལ་ སེམས་ མངོན་པར་ དགའ་བ␣ར་ གྱུར་ ནས་"
msgstr ""

msgctxt "88adeba393f5440abab9505eade8a3e1"
msgid "སེམས་ཅན་ དམྱལ་བ་ མྱོང་བ␣ར་ འགྱུར་བ␣འི་ ལ␣ས་ དེ་ ཟད་ དེ །"
msgstr ""

# ལྷ་དང་མི་རྣམས་[58b]ཀྱི་ནང་དུ་གང་དུ་བདེན་པ་རྣམས་ཀྱི་སྣོད་དུ་རུང་བར་འགྱུར་བ་དེར་ཉིང་མཚམས་<«ལི་»«ཅོ་»འཚམས།>འཛིན་པར་འགྱུར་རོ།
# །
msgctxt "25fb9f5dfca248bc98c188fa153c3caf"
msgid ""
"ལྷ་ དང་ མི་ རྣམས་ ཀྱི་ ནང་ དུ་ གང་ དུ་ བདེན་པ་ རྣམས་ ཀྱི་ སྣོད་ དུ་ རུང་བ␣ར་"
" འགྱུར་བ་ དེ␣ར་ ཉིང་མཚམས་ འཛིན་པ␣ར་ འགྱུར་ རོ ། །"
msgstr ""

# སྟེང་དུ་འགྲོ་བ་གང་<«གཡུང་»གར།>ཡིན་པ་དེ་དག་ནི་རྒྱལ་ཆེན་བཞིའི་རིས་རྣམས་དང་སུམ་ཅུ་<«ལི་»བཅུ།>རྩ་གསུམ་རྣམས་དང་འཐབ་བྲལ་རྣམས་དང་དགའ་ལྡན་རྣམས་དང་།
# འཕྲུལ་དགའ་རྣམས་དང་། གཞན་འཕྲུལ་དབང་བྱེད་རྣམས་དང་། ཚངས་རིས་རྣམས་དང་།
# ཚངས་པའི་མདུན་<«གཡུང་»བདུན།>ན་འདོན་པ་རྣམས་དང་། ཚངས་པ་ཆེན་པོ་རྣམས་དང་།
# འོད་ཆུང་རྣམས་དང་། ཚད་མེད་འོད་རྣམས་དང་། འོད་གསལ་རྣམས་དང་།
# དགེ་ཆུང་<«གཡུང་»«པེ་»ཆུང་དགེ།>རྣམས་དང་། ཚད་མེད་དགེ་རྣམས་དང་།
# དགེ་རྒྱས་རྣམས་དང་། སྤྲིན་<«གཡུང་»སྤྲོན།>མེད་རྣམས་དང་།
# བསོད་ནམས་<«གཡུང་»–ནམས།>སྐྱེས་རྣམས་དང་། འབྲས་བུ་ཆེ་<«གཡུང་»«པེ་»+བ།>རྣམས་དང་།
# མི་ཆེ་<«པེ»+བ།>རྣམས་དང་། མི་གདུང་<«གཡུང་»«པེ་»+བ>རྣམས་དང་།
# གྱ་ནོམ་སྣང་<«གཡུང་»«པེ་»+བ>རྣམས་དང་། ཤིན་ཏུ་མཐོང་<«པེ་»+བ།>རྣམས་དང་།
# འོག་མིན་གྱི་ལྷ་རྣམས་ཀྱི་བར་དུ་སོང་ནས<«གཡུང་»+ན།>།
msgctxt "cf1890a5ef5a47628e1264d9b0f40a55"
msgid ""
"སྟེང་ དུ་ འགྲོ་བ་ གང་ཡིན་པ་ དེ་དག་ ནི་ རྒྱལ་ཆེན་ བཞི␣འི་ རིས་ རྣམས་ དང་ "
"སུམ་ཅུ་རྩ་གསུམ་ རྣམས་ དང་ འཐབ་བྲལ་ རྣམས་ དང་ དགའ་ལྡན་ རྣམས་ དང་ །  "
"འཕྲུལ་དགའ་ རྣམས་ དང་ །  གཞན་འཕྲུལ་ དབང་ བྱེད་ རྣམས་ དང་ །  ཚངས་རིས་ རྣམས་ "
"དང་ །  ཚངས་པ␣འི་ མདུན་ན་འདོན་པ་ རྣམས་ དང་ །  ཚངས་པ་ ཆེན་པོ་ རྣམས་ དང་ །  "
"འོད་ཆུང་ རྣམས་ དང་ །  ཚད་ མེད་ འོད་ རྣམས་ དང་ །  འོད་གསལ་ རྣམས་ དང་ །  "
"དགེ་ཆུང་ རྣམས་ དང་ །  ཚད་ མེད་ དགེ་ རྣམས་ དང་ །  དགེ་རྒྱས་ རྣམས་ དང་ །  "
"སྤྲིན་ མེད་ རྣམས་ དང་ །  བསོད་ནམས་ སྐྱེས་ རྣམས་ དང་ །  འབྲས་བུ་ ཆེ་ རྣམས་ "
"དང་ །  མི་ ཆེ་ རྣམས་ དང་ །  མི་ གདུང་ རྣམས་ དང་ །  གྱ་ནོམ་ སྣང་ རྣམས་ དང་ ། "
" ཤིན་ཏུ་ མཐོང་ རྣམས་ དང་ །  འོག་མིན་ གྱི་ ལྷ་ རྣམས་ ཀྱི་ བར་ དུ་ སོང་ ནས །"
msgstr ""

# མི་རྟག་པ་དང་། སྡུག་བསྔལ་བ་དང་། སྟོང་པ་དང་།
# བདག་མེད་པའི་སྒྲ་སྒྲོགས་སོ<«ཞོལ་»སྒྲོག་གོ།>། །
msgctxt "bec9e9d8906e44b0ab3055eb7fce02c1"
msgid ""
"མི་རྟག་པ་ དང་ །  སྡུག་བསྔལ་བ་ དང་ །  སྟོང་པ་ དང་ །  བདག་མེད་པ␣འི་ སྒྲ་ "
"སྒྲོགས་ སོ ། །"
msgstr ""

# ཚིགས་སུ་བཅད་པ་གཉིས་ཀྱང་རྗོད་<«སྣར་»བརྗོད།>པར་བྱེད་དེ།
msgctxt "f64a6f68172642e9a6976801fc1d1766"
msgid "ཚིགས་ སུ་ བཅད་པ་ གཉིས་ ཀྱང་ རྗོད་པ␣ར་ བྱེད་ དེ །"
msgstr ""

msgctxt "c3ae51171051437cb51c84531ad47a2a"
msgid "ངེས་པར་ འབྱུང་བ་ བརྩམ་པ␣ར་ གྱིས ། །"
msgstr ""

msgctxt "f82aff34593d4fe595c29755251a19a0"
msgid "སངས་རྒྱས་ བསྟན་ ལ་ སྦྱོར་བ␣ར་ གྱིས ། །"
msgstr ""

# འདམ་བུའི་ཁྱིམ་ལ་གླང་ཆེན་བཞིན། ། འཆི་བདག་སྡེ་ནི་གཞོམ་པར་གྱིས།<«གཡུང་»+ང་།> །
msgctxt "5fb0f4a176e34534b307e1ae7262f7bf"
msgid ""
"འདམ་བུ␣འི་ ཁྱིམ་ ལ་ གླང་ཆེན་ བཞིན ། །  འཆི་བདག་ སྡེ་ ནི་ གཞོམ་པ␣ར་ གྱིས ། །"
msgstr ""

msgctxt "0ac3b01fb99646bb9411704bdfb2987d"
msgid ""
"གང་ཞིག་ ཆོས་ འདུལ་ འདི་ཉིད་ ལ ། །  བག་ ཡོད་ སྤྱོད་པ␣ར་ བྱེད་པ་ ནི ། །  "
"སྐྱེ་བ␣འི་ འཁོར་བ་ རབ་ སྤངས་ ནས ། །"
msgstr ""

msgctxt "24bd415c527844b7a31316b593499fda"
msgid "སྡུག་བསྔལ་ ཟད་པར་ བྱེད་པ␣ར་ འགྱུར ། །  ཞེས་"
msgstr ""

# རྗོད་<«སྣར་»– བརྗོད།>པར་བྱེད་དོ། །
msgctxt "76061bd631cb4ec0b8ad5934ca14cae1"
msgid "རྗོད་པ␣ར་ བྱེད་ དོ ། །"
msgstr ""

msgctxt "a61eb5e11b8a48038ea9677d57701382"
msgid ""
"དེ་ནས་ འོད་ཟེར་ དེ་དག་ གིས་ སྟོང་གསུམ་ གྱི་ སྟོང་ ཆེན་པོ␣འི་ འཇིག་རྟེན་ གྱི་"
" ཁམས་ བསྐོར་ ནས །"
msgstr ""

msgctxt "06d2ad1d224f4aa796066fdd17dd32c0"
msgid "བཅོམ་ལྡན་འདས་ ཀྱི་ ཕྱི་བཞིན་ འབྲང་ སྟེ་"
msgstr ""

# གལ་ཏེ་<«གཡུང་»«པེ་»+བཅོམ་ལྡན་འདས་ཀྱི་ཕྱི་བཞིན་འབྲང་སྟེ།>བཅོམ་ལྡན་འདས་ཀྱིས་འདས་པའི་ལས་ལུང་སྟོན་པར་བཞེད་ན་ནི་
msgctxt "880f1d78c6f541d996bc9ce62ae530f4"
msgid "གལ་ཏེ་ བཅོམ་ལྡན་འདས་ ཀྱིས་ འདས་པ␣འི་ ལ␣ས་ ལུང་སྟོན་པ␣ར་ བཞེད་ ན་ ནི་"
msgstr ""

msgctxt "aceb44c329b740e6ab2cb6af9a27f4aa"
msgid "བཅོམ་ལྡན་འདས་ ཀྱི་ སྣམ་ལོགས་ སུ་ མི་སྣང་བ␣ར་ འགྱུར་ རོ ། །"
msgstr ""

msgctxt "28c73f9dbd704b008d63cf0e01e1402e"
msgid "མ་འོངས་པ␣འི་ ལ␣ས་ ལུང་སྟོན་པ␣ར་ བཞེད་ ན་ ནི་"
msgstr ""

msgctxt "66af3be4ebd3444ab38916a8db054b6d"
msgid "མདུན་ལོགས་ སུ་ མི་སྣང་བ␣ར་ འགྱུར་ རོ ། །"
msgstr ""

msgctxt "d8d54d86504d45548371a2f716bcdff0"
msgid "སེམས་ཅན་ དམྱལ་བ␣ར་ སྐྱེ་བ␣ར་ ལུང་སྟོན་པ␣ར་ བཞེད་ ན་ ནི་"
msgstr ""

msgctxt "c13b83ae362e421d9ac7bf0ddec4b2fb"
msgid "ཞབས་ ཀྱི་ མཐིལ་ དུ་ མི་སྣང་བ␣ར་ འགྱུར་ རོ ། །"
msgstr ""

msgctxt "9426380fc95048c4905e8bfcc2cb6552"
msgid "དུད་འགྲོ␣ར་ སྐྱེ་བ␣ར་ ལུང་སྟོན་པ␣ར་ བཞེད་ ན་ ནི་"
msgstr ""

msgctxt "10245ccc80bc40cf944926da4917c411"
msgid "ཞབས་ ཀྱི་ རྟིང་པ␣ར་ མི་སྣང་བ␣ར་ འགྱུར་ རོ ། །"
msgstr ""

msgctxt "d43bfadd051c4543a7ca04e572535bd7"
msgid "ཡི་དགས་ སུ་ སྐྱེ་བ␣ར་ ལུང་སྟོན་པ␣ར་ བཞེད་ ན་ ནི་"
