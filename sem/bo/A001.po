# ﻿[182b]གྲོག་མ་གཉིས་པ་<«གཡུང་»མ།>ནི།
msgctxt "168d14331f5049a5a00ccb634527a237"
msgid "﻿གྲོག་མ་ གཉིས་པ་ ནི །"
msgstr ""

msgctxt "a0067c0e2e9f4f769ac7018b07ba91bf"
msgid "གླེང་ གཞི་ མཉན་ དུ་ཡོད་པ་ ན་ བཞུགས་ ཏེ །"
msgstr ""

msgctxt "ed05b7a5fc17462f83cbaaeee1ad297b"
msgid ""
"གཏན་ཚིགས་ རྒྱས་པ␣ར་ ནི་ དབྱིག་ དག␣འི་ རྟོགས་པ་ བརྗོད་པ་ ལ␣ས་ འབྱུང་བ་ བཞིན་ "
"ཏེ །"
msgstr ""

# གང་གི་ཚེ་ཚེ་དང་ལྡན་པ་ཀུན་ཤེས་ཀཽཎྜི་<«ལི་»«ཁུ་»ཀཽཎྚི།>ནྱས་བདེན་པ་རྣམས་མཐོང་བར་གྱུར་པ་དེའི་ཚེ།
# ལྷ་བརྒྱད་ཁྲིས་ཀྱང་བདེན་པ་མཐོང་བར་གྱུར་ཏོ། །
msgctxt "e6e96ba8707441038c704ea2e3b96478"
msgid ""
"གང་ གི་ ཚེ་ཚེ་ དང་ ལྡན་པ་ ཀུན་ཤེས་ ཀཽཎྜི་ ནྱས་ བདེན་པ་ རྣམས་ མཐོང་བ␣ར་ "
"གྱུར་པ་ དེ␣འི་ ཚེ །  ལྷ་ བརྒྱད་ ཁྲིས་ ཀྱང་ བདེན་པ་ མཐོང་བ␣ར་ གྱུར་ ཏོ ། །"
msgstr ""

msgctxt "493297148a98464a8720cb569f0f42bb"
msgid "དེ་ནས་ དགེ་སློང་ རྣམས་ ཀྱིས་ སངས་རྒྱས་ བཅོམ་ལྡན་འདས་ ལ་ གསོལ་པ །"
msgstr ""

msgctxt "0ee99f044b7243b983e6354682f96017"
msgid "བཙུན་པ་"
msgstr ""

# བཅོམ་ལྡན་འདས་ཀྱིས་ཀཽཎྜི་<«ལི་»«ཁུ་»ཀཽཎྚི།>ནྱ་ལ་སོགས་པ་<«ཞོལ་»+དང༌།>ལྷ་བརྒྱད་ཁྲི་དམ་པའི་ཆོས་ཀྱི་བཅུད་ཀྱིས་ཚིམ་པར་མཛད་ནས།
msgctxt "72bd489e18f945678aca53814bf377bd"
msgid ""
"བཅོམ་ལྡན་འདས་ ཀྱིས་ ཀཽཎྜི་ ནྱ་ ལ་སོགས་པ་ ལྷ་ བརྒྱད་ ཁྲི་ དམ་པ␣འི་ ཆོས་ ཀྱི་ "
"བཅུད་ ཀྱིས་ ཚིམ་པ␣ར་ མཛད་ ནས །"
msgstr ""

# གཡུང་དྲུང་གི་མཐར་ཐུག་པ་གྲུབ་པ་དང་བདེ་བའི་མྱ་ངན་ལས་འདས་པ་ལ་:བཀོད་པ་<«གཡུང་»བཀོས།
# «པེ་»-དཀོད་པ།>ལ་གཟིགས།
msgctxt "7f019cd778ce45efa5615309cf126995"
msgid ""
"གཡུང་དྲུང་ གི་ མཐར་ཐུག་པ་ གྲུབ་པ་ དང་ བདེ་བ␣འི་ མྱ་ངན་ ལ␣ས་ འདས་པ་ ལ་ "
"བཀོད་པ་ ལ་ གཟིགས །"
msgstr ""

msgctxt "db6b114c0f9240eda9a607a7747d1897"
msgid "བཅོམ་ལྡན་འདས་ ཀྱིས་ བཀའ་ སྩལ་པ །"
msgstr ""

msgctxt "32a22e4156dd4986ad7e71951de384f6"
msgid "ད་ལྟར་ འབའ་ཞིག་ མ་ ཡིན་ ཏེ །"
msgstr ""

# འདས་པའི་དུས་སུ་ཡང་ཀཽཎྜི་<«ལི་»«ཁུ་»ཀཽཎྚི།>ནྱ་ལ་སོགས་པ།<«ཞོལ་»+དང་།>
# ལྷ་བརྒྱད་ཁྲི་པོ་འདི་དག་ཉིད་ངས་ཇི་ལྟར་རང་གི་ཤ་དང་ཁྲག་གིས་ཚིམ་པར་བྱས་ནས།
# སྲོག་ཕངས་པ་བྱིན་པ་དེ་ཉོན་ཅིག །
msgctxt "f95628b422cc445e8647c11a543a9f24"
msgid ""
"འདས་པ␣འི་ དུས་ སུ་ ཡང་ ཀཽཎྜི་ ནྱ་ ལ་སོགས་པ །  ལྷ་ བརྒྱད་ ཁྲི་པོ་ འདི་དག་ "
"ཉིད་ ངས་ ཇི་ལྟར་ རང་ གི་ ཤ་ དང་ ཁྲག་ གིས་ ཚིམ་པ␣ར་ བྱས་ ནས །  སྲོག་ ཕངས་པ་ "
"བྱིན་པ་ དེ་ ཉོན་ ཅིག  །"
msgstr ""

msgctxt "1e46c4576d204f5a9e51f66b3fa29f99"
msgid "དགེ་སློང་ དག་"
msgstr ""

# སྔོན་བྱུང་བ་འདས་པའི་དུས་ན།
# རི་ཁྲོད་ཅིག་ནས་ཐག་མི་རིང་བ་<«གཡུང་»«པེ་»-བ།>ཞིག་:ན། ཁུང་ཞིག་<«གཡུང་»-ན།
# ཁུང་ཞིག>གི་ནང་ན་སྲོག་ཆགས་སྐྱིན་གོར་སྙིང་རྗེ་ཆེ་བ་བྱམས་པའི་བདག་ཉིད་ཅན་སེམས་ཅན་ལ་བྱམས་པ་ཞིག་གནས་པ་དེ།
# རྩ་བ་དང་འབྲས་བུས་འཚོ་ཞིང་
msgctxt "31d6b015283a4831a6c406a96509a052"
msgid ""
"སྔོན་བྱུང་བ་ འདས་པ␣འི་ དུས་ ན །  རི་ཁྲོད་ ཅིག་ ནས་ ཐག་ མི་ རིང་བ་ ཞིག་ ན །  "
"ཁུང་ ཞིག་ གི་ ནང་ ན་ སྲོག་ཆགས་ སྐྱིན་གོར་ སྙིང་རྗེ་ ཆེ་བ་ བྱམས་པ␣འི་ "
"བདག་ཉིད་ ཅན་ སེམས་ཅན་ ལ་ བྱམས་པ་ ཞིག་ གནས་པ་ དེ །  རྩ་བ་ དང་ འབྲས་བུ␣ས་ འཚོ་"
" ཞིང་"
msgstr ""

# སུ་ལ་ཡང་འཚེ་མི་འདོད་པ་<«པེ་»ག>ཞིག་འདུག་པ་དེ་ཕྱི་ཞིག་ན་གཟས་<«གཡུང་»«པེ་»ཟས།>ལ་སོང་སོང་བ་ལས།
msgctxt "6838d5ae0fde4bd0a5ed74a6b578e99a"
msgid ""
"སུ་ ལ་ ཡང་ འཚེ་ མི་འདོད་པ་ ཞིག་ འདུག་པ་ དེ་ ཕྱི་ ཞིག་ ན་ གཟ␣ས་ ལ་ སོང་ "
"སོང་བ་ ལ␣ས །"
msgstr ""

msgctxt "f78f853878854e3895743d4c849f9cb8"
msgid "རྔོན་པ་ ཞིག་ གིས་ མཐོང་ ནས་"
msgstr ""

msgctxt "ee2286ec6f43463d981ae361c134b244"
msgid "དེ་དེ␣ས་ གསོན་པོ␣ར་ བཟུང་ སྟེ །"
msgstr ""

# :པགས་པ་བཤུས་<«གཡུང་»«ལི་»«པེ་»ལྤགས་པ་བཤུས། «ཅོ་»པགས་པར་ཤུས།>ནས་
msgctxt "abf617116ef440cabe5e2a4636bc139e"
msgid "པགས་པ་ བཤུས་ ནས་"
msgstr ""

# བཏང་སྟེ་<«གཡུང་»སོ།>
msgctxt "d302a55537a14de4880438d1fc7b96b5"
msgid "བཏང་ སྟེ་"
msgstr ""

# རྔོན་པ་དེས་པགས་<«གཡུང་»«ལི་»«པེ་»ལྤགས།>པ་དེ་ཁྱེར་ཏེ་
msgctxt "f9fe1da432b74eff8392c4bafa5979ba"
msgid "རྔོན་པ་ དེ␣ས་ པགས་པ་ དེ་ ཁྱེར་ ཏེ་"
msgstr ""

msgctxt "d3892818c3ba4eac863ee589064f7cee"
msgid "སོང་ ངོ་ ། །"
msgstr ""

# དེ་ནས་སྐྱིན་གོར་<«གཡུང་»འཁོར། «པེ་»ཀོར།>དེ་ན་ཚ་མི་བཟོད་དེ་
msgctxt "8444493949874ff99fbe48579e4ef535"
msgid "དེ་ནས་ སྐྱིན་གོར་ དེ་ ན་ཚ་ མི་ བཟོད་ དེ་"
msgstr ""

msgctxt "313000422f4d4cf0b392425431936923"
msgid "ཆུ་འགྲམ་ ཞིག་ ཏུ་ སོང་ ནས་"
msgstr ""

# ས་:གཞི་བསིལ་<«གཡུང་»བཞི་བསིལ། «པེ་»«སྣར་»བཞི་གསིལ།>བའི་ཕྱོགས་སུ་ཉལ་ལོ། །
msgctxt "3bf2fcbfd3ba4c3bb7c4289b3d80021c"
msgid "ས་གཞི་ བསིལ་བ␣འི་ ཕྱོགས་ སུ་ ཉལ་ ལོ ། །"
msgstr ""

msgctxt "8300da2eab58466b9ddf0949460a48c7"
msgid "དེ་ནས་ ཧ་ཅང་ ཡང་ མི་ རིང་བ་ ཞིག་ ན་ གྲོག་ཚང་ ཞིག་ ཡོད་ དེ །"
msgstr ""

msgctxt "64d58ad401184b08bdd0cb9c9dc075bc"
msgid "གྲོག་ཚང་ དེ་ ན་ གྲོག་མ་ བརྒྱད་ ཁྲི་ གནས་ ཏེ །"
msgstr ""

msgctxt "f4029b93f09e4b1b8553e6a95c502986"
msgid "གྲོག་ཚང་ དེ␣འི་ ནང་ ནས་ གྲོག་མ་ ཞིག་ བྱུང་ སྟེ །"
msgstr ""

# གྲོག་མ་དེ་སྐྱིན་གོར་<«གཡུང་»འཁོར། «པེ་»ཀོར།>དེའི་ཐད་དུ་སོང་སྟེ་
msgctxt "2332173cebec449abd61bcb7e84409d9"
msgid "གྲོག་མ་ དེ་ སྐྱིན་གོར་ དེ␣འི་ ཐད་ དུ་ སོང་ སྟེ་"
msgstr ""

# བལྟས་ན་སྐྱིན་:གོར་པགས་<«གཡུང་»འཁོར་ལྤགས། «ལི་»གོར་ལྤགས།
# «པེ་»ཀོར་ལྤགས།>པ་མེད་པ་དེ་མཐོང་ནས་
msgctxt "d5dfc3894c284e2c8f4fc9955dfd3583"
msgid "བལྟས་ ན་ སྐྱིན་གོར་ པགས་པ་ མེད་པ་ དེ་ མཐོང་ ནས་"
msgstr ""

msgctxt "d59dd690b4504e0abd80e2b597765dbe"
msgid "རང་ གི་ ཚང་ དུ་ སོང་ སྟེ །"
msgstr ""

msgctxt "c5ca7d44a5a64a3da1c448c0b239ab29"
msgid "གྲོག་མ་ གཞན་དག་ ལ་ སྦྲན་ ནས །"
msgstr ""

# གྲོག་མ་བརྒྱད་ཁྲི་པོ་དེ་དག་ཚང་ནས་འཐོན་<«ཞོལ་»ཐོན།>ཏེ་
msgctxt "3c312b8d14564a0da897560ecd8cad3d"
msgid "གྲོག་མ་ བརྒྱད་ ཁྲི་པོ་ དེ་དག་ ཚང་ ནས་ འཐོན་ ཏེ་"
msgstr ""

# སྐྱིན་གོར་<«གཡུང་»འཁོར། «པེ་»འཀོར།>དེའི་ལུས་ལ་ཞུགས་སོ། །
msgctxt "cac3de83e3a54a82bef511398e73532a"
msgid "སྐྱིན་གོར་ དེ␣འི་ ལུས་ ལ་ ཞུགས་ སོ ། །"
msgstr ""

# དེ་ནས་དེ་དག་ཟ་<«ཅོ་»ཟབ།>བས་སད་དེ་
msgctxt "236cc89cd73647e78cd2f2240006b8a3"
