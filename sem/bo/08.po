msgctxt "cbc665bb8adb46b5b1e275bf70bfa7b3"
msgid "ལ␣ས་ བརྒྱ་ ཐམ་པ །"
msgstr ""

msgctxt "b6b12a39af7340608c53e0f012696077"
msgid "བམ་པོ་ གསུམ་པ །"
msgstr ""

# བདེ་བྱེད་མ་ཞེས་བྱ་བ་<«གཡུང་»བ། «ལི་»«སྣར་»«ཅོ་»«ཞོལ་»པ།>ནི།
msgctxt "424085511b5a49ce8644d74261f699c5"
msgid "བདེ་བྱེད་ མ་ ཞེས་ བྱ་བ་ ནི །"
msgstr ""

# གླེང་<«ལི་»གླིང་།>གཞི་མཉན་དུ་ཡོད་པ་ན་བཞུགས་ཏེ།
msgctxt "9a95659cc3f54bc8b89eac15915c4041"
msgid "གླེང་ གཞི་ མཉན་ དུ་ཡོད་པ་ ན་ བཞུགས་ ཏེ །"
msgstr ""

msgctxt "465a5d551cc8426dbac903471f36f2a9"
msgid "དེ␣འི་ ཚེ་ མཉན་ དུ་ཡོད་པ་ ན་ རྒྱལ་པོ་ གསལ་རྒྱལ་ རྒྱལ་པོ་ བྱེད་ ལ །"
msgstr ""

# བཱ་རཱ་ཎ་སཱི་ན་ནི་རྒྱལ་པོ་ཚངས་པས་བྱིན་ཞེས་བྱ་བ་རྒྱལ་པོ་བྱེད་པ་ལས་<«ཁུ་»ལ།>
msgctxt "52684d19f41748cb834553c5b20afc37"
msgid ""
"བཱ་རཱ་ཎ་སཱི་ ན་ ནི་ རྒྱལ་པོ་ ཚངས་པ␣ས་ བྱིན་ ཞེས་ བྱ་བ་ རྒྱལ་པོ་ བྱེད་པ་ ལ␣ས་"
msgstr ""

msgctxt "81558b333c714979b3ab68044167a5c4"
msgid "དེ་ གཉིས་ ཕན་ཚུན་ མི་ འཐུན་པ␣ར་ གྱུར་ ཏེ །"
msgstr ""

# དུས་དུས་སུ་སྐྱེ་བོ་མང་པོ་ཁ་འདོག་<«གཡུང་»དོག>པར་བྱེད་དོ། །
msgctxt "154a130a95c74f79b81d70ea1e56fa36"
msgid "དུས་ དུས་ སུ་ སྐྱེ་བོ་ མང་པོ་ ཁ་ འདོག་པ␣ར་ བྱེད་ དོ ། །"
msgstr ""

msgctxt "d404b8929c2744a99b6d6c4b3de04621"
msgid ""
"དེ་ནས་ ཕྱི་ ཞིག་ ན་ རྒྱལ་པོ་ ཚངས་པ␣ས་ བྱིན་ གྱིས་ དཔུང་ གི་ ཚོགས་ ཡན་ལག་ "
"བཞི་ གོ་ བསྐོན་ ཏེ་"
msgstr ""

# ཀོ་<«གཡུང་»«པེ་»ཀོ། «གཡུང་»«པེ་»ཀ>ས་ལའི་ཡུལ་དུ་སོང་ནས་
msgctxt "2ec3e5cbe35e49faaa436c0b0becb923"
msgid "ཀོ་ས་ལ␣འི་ ཡུལ་ དུ་ སོང་ ནས་"
msgstr ""

msgctxt "382d8f2a4fc24474ba6d74e99dbc86e1"
msgid "ཆུ་ ཀླུང་ཁང་ ལྡན་ གྱི་ འགྲམ་ དུ་ ཕྲུ་མ་ བཏབ་ སྟེ་"
msgstr ""

msgctxt "cdd47be63d53412d912d3e765c67283b"
msgid "འཁོད་ དོ ། །"
msgstr ""

msgctxt "bb659169d34647cca89c28f055866d3e"
msgid ""
"རྒྱལ་པོ་ གསལ་རྒྱལ་ གྱིས་ ཀྱང་ རྒྱལ་པོ་ ཚངས་པ␣ས་ བྱིན་ གྱིས་ དཔུང་ གི་ ཚོགས་ "
"ཡན་ལག་ བཞི་ གོ་ བསྐོན་ ནས །"
msgstr ""

msgctxt "5e0bdf7474b04dd98a88cfb456e60e44"
msgid "བདག་ དང་ གཡུལ་འགྱེད་ དུ་ འོངས་ སོ་ ཞེས་"
msgstr ""

msgctxt "db8272f62be74b3a854e3147e72a455d"
msgid "ཐོས་ སོ ། །"
msgstr ""

msgctxt "b8b6124e625042f0ab4bb4c306fd4d5b"
msgid "ཐོས་ ནས་"
msgstr ""

msgctxt "4dd1d94acf7348499b158aa37d29cbab"
msgid "དེ␣ས་ ཀྱང་ དཔུང་ གི་ ཚོགས་ ཡན་ལག་ བཞི་ གོ་ བསྐོན་ ནས་"
msgstr ""

msgctxt "bccc1dda894d43578effaaf205ba0459"
msgid "ཀཱ་ ཤི␣འི་ རྒྱལ་པོ་ དང་ གཡུལ་འགྱེད་ དུ་ སོང་ སྟེ །"
msgstr ""

# དེས་ཀྱང་ཆུ་ཀླུང་<«གཡུང་»–ཆུ། «པེ་»ཆུ་ལུང་།>ཁང་ལྡན་གྱི་འགྲམ་དུ་ཕྲུ་མ་བཏབ་སྟེ་
msgctxt "aa04804a561b4a348bfefe2a04e44432"
msgid "དེ␣ས་ ཀྱང་ ཆུ་ ཀླུང་ཁང་ ལྡན་ གྱི་ འགྲམ་ དུ་ ཕྲུ་མ་ བཏབ་ སྟེ་"
msgstr ""

msgctxt "0c8024ab294a4812b4ca7d55c33a3162"
msgid "འཁོད་ དོ ། །"
msgstr ""

msgctxt "d68244dcefa54ab9a9cf2ee78e509f40"
msgid ""
"དེ་ནས་ དེ་ གཉིས་ དེ་ ན་ འཁོད་པ␣ས་ ཕན་ཚུན་ དུ་ སྐྱེ་བོ་ མང་པོ་ ཁ་ འདོག་པ␣ར་ "
"བྱེད་ དོ ། །"
msgstr ""

msgctxt "f29d99b622354f7fab2e457e15617af9"
msgid "དེ་ གཉིས་ཀ་ ཡང་ མཐུ་ ཆེ་ ཞིང་ ཚུགས་པ␣ར་ དཀའ་བ་ ཡིན་ ཏེ །"
msgstr ""

msgctxt "fc2318503d624aeebd8868ec6b782b4c"
msgid "དེ་ གཉིས་ གཅིག་ གིས་ གཅིག་ མི་ ཐུབ་ ཅིང་"
msgstr ""

msgctxt "6d008e67f75e46a9835ae6fae30039ec"
msgid "འདུག་ གོ ། །"
msgstr ""

# དེ་ནས་ཕྱི་ཞིག་ན་དེ་ཁོ་ན་འཁོད་བཞིན་དུ་རྒྱལ་པོ་གསལ་[33a]རྒྱལ་གྱི་བུ་མོ་གཟུགས་བཟང་ཞིང་བལྟ་<«ལི་»«སྣར་»«ཅོ་»ལྟ།>ན་སྡུག་ལ་མཛེས་པ་ཞིག་བཙས་སོ།
# །
msgctxt "0aa887740a534db49b41e468a9939f65"
msgid ""
"དེ་ནས་ ཕྱི་ ཞིག་ ན་ དེ་ ཁོ་ན་ འཁོད་ བཞིན་ དུ་ རྒྱལ་པོ་ གསལ་རྒྱལ་ གྱི་ བུ་མོ་"
" གཟུགས་བཟང་ ཞིང་ བལྟ་ ན་ སྡུག་ ལ་ མཛེས་པ་ ཞིག་ བཙས་ སོ ། །"
msgstr ""

# ཀཱ་<«གཡུང་»«པེ་»ཀ>ཤིའི་རྒྱལ་པོ་ཚངས་པས་བྱིན་གྱི་བུ་གཟུགས་བཟང་ཞིང་བལྟ་<«ལི་»«སྣར་»«ཅོ་»ལྟ།>ན་སྡུག་ལ་མཛེས་པ་ཞིག་ཀྱང་བཙས་ནས།
msgctxt "60aa0bb61bce4d74a648dba692b6d576"
msgid ""
"ཀཱ་ ཤི␣འི་ རྒྱལ་པོ་ ཚངས་པ␣ས་ བྱིན་ གྱི་ བུ་ གཟུགས་བཟང་ ཞིང་ བལྟ་ ན་ སྡུག་ ལ་"
" མཛེས་པ་ ཞིག་ ཀྱང་ བཙས་ ནས །"
msgstr ""

msgctxt "c014b1c4079540d8addcb6395107c4ef"
msgid ""
"དེ␣ར་ དེ་ གཉིས་ སོ་སོ␣འི་ ཕོ་བྲང་ འཁོར་ དུ་ དགའ་བ␣འི་ རོལ་མོ་ བརྡུངས་ སོ ། །"
msgstr ""

msgctxt "0f41e5bdc86c4ceda72ace9dce56a222"
msgid "དེ་ནས་ དེ་ གཉིས་ ཀྱིས་ ཕན་ཚུན་ དུ་ ཐོས་ ནས་"
msgstr ""

# འདིར་དགའ་བའི་རོལ་མོ་བརྡུངས་པ་དེ་<«གཡུང་»«པེ་»ནི།
# «ལི་»«སྣར་»«ཅོ་»«ཞོལ་»ན།>ཅི་ཞིག་ཅེས་
msgctxt "a756f8a149434206bdd988602cd12f78"
msgid "འདི␣ར་ དགའ་བ␣འི་ རོལ་མོ་ བརྡུངས་པ་ དེ་ ཅི་ཞིག་ ཅེས་"
msgstr ""

msgctxt "3b3442bf07b045fa891237f874737707"
msgid "དྲིས་ ནས་"
msgstr ""

# བློན་པོ་རྣམས་ཀྱིས་བསྙད་<«གཡུང་»བརྙེད། «པེ་»བསྙེད།>དེ།
msgctxt "42c1d2d937bd436c82ff3afee8a43f1d"
msgid "བློན་པོ་ རྣམས་ ཀྱིས་ བསྙད་ དེ །"
msgstr ""

# རྒྱལ་པོ་གསལ་རྒྱལ་གྱི་ནི་<«གཡུང་»«ལི་»«པེ་»«སྣར་»«ཅོ་»«ཞོལ་»–ནི།>བུ་མོ་བཙས།
# ལྷའི་ནི་སྲས་པོ་བལྟམ་མོ་<«གཡུང་»«པེ་»བལྟ་མོ། «ཞོལ་»བལྟམས་སོ།>ཞེས་
msgctxt "f28e227ff9b844d0ac0ace3ff12cabcb"
msgid ""
"རྒྱལ་པོ་ གསལ་རྒྱལ་ གྱི་ ནི་ བུ་མོ་ བཙས །  ལྷ␣འི་ ནི་ སྲས་པོ་ བལྟམ་ མོ་ ཞེས་"
msgstr ""

msgctxt "43e60a98f73949acb5097db2ae5366e0"
msgid "བྱས་ སོ ། །"
msgstr ""

# རྒྱལ་པོ་གསལ་རྒྱལ་གྱི་བློན་པོ་རྣམས་ཀྱིས་<«ཞོལ་»–ཀྱིས།>ཀྱང་བསྙད་དེ།
msgctxt "38da10bf941a43d4beeeb5035502b5ea"
msgid "རྒྱལ་པོ་ གསལ་རྒྱལ་ གྱི་ བློན་པོ་ རྣམས་ ཀྱིས་ ཀྱང་ བསྙད་ དེ །"
msgstr ""

# ལྷའི་ནི་སྲས་མོ་བལྟམ<«ཞོལ་»བལྟམས།>།
# རྒྱལ་པོ་ཚངས་པས་བྱིན་གྱི་ནི་བུ་ཕོ་བཙས་སོ་ཞེས་
msgctxt "fe8fa9c13c4744a182efb8010a9d4d13"
msgid ""
"ལྷ␣འི་ ནི་ སྲས་མོ་ བལྟམ །  རྒྱལ་པོ་ ཚངས་པ␣ས་ བྱིན་ གྱི་ ནི་ བུ་ ཕོ་ བཙས་ སོ་"
" ཞེས་"
msgstr ""

msgctxt "418852d2594f4905b95966aaebb52648"
msgid "བྱས་ སོ ། །"
msgstr ""

msgctxt "40f16c7167a74f54928861887d057b25"
msgid "དེ་ནས་ དེ་ ཐོས་ མ་ ཐག་ ཏུ་ དེ་ གཉིས་ དགའ་བ␣ར་ གྱུར་ ཏེ །"
msgstr ""

# བདག་ཅག་གཉིས་ཕན་ཚུན་དགྲར་<«གཡུང་»«པེ་»དགྲ།>མི་བྱེད་པར་གཉེན་བྱེད་པའི་ཐབས་རྙེད་དོ་སྙམ་མོ།
# །
msgctxt "422517ae455b4691b4af5d0a3f84cef2"
msgid ""
"བདག་ཅག་ གཉིས་ ཕན་ཚུན་ དགྲ␣ར་ མི་ བྱེད་པ␣ར་ གཉེན་ བྱེད་པ␣འི་ ཐབས་ རྙེད་ དོ་ "
"སྙམ་ མོ ། །"
msgstr ""

msgctxt "ee0e8569bad34fdfb331221abd920732"
msgid ""
"དེ་ནས་ དེ་ ཐོས་ མ་ ཐག་ ཏུ་ རྒྱལ་པོ་ ཚངས་པ␣ས་ བྱིན་ གྱིས་ རྒྱལ་པོ་ གསལ་རྒྱལ་ "
"ལ་ ཕོ་ཉ་ བཏང་ སྟེ །"
msgstr ""

# ཁྱོད་ཀྱི་བུ་མོ་བཙས་པ་དེ་བདག་གི་<«ལི་»«སྣར་»«ཅོ་»གིས།>བུ་འདིའི་ཆུང་མར་<«གཡུང་»མ།>བྱིན་ཅིག་ཅེས་
msgctxt "6f7d123adbd54c4887b426055a264bbb"
