msgctxt "747b902e2dac49da80ee9967ce1ace7e"
msgid "བསོད་ནམས་ དག་ བྱས་ ཏེ་"
msgstr ""

msgctxt "efcb65b0250347adb46d80675b2cef9e"
msgid "ཚེ་ གཅིག་ ཏུ་ ཚངས་པ␣ར་ སྤྱོད་པ་ སྤྱད་ ཀྱང་"
msgstr ""

# ཡོན་ཏན་གྱི་ཚོགས་ཅི་ཡང་མ་ཐོབ་ཀྱིས<«ཞོལ་»ཀྱི།>།
msgctxt "4290b5464a4f4fc5905b653be9367838"
msgid "ཡོན་ཏན་ གྱི་ ཚོགས་ ཅི་ཡང་ མ་ ཐོབ་ ཀྱིས །"
msgstr ""

msgctxt "7515be15b07149a7b2ca59d6d9ad50d1"
msgid ""
"དགེ་བ␣འི་ རྩ་བ་ འདི␣ས་ བདག་ཅག་ གཉིས་ ཅི་ ནས་ གང་དང་གང་ དུ་ སྐྱེ་བ་ དེ་ དང་ "
"དེ␣ར་ ཕྱུག་ ཅིང་ ནོར་ མང་ ལ་ ལོངས་སྤྱོད་ ཆེ་བ␣འི་ རིགས་ སུ་ སྐྱེ་བ␣ར་ ཤོག་ "
"ཅིག  །"
msgstr ""

# གཟུགས་བཟང་ཞིང་བལྟ་<«ལི་»«སྣར་»«ཅོ་»ལྟ།>ན་སྡུག་ལ་མཛེས་པར་ཤོག་ཅིག །
msgctxt "8abc3e98c2b34e2b9a3e9d84c8c53c57"
msgid "གཟུགས་བཟང་ ཞིང་ བལྟ་ ན་ སྡུག་ ལ་ མཛེས་པ␣ར་ ཤོག་ ཅིག  །"
msgstr ""

# ཡང་དག་པར་རྫོགས་པའི་སངས་རྒྱས་འོད་སྲུང་གིས་བྲམ་ཟེའི་ཁྱེའུ་བླ་མ་ལུང་བསྟན་པ་གང་ཡིན་པའི་སངས་རྒྱས་བཅོམ་ལྡན་འདས་དེ་བདག་ཅག་གཉིས་ཀྱིས་མཉེས་པར་བྱེད་པར་<«གཡུང་»«པེ་»–བྱེད་པར།>གྱུར་ཅིག
# །
msgctxt "5a937ffd67bd49ff87242287bcd79d27"
msgid ""
"ཡང་དག་པ␣ར་ རྫོགས་པ␣འི་ སངས་རྒྱས་ འོད་སྲུང་ གིས་ བྲམ་ཟེ␣འི་ ཁྱེའུ་ བླ་མ་ "
"ལུང་བསྟན་པ་ གང་ཡིན་པ␣འི་ སངས་རྒྱས་ བཅོམ་ལྡན་འདས་ དེ་ བདག་ཅག་ གཉིས་ ཀྱིས་ "
"མཉེས་པ␣ར་ བྱེད་པ␣ར་ གྱུར་ཅིག  །"
msgstr ""

msgctxt "4dcd16a3291f4518a9662df7cd1cb6ea"
msgid "མི་ མཉེས་པ␣ར་ བྱེད་པ␣ར་ མ་ གྱུར་ཅིག  །"
