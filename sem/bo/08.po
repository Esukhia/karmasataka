msgctxt "24db21d30dcd4d3ba42a6dd0c8d66736"
msgid "བདག་ ནི་ འདོད་པ་ ཡོངས་སུ་ སྤྱོད་པ␣འི་ རྔོ་ མི་ ཐོགས་ ཀྱིས་"
msgstr ""

msgctxt "591ecc20310544419c4a7bef7c393634"
msgid ""
"བདག་ བཅོམ་ལྡན་འདས་ ཀྱི་ བསྟན་པ་ ལ་ རབ་ཏུ་ འབྱུང་བ␣ར་ གནང་ བར་ མཛོད་ ཅིག  །"
msgstr ""

# ཁྱེའུས་<«གཡུང་»«ལི་»«པེ་»«སྣར་»«ཅོ་»ཁྱེའུ།>སྨྲས་པ།
msgctxt "89e7e120577c48d1a30de724ba5c51e7"
msgid "ཁྱེའུ␣ས་ སྨྲས་པ །"
msgstr ""

msgctxt "44b9fa665cc940b19f276c0700a51cbb"
msgid "གནང་ ངོ་ ། །"
msgstr ""

# དེ་ནས་དེ་<«གཡུང་»«ལི་»«པེ་»«སྣར་»«ཅོ་»དེས།>ཐོས་མ་ཐག་ཏུ་བུ་མོ་བདེ་བྱེད་མ་བབས་ཏེ།
msgctxt "0bb3efa0155040d3b51d88682c3b25d3"
msgid "དེ་ནས་ དེ་ ཐོས་ མ་ ཐག་ ཏུ་ བུ་མོ་ བདེ་བྱེད་ མ་ བབས་ ཏེ །"
msgstr ""

msgctxt "b80a38da8779403d84a80e0382770981"
msgid "དེ␣ས་ སྐྱེ་བོ␣འི་ ཚོགས་ དེ་དག་ ལ་ ཆོས་ བསྟན་ ནས་"
msgstr ""

msgctxt "8bf76903db1a4997b3a677047f562fd8"
msgid "ཕ་མ་ གཉིས་ ལ་ གསོལ་ ཏེ །"
msgstr ""

# རྒྱལ་བུ་<«ཁུ་»»པོ།>རྒྱལ་བྱེད་ཀྱི་ཚལ་དུ་སོང་ནས།
msgctxt "670242ca812d4650b55bbe11023fbe9c"
msgid "རྒྱལ་བུ་ རྒྱལ་བྱེད་ ཀྱི་ ཚལ་ དུ་ སོང་ ནས །"
msgstr ""

# དེ་<«གཡུང་»»«ལི་»»«པེ་»»«སྣར་»»«ཅོ་»»«ཞོལ་»»–དེ།>བཅོམ་ལྡན་འདས་ག་ལ་བ་དེར་སོང་སྟེ་ཕྱིན་ནས།
msgctxt "b2ba2ecac0e4412ba64882eca558386a"
msgid "དེ་ བཅོམ་ལྡན་འདས་ ག་ལ་བ་ དེ␣ར་ སོང་ སྟེ་ ཕྱིན་ ནས །"
msgstr ""

msgctxt "3117c7b19048401e98eb04982adcd0e1"
msgid "བཅོམ་ལྡན་འདས་ ཀྱི་ ཞབས་ ལ་མགོ་ བོས་ ཕྱག་ འཚལ་ ཏེ །"
msgstr ""

msgctxt "d4bfad7aa4fc4c5a929d8aa5afa662f2"
msgid "བཅོམ་ལྡན་འདས་ ལ་ འདི་སྐད་ ཅེས་ གསོལ་ ཏོ ། །"
msgstr ""

msgctxt "a040247d32174ba19bd9540b1b57faf7"
msgid "བཙུན་པ་"
msgstr ""

# བདག་ལེགས་པར་གསུངས་པའི་ཆོས་འདུལ་བ་ལ་<«གཡུང་»»«པེ་»»–ལ།>རབ་ཏུ་འབྱུང་བ་དང་།
# བསྙེན་<«གཡུང་»སྙེན།
# «པེ་»བསྙན།>པར་རྫོགས་ཤིང་དགེ་སློང་མའི་དངོས་པོ་འཐོབ་ཏུ་རུང་ན།
msgctxt "d9ea85ad1eec4dfab40ad0e19191c15e"
msgid ""
"བདག་ ལེགས་པ␣ར་ གསུངས་པ␣འི་ ཆོས་ འདུལ་བ་ ལ་ རབ་ཏུ་ འབྱུང་བ་ དང་ །  བསྙེན་པ␣ར་"
" རྫོགས་ ཤིང་ དགེ་སློང་ མ␣འི་ དངོས་པོ་ འཐོབ་ ཏུ་ རུང་ ན །"
msgstr ""

msgctxt "5ca47ef0f2e44d229a08a0a51e087530"
msgid ""
"བདག་ ཀྱང་ བཅོམ་ལྡན་འདས་ ཀྱི་ ཐད་ དུ་ ཚངས་པ␣ར་ སྤྱོད་པ་ སྤྱད་པ␣ར་ འཚལ་ ལོ ། །"
msgstr ""

# དེ་ནས་བཅོམ་ལྡན་འདས་ཀྱིས་སྐྱེ་དགུའི་<«ཞོལ་»རྒུའི།>བདག་མོ་ཆེན་མོ་གཽ་ཏ་མཱི་ལ་གཏད་<«གཡུང་»མི་ལ་ཏད།
# «པེ་»མི་ལ་གཏད།>དོ། །
msgctxt "65d30fee3da44ec288d54e67771c31e8"
msgid ""
"དེ་ནས་ བཅོམ་ལྡན་འདས་ ཀྱིས་ སྐྱེ་དགུ␣འི་ བདག་མོ་ ཆེན་ མོ་ གཽ་ ཏ་ མཱི་ ལ་ གཏད་"
" དོ ། །"
msgstr ""

# སྐྱེ་དགུའི་<«ཞོལ་»རྒུའི།>བདག་མོ་ཆེན་མོ་གཽ་ཏ་མཱིས་<«གཡུང་»«པེ་»མཱས།>ཀྱང་དེ་རབ་ཏུ་ཕྱུང་སྟེ་
msgctxt "4c932564c19c463baf384ff26ee74a01"
msgid "སྐྱེ་དགུ␣འི་ བདག་མོ་ ཆེན་ མོ་ གཽ་ ཏ་ མཱིས་ ཀྱང་ དེ་ རབ་ཏུ་ ཕྱུང་ སྟེ་"
msgstr ""

msgctxt "5e8ae090c35b4f348348d6ea142b2303"
msgid "བསྙེན་པ␣ར་ རྫོགས་པ␣ར་ བྱས་ ནས་"
msgstr ""

msgctxt "2ad7b80b6d384232b3c00437cb3419ce"
msgid "དེ་ ལ་ལུང་ ཡང་ ཕོག་ གོ ། །"
msgstr ""

# དེས་ཀྱང་བརྩོན་<«གཡུང་»བཅོན།>པ་དང་བསྒྲུབ་<«ཞོལ་»སྒྲུབ།>པ་དང་འབད་པས་ཉོན་མོངས་པ་ཐམས་ཅད་སྤངས་ནས།
msgctxt "8039f00283a24ea78f4040eb4564f1bc"
msgid ""
"དེ␣ས་ ཀྱང་ བརྩོན་པ་ དང་ བསྒྲུབ་པ་ དང་ འབད་པ␣ས་ ཉོན་མོངས་པ་ ཐམས་ཅད་ སྤངས་ ནས "
"།"
msgstr ""

# དགྲ་བཅོམ་པ་ཉིད་མངོན་སུམ་དུ་བྱས་ཏེ<«གཡུང་»«པེ་»སྟེ།>།
msgctxt "8033ae7fb9f84f76904783072db48b67"
msgid "དགྲ་བཅོམ་པ་ ཉིད་ མངོན་སུམ་ དུ་ བྱས་ ཏེ །"
msgstr ""

# དགྲ་བཅོམ་པ་ཁམས་གསུམ་པའི་<«ལི་»«སྣར་»«ཅོ་»མའི།>འདོད་ཆགས་དང་བྲལ་བར་གྱུར་ནས།
msgctxt "4bd2ca8aaa5648c68616422e2fcd2885"
msgid "དགྲ་བཅོམ་པ་ ཁམས་ གསུམ་པ␣འི་ འདོད་ཆགས་ དང་ བྲལ་བ␣ར་ གྱུར་ ནས །"
msgstr ""

msgctxt "15c319f5bee8450f867ea001dd16f506"
msgid "གསེར་ དང་ བོང་བ␣ར་ མཉམ་པ །"
msgstr ""

# ནམ་མཁའ་དང་ལག་མཐིལ་<«གཡུང་»ཐིལ «ལི་»«ཅོ་»འཐིལ།>དུ་འདྲ་བའི་སེམས་དང་ལྡན་པ།
msgctxt "3bb0d81c7fbd4a1fb58effb6c0cc5b4b"
msgid "ནམ་མཁའ་ དང་ ལག་མཐིལ་ དུ་ འདྲ་བ␣འི་ སེམས་ དང་ ལྡན་པ །"
msgstr ""

# ཙན་དན་སྦངས་པ་ལྟ་བུར་བསིལ་བར་གྱུར་པ<«གཡུང་»«པེ་»འགྱུར་བ།>།
msgctxt "404241b6ad3241008e695d43f84a17bd"
msgid "ཙན་དན་ སྦངས་པ་ ལྟ་བུ␣ར་ བསིལ་བ␣ར་ གྱུར་པ །"
msgstr ""

msgctxt "5c2b7f41bdcb406b8baffbaa4d53a91c"
msgid "རིག་པ␣ས་ སྒོ་ང␣འི་ སྦུབས་ བཅོམ་པ །"
msgstr ""

msgctxt "aa906275f7b448a988029d2643eccc76"
msgid "རིག་པ་ དང་ མངོན་པར་ ཤེས་པ་ དང་ སོ་སོ་ ཡང་དག་པ␣ར་ རིག་པ་ ཐོབ་པ །"
msgstr ""

msgctxt "ecb6b25294204cebabccfb141e64d12d"
msgid "སྲིད་པ␣འི་ རྙེད་པ་ དང་ ཆགས་པ་ དང་ བཀུར་སྟི་ ལ་ རྒྱབ་ ཀྱིས་ ཕྱོགས་པ །"
msgstr ""

# དབང་[34b]པོ་དང་ཉེ་དབང་དང་བཅས་པའི་ལྷ་རྣམས་ཀྱིས་མཆོད་ཅིང་རྗེད་<«གཡུང་»«པེ་»«སྣར་»བརྗེད།>པ་དང་གུས་པར་སྨྲ་བའི་གནས་སུ་གྱུར་ཏེ།
msgctxt "62b9019db2474d43b4d8e5fe99688ad2"
msgid ""
"དབང་པོ་ དང་ ཉེ་དབང་ དང་ བཅས་པ␣འི་ ལྷ་ རྣམས་ ཀྱིས་ མཆོད་ ཅིང་ རྗེད་པ་ དང་ "
"གུས་པ␣ར་ སྨྲ་བ␣འི་ གནས་ སུ་ གྱུར་ ཏེ །"
msgstr ""

# བཅོམ་ལྡན་འདས་ཀྱིས་ཤེས་རབ་དང་ལྡན་པ་རྣམས་ཀྱི་མཆོག་ཏུ་བསྟན་ཏོ།
# །<«གཡུང་»བཅོམ་ལྡན་....བསྟན་སྟོ། «ལི་»«ཅོ་»–བཅོམ་ལྡན་....བསྟན་ཏོ།>
msgctxt "b23ce42f8faa4c87968fabfd6c705abc"
msgid ""
"བཅོམ་ལྡན་འདས་ ཀྱིས་ ཤེས་རབ་ དང་ ལྡན་པ་ རྣམས་ ཀྱི་ མཆོག་ ཏུ་ བསྟན་ ཏོ ། །"
msgstr ""

msgctxt "d9ee7d791d9449218b213fe484ce1cd3"
msgid "བདེ་བྱེད་ མ་ དེ་ལྟར་ རབ་ཏུ་ བྱུང་ ནས་"
msgstr ""

msgctxt "dfea84b2c2f647ccb09843f630452fe5"
msgid "རྒྱལ་པོ␣འི་ བུ་ དེ␣ས་ བསམས་པ །"
msgstr ""

msgctxt "d976de8e43b4472daa40b352ae9fadee"
msgid "བུད་མེད་ ཀྱིས་ ཀྱང་ འདི་ ལྟ་བུ␣འི་ ཆོས་ རྟོགས་པ␣ར་ འགྱུར་ ན །"
msgstr ""

msgctxt "aada2394132947179ce0b96ab13a88f3"
msgid "མ་ལ་"
msgstr ""

msgctxt "ad80565bdfbd49df88010b1ae2544895"
msgid ""
"བདག་ ཀྱང་ བཅོམ་ལྡན་འདས་ ཀྱི་ བསྟན་པ་ ལ་ རབ་ཏུ་ འབྱུང་ གོར་མ་ཆག་ སྙམ་ དུ་"
msgstr ""

msgctxt "bede0964e0204600844e559539406189"
msgid "བསམས་ ནས་"
msgstr ""

msgctxt "90bd400eb982463ca3d3055fa310c626"
msgid "དེ␣ས་ ཀྱང་ ཕ་མ་ ལ་ གསོལ་ ཏེ །"
msgstr ""

msgctxt "3a3b74627ffd4caabe10cc99fc28e601"
msgid "བཅོམ་ལྡན་འདས་ ཀྱི་ བསྟན་པ་ ལ་ རབ་ཏུ་ བྱུང་ ནས །"
msgstr ""

# དེས་ཀྱང་བརྩོན་པ་དང་བསྒྲུབ་<«ཞོལ་»སྒྲུབ།>པ་དང་འབད་པས་ཉོན་མོངས་པ་ཐམས་ཅད་སྤངས་ནས།
