msgid "དགྲ་བཅོམ་པ་ ཉིད་ མངོན་སུམ་ དུ་ བྱེད་པ␣ར་ གྱུར་ཅིག་ ཅེས་"
msgstr ""

msgctxt "6385de25a3114188b01768a436e3fa96"
msgid "བྱས་ སོ ། །"
msgstr ""

# དགེ་སློང་དག་<«ལི་»«ཅོ་»–དག>
msgctxt "5672ca6b44114d688ff367da4f5bb1ad"
msgid "དགེ་སློང་ དག་"
