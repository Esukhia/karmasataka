msgstr ""

# དེ་གཉིས་གཅིག་ལ་གཅིག་བཟོད་པ་<«སྣར་»«ཞོལ་» པར།>གསོལ་ཏེ་
msgctxt "5938935361bb4342b8fc7a98d7474d68"
msgid "དེ་ གཉིས་ གཅིག་ ལ་ གཅིག་ བཟོད་པ་ གསོལ་ ཏེ་"
msgstr ""

msgctxt "c276299bc02340c6b978a69c824c57e5"
msgid "མཛའ་བོ་ བྱས་ ནས་"
msgstr ""

# གཅིག་གིས་ཀྱང་བུ་མོ་ལ་གོས་དང་རྒྱན་མང་པོ་<«གཡུང་»«པེ་»པོས།>བསྐུར་རོ། །
msgctxt "0c47223ec6bf4bdc9bba48c211f7a154"
msgid "གཅིག་ གིས་ ཀྱང་ བུ་མོ་ ལ་ གོས་ དང་ རྒྱན་ མང་པོ་ བསྐུར་ རོ ། །"
msgstr ""

# གཅིག་གིས་ཀྱང་ཁྱེའུ་ལ་གོས་དང་རྒྱན་མང་པོ་<«གཡུང་»«པེ་»པོས།>བསྐུར་ཏེ།
msgctxt "17c887b8a6ac4d0fb2f843513878c35b"
msgid "གཅིག་ གིས་ ཀྱང་ ཁྱེའུ་ ལ་ གོས་ དང་ རྒྱན་ མང་པོ་ བསྐུར་ ཏེ །"
msgstr ""

msgctxt "252f79eba4dd499bbee2ea201a9b55e0"
msgid "དེ་ གཉིས་ ཕན་ཚུན་ གཉེན་ དུ་ བྱས་ ནས་"
msgstr ""

msgctxt "c49babcbc04f4a6f8d1a5c6eea933091"
msgid "མགུལ་ ནས་ འཇུས་ ཏེ་"
msgstr ""

msgctxt "7e46c09722484ddb8bb9c50a0540b7dd"
msgid "ཡུལ་ དེ་ནས་ དོང་ ངོ་ ། །"
msgstr ""

msgctxt "ae49e39dd8b445c6a0c33775f67668bc"
msgid "དེ་ནས་ རྒྱལ་པོ་ གསལ་རྒྱལ་ གྱིས་ བསམས་པ་"
msgstr ""

# བུ་མོ་འདི་བཙས་<«གཡུང་»ཙས།>པས་ང་<«ཅོ་»–ང་།>རྒྱལ་པོ་ཚངས་པས་བྱིན་དང་བདེ་བར་གྱུར་ཏོ་སྙམ་ནས།
msgctxt "3c2521b1951e44e9a0c700ed6cb846a7"
msgid ""
"བུ་མོ་ འདི་ བཙས་པ␣ས་ ང་རྒྱལ་པོ་ ཚངས་པ␣ས་ བྱིན་ དང་ བདེ་བ␣ར་ གྱུར་ ཏོ་ སྙམ་ "
"ནས །"
msgstr ""

msgctxt "c919cf14b57c493ba24c341eb78d9fba"
msgid "དེ␣འི་ བཙས་ སྟོན་ རྒྱས་པ␣ར་ བྱས་ ཏེ །"
msgstr ""

# མིང་བདེ་བྱེད་<«གཡུང་»–བྱེད།>མ་ཞེས་བཏགས་སོ། །
msgctxt "bfe4d06d2a134a1bb27e7e0ad3f77224"
msgid "མིང་ བདེ་བྱེད་ མ་ ཞེས་ བཏགས་ སོ ། །"
msgstr ""

# དེ་ནས་བུ་མོ་བདེ་བྱེད་མ་འོ་མ་དང་ཞོ་དང་མར་དང་ཞུན་མར་དང་མར་གྱི་ཉིང་གུས་<«གཡུང་»–ཉིང་།
# «ལི་»«པེ་»«སྣར་»«ཅོ་»སྙིང་གུས། «ཞོལ་»སྙིང་ཁུས།>བསྐྱེད་བསྲིངས་ཏེ།
msgctxt "258cf5badb4848faa37042ceb6204c94"
msgid ""
"དེ་ནས་ བུ་མོ་ བདེ་བྱེད་ མ་ འོ་མ་ དང་ ཞོ་ དང་ མར་ དང་ ཞུན་མར་ དང་ མར་ གྱི་ "
"ཉིང་ གུས་ བསྐྱེད་ བསྲིངས་ ཏེ །"
msgstr ""

# མཚོའི་ནང་གི་པད་མ་<«ཅོ་»+པད་མ།>བཞིན་དུ་སྐྱེ་དྲག་པར་གྱུར་ཏོ། །
msgctxt "291ec2a37cbf45feb79d573ab77eee65"
msgid "མཚོ␣འི་ ནང་ གི་ པད་མ་ བཞིན་ དུ་ སྐྱེ་ དྲག་པ␣ར་ གྱུར་ ཏོ ། །"
msgstr ""

msgctxt "27d16ef805c94122a7b4f32d2e75e9ff"
msgid "དེ་ནས་ གང་ གི་ ཚེ་ ཆེན་ མོ␣ར་ གྱུར་པ་ དེ␣འི་ ཚེ །"
msgstr ""

msgctxt "222c4c0dd21f494ab37d580805cd36ab"
msgid "བཅོམ་ལྡན་འདས་ ཀྱི་ བསྟན་པ་ ལ་ དད་པ་ རྙེད་ དེ །"
msgstr ""

msgctxt "c0500efd09464cbaa9398ca3b536c1d4"
msgid "དེ␣ས་ སྐྱབས་ སུ་ འགྲོ་བ་ དང་ བསླབ་པ␣འི་ གཞི་ རྣམས་ བླངས་ ནས །"
msgstr ""

msgctxt "fec32c827f964679a35200a0e6dbaa3f"
msgid "སྦྱིན་པ་ དག་ བྱིན་ ཏེ་"
msgstr ""

# བསོད་ནམས་<«ལི་»དེ་བསོད་ནམས། «ཅོ་»དེ་བསོད་སྙོམས།>དག་བྱས་སོ། །
msgctxt "5687aeac12ef4f02be917d83452c4a68"
msgid "བསོད་ནམས་ དག་ བྱས་ སོ ། །"
msgstr ""

msgctxt "46dc7b59e64d4fa196b57146b931d8ea"
msgid "དགེ་སློང་ མ␣འི་ གཙུག་ལག་ཁང་ དུ་ ཡང་ རྒྱུན་ མི་འཆད་པ␣ར་ འགྲོ་ ཞིང་"
msgstr ""

msgctxt "0e3f4f9aac37447a955c7f466329a69d"
msgid "ཆོས་ ཉན་ ཏོ ། །"
msgstr ""

msgctxt "e6421ffcad514f38bb4bef28f320ba4c"
msgid ""
"དེ་ནས་ ཕྱི་ ཞིག་ ན་ དེ␣ས་ ཕྱིར་ མི་ འོང་བ␣འི་ འབྲས་བུ་ མངོན་སུམ་ དུ་ བྱས་ ཏེ"
" །"
msgstr ""

msgctxt "3965dab86dd34c499fada58e9cba06df"
msgid "རྫུ་འཕྲུལ་ ཡང་ བསྒྲུབས་ ནས །"
msgstr ""

# རྫུ་འཕྲུལ་ཆེན་མོ་<«གཡུང་»«པེ་»པོ།>མཐུ་ཆེན་མོ་རྣམ་པར་ཐར་པ་བརྒྱད་ལ་བསམ་གཏན་པར་གྱུར་ཏོ།
# །
msgctxt "83d6f373f44d47de8cd89e9652bd752c"
msgid ""
"རྫུ་འཕྲུལ་ ཆེན་ མོ་ མཐུ་ཆེན་ མོ་ རྣམ་པ␣ར་ ཐར་པ་ བརྒྱད་ ལ་ བསམ་གཏན་པ␣ར་ གྱུར་"
" ཏོ ། །"
msgstr ""

# དེ་ནས་ཕྱི་ཞིག་ན་དེས་ཕ་མ་གཉིས་ཀྱི་མདུན་<«གཡུང་»བདུན།>དུ་རྫུ་འཕྲུལ་གྱི་ཆོ་འཕྲུལ་བསྟན་ཏེ་
msgctxt "8c6108388aea4c8fae2dd10402ab5bb3"
msgid ""
"དེ་ནས་ ཕྱི་ ཞིག་ ན་ དེ␣ས་ ཕ་མ་ གཉིས་ ཀྱི་ མདུན་ དུ་ རྫུ་འཕྲུལ་ གྱི་ "
"ཆོ་འཕྲུལ་ བསྟན་ ཏེ་"
msgstr ""

msgctxt "aad598df3061443b9ff1a9c928579678"
msgid "སྨྲས་པ །"
msgstr ""

msgctxt "4908e2de98644be0b487ab97d5c2aee4"
msgid "ཡབ་ཡུམ་"
msgstr ""

# བདག་གིས་ནི་ཆོས་འདི་ལྟ་བུ་ཐོབ་པས་ན་འདོད་པ་ཡོངས་སུ་སྤྱོད་པའི་རྔོ་མི་ཐོགས་ཀྱིས<«གཡུང་»ངོ་མི་ཐོག་གིས།
# «ལི་»«པེ་»«སྣར་»«ཅོ་»རྔོ་མི་ཐོག་གིས། «ཞོལ་»རྔོ་མི་ཐོགས་ཀྱི།>།
msgctxt "546a6342a1914e2fb55d3e5b5cecc7ac"
msgid ""
"བདག་ གིས་ ནི་ ཆོས་ འདི་ ལྟ་བུ་ ཐོབ་པ␣ས་ ན་ འདོད་པ་ ཡོངས་སུ་ སྤྱོད་པ␣འི་ རྔོ་"
" མི་ ཐོགས་ ཀྱིས །"
msgstr ""

# བདག་གིས་བཅོམ་ལྡན་འདས་ཀྱི་<«གཡུང་»«ལི་»«པེ་»«སྣར་»«ཅོ་»ཀྱིས།>བསྟན་པ་ལ་རབ་ཏུ་འབྱུང་བ་<«གཡུང་»«པེ་»བར།>གནང་བར་མཛད་དུ་གསོལ།
msgctxt "a09704942e924ad8a1569549081050de"
msgid ""
"བདག་ གིས་ བཅོམ་ལྡན་འདས་ ཀྱི་ བསྟན་པ་ ལ་ རབ་ཏུ་ འབྱུང་བ་ གནང་ བར་ མཛད་ དུ་ "
"གསོལ །"
msgstr ""

msgctxt "549dcaa2de16455392a87af6cf2dc397"
msgid "ཕ་མ་ གཉིས་ ཀྱིས་ སྨྲས་པ །"
msgstr ""

# བུ་མོ་<«ལི་»«སྣར་»–མོ།>
msgctxt "0c3419a89ee441b29f3f24747929c1a6"
msgid "བུ་མོ་"
msgstr ""

# ཁྱོད་བཙས་མ་ཐག་ཏུ་བདག་ཅག་གིས་ཁྱིམ་ཐབ་ལ་བྱིན་ཟིན་གྱིས་<«ཞོལ་»གྱི།>
msgctxt "56d8996c0b5c41d99e68e3348131b5c5"
msgid "ཁྱོད་ བཙས་མ་ ཐག་ ཏུ་ བདག་ཅག་ གིས་ ཁྱིམ་ཐབ་ ལ་ བྱིན་ ཟིན་ གྱིས་"
