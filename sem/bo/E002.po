msgctxt "line 157, E002.txt"
msgid "དེ་ནས་ ལྷ␣འི་ དབང་པོ་ བརྒྱ་བྱིན་ གྱིས་ བསམས་པ །"
msgstr ""

msgctxt "line 158, E002.txt"
msgid "ཅི་ འདི་ སེམས་ཅན་ བརྟན་པོ་ ཡིན་ ནམ །"
