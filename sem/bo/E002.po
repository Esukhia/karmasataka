msgctxt "line 90, E002.txt"
msgid "བྱང་ཆུབ་ ཡན་ལག་ བཅུད་ ཐོས་ ནས ། །"
msgstr ""

msgctxt "line 91, E002.txt"
msgid "བྱང་ཆུབ་ ཡན་ལག་ བཅུད་ ཀྱང་ རྟོགས ། །"
msgstr ""

