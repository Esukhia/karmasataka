# །<«ལི་»«ཅོ་»-དོ།།>
msgctxt "line 139, E002.txt"
msgid "ཡོ་བྱད་ གཞན་ འདོད་པ་ ལ་ ནི་ ཡོ་བྱད་ གཞན་ སྦྱིན་པ␣ར་ བྱེད་ དོ ། །"
msgstr ""

msgctxt "line 140, E002.txt"
msgid "ནད་གསོ་བ␣འི་ ནད་བྲང་ བཅས་ ཏེ །"
