msgctxt "line 108, E002.txt"
msgid "རྟག་ ཏུ་ སྲིད་ འགྲོ་ སྐྱོ་ གྱུར་ ན ། །"
msgstr ""

msgctxt "line 109, E002.txt"
msgid "ལྷ་ དང་ མི་ལ་ ཆགས་པ་ མེད ། །"
msgstr ""

