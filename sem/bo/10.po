msgstr ""

msgctxt "1742d225dbe744c3b41255d72f08e77a"
msgid "སྟན་ བཤམས་པ་ ལ་ འདུག་ གོ ། །"
msgstr ""

msgctxt "befe1babc103441facef2cc1b770ff03"
msgid "དེ་ འདུག་ ནས་"
msgstr ""

msgctxt "c39cf81450d34c5f8b9057f9f40922ec"
msgid "ཕ་མ་ གཉིས་ ལ་ དེ་ དང་ འཐུན་པ␣འི་ ཆོས་ བསྟན་ ཏེ །"
msgstr ""

# དེ་ནས་ཁྱིམ་བདག་དེ་འཁོར་དང་བཅས་པས་སྟན་དེ་ཉིད་ལ་འཁོད་བཞིན་དུ་འཇིག་ཚོགས་ལ་ལྟ་བའི་རིའི་རྩེ་མོ་མཐོན་པོ་ཉི་ཤུ་ཡེ་ཤེས་<«གཡུང་»ཤེ།>རྡོ་རྗེས་བཅོམ་སྟེ།
msgctxt "c0ec211473a24767a108de69c767a951"
msgid ""
"དེ་ནས་ ཁྱིམ་བདག་ དེ་ འཁོར་ དང་ བཅས་པ␣ས་ སྟན་ དེ་ཉིད་ ལ་ འཁོད་ བཞིན་ དུ་ "
"འཇིག་ཚོགས་ ལ་ ལྟ་བ␣འི་ རི␣འི་ རྩེ་མོ་ མཐོན་པོ་ ཉི་ཤུ་ ཡེ་ཤེས་ རྡོ་རྗེ␣ས་ "
"བཅོམ་ སྟེ །"
msgstr ""

msgctxt "62ceac52dd4b4e7d932724c5c565efaf"
msgid "རྒྱུན་ དུ་ ཞུགས་པ␣འི་ འབྲས་བུ་ མངོན་སུམ་ དུ་ བྱས་ ནས་"
msgstr ""

# སྐྱབས་སུ་འགྲོ་བ་དང་།
# བསླབ་པའི་གཞི་རྣམས་ལ་གནས་པར་བྱས་ཏེ<«གཡུང་»«ལི་»«པེ་»«སྣར་»«ཅོ་»བྱ་སྟེ།>།
msgctxt "f1b3fba288884d0681747241ba1cc6e8"
msgid "སྐྱབས་ སུ་ འགྲོ་བ་ དང་ །  བསླབ་པ␣འི་ གཞི་ རྣམས་ ལ་ གནས་པ␣ར་ བྱས་ ཏེ །"
msgstr ""

msgctxt "8ffe324cffe14609bec75a40659c9ead"
msgid "བདེན་པ་ མཐོང་ ནས་"
msgstr ""

# སྦྱིན་པ་དག་སྦྱིན་<«ཁུ་»བྱིན།>ཞིང་
msgctxt "b6d60a2b92f744e495bbe204b49e6e5b"
msgid "སྦྱིན་པ་ དག་ སྦྱིན་ ཞིང་"
msgstr ""

msgctxt "8924ec682cd24a498ff28556a47af23b"
msgid "བསོད་ནམས་ དག་བྱེད་པ␣ར་ གྱུར་ ཏོ ། །"
msgstr ""

# དེ་ནས་དགེ་སློང་རྣམས་ཀྱིས། བཅོམ་ལྡན་འདས་ལ་ཞུས་པ<«གཡུང་»«པེ་»ཞུ་བ།>།
msgctxt "e9e331c709b34bb7aa22a1338dde6b44"
msgid "དེ་ནས་ དགེ་སློང་ རྣམས་ ཀྱིས །  བཅོམ་ལྡན་འདས་ ལ་ ཞུས་པ །"
msgstr ""

msgctxt "af158fa7b99644729720b888846736da"
msgid "བཙུན་པ་"
msgstr ""

msgctxt "29c4227e456348648ace6bdfa2f56d33"
msgid "སྣ་མ␣འི་ མེ་ཏོག་ གིས་ ལ␣ས་ ཅི་ཞིག་ བགྱིས་ ན །"
msgstr ""

msgctxt "a94b6f66ceb04f3fa11687ec2bc6dbc6"
msgid ""
"ལ␣ས་ དེ␣འི་ རྣམ་པ␣ར་ སྨིན་པ␣ས་ ཕྱུག་ ཅིང་ ནོར་ མང་ ལ་ ལོངས་སྤྱོད་ ཆེ་བ␣འི་ "
"རིགས་ སུ་ སྐྱེས་ ཏེ །"
msgstr ""

# གཟུགས་བཟང་ཞིང་བལྟ་<«ལི་»«སྣར་»«ཅོ་»ལྟ།>ན་སྡུག་ལ་མཛེས་པར་གྱུར་ལ།
msgctxt "9cf239c96be745d9ab2d7cca7b4d5ed8"
msgid "གཟུགས་བཟང་ ཞིང་ བལྟ་ ན་ སྡུག་ ལ་ མཛེས་པ␣ར་ གྱུར་ ལ །"
msgstr ""

# བཙས་མ་ཐག་ཏུ་ཡང་ལུས་ཐམས་ཅད་ནས་<«གཡུང་»«ལི་»«པེ་»«སྣར་»«ཅོ་»«ཞོལ»–ནས།>སྣ་མའི་མེ་ཏོག་གི་ངད་ལྡང་ཞིང་
msgctxt "3e15c36792a94ac1bbeb134fcdf77b3b"
msgid "བཙས་མ་ ཐག་ ཏུ་ ཡང་ ལུས་ ཐམས་ཅད་ ནས་ སྣ་མ␣འི་ མེ་ཏོག་ གི་ ངད་ ལྡང་ ཞིང་"
msgstr ""

# མངལ་དུ་ཞུགས་པ་དང་བཙས་པའི་ཚེ་ཡང་ཁྱིམ་དུ་སྣ་མའི་མེ་ཏོག་གི་ཆར་བབ་<«ཞོལ་»ཕབ།>ལ།
msgctxt "5c426a10e54842d0b43e43175c2d4c34"
msgid ""
"མངལ་ དུ་ ཞུགས་པ་ དང་ བཙས་པ␣འི་ ཚེ་ ཡང་ ཁྱིམ་ དུ་ སྣ་མ␣འི་ མེ་ཏོག་ གི་ ཆ␣ར་ "
"བབ་ལ །"
msgstr ""

msgctxt "eb0ea88a1d994c9dad86c9895c815f22"
msgid "འདི␣ས་ བཅོམ་ལྡན་འདས་ ཀྱང་ མཉེས་པ␣ར་ བགྱིས་ ཏེ །"
msgstr ""

# མི་མཉེས་པར་མ་བགྱིས་ནས<«གཡུང་»«ལི་»«པེ་»«སྣར་»«ཅོ་»–ནས།>།
msgctxt "e3dcf145d8d446c29f3e803f5bc419e2"
msgid "མི་ མཉེས་པ␣ར་ མ་ བགྱིས་ ནས །"
msgstr ""

# བཅོམ་ལྡན་འདས་ཀྱི་བསྟན་པ་ལ་<«གཡུང་»–ལ།>རབ་ཏུ་བྱུང་སྟེ།
msgctxt "3514a85979754d09b2f1603eb32fed0f"
msgid "བཅོམ་ལྡན་འདས་ ཀྱི་ བསྟན་པ་ ལ་ རབ་ཏུ་ བྱུང་ སྟེ །"
msgstr ""

msgctxt "827389fbec3744d99ab0cb4977abf725"
msgid "ཉོན་མོངས་པ་ ཐམས་ཅད་ སྤངས་ ནས །"
msgstr ""

# དགྲ་བཅོམ་པ་ཉིད་མངོན་སུམ་[40b]དུ་བགྱིས་པ་<«གཡུང་»«ལི་»«པེ་»«སྣར་»«ཅོ་»«ཞོལ་»–པ།>ལགས།
msgctxt "5db9a9971a13444fb149dd21ee1197fd"
msgid "དགྲ་བཅོམ་པ་ ཉིད་ མངོན་སུམ་ དུ་ བགྱིས་པ་ ལགས །"
msgstr ""

msgctxt "8387279c1cb34374bc2a353d29fcf5ea"
msgid "ཕ་མ་ གཉིས་ ཀྱིས་ ཀྱང་ ལ␣ས་ ཅི་ བགྱིས་ ན །"
msgstr ""

msgctxt "2603a4e468594353ae458ab25df74258"
msgid "དེ␣ས་ དེ་ ཁོ་ན་ ལ་ བརྟེན་ ནས་"
msgstr ""

msgctxt "3fc5fb8d1df04744a3d93c205383ed5e"
msgid "བཅོམ་ལྡན་འདས་ མཉེས་པ␣ར་ བགྱིས་ ཏེ་"
msgstr ""

# མི་<«གཡུང་»«ལི་»«པེ་»«སྣར་»«ཅོ་»«ཞོལ་»མ།>མཉེས་པར་མ་བགྱིས་ལགས།
msgctxt "5b54479e75c54380808dbac3dfc25835"
msgid "མི་ མཉེས་པ␣ར་ མ་ བགྱིས་ ལགས །"
msgstr ""

# བཅོམ་ལྡན་འདས་ཀྱིས་བཀའ་<«གཡུང་»ཀྱིབ་བགང་།>སྩལ་པ།
msgctxt "61ab593b0efd42afa72a7f04314f88ce"
msgid "བཅོམ་ལྡན་འདས་ ཀྱིས་ བཀའ་ སྩལ་པ །"
msgstr ""

msgctxt "36355800f651428a8d6d033f0abdb231"
msgid "སྨོན་ལམ་ གྱི་ དབང་ ལ␣ས་ གྱུར་ ཏོ ། །"
msgstr ""

msgctxt "be4af3c77d424803a72df863d9bf3f83"
msgid "བཙུན་པ་"
msgstr ""

msgctxt "c6c8b6faf11846c29691db91838fd256"
msgid "འདི␣ས་ སྨོན་ལམ་ གང་ དུ་ བཏབ་ ལགས །"
msgstr ""

msgctxt "5118826b9a0a4e70912b87e5d8e8f0b6"
msgid "དགེ་སློང་ དག་"
msgstr ""

msgctxt "ca40e76d05004c30bd673e81f58444e0"
msgid "སྔོན་བྱུང་བ་ འདས་པ␣འི་ དུས་ ན །"
msgstr ""

# བསྐལ་པ་བཟང་པོ་འདི་ཉིད་ལ་སྐྱེ་དགུའི་<«ཞོལ་»རྒུའི།>ཚེ་ལོ་ཉི་ཁྲི་ཐུབ་པ་ན།
# ཡང་དག་པར་རྫོགས་པའི་སངས་རྒྱས་རིག་པ་དང་ཞབས་སུ་ལྡན་པ་བདེ་བར་གཤེགས་པ།
# འཇིག་རྟེན་མཁྱེན་པ། སྐྱེས་བུ་འདུལ་བ་ཁ་ལོ་སྒྱུར་བ།
# བླ་ན་མེད་པ་ལྷ་དང་མི་རྣམས་ཀྱི་སྟོན་པ།
# སངས་རྒྱས་བཅོམ་ལྡན་འདས་འོད་སྲུང་ཞེས་བྱ་བ་<«ཅོ་»–བ།>འཇིག་རྟེན་དུ་བྱུང་སྟེ།
msgctxt "a2292f41b97f4f8e85690b7b0801a7f3"
msgid ""
"བསྐལ་པ་ བཟང་པོ་ འདི་ཉིད་ ལ་ སྐྱེ་དགུ␣འི་ ཚེ་ལོ་ ཉི་ཁྲི་ ཐུབ་པ་ ན །  "
"ཡང་དག་པ␣ར་ རྫོགས་པ␣འི་ སངས་རྒྱས་ རིག་པ་ དང་ ཞབས་ སུ་ ལྡན་པ་ བདེ་བ␣ར་ གཤེགས་པ"
" །  འཇིག་རྟེན་ མཁྱེན་པ །  སྐྱེས་བུ་ འདུལ་བ་ ཁ་ལོ་ སྒྱུར་བ །  བླ་ ན་ མེད་པ་ "
"ལྷ་ དང་ མི་ རྣམས་ ཀྱི་ སྟོན་པ །  སངས་རྒྱས་ བཅོམ་ལྡན་འདས་ འོད་སྲུང་ ཞེས་ "
"བྱ་བ་ འཇིག་རྟེན་ དུ་ བྱུང་ སྟེ །"
msgstr ""

# དེའི་ཚེ་བཱ་རཱ་ཎ་སཱི་ན་ཁྱིམ་བདག་ཕྱུག་ཅིང་ནོར་མང་ལ་ལོངས་སྤྱོད་ཆེ་བ་ཡོངས་སུ་འཛིན་པ།
# ཡངས་ཤིང་རྒྱ་<«གཡུང་»«པེ་»–རྒྱ།>ཆེ་བ་རྣམ་ཐོས་ཀྱི་བུའི་ནོར་དང་ལྡན་པ།
# རྣམ་ཐོས་ཀྱི་བུའི་ནོར་དང་འགྲན་པ་ཞིག་གནས་པ་དེས་ཐབས་ཟླར་བབ་པ་ལས། ཆུང་མ་བླངས་ནས།
msgctxt "66c0624f4ccf47ae80a7b6ac0c67fb19"
msgid ""
"དེ␣འི་ ཚེ་ བཱ་རཱ་ཎ་སཱི་ ན་ ཁྱིམ་བདག་ ཕྱུག་ ཅིང་ ནོར་ མང་ ལ་ ལོངས་སྤྱོད་ "
"ཆེ་བ་ ཡོངས་སུ་ འཛིན་པ །  ཡངས་ ཤིང་ རྒྱ་ཆེ་བ་ རྣམ་ཐོས་ ཀྱི་ བུ␣འི་ ནོར་ དང་ "
"ལྡན་པ །  རྣམ་ཐོས་ ཀྱི་ བུ␣འི་ ནོར་ དང་ འགྲན་པ་ ཞིག་ གནས་པ་ དེ␣ས་ ཐབས་ ཟླ␣ར་ "
"བབ་པ་ ལ␣ས །  ཆུང་མ་ བླངས་ ནས །"
msgstr ""

msgctxt "7dff12826a5142e19f5e4963d7798af1"
msgid "དེ་དེ་ དང་ ལྷན་ཅིག་ ཏུ་ རྩེ་ ཞིང་ དགའ་ ལ་ དགའ་མགུ␣ར་ སྤྱོད་པ་ ལ␣ས །"
msgstr ""

# ཕྱི་ཞིག་ན་<«གཡུང་»«ལི་»«པེ་»«སྣར་»«ཅོ་»«ཞོལ་»–ན།>དེའི་ཆུང་མ་ལ་བུ་ཆགས་ནས།
msgctxt "df38f62f448240efbd76a7d4121c42e3"
msgid "ཕྱི་ ཞིག་ ན་ དེ␣འི་ ཆུང་མ་ ལ་ བུ་ ཆགས་ ནས །"
msgstr ""

# དེས་ཟླ་བ་དགུའམ་བཅུ་ལོན་པ་དང་།
# ཁྱེའུ་གཟུགས་བཟང་ཞིང་བལྟ་<«ལི་»«སྣར་»«ཅོ་»ལྟ།>ན་སྡུག་ལ་མཛེས་པ་ཞིག་བཙས་སོ། །
msgctxt "8241f7a1f0d14901867c7d8cc91d9c25"
msgid ""
"དེ␣ས་ ཟླ་བ་ དགུ␣འམ་ བཅུ་ ལོན་པ་ དང་ །  ཁྱེའུ་ གཟུགས་བཟང་ ཞིང་ བལྟ་ ན་ སྡུག་ "
"ལ་ མཛེས་པ་ ཞིག་ བཙས་ སོ ། །"
msgstr ""

# དེ་ནས་དེ་ཆེར་<«གཡུང་»«པེ་»ཕྱིར།>སྐྱེས་པ་དང་།
msgctxt "e90fbd1e042d441dbcde652c72d6a17a"
msgid "དེ་ནས་ དེ་ ཆེ␣ར་ སྐྱེས་པ་ དང་ །"
msgstr ""

# དེས་ཡང་དག་པར་རྫོགས་པའི་སངས་རྒྱས་འོད་སྲུང་གི་བསྟན་<«གཡུང་»སྟན།>པ་ལ་དད་པ་རྙེད་ནས།
msgctxt "b24a9af644494280b3ca957d0f5924aa"
msgid ""
"དེ␣ས་ ཡང་དག་པ␣ར་ རྫོགས་པ␣འི་ སངས་རྒྱས་ འོད་སྲུང་ གི་ བསྟན་པ་ ལ་ དད་པ་ རྙེད་ "
"ནས །"
msgstr ""

# དེས་ཕ་མ་ལ་གསོལ་ཏེ<«གཡུང་»ཏོ།>།
msgctxt "5daba5e294b64b9398df89f5c545532a"
msgid "དེ␣ས་ ཕ་མ་ ལ་ གསོལ་ ཏེ །"
msgstr ""

# རབ་ཏུ་བྱུང་<«གཡུང་»«པེ་»«ཅོ་»འབྱུང་།>ངོ་། །
msgctxt "25669796734546caba43dee80cc7af30"
msgid "རབ་ཏུ་ བྱུང་ ངོ་ ། །"
msgstr ""

msgctxt "ad3cde96e8f644888bac708c1c5f18b7"
msgid "རབ་ཏུ་ བྱུང་ ནས་"
msgstr ""

msgctxt "d19e25a695ce47b98a2a31a5446bc6f9"
msgid "སྡེ་སྣོད་ གསུམ་ བསླབས་ ཏེ །"
msgstr ""

# རིག་པ་དང་གྲོལ་བའི་སྤོབས་<«གཡུང་»སྨོབས།>པ་དང་ལྡན་པའི་ཆོས་སྨྲ་བར་གྱུར་ནས་
msgctxt "de5caed66f6d4690820a197a1b774e44"
msgid "རིག་པ་ དང་ གྲོལ་བ␣འི་ སྤོབས་པ་ དང་ ལྡན་པ␣འི་ ཆོས་ སྨྲ་བ␣ར་ གྱུར་ ནས་"
msgstr ""

# དེས་ཕ་མ་གཉིས་དད་པ་<«གཡུང་»«པེ་»–པ།>ཕུན་སུམ་ཚོགས་པ་ལ་བཀོད་དེ<«གཡུང་»དོ།>།
msgctxt "313a9f4e44a1483cbd4913399e8c7536"
msgid "དེ␣ས་ ཕ་མ་ གཉིས་ དད་པ་ ཕུན་སུམ་ ཚོགས་པ་ ལ་ བཀོད་ དེ །"
msgstr ""

# སྐྱབས་སུ་འགྲོ་བ་དང་། བསླབ་པའི་གཞི་རྣམས་ལ་གནས་པར་བྱས་སོ<«གཡུང་»«པེ་»བྱའོ།>། །
msgctxt "c826f5a5cf294519b384008a1a5b5048"
msgid ""
"སྐྱབས་ སུ་ འགྲོ་བ་ དང་ །  བསླབ་པ␣འི་ གཞི་ རྣམས་ ལ་ གནས་པ␣ར་ བྱས་ སོ ། །"
msgstr ""

msgctxt "69e7c0b95a4142c5b7250c8fc6796708"
msgid "སྦྱིན་པ་ དང་ སྦྱིན་པ␣འི་ བགོ་བཤའ་ ལ་ ཡང་ བཙུད་ དོ ། །"
msgstr ""

msgctxt "dcc9d5861e5c49899c3f68984eafe244"
msgid "ཕྱི་ ཞིག་ ན་ དེ␣ས་ བསམས་པ །"
msgstr ""

msgctxt "aaecc46aace641cdb58a2583cb4b28cd"
msgid ""
"བདག་ གིས་ ཐོས་པ␣འི་ སྒོ་ ནས་ བྱ་བ་ གང་ཡིན་པ་ དེ་ ནི་ བདག་ གིས་ བྱས་ ཟིན་ "
"གྱིས །"
msgstr ""

msgctxt "ccbfed7e1f2f430ca1569e4b292055c0"
msgid "མ་ལ་"
msgstr ""

msgctxt "51cb7520c9124d9db968b31a5bcf44ec"
msgid "ད་ནི་ བདག་ གིས་ དགེ་འདུན་ གྱི་ ཞལ་ཏ་ བྱ␣འོ་ སྙམ་ དུ་"
msgstr ""

msgctxt "c92776e2aba349c4be1580d58daa9026"
msgid "བསམས་ ནས །"
msgstr ""

msgctxt "0d3f32f2b40c41dc818b343c94affd06"
msgid "ཕ་མ་ གཉིས་ དང་ བྲམ་ཟེ་ དང་ ཁྱིམ་བདག་ དད་པ་ ཅན་ གཞན་དག་ ལ་ བསྐུལ་ ཏེ །"
msgstr ""

# དགེ་འདུན་ལ་འབྲས་ཆན་<«ལི་»«ཅོ་»ཆེན།>དང་ཐུག་པ་<«གཡུང་»«པེ་»+དང་།>བཏུང་བ་དང་སྐྱོ་མ་རྣམས་[41a]དང་གོས་དང་ཟས་དང་མལ་ཆ་དང་སྟན་དང་ནད་གསོས་དང་སྨན་ཟོང་རྣམས་ཕུལ་ལོ།
# །
msgctxt "3bd735e11b744f9ebcca11ef043aa618"
msgid ""
"དགེ་འདུན་ ལ་ འབྲས་ཆན་ དང་ ཐུག་པ་ བཏུང་བ་ དང་ སྐྱོ་མ་ རྣམས་ དང་ གོས་ དང་ ཟས་ "
"དང་ མལ་ཆ་ དང་ སྟན་ དང་ ནད་གསོ␣ས་ དང་ སྨན་ཟོང་ རྣམས་ ཕུལ་ ལོ ། །"
msgstr ""

msgctxt "d4d65e88abd04d27aff9e6c68009d106"
msgid "ཁབ་ ཀྱང་ ཕུལ་ ལོ ། །"
msgstr ""

# དེ་བཞིན་དུ་དབུ་སྐྲ་དང་སེན་མོའི་མཆོད་རྟེན་རྣམས་ལ་ཡང་ཏིལ་མར་གྱིས་བྱུག་<«གཡུང་»དབྱུག>པ་དང་སྤོས་ཀྱིས་བྱུག་པ་དང་།
# མར་མེའི་ཕྲེང་བ་དང་སྣ་མའི་མེ་ཏོག་གི་ཕྲེང་བ་དང་གདུགས་ཀྱང་ཕུལ་
msgctxt "9dc2a21ed244418b890df3ebcd9eb15e"
msgid ""
"དེ་བཞིན་ དུ་ དབུ་སྐྲ་ དང་ སེན་མོ␣འི་ མཆོད་རྟེན་ རྣམས་ ལ་ ཡང་ ཏིལ་མར་ གྱིས་ "
"བྱུག་པ་ དང་ སྤོས་ ཀྱིས་ བྱུག་པ་ དང་ །  མར་མེ␣འི་ ཕྲེང་བ་ དང་ སྣ་མ␣འི་ "
"མེ་ཏོག་ གི་ ཕྲེང་བ་ དང་ གདུགས་ ཀྱང་ ཕུལ་"
msgstr ""

msgctxt "c7b2ad592d5245c4b07827eaf9dee781"
msgid "སྣ་མ␣འི་ མེ་ཏོག་ གི་ སིལ་མ␣ས་ ཀྱང་ གཏོར་ ནས །"
msgstr ""

msgctxt "cc030d79d9984f5e81f75f4a382f989a"
msgid "སྨོན་ལམ་ བཏབ་པ །"
msgstr ""

msgctxt "ab9e4ab4bd91427eab17901d3ca9d194"
msgid "ཀྱེ་མ་"
msgstr ""

# དགེ་བའི་རྩ་བ་འདིས་ན<«སྣར་»«ཞོལ་»ན་བདག«ཅོ་»ནི།>།
# གང་དང་གང་དུ་སྐྱེ་བ་དེ་དང་དེར་ཕྱུག་ཅིང་ནོར་མང་ལ་ལོངས་སྤྱོད་ཆེ་བའི་རིགས་སུ་སྐྱེ་བར་ཤོག་ཅིག
# །
msgctxt "96d68ef70d3e4b12a6ae9325e03e480b"
msgid ""
"དགེ་བ␣འི་ རྩ་བ་ འདི␣ས་ ན །  གང་དང་གང་ དུ་ སྐྱེ་བ་ དེ་ དང་ དེ␣ར་ ཕྱུག་ ཅིང་ "
"ནོར་ མང་ ལ་ ལོངས་སྤྱོད་ ཆེ་བ␣འི་ རིགས་ སུ་ སྐྱེ་བ␣ར་ ཤོག་ ཅིག  །"
msgstr ""

# གཟུགས་བཟང་ཞིང་བལྟ་<«ལི་»«སྣར་»«ཅོ་»ལྟ།>ན་སྡུག་ལ་མཛེས་པར་ཤོག་ཅིག །
msgctxt "33e3e5061cc54b3d857794dcf1454f7c"
msgid "གཟུགས་བཟང་ ཞིང་ བལྟ་ ན་ སྡུག་ ལ་ མཛེས་པ␣ར་ ཤོག་ ཅིག  །"
msgstr ""

# བདག་གི་<«གཡུང་»«ལི་»«པེ་»«སྣར་»«ཅོ་»གིས།>ལུས་ཐམས་ཅད་ནས་ཀྱང་སྣ་མའི་མེ་ཏོག་གི་<«གཡུང་»«ལི་»གྱི།>དྲིའི་ངད་ལྡང་བར་<«གཡུང་»«པེ་»ལྡང་བ།
# «ཁུ་»ལྡན་པར།>ཤོག་ཅིག །
msgctxt "5ffd46879f84414a804425a931d8137d"
msgid ""
"བདག་ གི་ ལུས་ ཐམས་ཅད་ ནས་ ཀྱང་ སྣ་མ␣འི་ མེ་ཏོག་ གི་ དྲི␣འི་ ངད་ ལྡང་བ␣ར་ "
"ཤོག་ ཅིག  །"
msgstr ""

msgctxt "919a1cce90a04c73a7ee8f9bcdcc649b"
msgid ""
"མངལ་ དུ་ ཞུགས་པ་ དང་ བཙས་པ␣འི་ ཚེ་ ཡང་ བདག་ གི་ ཁྱིམ་ དུ་ སྣ་མ␣འི་ མེ་ཏོག་ "
"གི་ ཆ␣ར་ འབབ་པ␣ར་ ཤོག་ ཅིག  །"
msgstr ""

# ཡང་དག་པར་རྫོགས་པའི་སངས་རྒྱས་འོད་སྲུང་གིས་བྲམ་<«གཡུང་»གྲམ།>ཟེའི་ཁྱེའུ་བླ་མ་ལུང་བསྟན་པ་གང་ཡིན་པའི་སངས་རྒྱས་བཅོམ་ལྡན་འདས་དེ་བདག་གིས་མཉེས་པར་བྱེད་པར་གྱུར་ཅིག
# །
msgctxt "0fbaeb1238404ead921323810b89d728"
msgid ""
"ཡང་དག་པ␣ར་ རྫོགས་པ␣འི་ སངས་རྒྱས་ འོད་སྲུང་ གིས་ བྲམ་ཟེ␣འི་ ཁྱེའུ་ བླ་མ་ "
"ལུང་བསྟན་པ་ གང་ཡིན་པ␣འི་ སངས་རྒྱས་ བཅོམ་ལྡན་འདས་ དེ་ བདག་ གིས་ མཉེས་པ␣ར་ "
"བྱེད་པ␣ར་ གྱུར་ཅིག  །"
msgstr ""

msgctxt "b8f4f24f0e284803933df05718d44996"
msgid "མི་ མཉེས་པ␣ར་ བྱེད་པ␣ར་ མ་ གྱུར་ཅིག  །"
msgstr ""

# དེ་ཁོ་ནའི་བསྟན་པ་ལ་རབ་ཏུ་བྱུང་ནས<«གཡུང་»ནོས།>།
msgctxt "0f7fc700a99d4c1495f7e5e324460e24"
msgid "དེ་ ཁོ་ན␣འི་ བསྟན་པ་ ལ་ རབ་ཏུ་ བྱུང་ ནས །"
msgstr ""

msgctxt "9778fda1ec8d4334a94fe9dedeecd560"
msgid "ཉོན་མོངས་པ་ ཐམས་ཅད་ སྤངས་ ཏེ །"
msgstr ""

msgctxt "1ad8deea51974e79981c3b5e8c55900e"
msgid "དགྲ་བཅོམ་པ་ ཉིད་ མངོན་སུམ་ དུ་ བྱེད་པ␣ར་ གྱུར་ཅིག  །"
msgstr ""

msgctxt "a79888b6a583461794d1c7caa1f5a283"
msgid "ཤེས་རབ་ ཀྱང་ རྣོ་བ␣ར་ གྱུར་ཅིག  །"
msgstr ""

# བསམ་གཏན་དང་། རྣམ་པར་ཐར་པ་དང་། ཏིང་ངེ་འཛིན་དང་།
# སྙོམས་པར་འཇུག་པ་རྣམས་ཀྱང་ཐོབ་སྟེ<«གཡུང་»«པེ་»ཏེ།>།
msgctxt "8e11bea2fccd445fb96eb7fa1a6e140e"
msgid ""
"བསམ་གཏན་ དང་ །  རྣམ་པ␣ར་ ཐར་པ་ དང་ །  ཏིང་ངེ་འཛིན་ དང་ །  སྙོམས་པར་ འཇུག་པ་ "
"རྣམས་ ཀྱང་ ཐོབ་ སྟེ །"
msgstr ""

msgctxt "bf136284495741b984ce2718d9dc83a4"
msgid "སྙོམས་པར་ འཇུག་པ་ དང་ ལྡང་བ་ ལ་ མྱུར་བ␣ར་ གྱུར་ཅིག  །"
msgstr ""

msgctxt "ae96682757414886b7c8b691be60fad4"
msgid ""
"ཁབ་ ལན་ རེ་ འཛུགས་པ␣འི་ ཡུན་ ཙམ་ ལ་ ཡང་ །  ཏིང་ངེ་འཛིན་ གཞན་ དང་ །  གཞན་དག་ "
"ལ་ སྙོམས་པར་ འཇུག་ ཅིང་ ལྡང་བ␣ར་ གྱུར་ཅིག་ ཅེས་"
msgstr ""

msgctxt "ea8307e9afc4443b83fcc4bce6032aff"
msgid "བྱས་ སོ ། །"
msgstr ""

msgctxt "cd555c07f1af4d5c82af80ec34421758"
msgid "དེ་ནས་ ཕ་མ་ གཉིས་ ཀྱིས་ དེ་ མཐོང་ ནས་"
msgstr ""

# དྲིས་<«གཡུང་»བྲིས།>པ།
msgctxt "58de7207950649e2aeae07f76e02e4ce"
msgid "དྲིས་པ །"
msgstr ""

msgctxt "ef2f6c5970534fbf871f05d440c0ce75"
msgid "བུ་ ཁྱོད་ ཀྱིས་ སྨོན་ལམ་ ཅི་ཞིག་ བཏབ །"
msgstr ""

# དེས་དེ་གཉིས་ལ་བདག་གིས་སྨོན་ལམ་འདི་ལྟར་<«ཅོ་»འདིར།>བཏབ་པོ་ཞེས་
msgctxt "568ae35a34a74132b474ae04bea02d36"
msgid "དེ␣ས་ དེ་ གཉིས་ ལ་ བདག་ གིས་ སྨོན་ལམ་ འདི་ལྟར་ བཏབ་པོ་ ཞེས་"
msgstr ""

# ཐམས་ཅད་བསྙད་<«གཡུང་»བརྙེད།>དོ། །
msgctxt "207d5aae547d4c50a3695dcc1c85780b"
msgid "ཐམས་ཅད་ བསྙད་ དོ ། །"
msgstr ""

msgctxt "d3c28e60ef0b46feb278cc93b3c56c1b"
msgid "ཕ་མ་ གཉིས་ ཀྱིས་ སྨྲས་པ །"
msgstr ""

msgctxt "2a6f81bacf8c4ff395f589efa450ac7d"
msgid "ཁྱོད་ ཁོ་ན་ བདག་ཅག་ གཉིས་ ཀྱི་ བུར་ གྱུར་ཅིག  །"
msgstr ""

msgctxt "5c3e1cc5013745a0919215bbacf992bd"
msgid "བདག་ཅག་ གཉིས་ ཀྱང་ ཁྱོད་ ཀྱི་ ཕ་མ␣ར་ གྱུར་ཅིག  །"
msgstr ""

# ཁྱོད་ཁོ་ན་ལ་<«གཡུང་»«ལི་»«པེ་»«སྣར་»«ཅོ་»ལས།>བརྟེན་ཏེ་
msgctxt "e97a3251227e4d9ab6be6130f5fba3ef"
msgid "ཁྱོད་ ཁོ་ན་ ལ་ བརྟེན་ ཏེ་"
msgstr ""

msgctxt "02bd5a9ebac148b9bb390ca1c25135f2"
msgid ""
"བདག་ཅག་ གཉིས་ ཀྱིས་ སངས་རྒྱས་ བཅོམ་ལྡན་འདས་ མཉེས་པ␣ར་ བྱེད་པ␣ར་ གྱུར་ཅིག  །"
msgstr ""

# མི་མཉེས་པར་བྱེད་པར་<«གཡུང་»«པེ་»–བྱེད་པར།>མ་གྱུར་ཅིག་ཅེས་
msgctxt "dfda6386f5544e2580c3c7499cd10144"
msgid "མི་ མཉེས་པ␣ར་ བྱེད་པ␣ར་ མ་ གྱུར་ཅིག་ ཅེས་"
msgstr ""

msgctxt "f5ad4c67cc1443bba6d795e0b770108e"
msgid "བྱས་ སོ ། །"
msgstr ""

msgctxt "3b25306b87cd45b49357dac5587648ad"
msgid "དགེ་སློང་ དག་"
msgstr ""

# ཇི་<«གཡུང་»«ལི་»«པེ་»«སྣར་»«ཅོ་»ཅི།>སྙམ་དུ་སེམས།
msgctxt "382cc580f7464f3fb5e14548a49a6900"
msgid "ཇི་སྙམ་ དུ་ སེམས །"
msgstr ""

msgctxt "1d31564e1d4f48d18111030633e5cf7f"
msgid ""
"དེ␣འི་ ཚེ་ དགེ་སློང་ དུ་ གྱུར་པ་ གང་ཡིན་པ་ དེ་ ནི་ སྣ་མ␣འི་ མེ་ཏོག་ འདི་ཉིད་"
" ཡིན་ ཏེ །"
msgstr ""

msgctxt "f4a7ed2d14284fa8a081f1f07471b868"
msgid ""
"དེ་དེ␣ར་ ཡང་དག་པ␣ར་ རྫོགས་པ␣འི་ སངས་རྒྱས་ འོད་སྲུང་ གི་ བསྟན་པ་ ལ་ རབ་ཏུ་ "
"བྱུང་ ནས །"
msgstr ""

msgctxt "2f01af8ea7fe4ed299c3bd19b3919c2f"
msgid "ཆོས་བཞིན་ དུ་ ཞལ་ཏ་ བྱས་ ཏེ །"
msgstr ""

# སངས་རྒྱས་དང་། ཆོས་དང་དགེ་འདུན་ལ་བཀུར་སྟི་<«གཡུང་»«པེ་»སྟིར།>བྱས་ནས།
msgctxt "d760129aa2d743a68e1fca88115bb9eb"
msgid "སངས་རྒྱས་ དང་ །  ཆོས་ དང་ དགེ་འདུན་ ལ་ བཀུར་སྟི་ བྱས་ ནས །"
msgstr ""

# སྨོན་ལམ་བཏབ་པའི་ལས་དེའི་རྣམ་པར་སྨིན་པས་གང་དང་གང་དུ་སྐྱེས་པ་དེ་དང་དེར་ཕྱུག་ཅིང་ནོར་མང་ལ་ལོངས་སྤྱོད་ཆེ་བའི་རིགས་སུ་<«གཡུང་»–སུ།>སྐྱེས་ཏེ།
msgctxt "f4de9cb3dd1c4c54963b2bdddba2be25"
msgid ""
"སྨོན་ལམ་ བཏབ་པ␣འི་ ལ␣ས་ དེ␣འི་ རྣམ་པ␣ར་ སྨིན་པ␣ས་ གང་དང་གང་ དུ་ སྐྱེས་པ་ དེ་"
" དང་ དེ␣ར་ ཕྱུག་ ཅིང་ ནོར་ མང་ ལ་ ལོངས་སྤྱོད་ ཆེ་བ␣འི་ རིགས་ སུ་ སྐྱེས་ ཏེ །"
msgstr ""

# གཟུགས་བཟང་ཞིང་བལྟ་<«ལི་»«སྣར་»«ཅོ་»ལྟ།>ན་སྡུག་ལ་མཛེས་པར་གྱུར་ལ་
msgctxt "9659c9e6e1ef4be7a3612342b6dee018"
msgid "གཟུགས་བཟང་ ཞིང་ བལྟ་ ན་ སྡུག་ ལ་ མཛེས་པ␣ར་ གྱུར་ ལ་"
msgstr ""

# བཙས་<«གཡུང་»གཙས།>མ་ཐག་ཏུ་ཡང་དེའི་ལུས་ཐམས་ཅད་ནས།
# སྣ་མའི་མེ་ཏོག་གི་དྲིའི་ངད་ལྡང་བར་གྱུར་པ་དང་།
msgctxt "ef301897299d4597b4ef19c443bd6ff2"
msgid ""
"བཙས་མ་ ཐག་ ཏུ་ ཡང་ དེ␣འི་ ལུས་ ཐམས་ཅད་ ནས །  སྣ་མ␣འི་ མེ་ཏོག་ གི་ དྲི␣འི་ "
"ངད་ ལྡང་བ␣ར་ གྱུར་པ་ དང་ །"
msgstr ""

# མངལ་དུ་ཞུགས་པ་དང་།
# བཙས་པའི་ཚེ་ཡང་དེའི་ཁྱིམ་དུ་སྣ་མའི་མེ་ཏོག་གི་ཆར་ཕབ་སྟེ<«སྣར་»«ཞོལ་»བབ་སྟེ།
# «ཁུ་»ཕབ་ཏེ།>།
msgctxt "18af3289010b4988a18962fd2dbbb466"
msgid ""
"མངལ་ དུ་ ཞུགས་པ་ དང་ །  བཙས་པ␣འི་ ཚེ་ ཡང་ དེ␣འི་ ཁྱིམ་ དུ་ སྣ་མ␣འི་ མེ་ཏོག་ "
"གི་ ཆ␣ར་ ཕབ་ སྟེ །"
msgstr ""

msgctxt "d25bc98afc5447c6b1ea596309eae8a5"
msgid "དགེ་སློང་ དག་"
msgstr ""

# ང་ཡང་དག་པར་རྫོགས་པའི་སངས་རྒྱས་འོད་སྲུང་དང་ཤིན་ཏུ་མཉམ་པ་དང་།
# ཤུགས་མཉམ་པ་དང་མཛད་པ་མཉམ་པ་དང་ཐབས་མཉམ་པ་<«གཡུང་»«པེ་»–དང་ཐབས་མཉམ་པ།>རྙེད་<«ཁུ་»བརྙེས།>པར་གྱུར་ནས།
msgctxt "ea74e34b0249471ab41d2862e5a90dc0"
msgid ""
"ང་ ཡང་དག་པ␣ར་ རྫོགས་པ␣འི་ སངས་རྒྱས་ འོད་སྲུང་ དང་ ཤིན་ཏུ་ མཉམ་པ་ དང་ །  "
