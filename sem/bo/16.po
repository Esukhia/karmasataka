msgctxt "54e2ff1eba04475c8205911c1933e815"
msgid "ཤིང་རྟ་ གསུམ་པ་ ནི །"
msgstr ""

# གླེང་གཞི་<«ཞོལ་»བཞི།>མཉན་དུ་ཡོད་པ་ན་བཞུགས་ཏེ།
msgctxt "8a9fd3a481d246699e19cfd38a6e80dd"
msgid "གླེང་ གཞི་ མཉན་ དུ་ཡོད་པ་ ན་ བཞུགས་ ཏེ །"
msgstr ""

msgctxt "c1687ad16aeb4266bf275fb974fddfb0"
msgid "བཅོམ་ལྡན་འདས་ སྔ་དྲོ་ ཤམ་ཐབས་ དང་ ཆོས་གོས་ གསོལ་ ཏེ །"
msgstr ""

msgctxt "6348421efad04ea5b91737b8a3111c4a"
msgid "ལྷུང་བཟེད་ བསྣམས་ ནས་"
msgstr ""

# མཉན་<«གཡུང་»«པེ་»གཉན།>དུ་ཡོད་པར་བསོད་སྙོམས་ལ་གཤེགས་སོ། །
msgctxt "597e908eedba471bad70b480d38fbf1b"
msgid "མཉན་ དུ་ཡོད་པ␣ར་ བསོད་སྙོམས་ ལ་ གཤེགས་ སོ ། །"
msgstr ""

msgctxt "bf224d552fd04351b92620f4a3c97704"
msgid "བྲམ་ཟེ་ ཞིག་ ཀྱང་ ཤིང་རྟ་ ལ་ ཞོན་ ཏེ་"
msgstr ""

msgctxt "45cc8089bba347b2b368413b13c927ab"
msgid "ཡུལ་ རྒྱུ་ ཞིང་"
msgstr ""

msgctxt "d17dae7d7bf7411e88610ed774558d33"
msgid "ཆས་པ་ ལ␣ས །"
msgstr ""

msgctxt "544cb008ba15459db3462714bfda425b"
msgid "བཅོམ་ལྡན་འདས་ མཐོང་ ནས །"
msgstr ""

msgctxt "81d3acd866fc41ef8419cf8f05cc7a7f"
msgid "བཀྲ་ མི་ ཤི␣ས་ སོ་ སྙམ་ སྟེ་"
msgstr ""

# ཕན་ཚུན་དུ་བྲོས་སོ<«གཡུང་»«པེ་»སྟེ།>། །
msgctxt "60e9cdecb25543bc921b79d6823898b2"
msgid "ཕན་ཚུན་ དུ་ བྲོས་ སོ ། །"
msgstr ""

# བཅོམ་ལྡན་འདས་ཀྱིས་ཀྱང་།
# དེ་ལ་ཐུགས་བརྩེ་བའི་ཕྱིར་དེ་དང་དེ་ཁོ་ན་ན་<«གཡུང་»«པེ་»–ན།>བཞུགས་པར་གྱུར་ཏོ། །
msgctxt "24b114b59b7c455aa791ac864bca0c03"
msgid ""
"བཅོམ་ལྡན་འདས་ ཀྱིས་ ཀྱང་ །  དེ་ ལ་ ཐུགས་ བརྩེ་བ␣འི་ ཕྱིར་ དེ་ དང་ དེ་ ཁོ་ན་ "
"ན་ བཞུགས་པ␣ར་ གྱུར་ ཏོ ། །"
msgstr ""

# དེ་ནས་རེ་ཞིག་ན་གྲོང་ཁྱེར་གྱི་སྒོ་བཞི་ཆར་དུ་<«གཡུང་»«ལི་»«པེ་»«ཅོ་»–དུ།>ཡང་དེའི་མདུན་ན་བཅོམ་ལྡན་འདས་བཞུགས་པར་གྱུར་ནས་
msgctxt "431e26503f4048f3a33047aecf1ec98e"
msgid ""
"དེ་ནས་ རེ་ཞིག་ ན་ གྲོང་ཁྱེར་ གྱི་ སྒོ་ བཞི་ཆ␣ར་ དུ་ ཡང་ དེ␣འི་ མདུན་ ན་ "
"བཅོམ་ལྡན་འདས་ བཞུགས་པ␣ར་ གྱུར་ ནས་"
msgstr ""

msgctxt "9aa787fa0e6b4695bcd43065746b01b8"
msgid "དེ␣ས་ བསམས་པ་"
msgstr ""

# དགེ་སྦྱོང་<«གཡུང་»སློང་།>གཽ་ཏ་མ་འདི་ནི་རྫུ་འཕྲུལ་ཆེ་ཞིང་
msgctxt "f5e32c6d1f3c492d992eb724f4ab8acc"
msgid "དགེ་སྦྱོང་ གཽ་ ཏ་ མ་ འདི་ ནི་ རྫུ་འཕྲུལ་ ཆེ་ ཞིང་"
msgstr ""

# མཐུ་ཆེ་<«གཡུང་»ཆེད།>བ་ཡིན་ནོ་སྙམ་ནས།
msgctxt "4c1b1e008a0e495ea97b75d44abb401c"
msgid "མཐུ་ ཆེ་བ་ ཡིན་ ནོ་ སྙམ་ ནས །"
msgstr ""

msgctxt "bb3fc4ce786b4bf8bfec9ad833d254fc"
msgid "དེ་ བཅོམ་ལྡན་འདས་ ལ་ དགའ་བ་ སྐྱེས་ སོ ། །"
msgstr ""

msgctxt "7dc24e7df03f420c9cb0256341672a11"
msgid "དེ་ དགའ་བ་ སྐྱེས་ ནས །"
msgstr ""

msgctxt "1db6abc5be63495a88a37a56a1b8fc38"
msgid "བཅོམ་ལྡན་འདས་ ཀྱི་ སྟེང་ དུ་ མེ་ཏོག་ སྤར་གང་ གཏོར་ ཏོ ། །"
msgstr ""

msgctxt "9538933c26344d9095193733cd2fadbf"
msgid "དེ␣འི་ སྐབས་ སུ་ བཅོམ་ལྡན་འདས་ ཀྱིས་ འཛུམ་པ་ མཛད་ དེ །"
msgstr ""

# འདི་<«ཁུ་»དེ།>ནི་སངས་རྒྱས་བཅོམ་ལྡན་འདས་རྣམས་གང་གི་ཚེ་འཛུམ་པ་མཛད་པའི་ཆོས་ཉིད་དེ།
msgctxt "cd6efadf796f42ab87e81fd4d95cb9b4"
msgid ""
"འདི་ ནི་ སངས་རྒྱས་ བཅོམ་ལྡན་འདས་ རྣམས་ གང་ གི་ ཚེ་ འཛུམ་པ་ མཛད་པ␣འི་ "
"ཆོས་ཉིད་ དེ །"
msgstr ""

msgctxt "f84ffd8290ad4c1a87f1daf7759b0995"
msgid ""
"དེ␣འི་ ཚེ་ ཞལ་ ནས་ འོད་ཟེར་ སྔོན་པོ་ དང་ །  སེར་པོ་ དང་ །  དམར་པོ་ དང་ །  "
"དཀར་པོ་ རྣམས་ བྱུང་ ནས་"
msgstr ""

msgctxt "faba91a2b6cb4fe6b70b3d1875f0f63f"
msgid "ཁ་ཅིག་ ནི་ འོག་ ཏུ་ འགྲོ␣འོ ། །"
msgstr ""

msgctxt "75119e9c6d63493fb941a3c8ca519ab2"
msgid "ཁ་ཅིག་ ནི་ སྟེང་ དུ་ འགྲོ␣འོ ། །"
msgstr ""

# འོག་ཏུ་འགྲོ་བ་གང་ཡིན་པ་དེ་དག་ནི་སེམས་ཅན་དམྱལ་བ་ཡང་སོས་དང་། ཐིག་ནག་དང་།
# བསྡུས་འཇོམས་དང་། ངུ་འབོད་དང་། ངུ་འབོད་ཆེན་པོ་དང་། ཚ་བ་དང་། རབ་ཏུ་ཚ་བ་དང་།
# མནར་མེད་པ་དང་། ཆུ་བུར་ཅན་དང་། ཆུ་བུར་རྡོལ་བ་<«གཡུང་»«པེ་»–བ།>དང་།
# སོ་ཐམས་ཐམས་<«སྣར་»ཐམ་ཐམ།>དང་། ཀྱི་ཧུད་ཟེར་<«གཡུང་»«པེ་»«སྣར་»«ཅོ་»+བ།>དང་།
# ཨ་ཆུ་ཟེར་བ་<«གཡུང་»–བ།>དང་། ཨུད་པལ་<«གཡུང་»དཔལ།>ལྟར་གས་པ་དང་།
# པད་མ་ལྟར་གས་པ་དང་། པད་མ་ལྟར་གས་པ་<«སྣར་»ཕ།>ཆེན་པོར་སོང་ནས།
msgctxt "299b1c6c672944c7b7dd69cab0b2033b"
msgid ""
"འོག་ ཏུ་ འགྲོ་བ་ གང་ཡིན་པ་ དེ་དག་ ནི་ སེམས་ཅན་ དམྱལ་བ་ ཡང་སོས་ དང་ །  "
"ཐིག་ནག་ དང་ །  བསྡུས་འཇོམས་ དང་ །  ངུ་འབོད་ དང་ །  ངུ་འབོད་ ཆེན་པོ་ དང་ །  "
"ཚ་བ་ དང་ །  རབ་ཏུ་ ཚ་བ་ དང་ །  མནར་མེད་པ་ དང་ །  ཆུ་བུར་ ཅན་ དང་ །  ཆུ་བུར་ "
"རྡོལ་བ་ དང་ །  སོ་ ཐམས་ ཐམས་ དང་ །  ཀྱི་ཧུད་ ཟེར་ དང་ །  ཨ་ཆུ་ ཟེར་བ་ དང་ ། "
" ཨུད་ པལ་ ལྟར་ གས་པ་ དང་ །  པད་མ་ ལྟར་ གས་པ་ དང་ །  པད་མ་ ལྟར་ གས་པ་ "
"ཆེན་པོ␣ར་ སོང་ ནས །"
msgstr ""

msgctxt "a6e213ffecc8431db79af656a0adfb4d"
msgid "ཚ་བ␣འི་ སེམས་ཅན་ དམྱལ་བ་ གང་ཡིན་པ་ དེ་དག་ ཏུ་ ནི་ བསིལ་བ␣ར་ གྱུར་ ཏེ་"
msgstr ""

msgctxt "ae1e3895c8ef49f1bf3089bf53060a2a"
msgid "འབབ་བོ ། །"
msgstr ""

msgctxt "2c9646e1f2f04aa4802ffe75252d14a3"
msgid "གྲང་བ␣འི་ སེམས་ཅན་ དམྱལ་བ་ གང་ཡིན་པ་ དེ་དག་ ཏུ་ ནི་ དྲོ་བ␣ར་ གྱུར་ ཏེ་"
msgstr ""

msgctxt "aeb2cffeecaf4f4b96831b01a06af6b6"
msgid "འབབ་བོ ། །"
msgstr ""

# དེས་ན་<«གཡུང་»དེ་ནས།
# «ལི་»«པེ་»«ཅོ་»དེ་ན།>སེམས་ཅན་དེ་དག་གི་གནོད་པའི་ཁྱད་པར་རྣམས་[56b]རྩེ་ཞིལ་བར་གྱུར་ཏོ<«གཡུང་»«ལི་»«པེ་»«སྣར་»«ཅོ་»«ཞོལ་»འགྱུར་རོ།>།
# །
msgctxt "42f9427949a1401795d15a42e59526e3"
msgid ""
"དེས་ན་ སེམས་ཅན་ དེ་དག་ གི་ གནོད་པ␣འི་ ཁྱད་པར་ རྣམས་ རྩེ་ ཞིལ་ བར་ གྱུར་ ཏོ "
"། །"
msgstr ""

msgctxt "48cbf9e55a22464fb717e59ea7bf0b75"
msgid "དེ་ནས་ དེ་དག་ འདི་ སྙམ་ དུ་ སེམས་ ཏེ །"
msgstr ""

msgctxt "8dc54cfb0d6d41dd98ed9f77785bf6e8"
msgid "ཀྱེ་"
msgstr ""

msgctxt "a7f73ccc6b8345a7bea1db4248a668cc"
msgid "ཅི་ བདག་ཅག་ འདི་ ནས་ འཆི་ འཕོས་ སམ་"
msgstr ""

msgctxt "3779f730ba1745bfabe7cd65393bc923"
msgid "འོན་ཏེ་ གཞན་ དུ་ སྐྱེས་ སམ་ སྙམ་ མོ ། །"
msgstr ""

msgctxt "d127e85479674e7aa997013d7f259870"
msgid ""
"དེ་ནས་ དེ་དག་ དད་པ་ བསྐྱེད་པ␣འི་ ཕྱིར་ བཅོམ་ལྡན་འདས་ སྤྲུལ་པ་ འགྱེད་པ␣ར་ "
"མཛད་ དེ །"
msgstr ""

msgctxt "bf89c3f245b842ab93e9efd032db1930"
msgid "དེ་དག་ གིས་ སྤྲུལ་པ་ མཐོང་ ནས་"
msgstr ""

msgctxt "3f2b5965b4404f51b27acadf862efab9"
msgid "འདི་ སྙམ་ དུ་ སེམས་ ཏེ །"
msgstr ""

msgctxt "12f9448596d24ca9948f5f3ffd901eac"
msgid "ཀྱེ་"
msgstr ""

msgctxt "cc4d413310dd44fa9341a78daf9bdd93"
msgid "བདག་ཅག་ ནི་ འདི་ ནས་ ཀྱང་ ཤི་འཕོས་ པར་མ་ གྱུར་ ལ་"
msgstr ""

msgctxt "5ac23becadf64357af22ea980761ddf7"
msgid "གཞན་ དུ་ ཡང་ མ་ སྐྱེས་ མོད་ ཀྱི །"
msgstr ""

# འོན་ཀྱང་སྔོན་མ་བྱུང་བའི་སེམས་ཅན་སྣང་བ་འདིའི་མཐུས་བདག་ཅག་<«གཡུང་»ཅིག>གི་གནོད་པའི་ཁྱད་པར་རྣམས་རྩེ་ཞིལ་བར་གྱུར་ཏོ་སྙམ་མོ།
# །
msgctxt "2271db11a6bc43da884eefdc41ddcd27"
msgid ""
"འོན་ཀྱང་ སྔོན་མ་ བྱུང་བ␣འི་ སེམས་ཅན་ སྣང་བ་ འདི␣འི་ མཐུ␣ས་ བདག་ཅག་ གི་ "
"གནོད་པ␣འི་ ཁྱད་པར་ རྣམས་ རྩེ་ ཞིལ་ བར་ གྱུར་ ཏོ་ སྙམ་ མོ ། །"
msgstr ""

msgctxt "d0654a5bc30c4517bf86730f338a37a4"
msgid "དེ་ནས་ དེ་དག་ སྤྲུལ་པ་ ལ་ སེམས་ མངོན་པར་ དགའ་བ␣ར་ གྱུར་ ནས །"
msgstr ""

msgctxt "4fbbb9dcd57a479fb003ba07b61576c3"
msgid "སེམས་ཅན་ དམྱལ་བ་ མྱོང་བ␣ར་ འགྱུར་བ␣འི་ ལ␣ས་ དེ་ ཟད་ དེ །"
msgstr ""

# ལྷ་དང་མི་རྣམས་ཀྱི་ནང་དུ་གང་དུ་<«གཡུང་»«ལི་»«པེ་»«སྣར་»«ཅོ་»«ཞོལ་»–གང་དུ།>བདེན་པ་རྣམས་ཀྱི་སྣོད་དུ་རུང་བར་འགྱུར་བ་དེར་ཉིང་མཚམས་འཛིན་པར་འགྱུར་རོ།
# །
msgctxt "79704663ffbd487d8e8d93608539172a"
msgid ""
"ལྷ་ དང་ མི་ རྣམས་ ཀྱི་ ནང་ དུ་ གང་ དུ་ བདེན་པ་ རྣམས་ ཀྱི་ སྣོད་ དུ་ རུང་བ␣ར་"
" འགྱུར་བ་ དེ␣ར་ ཉིང་མཚམས་ འཛིན་པ␣ར་ འགྱུར་ རོ ། །"
msgstr ""

# སྟེང་དུ་འགྲོ་བ་གང་ཡིན་པ་དེ་དག་ནི་རྒྱལ་ཆེན་བཞིའི་རིས་རྣམས་དང་།
# སུམ་ཅུ་<«གཡུང་»«ལི་»«པེ་»བཅུ།>རྩ་གསུམ་པ་རྣམས་དང་། འཐབ་བྲལ་རྣམས་དང་།
# དགའ་ལྡན་རྣམས་དང་། འཕྲུལ་དགའ་<«གཡུང་»དགའ་ལྡན།>རྣམས་དང་།
# གཞན་འཕྲུལ་དབང་བྱེད་རྣམས་དང་། ཚངས་རིས་རྣམས་དང་།
# ཚངས་པའི་མདུན་<«གཡུང་»འདུན།>ན་འདོན་པ་རྣམས་དང་། ཚངས་པ་ཆེན་པོ་རྣམས་དང་།
# འོད་ཆུང་རྣམས་དང་། ཚད་མེད་འོད་རྣམས་དང་། འོད་གསལ་རྣམས་དང་། དགེ་ཆུང་རྣམས་དང་།
# ཚད་མེད་དགེ་རྣམས་དང་། དགེ་རྒྱས་རྣམས་དང་། སྤྲིན་མེད་རྣམས་དང་།
# བསོད་ནམས་སྐྱེས་རྣམས་དང་། འབྲས་བུ་ཆེ་རྣམས་དང་།
# མི་ཆེ་<«གཡུང་»«པེ་»+བ>རྣམས་དང་། མི་གདུང་<«པེ»+བ།>རྣམས་དང་།
# གྱ་ནོམ་སྣང་རྣམས་དང་། ཤིན་ཏུ་མཐོང་རྣམས་དང་།
# འོག་མིན་གྱི་ལྷ་རྣམས་ཀྱི་བར་དུ་སོང་ནས།
msgctxt "89ca56cdd2ef4d4fab4e04603209b7c9"
msgid ""
"སྟེང་ དུ་ འགྲོ་བ་ གང་ཡིན་པ་ དེ་དག་ ནི་ རྒྱལ་ཆེན་ བཞི␣འི་ རིས་ རྣམས་ དང་ །  "
"སུམ་ཅུ་རྩ་གསུམ་པ་ རྣམས་ དང་ །  འཐབ་བྲལ་ རྣམས་ དང་ །  དགའ་ལྡན་ རྣམས་ དང་ །  "
"འཕྲུལ་དགའ་ རྣམས་ དང་ །  གཞན་འཕྲུལ་ དབང་ བྱེད་ རྣམས་ དང་ །  ཚངས་རིས་ རྣམས་ "
"དང་ །  ཚངས་པ␣འི་ མདུན་ན་འདོན་པ་ རྣམས་ དང་ །  ཚངས་པ་ ཆེན་པོ་ རྣམས་ དང་ །  "
"འོད་ཆུང་ རྣམས་ དང་ །  ཚད་ མེད་ འོད་ རྣམས་ དང་ །  འོད་གསལ་ རྣམས་ དང་ །  "
"དགེ་ཆུང་ རྣམས་ དང་ །  ཚད་ མེད་ དགེ་ རྣམས་ དང་ །  དགེ་རྒྱས་ རྣམས་ དང་ །  "
"སྤྲིན་ མེད་ རྣམས་ དང་ །  བསོད་ནམས་ སྐྱེས་ རྣམས་ དང་ །  འབྲས་བུ་ ཆེ་ རྣམས་ "
"དང་ །  མི་ ཆེ་ རྣམས་ དང་ །  མི་ གདུང་ རྣམས་ དང་ །  གྱ་ནོམ་ སྣང་ རྣམས་ དང་ ། "
" ཤིན་ཏུ་ མཐོང་ རྣམས་ དང་ །  འོག་མིན་ གྱི་ ལྷ་ རྣམས་ ཀྱི་ བར་ དུ་ སོང་ ནས །"
msgstr ""

# མི་རྟག་པ་དང་། སྡུག་བསྔལ་བ་དང་། སྟོང་པ་དང་།
# བདག་མེད་པའི་སྒྲ་སྒྲོགས་སོ<«ཞོལ་»སྒྲོག་གོ།>། །
msgctxt "a5214d15757f4023bd888048db6f9c20"
msgid ""
"མི་རྟག་པ་ དང་ །  སྡུག་བསྔལ་བ་ དང་ །  སྟོང་པ་ དང་ །  བདག་མེད་པ␣འི་ སྒྲ་ "
"སྒྲོགས་ སོ ། །"
msgstr ""

# ཚིགས་སུ་བཅད་པ་གཉིས་ཀྱང་རྗོད་<«སྣར་»བརྗོད།>པར་བྱེད་དེ།
msgctxt "4c388f7e6beb48339bd0b74f09a794dc"
msgid "ཚིགས་ སུ་ བཅད་པ་ གཉིས་ ཀྱང་ རྗོད་པ␣ར་ བྱེད་ དེ །"
