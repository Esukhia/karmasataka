msgctxt "41bc3e3368c94aeb851c15868986c588"
msgid ""
"དེ་ གཉིས་ ཀྱིས་ སྟན་ དེ་ ལ་ འདུག་ བཞིན་ དུ་ རྒྱུན་ དུ་ ཞུགས་པ␣འི་ འབྲས་བུ་ "
"མངོན་སུམ་ དུ་ བྱས་ ཏེ །"
msgstr ""

msgctxt "beddc4a7c47943e0920e411d9e17581c"
msgid "དེ་ གཉིས་ ཀྱིས་ བདེན་པ་ མཐོང་ ནས་"
msgstr ""

msgctxt "9cecd1741c144e7399979973eab2ea18"
msgid "སྨྲས་པ །"
msgstr ""

msgctxt "f23edab707bf4a04a6eec08b72f3c09e"
msgid "བཙུན་པ་"
msgstr ""

# བདག་ཅག་གཉིས་<«གཡུང་»«ལི་»«པེ་»«སྣར་»«ཅོ་»«ཞོལ་»གིས།>ལེགས་པར་གསུངས་པའི་ཆོས་འདུལ་བ་ལ་རབ་ཏུ་འབྱུང་<«གཡུང་»«པེ་»«སྣར་»«ཞོལ་»བྱུང་།>བ་དང་བསྙེན་པར་རྫོགས་ཤིང་དགེ་སློང་མའི་དངོས་པོ་འཐོབ་ཏུ་རུང་ན།
# བདག་ཅག་གཉིས་ཀྱང་བཅོམ་ལྡན་འདས་ཀྱི་ཐད་དུ་ཚངས་པར་སྤྱོད་པ་སྤྱད་པར་འཚལ་ལོ། །
msgctxt "ca90f380a65443eb946fe894b9c7fe88"
msgid ""
"བདག་ཅག་ གཉིས་ ལེགས་པ␣ར་ གསུངས་པ␣འི་ ཆོས་ འདུལ་བ་ ལ་ རབ་ཏུ་ འབྱུང་བ་ དང་ "
"བསྙེན་པ␣ར་ རྫོགས་ ཤིང་ དགེ་སློང་ མ␣འི་ དངོས་པོ་ འཐོབ་ ཏུ་ རུང་ ན །  བདག་ཅག་ "
"གཉིས་ ཀྱང་ བཅོམ་ལྡན་འདས་ ཀྱི་ ཐད་ དུ་ ཚངས་པ␣ར་ སྤྱོད་པ་ སྤྱད་པ␣ར་ འཚལ་ ལོ "
"། །"
msgstr ""

# དེ་ནས་ཚེ་དང་ལྡན་པ་པད་མའི་མདོག་གིས་དེ་[71b]གཉིས་དགེ་སློང་མའི་གཙུག་<«གཡུང་»«པེ་»ཙུག>ལག་ཁང་དུ་ཁྲིད་དེ།
msgctxt "6d65f256a7944ffba81f1f106ba32e4e"
msgid ""
"དེ་ནས་ ཚེ་ དང་ ལྡན་པ་ པད་མ␣འི་ མདོག་ གིས་ དེ་ གཉིས་ དགེ་སློང་ མ␣འི་ "
"གཙུག་ལག་ཁང་ དུ་ ཁྲིད་ དེ །"
msgstr ""

# དགེ་སློང་མ་རྣམས་ལ་གཏད་<«གཡུང་»«པེ་»བཏད།>ནས་
msgctxt "d6dd66da3c994068ae9afca54088622a"
msgid "དགེ་སློང་ མ་ རྣམས་ ལ་ གཏད་ ནས་"
msgstr ""

# དགེ་སློང་མ་རྣམས་ཀྱིས་དེ་གཉིས་རབ་ཏུ་ཕྱུང་<«ལི་»«སྣར་»«ཅོ་»བྱུང་།>སྟེ།
msgctxt "f173b034ec7c4bada285aa020a0afd11"
msgid "དགེ་སློང་ མ་ རྣམས་ ཀྱིས་ དེ་ གཉིས་ རབ་ཏུ་ ཕྱུང་ སྟེ །"
