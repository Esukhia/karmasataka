msgid "ཇི་སྙམ་ དུ་ སེམས །"
msgstr ""

# དེའི་ཚེ་དགེ་སློང་དུ་གྱུར་པ་གང་ཡིན་པ་དེ་ནི་པད་མའི་མདོག་འདི་ཉིད་<«གཡུང་»«ལི་»«པེ་»«སྣར་»«ཅོ་»«ཞོལ་»–ཉིད།>ཡིན་ནོ།
# །
msgctxt "2118fddf0cbf40d6957d79954a0f4b13"
msgid ""
"དེ␣འི་ ཚེ་ དགེ་སློང་ དུ་ གྱུར་པ་ གང་ཡིན་པ་ དེ་ ནི་ པད་མ␣འི་ མདོག་ འདི་ཉིད་ "
"ཡིན་ ནོ ། །"
msgstr ""

msgctxt "0b25b130e08443a5ae799bdaf52a3c5b"
msgid ""
"དེ␣འི་ ཚེ་ དགེ་སློང་ མ་ གཉིས་ སུ་ གྱུར་པ་ གང་ཡིན་པ་ དེ་ གཉིས་ ནི་ དགེ་སློང་ "
"མ་ འདི་ གཉིས་ ཁོ་ན་ ཡིན་ ནོ ། །"
msgstr ""

# དགེ་སློང་དེས་དེར་སངས་རྒྱས་ལ་སོགས་པ་དགེ་སློང་གི་དགེ་བདུན་<«གཡུང་»«ལི་»«པེ་»«སྣར་»«ཁུ་»འདུན།
# «ཅོ་»འདུན་ལ།>བཤོས་གསོལ་ཏེ།
msgctxt "82e6568b118b44c4a27dbabd19ba56a4"
msgid ""
"དགེ་སློང་ དེ␣ས་ དེ␣ར་ སངས་རྒྱས་ ལ་སོགས་པ་ དགེ་སློང་ གི་ དགེ་ བདུན་ བཤོས་ "
"གསོལ་ ཏེ །"
msgstr ""

# དབུ་སྐྲ་<«གཡུང་»དཀྲ།>དང་སེན་མོའི་མཆོད་རྟེན་རྣམས་ལ་བཀུར་སྟི་བྱས་ནས།
msgctxt "ccf17f4902624b47ae535393641ae991"
msgid "དབུ་སྐྲ་ དང་ སེན་མོ␣འི་ མཆོད་རྟེན་ རྣམས་ ལ་ བཀུར་སྟི་ བྱས་ ནས །"
msgstr ""

# སྨོན་ལམ་བཏབ་པའི་ལས་དེའི་རྣམ་པར་སྨིན་<«ལི་»སྦྱིན།>པས་ཕྱུག་ཅིང་ནོར་མང་ལ་ལོངས་སྤྱོད་ཆེ་བའི་རིགས་སུ་སྐྱེས་ཏེ།
msgctxt "4c3965e3817149d8afa967f78347900a"
msgid ""
"སྨོན་ལམ་ བཏབ་པ␣འི་ ལ␣ས་ དེ␣འི་ རྣམ་པ␣ར་ སྨིན་པ␣ས་ ཕྱུག་ ཅིང་ ནོར་ མང་ ལ་ "
"ལོངས་སྤྱོད་ ཆེ་བ␣འི་ རིགས་ སུ་ སྐྱེས་ ཏེ །"
msgstr ""

# དགེ་སློང་དག་ང་ཡང་དག་པར་རྫོགས་པའི་སངས་རྒྱས་འོད་སྲུང་དང་ཤིན་ཏུ་མཉམ་པ་དང་ཤུགས་མཉམ་པ་དང་མཛད་པ་མཉམ་པ་དང་ཐབས་མཉམ་པ་བརྙེས་<«གཡུང་»«ལི་»«པེ་»«སྣར་»«ཅོ་»«ཞོལ་»རྙེད།>པར་གྱུར་ནས།
msgctxt "ddc2ea23dadd446c9da167c35ddaa48b"
msgid ""
"དགེ་སློང་ དག་ ང་ ཡང་དག་པ␣ར་ རྫོགས་པ␣འི་ སངས་རྒྱས་ འོད་སྲུང་ དང་ ཤིན་ཏུ་ "
"མཉམ་པ་ དང་ ཤུགས་ མཉམ་པ་ དང་ མཛད་པ་ མཉམ་པ་ དང་ ཐབས་ མཉམ་པ་ བརྙེས་པ␣ར་ གྱུར་ "
"ནས །"
msgstr ""

# འདིས་<«ཅོ་»ལྡིས།>ང་མཉེས་པར་བྱས་ཏེ་
msgctxt "ae14569a40cd4d94a432bce8b634c41d"
msgid "འདི␣ས་ ང་ མཉེས་པ␣ར་ བྱས་ ཏེ་"
msgstr ""

msgctxt "957f76bfac8448d19a7582d494e677b4"
msgid "མི་ མཉེས་པ␣ར་ མ་ བྱས་ ལ །"
msgstr ""

# ང་<«པེ་»དེ།>ཉིད་ཀྱི་བསྟན་པ་ལ་རབ་ཏུ་བྱུང་ནས་
msgctxt "a1539782d8884981a11811988a1838f6"
msgid "ང་ཉིད་ ཀྱི་ བསྟན་པ་ ལ་ རབ་ཏུ་ བྱུང་ ནས་"
msgstr ""

msgctxt "6af200328a0d4555985e79212cfd6a4b"
msgid "ཉོན་མོངས་པ་ ཐམས་ཅད་ སྤངས་ ཏེ་"
msgstr ""

msgctxt "c866f7a5076345e0b5203c5023b6237d"
msgid "དགྲ་བཅོམ་པ་ ཉིད་ མངོན་སུམ་ དུ་ བྱས་ སོ ། །"
msgstr ""

msgctxt "cf82e82d2d2c4e74957f262b9f983d87"
msgid "དགེ་སློང་ མ་ དེ་ གཉིས་ ཀྱིས་ དེ␣ར་ སྨོན་ལམ་ བཏབ་ སྟེ །"
msgstr ""

msgctxt "e6115baf170c43c0ac016c151c992cf2"
msgid "བདག་ཅག་ གཉིས་ ཀྱིས་ འདི་ཉིད་ ལ་ བརྟེན་ ཏེ་"
msgstr ""

msgctxt "da646e8eec3c47ff9df1047c86a5eff7"
msgid "སངས་རྒྱས་ བཅོམ་ལྡན་འདས་ དེ་ མཉེས་པ␣ར་ བྱེད་པ␣ར་ གྱུར་ཅིག  །"
msgstr ""

msgctxt "8dbaf4cc567546f7986b528adde8497d"
msgid "མི་ མཉེས་པ␣ར་ བྱེད་པ␣ར་ མ་ གྱུར་ཅིག  །"
msgstr ""

msgctxt "2c7c95d4141643c59638ef33870fc033"
msgid "དེ་ ཁོ་ན␣འི་ བསྟན་པ་ ལ་ རབ་ཏུ་ བྱུང་ ནས་"
msgstr ""

msgctxt "0c370fc49f90457d86562d3c58da3a03"
msgid "ཉོན་མོངས་པ་ ཐམས་ཅད་ སྤངས་ ཏེ་"
msgstr ""

msgctxt "4c89662ef8b643f8840a94f0323b7469"
msgid "དགྲ་བཅོམ་པ་ ཉིད་ མངོན་སུམ་ དུ་ བྱེད་པ␣ར་ གྱུར་ཅིག་ ཅེས་"
msgstr ""

# བྱས་པས་ནི། འདི་གཉིས་ཀྱིས་འདི་ལས་<«ཞོལ་»ལ།>བརྟེན་ནས་
msgctxt "3185814e864b49c0b11bf41fed889548"
msgid "བྱས་པ␣ས་ ནི །  འདི་ གཉིས་ ཀྱིས་ འདི་ ལ␣ས་ བརྟེན་ ནས་"
msgstr ""

# ད་<«ཞོལ་»དེ།>ལྟར་ང་མཉེས་པར་བྱས་ཏེ་
msgctxt "1d4fa9b116974c79b4c19ef9ba5e1322"
msgid "ད་ལྟར་ ང་ མཉེས་པ␣ར་ བྱས་ ཏེ་"
msgstr ""

msgctxt "90654acd5f144a619c667791db11202d"
msgid "མི་ མཉེས་པ␣ར་ མ་ བྱས་ ལ །"
msgstr ""

msgctxt "dc0de3ad56d44d27a38ab45c5693ca00"
msgid "ང་ཉིད་ ཀྱི་ བསྟན་པ་ ལ་ རབ་ཏུ་ བྱུང་ ནས་"
msgstr ""

msgctxt "13bc3eaa91f8462cb35dc883037f505e"
msgid "ཉོན་མོངས་པ་ ཐམས་ཅད་ སྤངས་ ཏེ་"
msgstr ""

msgctxt "6d21659dbddb4a7cb6c3de8fa294b1d6"
msgid "དགྲ་བཅོམ་པ་ ཉིད་ མངོན་སུམ་ དུ་ བྱས་ སོ ། །"
msgstr ""

# དེ་གཉིས་ཀྱིས་དེར་དགེ་སློང་མ་མང་པོ་<«ལི་»«སྣར་»«ཅོ་»མོ།>ཞིག་ལ་གཅིག་གིས་ནི་རིགས་ངན་མོ་ཞེས་བྱ་བའི་ཚིག་བརྗོད་ལ<«གཡུང་»«ལི་»«པེ་»«སྣར་»«ཅོ་»པ།>།
msgctxt "111a730ca9c44cf184c277d1f935f815"
msgid ""
"དེ་ གཉིས་ ཀྱིས་ དེ␣ར་ དགེ་སློང་ མ་ མང་པོ་ ཞིག་ ལ་ གཅིག་ གིས་ ནི་ རིགས་ངན་ "
"མོ་ ཞེས་ བྱ་བ␣འི་ ཚིག་ བརྗོད་ ལ །"
msgstr ""

msgctxt "c93ab9169f5944e3a0400ca74bdce463"
msgid ""
"གཅིག་ གིས་ ནི་ སྨད་འཚོང་མ་ ཞེས་ བྱ་བ␣འི་ ཚིག་ བརྗོད་པ␣འི་ ལ␣ས་ དེ␣འི་ "
"རྣམ་པ␣ར་ སྨིན་པ␣ས་ ནི །"
msgstr ""

# གཅིག་<«གཡུང་»«ལི་»«པེ་»«སྣར་»«ཅོ་»–གཅིག>རིགས་ངན་མོའི་རིགས་སུ་སྐྱེས་ལ་
msgctxt "719c351593d949fb8dca468b94eade48"
msgid "གཅིག་ རིགས་ངན་ མོ␣འི་ རིགས་ སུ་ སྐྱེས་ ལ་"
msgstr ""

msgctxt "4a73ed965527456ab670a16a7146d115"
msgid "གཅིག་ ནི་ སྨད་འཚོང་མ␣ར་ གྱུར་ ཏོ ། །"
msgstr ""

msgctxt "83378ddcfc3b4016bf0ca9de0a966b3e"
msgid "བཙུན་པ་ པད་མ␣འི་ མདོག་ གིས་ ལ␣ས་ ཅི་ཞིག་ བགྱིས་ ན་"
msgstr ""

# ལས་དེའི་རྣམ་པར་སྨིན་པས་ཕྱུག་<«གཡུང་»ཕྱུ།
# «ཅོ་»ཕྱག>ཅིང་ནོར་མང་ལ་ལོངས་སྤྱོད་ཆེ་བའི་རིགས་སུ་སྐྱེས་ལ།
msgctxt "8eab43dbd32c4162a8024b96ec1fc60b"
msgid ""
"ལ␣ས་ དེ␣འི་ རྣམ་པ␣ར་ སྨིན་པ␣ས་ ཕྱུག་ ཅིང་ ནོར་ མང་ ལ་ ལོངས་སྤྱོད་ ཆེ་བ␣འི་ "
"རིགས་ སུ་ སྐྱེས་ ལ །"
msgstr ""

# གཟུགས་བཟང་ཞིང་བལྟ་<«ལི་»«སྣར་»«ཅོ་»ལྟ།>ན་སྡུག་ལ་མཛེས་པར་གྱུར་ཏེ་
msgctxt "796c877bc7d24c51bfdc10a9624f839d"
msgid "གཟུགས་བཟང་ ཞིང་ བལྟ་ ན་ སྡུག་ ལ་ མཛེས་པ␣ར་ གྱུར་ ཏེ་"
msgstr ""

msgctxt "602db60d831945e4ac8d85312051e6f2"
msgid "པད་མ␣འི་ སྙིང་པོ␣འི་ མདོག་ འདྲ་བ␣ར་ གྱུར་ ལགས །"
msgstr ""

msgctxt "7b37a492196a435884839ff6a1ba135d"
msgid "བཅོམ་ལྡན་འདས་ ཀྱིས་ བཀའ་ སྩལ་པ །"
msgstr ""

msgctxt "231dac279e944435a7abd542f7696c18"
msgid "སྨོན་ལམ་ གྱི་ དབང་ ལ␣ས་ གྱུར་ ཏོ ། །"
msgstr ""

msgctxt "1c8d4e1d1ea7409f9f8c87a330618220"
msgid "བཙུན་པ་ འདི␣ས་ གང་ དུ་ སྨོན་ལམ་ བཏབ་ ལགས །"
msgstr ""

