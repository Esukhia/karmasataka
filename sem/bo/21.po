msgctxt "2e6fa72c564f418abc83e88d57f0a11d"
msgid "ཁྱིམ་བདག་ དེ་ཡང་ སྦྱིན་པ་ དག་ སྦྱིན་ ཞིང་"
msgstr ""

msgctxt "f301c3493cdd486185dbfefcf4e34d30"
