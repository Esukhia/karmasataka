msgctxt "d6c80ba9be43447c856ba2eddf1175a5"
msgid "བཅོམ་ལྡན་འདས་ འོད་སྲུང་ གི་ བསྟན་པ་ ལ་ རབ་ཏུ་ བྱུང་ ངོ་ ། །"
msgstr ""

msgctxt "08fe00b8eb724f8897098ff672548eb8"
msgid "དེ་ རབ་ཏུ་ བྱུང་ ནས་"
msgstr ""

msgctxt "f63f3aeba4164f119ace2d303d29e9b4"
msgid "སྡེ་སྣོད་ གསུམ་ བསླབས་ ཏེ །"
msgstr ""

# རིག་<«གཡུང་»«ལི་»«པེ་»«སྣར་»«ཅོ་»རིགས།>པ་དང་གྲོལ་བའི་སྤོབས་<«ཅོ་»སློབས།>པ་དང་ལྡན་པའི་ཆོས་སྨྲ་བ།
# གོས་དང་ཟས་དང་མལ་ཆ་དང་སྟན་དང་ནད་གསོས་དང་སྨན་ཟོང་རྣམས་རྙེད་<«ཅོ་»བརྙེད།>པར་གྱུར་ཏོ།
# །
