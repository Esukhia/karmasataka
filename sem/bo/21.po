msgctxt "bdc6fa62cdaf4177bd0cbcfbd827dc7b"
msgid ""
"ཁྱིམ་བདག་ དེ␣ས་ ཆུང་མ་ དེ་ གཉིས་ དགེ་སློང་ མ␣འི་ གཙུག་ལག་ཁང་ དུ་ ཁྲིད་ དེ་"
msgstr ""

# དགེ་སློང་མ་རྣམས་ལ་གཏད་<«ཁུ་»བཏད།>དོ། །
msgctxt "45be3430d3ba4683b28ee6b355ace6d5"
msgid "དགེ་སློང་ མ་ རྣམས་ ལ་ གཏད་ དོ ། །"
msgstr ""

msgctxt "98b616aa825240a593e222b82faba5e7"
msgid "དགེ་སློང་ མ་ རྣམས་ ཀྱིས་ ཀྱང་ དེ་ གཉིས་ རབ་ཏུ་ ཕྱུང་ སྟེ་"
