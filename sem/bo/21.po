msgctxt "a391e433041449809d9041397b79efa9"
msgid "ནམ་མཁའ་ དང་ ལག་མཐིལ་ དུ་ འདྲ་བ␣འི་ སེམས་ དང་ ལྡན་པ །"
msgstr ""

# ཙན་དན་སྦངས་པ་ལྟ་བུར་བསིལ་<«གཡུང་»གསིལ།>བར་གྱུར་པ།
msgctxt "155132d2aad544fc92a0bc7f564e291d"
msgid "ཙན་དན་ སྦངས་པ་ ལྟ་བུ␣ར་ བསིལ་བ␣ར་ གྱུར་པ །"
msgstr ""

msgctxt "9b0df638ea7746d1827d175c86d2d6e1"
msgid "རིག་པ␣ས་ སྒོ་ང␣འི་ སྦུབས་ བཅོམ་པ །"
msgstr ""

msgctxt "475c63dbdf17478391866efc7e6b738b"
msgid "རིག་པ་ དང་ མངོན་པར་ ཤེས་པ་ དང་ སོ་སོ་ ཡང་དག་པ␣ར་ རིག་པ་ ཐོབ་པ །"
msgstr ""

# སྲིད་པའི་རྙེད་པ་དང་ཆགས་པ་དང་བཀུར་<«གཡུང་»«པེ་»སྐུར།>སྟི་ལ་རྒྱབ་ཀྱིས་ཕྱོགས་པ་
msgctxt "52d7efbd59954e0492b17d91f8d27c16"
msgid "སྲིད་པ␣འི་ རྙེད་པ་ དང་ ཆགས་པ་ དང་ བཀུར་སྟི་ ལ་ རྒྱབ་ ཀྱིས་ ཕྱོགས་པ་"
msgstr ""

# དབང་པོ་དང་ཉེ་དབང་དང་བཅས་པའི་ལྷ་རྣམས་ཀྱིས་མཆོད་ཅིང་།
# རྗེད་<«གཡུང་»«པེ་»«སྣར་»བརྗེད།>པ་དང་གུས་པར་སྨྲ་བའི་གནས་སུ་གྱུར་ཏོ། །
msgctxt "b135c52e32bd48a3b2b7cfc3a32715de"
msgid ""
"དབང་པོ་ དང་ ཉེ་དབང་ དང་ བཅས་པ␣འི་ ལྷ་ རྣམས་ ཀྱིས་ མཆོད་ ཅིང་ །  རྗེད་པ་ དང་ "
"གུས་པ␣ར་ སྨྲ་བ␣འི་ གནས་ སུ་ གྱུར་ ཏོ ། །"
msgstr ""

msgctxt "de8493d7a0e94598acdf285fe2bf202e"
msgid "དེ་ནས་ ཚེ་ དང་ ལྡན་པ་ པད་མ␣འི་ མདོག་ གིས་ བསམས་པ །"
msgstr ""

# འདི་ལྟར་བདག་གིས་གཞན་གྱི་རྒྱུད་ནི་<«གཡུང་»«ལི་»«པེ་»«སྣར་»«ཅོ་»–ནི།>དག་པར་བྱེད་ནུས་ན་
msgctxt "6008dadab8034bd29ea3bda67a9bf2c9"
msgid "འདི་ལྟར་ བདག་ གིས་ གཞན་ གྱི་ རྒྱུད་ ནི་ དག་པ␣ར་ བྱེད་ ནུས་ ན་"
msgstr ""

# བདག་གི་རྒྱུད་དག་པར་བྱེད་<«གཡུང་»–ནུས་ན། བདག་གི་རྒྱུད་དག་པར་བྱེད།
# «ལི་»«པེ་»«སྣར་»«ཅོ་»ནུས་བདག་གི «ཞོལ་»ནུས་ན་བདག་གི་རྒྱུད་ནི།>མ་ནུས་སོ། །
msgctxt "dfb58d39a8ac4769bb7e1a46e2e7915f"
msgid "བདག་ གི་ རྒྱུད་ དག་པ␣ར་ བྱེད་ མ་ནུ␣ས་ སོ ། །"
msgstr ""

msgctxt "6fd6534daa8945a6a14638a069b5ad8b"
msgid ""
"བཅོམ་ལྡན་འདས་ ཀྱིས་ ཀྱང་ མང་ དུ་ ཐོས་པ་ ལ་ ཕན་ཡོན་ རྣམ་པ་ ལྔ་ ཡོད་ དེ །"
msgstr ""

# ཕུང་པོ་ལ་མཁས་པ་དང་ཁམས་ལ་མཁས་པ་དང་སྐྱེ་མཆེད་ལ་མཁས་པ་དང་རྟེན་ཅིང་འབྲེལ་པར་འབྱུང་བ་ལ་མཁས་པ་དང་།
# གནས་དང་གནས་མ་ཡིན་པ་ལ་མཁས་པར་འགྱུར་རོ་ཞེས་གསུངས་ཀྱིས<«གཡུང་»«ཞོལ་»ཀྱི།>།
msgctxt "f30444ab046846a19298f574e2a16c9b"
msgid ""
"ཕུང་པོ་ ལ་ མཁས་པ་ དང་ ཁམས་ ལ་ མཁས་པ་ དང་ སྐྱེ་མཆེད་ ལ་ མཁས་པ་ དང་ རྟེན་ ཅིང་"
" འབྲེལ་པ␣ར་ འབྱུང་བ་ ལ་ མཁས་པ་ དང་ །  གནས་ དང་ གནས་མ་ ཡིན་པ་ ལ་ མཁས་པ␣ར་ "
"འགྱུར་ རོ་ ཞེས་ གསུངས་ ཀྱིས །"
msgstr ""

msgctxt "88cd0c2766814180b653fce905313629"
msgid ""
"བདག་ གིས་ ཀྱང་ ཉོན་མོངས་པ་ ཟད་པར་ བྱ་བ␣འི་ ཕྱིར་ བརྩམ་པ␣ར་ བྱ␣འོ་ སྙམ་ ནས །"
msgstr ""

# དེས་<«གཡུང་»«པེ་»དེ་ནས།>བརྩོན་པ་དང་བསྒྲུབ་<«ཞོལ་»སྒྲུབ།>པ་དང་འབད་པས་ཉོན་མོངས་པ་ཐམས་ཅད་སྤངས་ནས་
msgctxt "bd5944246eb44e908e4bd3d0edd47162"
msgid ""
"དེ␣ས་ བརྩོན་པ་ དང་ བསྒྲུབ་པ་ དང་ འབད་པ␣ས་ ཉོན་མོངས་པ་ ཐམས་ཅད་ སྤངས་ ནས་"
msgstr ""

msgctxt "ad24a4ea7ec24c0c900d512b6f53fc50"
msgid "དགྲ་བཅོམ་པ་ ཉིད་ མངོན་སུམ་ དུ་ བྱས་ ཏེ །"
msgstr ""

msgctxt "dfaf66bbe3d446ef8100ca41b37d4ddc"
msgid "དེ་ དགྲ་བཅོམ་པ་ ཁམས་ གསུམ་པ␣འི་ འདོད་ཆགས་ དང་ བྲལ་བ␣ར་ གྱུར་ ནས་"
msgstr ""

# གསེར་དང་བོང་བར་མཉམ་<«ཞོལ་»མཉན།>པ།
msgctxt "9723b95c038f4aeca349ef343287c468"
msgid "གསེར་ དང་ བོང་བ␣ར་ མཉམ་པ །"
msgstr ""

msgctxt "d9f382c92a414299b1cc04e7a1a84fd0"
msgid "ནམ་མཁའ་ དང་ ལག་མཐིལ་ དུ་ འདྲ་བ␣འི་ སེམས་ དང་ ལྡན་པ །"
msgstr ""

msgctxt "3b05ac7b19bc4cce9bfa86a5aa98b749"
msgid "ཙན་དན་ སྦངས་པ་ ལྟ་བུ␣ར་ བསིལ་བ␣ར་ གྱུར་པ །"
msgstr ""

# རིག་པས་སྒོ་ངའི་<«ཅོ་»བའི།>སྦུབས་བཅོམ་པ།
msgctxt "51a85bdf5cd94cfeac5f8dc1dd7c62e8"
msgid "རིག་པ␣ས་ སྒོ་ང␣འི་ སྦུབས་ བཅོམ་པ །"
msgstr ""

# རིག་པ་<«ཞོལ་»རིགས།>དང་མངོན་པར་ཤེས་པ་དང་སོ་སོ་ཡང་དག་པར་རིག་པ་ཐོབ་པ།
msgctxt "57005e43dd054d09a3b46ad0c9fbf0af"
msgid "རིག་པ་ དང་ མངོན་པར་ ཤེས་པ་ དང་ སོ་སོ་ ཡང་དག་པ␣ར་ རིག་པ་ ཐོབ་པ །"
msgstr ""

msgctxt "cefcffcd026e4aeabf55aa51751bfd55"
msgid "སྲིད་པ␣འི་ རྙེད་པ་ དང་ ཆགས་པ་ དང་ བཀུར་སྟི་ ལ་ རྒྱབ་ ཀྱིས་ ཕྱོགས་པ །"
msgstr ""

# དབང་པོ་དང་ཉེ་དབང་དང་<«ལི་»«སྣར་»«ཅོ་»«ཞོལ་»–དང།>[72a]བཅས་པའི་ལྷ་རྣམས་ཀྱིས་མཆོད་ཅིང་རྗེད་<«གཡུང་»«པེ་»«སྣར་»བརྗེད།>པ་དང་གུས་པར་སྨྲ་བའི་གནས་སུ་གྱུར་ཏོ།
# །
msgctxt "f79efdf078ae4742b29fb6e5817c57e6"
msgid ""
"དབང་པོ་ དང་ ཉེ་དབང་ དང་ བཅས་པ␣འི་ ལྷ་ རྣམས་ ཀྱིས་ མཆོད་ ཅིང་ རྗེད་པ་ དང་ "
"གུས་པ␣ར་ སྨྲ་བ␣འི་ གནས་ སུ་ གྱུར་ ཏོ ། །"
msgstr ""

msgctxt "c9bb80b8e7634878bb0bf74f20a4dd26"
msgid "དེ␣ས་ དགྲ་བཅོམ་པ་ ཉིད་ ཐོབ་ ནས་"
msgstr ""

msgctxt "884750d7a9db48228dbf48d7b6078dea"
msgid "ཡུལ་ བཅོམ་བརླག་ ཏུ་ ཇི་སྲིད་ དགའ་བ␣འི་ བར་ དུ་ འདུག་ འདུག་ ནས་"
msgstr ""

msgctxt "f41728c200d84d4eb5b6cf947402d0c5"
msgid "མཉན་ དུ་ཡོད་པ␣ར་ སོང་ ངོ་ ། །"
msgstr ""

# དེ་<«གཡུང་»«ལི་»«པེ་»«སྣར་»«ཅོ་»–དེ།>དེར་སོང་ནས་
msgctxt "d20e6138b6f248b6851103cd1a6603c3"
msgid "དེ་དེ␣ར་ སོང་ ནས་"
msgstr ""

# གཏམ་དེ་རྒྱས་པར་དགེ་སློང་རྣམས་ལ་བསྙད་<«གཡུང་»བརྙད།>དོ། །
msgctxt "8cfdc4a0150947bd8fdfe048020f8d83"
msgid "གཏམ་ དེ་ རྒྱས་པ␣ར་ དགེ་སློང་ རྣམས་ ལ་ བསྙད་ དོ ། །"
msgstr ""

msgctxt "a4d7254bffec4e42b29bdb04ad4f0a42"
msgid "དེ་ནས་ དགེ་སློང་ རྣམས་ ཀྱིས་ བཅོམ་ལྡན་འདས་ ལ་ ཞུས་པ །"
msgstr ""

msgctxt "ecf2ec10a46041c9bb7ef1b220a1542c"
msgid "བཙུན་པ་"
msgstr ""

msgctxt "c1ed73d2413444ad8cb43a1d2fe20d35"
msgid ""
"བུ་མོ་ སྨད་འཚོང་མ་ འདོད་པ␣འི་ འདོད་ཆགས་ ཀྱི་ དབང་ དུ་ གྱུར་པ␣ས །  ཚེ་ དང་ "
"ལྡན་པ་ པད་མ␣འི་ མདོག་ ཇི་ལྟར་ མི་ དགའ་བ␣ར་ བགྱིས་པ་ དང་ སླར་ དེ་ཉིད་ ལ་ "
"བརྟེན་ ནས་ རབ་ཏུ་ བྱུང་ སྟེ །"
msgstr ""

msgctxt "426e2354ed5f4aacbe0358d9b71ddd69"
msgid "ཉོན་མོངས་པ་ ཐམས་ཅད་ སྤངས་ ནས་"
msgstr ""

msgctxt "dd7f1595535e49468a3f689a234edaa2"
msgid "དགྲ་བཅོམ་པ་ ཉིད་ མངོན་སུམ་ དུ་ བགྱིས་པ་ ལ་ གཟིགས །"
msgstr ""

msgctxt "649e748dbbe2497982a291d975bd562f"
msgid "བཅོམ་ལྡན་འདས་ ཀྱིས་ བཀའ་ སྩལ་པ །"
msgstr ""

msgctxt "5eaddc45999d428faa30459f30d0b4e2"
msgid "འདི་ ནི་ ཚེ་རབས་ ལྔ་ བརྒྱ␣འི་ བར་ དུ་ འདི␣འི་ ཆུང་མ␣ར་ གྱུར་ ཏེ །"
msgstr ""

msgctxt "ad424d1fc1994c739bbf28d2b7ae2908"
