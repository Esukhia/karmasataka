msgctxt "4d7f7abec0164c0db15331ae9d28d36b"
msgid "དེ␣ར་ སྐྱེས་པ་ མཐོང་ ནས་"
msgstr ""

msgctxt "96e939066f6642ec803b4645b9013173"
msgid "དེ␣འི་ བློ་ སྐྱེས་པ་"
msgstr ""

# འཕགས་པ་ཀུན་དགའ་བོ་ནི་བདག་ལ་དཀའ་<«ཅོ་»དགའ།>བ་བྱེད་བྱེད་པ་ཡིན་ནོ་སྙམ་དུ་
msgctxt "126ec42d01494811a875f3ad41e9a3a2"
msgid "འཕགས་པ་ ཀུན་དགའ་བོ་ ནི་ བདག་ ལ་ དཀའ་བ་ བྱེད་ བྱེད་པ་ ཡིན་ ནོ་ སྙམ་ དུ་"
msgstr ""

msgctxt "7c857980f875461bac2e895bda06a2f9"
msgid "རིག་ ནས །"
msgstr ""

msgctxt "846f2434c07043a7b5ff3ead803fac5b"
msgid "དེ␣ར་ རྟག་ ཏུ་ འགྲོ་ ཞིང་ བཀུར་སྟི་ བྱེད་ དོ ། །"
msgstr ""

msgctxt "468a4dfe7d11487ab4452a16b249fdb9"
msgid "དེ་ནས་ དགེ་སློང་ དག་ གིས་ སངས་རྒྱས་ བཅོམ་ལྡན་འདས་ ལ་ ཞུས་པ །"
msgstr ""

# བཙུན་པ་<«གཡུང་»-པ།>གསེར་མདོག་གིས་ལས་ཅི་བགྱིས་ན།
# ལས་དེའི་རྣམ་པར་སྨིན་པས་ཕྱུག་ཅིང་ནོར་མང་ལ་ལོངས་སྤྱོད་ཆེ་བའི་རིགས་སུ་སྐྱེས་ཏེ།
msgctxt "07af0e5d3d4748328d2592c02f3e3f72"
msgid ""
"བཙུན་པ་ གསེར་མདོག་ གིས་ ལ␣ས་ ཅི་ བགྱིས་ ན །  ལ␣ས་ དེ␣འི་ རྣམ་པ␣ར་ སྨིན་པ␣ས་ "
"ཕྱུག་ ཅིང་ ནོར་ མང་ ལ་ ལོངས་སྤྱོད་ ཆེ་བ␣འི་ རིགས་ སུ་ སྐྱེས་ ཏེ །"
msgstr ""

msgctxt "1c958d8b8ea74defb2cbb7c2a9cc7135"
msgid "གསེར་ གྱི་ མདོག་ ཅན་ དུ་ གྱུར་ ལ །"
msgstr ""

msgctxt "b9882cd9e04e42bab068731ab3f25d0b"
msgid "བཅོམ་ལྡན་འདས་ ཀྱང་ འདི␣ས་ མཉེས་པ␣ར་ བགྱིས་ ཏེ །"
msgstr ""

msgctxt "0974f79d5a064a33ad07a93b50c2aab5"
msgid "མི་ མཉེས་པ␣ར་ མ་ བགྱིས་ ལ །"
msgstr ""

# བཅོམ་ལྡན་འདས་ཀྱི་<«ལི་»«སྣར་»«ཅོ་»ཀྱིས།>བསྟན་པ་ལ་རབ་ཏུ་བྱུང་ནས་
msgctxt "b6f5df2f81454584b22c9e85d8b33105"
msgid "བཅོམ་ལྡན་འདས་ ཀྱི་ བསྟན་པ་ ལ་ རབ་ཏུ་ བྱུང་ ནས་"
msgstr ""

msgctxt "e8aee73ee0be4d578930527a21d1feef"
msgid "ཉོན་མོངས་པ་ ཐམས་ཅད་ སྤངས་ ཏེ །"
msgstr ""

msgctxt "68ddcae6726247238a14bb78d40da13b"
msgid "དགྲ་བཅོམ་པ་ ཉིད་ མངོན་སུམ་ དུ་ བགྱིས་ ལགས །"
msgstr ""

msgctxt "cb6ccb8b474e4ff291985c0a2d25fbb4"
msgid "བཅོམ་ལྡན་འདས་ ཀྱིས་ བཀའ་ སྩལ་པ །"
msgstr ""

# དགེ་སློང་<«གཡུང་»-སློང་།>དག་མཉན་དུ་ཡོད་པ་འདི་<«གཡུང་»«ལི་»«པེ་»«སྣར་»«ཅོ་»-འདི།>ཉིད་ནས་<«སྣར་»«ཞོལ་»ན།>ས་ཕྱོགས་ག་གེ་མོ་ཞིག་ན།
# རིགས་ངན་གྱི་བུ་མོ་མི་སྡུག་པའི་སྣ་གྲངས་བཅྭ་<«གཡུང་»«པེ་»«སྣར་»«ཅོ་»«ཞོལ་»བཅོ།>བརྒྱད་དང་ལྡན་:པ་ཞིག་<«ཅོ་»པའི།>འདུག་པ་ཁྱོད་ཀྱིས་མཐོང་ངམ།
msgctxt "9fbd2aedb4484474b82c800420629e76"
msgid ""
"དགེ་སློང་ དག་ མཉན་ དུ་ཡོད་པ་ འདི་ཉིད་ ནས་ ས་ཕྱོགས་ ག་གེ་མོ་ ཞིག་ ན །  "
"རིགས་ངན་ གྱི་ བུ་མོ་ མི་ སྡུག་པ␣འི་ སྣ་གྲངས་ བཅྭ་ བརྒྱད་ དང་ ལྡན་པ་ ཞིག་ "
"འདུག་པ་ ཁྱོད་ ཀྱིས་ མཐོང་ ངམ །"
msgstr ""

msgctxt "5ee6d82cf92d4fbc9a2962796120b773"
msgid "བཅོམ་ལྡན་འདས་ མཐོང་ ལགས་སོ ། །"
msgstr ""

msgctxt "59405ccf06bd4846865b6a3234f7e159"
msgid "དེ་དེ་ ནས་ ཤི་འཕོས་ ནས་"
msgstr ""

msgctxt "e30d8ba7987347bc879eca1866d19a91"
msgid "འདི␣ར་ སྐྱེས་ ཏེ །"
msgstr ""

# དེས་དབུ་སྐྲ་དང་སེན་མོའི་མཆོད་རྟེན་ལ་ཏིལ་མར་:གྱི་བྱུག་<«གཡུང་»«ལི་»«པེ་»«སྣར་»«ཅོ་»གྱིས་བྱུགས།>པ་དང་།
# སྦྱར་བའི་སྤོས་ཀྱི་<«གཡུང་»«ལི་»«པེ་»«སྣར་»«ཅོ་»ཀྱིས།>བྱུག་པ་དང་།
# གུར་གུམ་གྱི་<«གཡུང་»«ལི་»«པེ་»«སྣར་»«ཅོ་»གྱིས།>བྱུག་པ་དང་།
# སྤོས་ཀྱི་<«གཡུང་»«ལི་»«པེ་»«སྣར་»«ཅོ་»ཀྱིས།>བྱུག་པ་ཕུལ་ནས་
msgctxt "a6048377fad54c048c8bcfdfe61c8db6"
msgid ""
"དེ␣ས་ དབུ་སྐྲ་ དང་ སེན་མོ␣འི་ མཆོད་རྟེན་ ལ་ ཏིལ་མར་ གྱི་ བྱུག་པ་ དང་ །  "
"སྦྱར་བ␣འི་ སྤོས་ ཀྱི་ བྱུག་པ་ དང་ །  གུར་གུམ་ གྱི་ བྱུག་པ་ དང་ །  སྤོས་ ཀྱི་"
" བྱུག་པ་ ཕུལ་ ནས་"
msgstr ""

msgctxt "84ba8f1329ff4b29baa5e33b152ae071"
msgid "ང་ ལ་ གོས་ ཕུལ་ ཏེ་ ང་ ལ་ སེམས་ དགའ་བ་ བསྐྱེད་ ནས་"
msgstr ""

# ཤི་བའི་:འོག་ཏུ་<«གཡུང་»«ལི་»«པེ་»«སྣར་»«ཅོ་»«ཞོལ་»-འོག་ཏུ།>ལས་དེའི་རྣམ་པར་སྨིན་པས།
# འདི་ད་ལྟར་ཕྱུག་ཅིང་ནོར་མང་ལ་ལོངས་སྤྱོད་ཆེ་བའི་རིགས་སུ་སྐྱེས་ཏེ།
msgctxt "7be9e2b72ef54b0e98f5203d77b9ef4e"
msgid ""
"ཤི་བ␣འི་ འོག་ ཏུ་ ལ␣ས་ དེ␣འི་ རྣམ་པ␣ར་ སྨིན་པ␣ས །  འདི་ ད་ལྟར་ ཕྱུག་ ཅིང་ "
"ནོར་ མང་ ལ་ ལོངས་སྤྱོད་ ཆེ་བ␣འི་ རིགས་ སུ་ སྐྱེས་ ཏེ །"
msgstr ""

# གཟུགས་བཟང་ཞིང་བལྟ་<«ལི་»«སྣར་»«ཅོ་»ལྟ།>ན་སྡུག་ལ་མཛེས་པ་གསེར་གྱི་མདོག་ཅན་དུ་གྱུར་ཏོ།
# །
msgctxt "93716aecdb1b483a982eebf537d08e6c"
msgid ""
"གཟུགས་བཟང་ ཞིང་ བལྟ་ ན་ སྡུག་ ལ་ མཛེས་པ་ གསེར་ གྱི་ མདོག་ ཅན་ དུ་ གྱུར་ ཏོ "
"། །"
msgstr ""

msgctxt "37785f0dcf5f4659bef4fe3b3d1d1392"
msgid ""
"བཙུན་པ་ བུ་མོ་ འདི␣ས་ ལ␣ས་ ཅི་ བགྱིས་ ན་ ལ␣ས་ དེ␣འི་ རྣམ་པ␣ར་ སྨིན་པ␣ས །  "
"རིགས་ངན་ གྱི་ རིགས་ སུ་ སྐྱེས་ ཏེ །"
msgstr ""

# མི་སྡུག་པའི་སྣ་<«གཡུང་»«ལི་»«སྣར་»རྣམ།>གྲངས་བཅྭ་<«གཡུང་»«པེ་»«སྣར་»«ཅོ་»«ཞོལ་»རྣམ་པ་བཅོ།
# «ལི་»རྣམ་པ་བཙཱ།>བརྒྱད་དང་ལྡན་པར་གྱུར་ལགས།
msgctxt "d3bc2a003a3a44989024a280ccc6a52b"
msgid "མི་ སྡུག་པ␣འི་ སྣ་གྲངས་ བཅྭ་ བརྒྱད་ དང་ ལྡན་པ␣ར་ གྱུར་ ལགས །"
msgstr ""

msgctxt "99193e56fbe34e99977dabf5ca28720a"
msgid "བཅོམ་ལྡན་འདས་ ཀྱིས་ བཀའ་ སྩལ་པ །"
msgstr ""

# དགེ་སློང་དག་འདི་ཉིད་ཀྱིས་<«ཞོལ་»ཀྱི།>ལས་རྣམས་བྱས་ཤིང་བསགས་ཏེ་
msgctxt "84b85d1f58b546b3ac574b4664903e2f"
msgid "དགེ་སློང་ དག་ འདི་ཉིད་ ཀྱིས་ ལ␣ས་ རྣམས་ བྱས་ ཤིང་ བསགས་ ཏེ་"
msgstr ""

# དགེ་སློང་དག་སྔོན་:བྱུང་བ་<«ལི་»«སྣར་»«ཅོ་»«ཞོལ་»-བྱུང་བ།>འདས་པའི་དུས་ན།
# བསྐལ་<«གཡུང་»སྐལ།>[78a]པ་བཟང་པོ་འདི་ཉིད་ལ་སྐྱེ་དགུའི་<«ཞོལ་»རྒུའི།>ཚེ་ལོ་ཉི་ཁྲི་ཐུབ་པ་ན།
# ཡང་དག་པར་རྫོགས་པའི་སངས་རྒྱས་རིག་པ་<«ཞོལ་»རིམ།>དང་ཞབས་སུ་ལྡན་པ་བདེ་བར་གཤེགས་པ་འཇིག་རྟེན་མཁྱེན་པ།
# སྐྱེས་བུ་གདུལ་བྱའི་<«གཡུང་»«པེ་»བའི།>ཁ་ལོ་སྒྱུར་བ།
# བླ་ན་མེད་པ་ལྷ་དང་མི་རྣམས་ཀྱི་སྟོན་པ།
# སངས་རྒྱས་བཅོམ་ལྡན་འདས་འོད་སྲུང་ཞེས་བྱ་བ་འཇིག་རྟེན་དུ་བྱུང་སྟེ།
msgctxt "4641ea7d06be4c42bdf969a9c4f1b462"
msgid ""
"དགེ་སློང་ དག་ སྔོན་བྱུང་བ་ འདས་པ␣འི་ དུས་ ན །  བསྐལ་པ་ བཟང་པོ་ འདི་ཉིད་ ལ་ "
"སྐྱེ་དགུ␣འི་ ཚེ་ལོ་ ཉི་ཁྲི་ ཐུབ་པ་ ན །  ཡང་དག་པ␣ར་ རྫོགས་པ␣འི་ སངས་རྒྱས་ "
"རིག་པ་ དང་ ཞབས་ སུ་ ལྡན་པ་ བདེ་བ␣ར་ གཤེགས་པ་ འཇིག་རྟེན་ མཁྱེན་པ །  སྐྱེས་བུ་"
" གདུལ་བྱ␣འི་ ཁ་ལོ་ སྒྱུར་བ །  བླ་ ན་ མེད་པ་ ལྷ་ དང་ མི་ རྣམས་ ཀྱི་ སྟོན་པ ། "
" སངས་རྒྱས་ བཅོམ་ལྡན་འདས་ འོད་སྲུང་ ཞེས་ བྱ་བ་ འཇིག་རྟེན་ དུ་ བྱུང་ སྟེ །"
msgstr ""

# དེའི་ཚེ་བཱ་རཱ་ཎ་སཱི་ན་ཁྱིམ་བདག་ཅིག་གནས་པ་དེས་ཐབས་ཟླར་བབ་<«གཡུང་»པ།>པ་ལས་ཆུང་མ་བླངས་ནས།
msgctxt "d6adde6ac6634149b18f6123ba5daa11"
msgid ""
"དེ␣འི་ ཚེ་ བཱ་རཱ་ཎ་སཱི་ ན་ ཁྱིམ་བདག་ ཅིག་ གནས་པ་ དེ␣ས་ ཐབས་ ཟླ␣ར་ བབ་པ་ ལ␣ས་"
" ཆུང་མ་ བླངས་ ནས །"
msgstr ""

# དེ་དེ་དང་ལྷན་ཅིག་ཏུ་རྩེ་ཞིང་:དགའ་ལ་<«གཡུང་»«པེ་»-དགའལ།>དགའ་མགུར་སྤྱོད་པ་ལས་
msgctxt "4f3e3e91a2374177bda2466cd49e3e94"
msgid "དེ་དེ་ དང་ ལྷན་ཅིག་ ཏུ་ རྩེ་ ཞིང་ དགའ་ ལ་ དགའ་མགུ␣ར་ སྤྱོད་པ་ ལ␣ས་"
msgstr ""

msgctxt "e0f3f34a1bd842b2bd75e4a47d96ef26"
msgid "ཕྱི་ ཞིག་ ན །  དེ␣འི་ ཆུང་མ་ ལ་ བུ་ ཆགས་ ནས །"
msgstr ""

msgctxt "3fc1d004f5634ed1b1b0c7bcec568e8a"
msgid "དེ་ ཟླ་བ་ དགུ་ འམ་ བཅུ་ ལོན་པ་ དང་"
msgstr ""

# བུ་མོ་གཟུགས་བཟང་ཞིང་བལྟ་<«གཡུང་»བསྟ།
# «ལི་»«པེ་»«སྣར་»«ཅོ་»ལྟ།>ན་སྡུག་ལ་མཛེས་པ་ཞིག་བཙས་ནས་<«གཡུང་»«ལི་»«པེ་»«ཅོ་»ན།>
msgctxt "7694840849b94b51b5e880f3528be7c4"
msgid "བུ་མོ་ གཟུགས་བཟང་ ཞིང་ བལྟ་ ན་ སྡུག་ ལ་ མཛེས་པ་ ཞིག་ བཙས་ ནས་"
msgstr ""

msgctxt "8f24a5c96b4f46c1a2ad6b9551acefd4"
msgid "དེ␣འི་ བཙས་ སྟོན་ རྒྱས་པ␣ར་ བྱས་ ཏེ །"
msgstr ""

msgctxt "a2abebe6f36a4419bfb603923ae02337"
msgid "རིགས་ དང་ འཐུན་པ␣འི་ མིང་ བཏགས་ ནས་"
msgstr ""

# འོ་མ་དང་ཞོ་དང་མར་དང་ཞུན་མར་དང་མར་གྱི་:ཉིང་གུས་བསྐྱེད་བསྲིངས་<«གཡུང་»སྙིང་ཁུས་སྐྱེད་སྲིངས།
# «ལི་»«པེ་»སྙིང་ཁུས་བསྐྱེད་བསྲིངས།
# «སྣར་»«ཅོ་»«ཞོལ་»ཉིང་ཁུས་བསྐྱེད་བསྲིངས།>ཏེ།
msgctxt "cd83f88bfadb4e5890e0459d9bc645a4"
msgid ""
"འོ་མ་ དང་ ཞོ་ དང་ མར་ དང་ ཞུན་མར་ དང་ མར་ གྱི་ ཉིང་ གུས་ བསྐྱེད་ བསྲིངས་ ཏེ "
"།"
msgstr ""

# དེ་གང་གི་ཚེ་<«གཡུང་»-ཚེ།>ཆེན་མོར་གྱུར་པ་དེའི་ཚེ།དེས་ཡང་དག་པར་རྫོགས་པའི་སངས་རྒྱས་འོད་སྲུང་གི་བསྟན་པ་ལ་དད་པ་རྙེད་ནས།
msgctxt "b2eebac17edd48dc9f09c636bf2e14d7"
msgid ""
"དེ་ གང་ གི་ ཚེ་ ཆེན་ མོ␣ར་ གྱུར་པ་ དེ␣འི་ ཚེ ། དེ␣ས་ ཡང་དག་པ␣ར་ རྫོགས་པ␣འི་ "
"སངས་རྒྱས་ འོད་སྲུང་ གི་ བསྟན་པ་ ལ་ དད་པ་ རྙེད་ ནས །"
msgstr ""

msgctxt "5da39c8ae24f44738bb9e023e2951b6a"
msgid "དེ␣ས་ ཕ་མ་ ལ་ གསོལ་ ཏེ་"
msgstr ""

# ཡང་དག་པར་རྫོགས་པའི་སངས་རྒྱས་འོད་སྲུང་གི་<«ཞོལ་»གིས།>བསྟན་པ་ལ་རབ་ཏུ་བྱུང་ནས།
msgctxt "57476294b0d84526954236ef243f773d"
msgid ""
"ཡང་དག་པ␣ར་ རྫོགས་པ␣འི་ སངས་རྒྱས་ འོད་སྲུང་ གི་ བསྟན་པ་ ལ་ རབ་ཏུ་ བྱུང་ ནས །"
msgstr ""

# དེ་<«ཁུ་»དེས།>འཐབ་ཀྲོལ་<«ཁུ་»གྲོལ།>ཅན་ཁ་ཚས་ངན་པར་གྱུར་ཏོ། །
msgctxt "c6eff272d1d54b8f951a764b7c8be8a2"
msgid "དེ་ འཐབ་ ཀྲོལ་ ཅན་ ཁ་ཚ␣ས་ ངན་པ␣ར་ གྱུར་ ཏོ ། །"
msgstr ""

# དེས་བྱད་གཟུགས་དང་ལང་ཚོ་དང་དབང་ཕྱུག་དང་ཆོ་<«ལི་»«སྣར་»«ཞོལ་»ཚོ།>རིགས་ཀྱི་རྒྱགས་པས་དྲེགས་ནས་
msgctxt "6d3a92179f4a43d6a517d3daff279e7d"
msgid ""
"དེ␣ས་ བྱད་གཟུགས་ དང་ ལང་ཚོ་ དང་ དབང་ཕྱུག་ དང་ ཆོ་རིགས་ ཀྱི་ རྒྱགས་པ␣ས་ "
"དྲེགས་ ནས་"
msgstr ""

# སློབ་པ་དང་མི་སློབ་པའི་དགེ་སློང་མ་མང་པོ་<«གཡུང་»«ལི་»«པེ་»«སྣར་»«ཅོ་»«ཞོལ་»མོ།>ཞིག་ལ་བྱད་གཟུགས་མི་སྡུག་པའི་ལུས་དང་ངག་གི་ལས་<«གཡུང་»«པེ་»བས།>རྣམས་ཀྱིས་:དཔྱས་ཏེ<«གཡུང་»«པེ་»དཔྱང་སྟེ།
# «ལི་»དཔྱ་སྟེ། «སྣར་»«ཞོལ་»འཕྱ་སྟེ། «ཅོ་»དཔྱས་སྟེ།>།
msgctxt "b18465f04aa449c5bd7d209a9ae9866d"
msgid ""
"སློབ་པ་ དང་ མི་སློབ་པ␣འི་ དགེ་སློང་ མ་ མང་པོ་ ཞིག་ ལ་ བྱད་གཟུགས་ མི་ "
"སྡུག་པ␣འི་ ལུས་ དང་ ངག་གི་ལས་ རྣམས་ ཀྱིས་ དཔྱ␣ས་ ཏེ །"
msgstr ""

msgctxt "06d776236022461192d8b8558783ec6c"
msgid "རིགས་ངན་ ཞེས་ བྱ་བ␣འི་ ཚིག་ ཀྱང་ སྨྲས་པ་ ལ␣ས །"
msgstr ""

msgctxt "59e7ba2642e04531a205642cdfb0678e"
msgid "ཕྱིས་ དེ་ འགྱོད་པ་ སྐྱེས་ སོ ། །"
msgstr ""

msgctxt "b9af110a145041f19d9b31d12c808133"
msgid "དེ་ནས་ དེ␣ས་ ཚེ་ གཅིག་ ཏུ་ ཚངས་པ␣ར་ སྤྱོད་པ་ སྤྱད་ ནས །"
msgstr ""

msgctxt "5aa8011cbfec4d43a81a57e8506a595e"
msgid "སྨོན་ལམ་ བཏབ་པ །"
msgstr ""

# འདི་ལྟར་<«གཡུང་»«པེ་»ལར།>བདག་གིས་ཡང་དག་པར་རྫོགས་པའི་སངས་རྒྱས་འོད་སྲུང་གི་བསྟན་པ་ལ་ཚེ་གཅིག་<«གཡུང་»ཅིག>ཏུ་ཚངས་པར་སྤྱོད་པ་དག་<«གཡུང་»«ལི་»«པེ་»«སྣར་»«ཅོ་»«ཞོལ་»-དག>སྤྱད་ཀྱང་།
# ཡོན་ཏན་གྱི་ཚོགས་ཅི་ཡང་མ་ཐོབ་ཀྱིས<«ཞོལ་»ཀྱི།>།
msgctxt "99c15eb9ff784d919a1dfd293a42ca68"
msgid ""
"འདི་ལྟར་ བདག་ གིས་ ཡང་དག་པ␣ར་ རྫོགས་པ␣འི་ སངས་རྒྱས་ འོད་སྲུང་ གི་ བསྟན་པ་ ལ་"
" ཚེ་ གཅིག་ ཏུ་ ཚངས་པ␣ར་ སྤྱོད་པ་ དག་ སྤྱད་ ཀྱང་ །  ཡོན་ཏན་ གྱི་ ཚོགས་ ཅི་ཡང་"
" མ་ ཐོབ་ ཀྱིས །"
msgstr ""

# ཡང་དག་པར་རྫོགས་པའི་སངས་རྒྱས་འོད་སྲུང་གིས་བྲམ་ཟེའི་ཁྱེའུ་བླ་མ་ལུང་བསྟན་<«གཡུང་»«པེ་»སྟན།>པ་གང་ཡིན་པའི་སངས་རྒྱས་བཅོམ་ལྡན་འདས་དེ་བདག་གིས་མཉེས་པར་བྱེད་པར་གྱུར་ཅིག
# །
msgctxt "50ee52cb010e4073b184919b60da69c7"
msgid ""
"ཡང་དག་པ␣ར་ རྫོགས་པ␣འི་ སངས་རྒྱས་ འོད་སྲུང་ གིས་ བྲམ་ཟེ␣འི་ ཁྱེའུ་ བླ་མ་ "
"ལུང་བསྟན་པ་ གང་ཡིན་པ␣འི་ སངས་རྒྱས་ བཅོམ་ལྡན་འདས་ དེ་ བདག་ གིས་ མཉེས་པ␣ར་ "
"བྱེད་པ␣ར་ གྱུར་ཅིག  །"
msgstr ""

msgctxt "79aae9e0c334467cbd1d2067f364393d"
msgid "མི་ མཉེས་པ␣ར་ བྱེད་པ␣ར་ མ་ གྱུར་ཅིག  །"
msgstr ""

msgctxt "ad1bc79eeb724ae99afbe77eb6dd275d"
msgid "དེ་ ཁོ་ན␣འི་ བསྟན་པ་ ལ་ རབ་ཏུ་ བྱུང་ ནས་"
msgstr ""

msgctxt "e2c7c86d18644441ad046a055c57533f"
msgid "ཉོན་མོངས་པ་ ཐམས་ཅད་ སྤངས་ ཏེ །"
msgstr ""

msgctxt "aafc9b06902e4b4993d34b4db5799a09"
msgid "དགྲ་བཅོམ་པ་ ཉིད་ མངོན་སུམ་ དུ་ བྱེད་པ␣ར་ གྱུར་ཅིག  །"
msgstr ""

# བདག་གིས་ཚངས་པ་མཚུངས་པར་སྤྱོད་པ་དེ་དག་ལ་ཚིག་རྩུབ་པོ་སྨྲས་པའི་སྡིག་<«གཡུང་»«པེ་»སྔགས།>པའི་ལས་དེའི་འབྲས་བུ་བདག་ལ་སྨིན་པར་མ་གྱུར་ཅིག་ཅེས་
msgctxt "5c49bbf66d504b8e93a65f44620a38f4"
msgid ""
"བདག་ གིས་ ཚངས་པ་ མཚུངས་པ␣ར་ སྤྱོད་པ་ དེ་དག་ ལ་ ཚིག་རྩུབ་པོ་ སྨྲས་པ␣འི་ "
"སྡིག་པ␣འི་ ལ␣ས་ དེ␣འི་ འབྲས་བུ་ བདག་ ལ་ སྨིན་པ␣ར་ མ་ གྱུར་ཅིག་ ཅེས་"
msgstr ""

msgctxt "1dab926691864b5db81d563185d3c868"
msgid "བྱས་ སོ ། །"
msgstr ""

msgctxt "94708c64a66f4ff9a8c04fdf5be25ddb"
msgid "དགེ་སློང་ དག་ ཇི་སྙམ་ དུ་ སེམས །"
msgstr ""

# དེའི་ཚེ་ཡང་དག་པར་རྫོགས་པའི་སངས་རྒྱས་འོད་:སྲུང་གི་<«གཡུང་»«པེ་»སྲུངས་གི།
# «ཞོལ་»སྲུང་གིས།>བསྟན་པ་ལ་རབ་ཏུ་བྱུང་<«གཡུང་»«པེ་»འབྱུང་།>བའི་དགེ་སློང་མར་གྱུར་པ་གང་ཡིན་པ་དེ་ནི།
# མི་སྡུག་པ་འདི་ཉིད་ཡིན་ཏེ།
msgctxt "cbcdd90a5396406da04cfc7d99d4cc61"
msgid ""
"དེ␣འི་ ཚེ་ ཡང་དག་པ␣ར་ རྫོགས་པ␣འི་ སངས་རྒྱས་ འོད་སྲུང་ གི་ བསྟན་པ་ ལ་ རབ་ཏུ་ "
"བྱུང་བ␣འི་ དགེ་སློང་ མར་ གྱུར་པ་ གང་ཡིན་པ་ དེ་ ནི །  མི་ སྡུག་པ་ འདི་ཉིད་ "
"ཡིན་ ཏེ །"
msgstr ""

# དེས་དེར་བྱད་གཟུགས་དང་ལང་ཚོ་<«གཡུང་»«པེ་»འཚོ།>དང་དབང་ཕྱུག་དང་ཆོ་<«སྣར་»«ཞོལ་»ཚོ།>རིགས་ཀྱི་རྒྱགས་པས་དྲེགས་ནས་
msgctxt "ea78c04f5593437a8e565796e1081427"
msgid ""
"དེ␣ས་ དེ␣ར་ བྱད་གཟུགས་ དང་ ལང་ཚོ་ དང་ དབང་ཕྱུག་ དང་ ཆོ་རིགས་ ཀྱི་ རྒྱགས་པ␣ས་"
" དྲེགས་ ནས་"
msgstr ""

# དགེ་སློང་མ་མང་པོ་<«གཡུང་»མེ། «ལི་»«སྣར་»མོ།>ཞིག་ལ་མི་སྡུག་པའི་ཚུལ་གྱིས།
# ལུས་ཀྱི་ལས་ཀྱིས་འཕྱས་པ་དང་།
# རིགས་ངན་ཞེས་བྱ་བའི་ཚིག་བརྗོད་པའི་ལས་དེའི་རྣམ་པར་སྨིན་པས་གང་དང་གང་དུ་སྐྱེས་པ་:དེ་དང་<«ལི་»«སྣར་»«ཞོལ་»-དེ་དང་།>དེར་རིགས་ངན་གྱི་རིགས་སུ་སྐྱེས་ཏེ།
msgctxt "7460886eca9a4b5abf8eeb014e05a92d"
msgid ""
"དགེ་སློང་ མ་ མང་པོ་ ཞིག་ ལ་ མི་ སྡུག་པ␣འི་ ཚུལ་ གྱིས །  ལུས་ ཀྱི་ ལ␣ས་ ཀྱིས་"
" འཕྱས་པ་ དང་ །  རིགས་ངན་ ཞེས་ བྱ་བ␣འི་ ཚིག་ བརྗོད་པ␣འི་ ལ␣ས་ དེ␣འི་ རྣམ་པ␣ར་"
" སྨིན་པ␣ས་ གང་དང་གང་ དུ་ སྐྱེས་པ་ དེ་ དང་ དེ␣ར་ རིགས་ངན་ གྱི་ རིགས་ སུ་ "
"སྐྱེས་ ཏེ །"
msgstr ""

# མི་སྡུག་པའི་སྣ་<«གཡུང་»«ལི་»«པེ་»«སྣར་»རྣམ།>གྲངས་བཅྭ་<«སྣར་»«ཅོ་»«ཞོལ་»བཅོ།>བརྒྱད་དང་ཡང་ལྡན་པར་གྱུར་ཏོ།
# །
msgctxt "fc24c18dac51409696d38887eba44f8f"
msgid "མི་ སྡུག་པ␣འི་ སྣ་གྲངས་ བཅྭ་ བརྒྱད་ དང་ ཡང་ ལྡན་པ␣ར་ གྱུར་ ཏོ ། །"
msgstr ""

msgctxt "accd9a4627dc4fa7a06edb367159f1a9"
msgid "དེ་ནས་ ཚེ་ གཅིག་ ཏུ་ ཚངས་པ␣ར་ སྤྱོད་པ་ སྤྱད་ ནས་"
msgstr ""

# འཆི་ཀར་<«ཞོལ་»ཁར།>སྨོན་ལམ་བཏབ་སྟེ།
msgctxt "1829a9607bc24d51ae02ceffc881e322"
msgid "འཆི་ཀ␣ར་ སྨོན་ལམ་ བཏབ་ སྟེ །"
msgstr ""

# ཡང་དག་པར་རྫོགས་པའི་སངས་རྒྱས་འོད་སྲུང་གིས་བྲམ་ཟེའི་ཁྱེའུ་བླ་མ་ལུང་བསྟན་པ་གང་ཡིན་པའི་སངས་རྒྱས་བཅོམ་ལྡན་འདས་དེ་<«གཡུང་»«པེ་»+དག>བདག་གིས་མཉེས་པར་བྱེད་པར་གྱུར་ཅིག
# །
msgctxt "bf50bc0b75c34bad9adb004ef7b5f4f3"
msgid ""
"ཡང་དག་པ␣ར་ རྫོགས་པ␣འི་ སངས་རྒྱས་ འོད་སྲུང་ གིས་ བྲམ་ཟེ␣འི་ ཁྱེའུ་ བླ་མ་ "
"ལུང་བསྟན་པ་ གང་ཡིན་པ␣འི་ སངས་རྒྱས་ བཅོམ་ལྡན་འདས་ དེ་ བདག་ གིས་ མཉེས་པ␣ར་ "
"བྱེད་པ␣ར་ གྱུར་ཅིག  །"
msgstr ""

msgctxt "62df227814d0432f99edd7c555373165"
msgid "མི་ མཉེས་པ␣ར་ བྱེད་པ␣ར་ མ་ གྱུར་ཅིག  །"
msgstr ""

msgctxt "307318493e47495eaf2d281e58a22c79"
msgid "དེ་ ཁོ་ན␣འི་ བསྟན་པ་ ལ་ རབ་ཏུ་ བྱུང་ ནས་"
msgstr ""

msgctxt "ddb38ad1b5dc4ed98b8a7758f2620603"
msgid "ཉོན་མོངས་པ་ ཐམས་ཅད་ སྤངས་ ཏེ་"
msgstr ""

msgctxt "475f3f8eea9b4184b676a4e1f871b8c9"
msgid "དགྲ་བཅོམ་པ་ ཉིད་ མངོན་སུམ་ དུ་ བྱེད་པ␣ར་ གྱུར་ཅིག་ ཅེས་"
msgstr ""

msgctxt "acc89223efb7412cac01a11dd0f4c08e"
msgid "བྱས་པ་ དེས་ན །"
msgstr ""

# དགེ་སློང་དག་ང་ཡང་དག་པར་རྫོགས་པའི་སངས་རྒྱས་འོད་སྲུང་དང་ཤིན་ཏུ་མཉམ་པ་དང་ཤུགས་མཉམ་པ་དང་མཛད་པ་མཉམ་པ་དང་ཐབས་མཉམ་པ་བརྙེས་<«ལི་»«སྣར་»«ཅོ་»བརྙེད།>པར་གྱུར་ནས་
msgctxt "f6e1207c490d4904896bfe973cd67535"
msgid ""
"དགེ་སློང་ དག་ ང་ ཡང་དག་པ␣ར་ རྫོགས་པ␣འི་ སངས་རྒྱས་ འོད་སྲུང་ དང་ ཤིན་ཏུ་ "
"མཉམ་པ་ དང་ ཤུགས་ མཉམ་པ་ དང་ མཛད་པ་ མཉམ་པ་ དང་ ཐབས་ མཉམ་པ་ བརྙེས་པ␣ར་ གྱུར་ "
"ནས་"
msgstr ""

msgctxt "82a917530c754c459b9adae5cef458fe"
msgid "འདི␣ས་ ང་ མཉེས་པ␣ར་ བྱས་ ཏེ་"
msgstr ""

# མི་མཉེས་<«གཡུང་»མཉམ།>པར་མ་བྱས་ལ།
msgctxt "4f5a7689f43e46ec8a84e4aee115835b"
msgid "མི་ མཉེས་པ␣ར་ མ་ བྱས་ ལ །"
msgstr ""

msgctxt "ab8378932e464d60a8981cd2d12fb6bf"
msgid "ང་ཉིད་ ཀྱི་ བསྟན་པ་ ལ་ རབ་ཏུ་ བྱུང་ ནས་"
msgstr ""

# ཉོན་མོངས་པ་:ཐམས་<«གཡུང་»«ལི་»«པེ་»«སྣར་»«ཅོ་»«ཞོལ་»-ཐམསཅད།>ཅད་སྤངས་ཏེ་
msgctxt "45830d8521cf4b2496d947fd2e60fa18"
msgid "ཉོན་མོངས་པ་ ཐམས་ཅད་ སྤངས་ ཏེ་"
msgstr ""

msgctxt "2125ebe8843047eface025764cdd7f96"
msgid "དགྲ་བཅོམ་པ་ ཉིད་ མངོན་སུམ་ དུ་ བྱས་ སོ །། །།"
msgstr ""
