msgctxt "efec432abcef4e669eaa0226ff5856bb"
msgid "བཅོམ་ལྡན་འདས་ ལ་ གོས་ དེ་ ཕུལ་ ཏེ །"
msgstr ""

msgctxt "82927bad41874190b2b8a0196da2675f"
msgid "ཐལ་མོ་ སྦྱར་ ནས །"
