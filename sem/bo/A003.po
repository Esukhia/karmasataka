msgctxt "fc29290580aa4f688ffcae7d739377e3"
msgid ""
"དེ་ནས་ དེ་ བཅོམ་ལྡན་འདས་ ལ་ སེམས་ དགའ་བ␣ར་ གྱུར་ བཞིན་ དུ་ ཤི་འཕོས་ ཏེ་"
msgstr ""

msgctxt "6aa00a66d4c542a28854b13b41149ef2"
msgid "དུས་ ལ␣ས་ འདས་ ནས་"
msgstr ""

# མཉན་དུ་ཡོད་པ་དེ་ཉིད་དུ་ཚོང་དཔོན་ཞིག་གི་ཁྱིམ་དུ་ཆུང་མ་ཆེན་མའི་<«པེ་»མོའི།>ལྟོར་ཉིང་<«གཡུང་»«པེ་»-ཉིང་།>མཚམས་སྦྱར་ནས།
msgctxt "3e873edcbf274e9693869b5e963ee7a6"
msgid ""
"མཉན་ དུ་ཡོད་པ་ དེ་ཉིད་ དུ་ ཚོང་དཔོན་ ཞིག་ གི་ ཁྱིམ་ དུ་ ཆུང་མ་ ཆེན་མ␣འི་ "
"ལྟོ␣ར་ ཉིང་མཚམས་ སྦྱར་ ནས །"
msgstr ""

msgctxt "eb7a7e7e8d4241f58d3e8a32e8d8dc16"
msgid ""
"དེ་ ཟླ་བ་ དགུ␣འམ་ བཅུ་ ལོན་པ་ དང་ བུ་མོ་ གསེར་ གྱི་ མདོག་ ཅན་ ཞིག་ བཙས་ ཏེ །"
msgstr ""

msgctxt "3cbd9467ea1e4cd1a09ad40c92958e0c"
msgid "དེ␣འི་ བཙས་ སྟོན་ རྒྱས་པ␣ར་ བྱས་ ནས །"
msgstr ""

# བུ་མོ་འདིའི་མིང་ཇི་སྐད་གདགས་ཞེས་<«གཡུང་»«ལི་»«པེ་»«ཅོ་»ཤེས།>
msgctxt "c9925e40a1b94fb38d5c09ee0cd3d9b7"
msgid "བུ་མོ་ འདི␣འི་ མིང་ ཇི་སྐད་ གདགས་ ཞེས་"
msgstr ""

msgctxt "684123bc17364c748a096ba1359a340e"
msgid "མིང་འདོགས་པ␣ར་ བྱེད་ དེ །"
msgstr ""

# བུ་མོ་འདི་གཟུགས་བཟང་ཞིང་བལྟ་<«ལི་»«སྣར་»«ཅོ་»ལྟ།>ན་སྡུག་ལ་མཛེས་ཏེ།
msgctxt "f8ce199be84b44669631065784e9e58d"
msgid "བུ་མོ་ འདི་ གཟུགས་བཟང་ ཞིང་ བལྟ་ ན་ སྡུག་ ལ་ མཛེས་ ཏེ །"
msgstr ""

# གསེར་གྱི་མདོག་ཅན་ཡིན་པས་ན།
# དེའི་ཕྱིར་བུ་མོ་འདིའི་མིང་གསེར་གྱི་<«གཡུང་»«ལི་»«པེ་»«སྣར་»«ཅོ་»«ཞོལ་»-གྱི།>མདོག་ཅེས་
msgctxt "57853a75aded4bb3b13f433f6b793221"
msgid ""
"གསེར་ གྱི་ མདོག་ ཅན་ ཡིན་པ␣ས་ ན །  དེ␣འི་ ཕྱིར་ བུ་མོ་ འདི␣འི་ མིང་ གསེར་ "
"གྱི་ མདོག་ ཅེས་"
msgstr ""

msgctxt "303a3dd55fa5468ea4bc52eae0f1a0a3"
msgid "གདགས་ སོ་ ཞེས་"
msgstr ""

msgctxt "d80abebe85014a708becb3ace31c41e0"
msgid "ཟེར་ རོ ། །"
msgstr ""

# དེ་ནས་བུ་མོ་གསེར་གྱི་<«གཡུང་»«ལི་»«པེ་»«སྣར་»«ཅོ་»«ཞོལ་»-གྱི།>མདོག་ཅན་<«གཡུང་»«ལི་»«པེ་»«སྣར་»«ཅོ་»«ཞོལ་»-ཅན།>འོ་མ་དང་ཞོ་དང་མར་དང་ཞུན་<«གཡུང་»ཞུ།>མར་དང་མར་གྱི་:ཉིང་གུས་བསྐྱེད་བསྲིངས་ཏེ<«གཡུང་»སྙིང་ཁུས་བསྐྱེད་བསྲིང་ཏེ།
# «ལི་»«ཅོ་»སྙིང་ཁུས་བསྐྱེད་བསྲིངས་ཏེ། «པེ་»སྙིང་ཁུས་བསྐྱེད་བསྲིང་སྟེ།
# «སྣར་»«ཞོལ་»ཉིང་ཁུས་བསྐྱེད་བསྲིངས་ཏེ།>།
msgctxt "5d5513b7b4ac497ead871e250172ae12"
msgid ""
"དེ་ནས་ བུ་མོ་ གསེར་ གྱི་ མདོག་ ཅན་ འོ་མ་ དང་ ཞོ་ དང་ མར་ དང་ ཞུན་མར་ དང་ མར་"
" གྱི་ ཉིང་ གུས་ བསྐྱེད་ བསྲིངས་ ཏེ །"
msgstr ""

# དེ་གང་གི་ཚེ་བཙས་ནས། ལོ་བདུན་ལོན་པ་དེའི་ཚེ།
# དེས་བཅོམ་ལྡན་འདས་ཀྱི་<«ཅོ་»ཀྱིས།>བསྟན་པ་ལ་དད་པ་རྙེད་<«ཅོ་»བརྙེད།>ནས་
msgctxt "12efca2cd2ea4339b852784cc7dc496a"
msgid ""
"དེ་ གང་ གི་ ཚེ་ བཙས་ ནས །  ལོ་ བདུན་ ལོན་པ་ དེ␣འི་ ཚེ །  དེ␣ས་ བཅོམ་ལྡན་འདས་"
" ཀྱི་ བསྟན་པ་ ལ་ དད་པ་ རྙེད་ ནས་"
msgstr ""

# དེས་<«གཡུང་»«པེ་»དེ།>ཕ་མ་ལ་གསོལ་ཏེ།
msgctxt "4dbf0933064044489767bcb5278dc172"
msgid "དེ␣ས་ ཕ་མ་ ལ་ གསོལ་ ཏེ །"
msgstr ""

msgctxt "a6204a25c65d417a9a5d10a93bb76caa"
msgid "བཅོམ་ལྡན་འདས་ ཀྱི་ བསྟན་པ་ ལ་ རབ་ཏུ་ བྱུང་ ནས་"
msgstr ""

msgctxt "e16a59ac848f440585eb97fa12f081dc"
msgid "ཉོན་མོངས་པ་ ཐམས་ཅད་ སྤངས་ ཏེ །"
msgstr ""

msgctxt "d3fc4ccf2c9b4ee29151f00a6e15f350"
msgid "དགྲ་བཅོམ་པ་ ཉིད་ མངོན་སུམ་ དུ་ བྱས་ ནས །"
msgstr ""

# རྫུ་འཕྲུལ་ཆེ་ཞིང་མཐུ་:ཆེ་བར་<«གཡུང་»ཆེར་བར
# «ལི་»«པེ་»«སྣར་»«ཅོ་»«ཞོལ་»ཆེར།>གྱུར་ཏོ། །
msgctxt "ba3cee336940472eb9ca49b3cd375399"
msgid "རྫུ་འཕྲུལ་ ཆེ་ ཞིང་ མཐུ་ ཆེ་བ␣ར་ གྱུར་ ཏོ ། །"
msgstr ""

msgctxt "2a42075577784fb08337343fcbabe829"
msgid "དེ་ནས་ དེ་ དགྲ་བཅོམ་པ་ ཉིད་ ཐོབ་ ནས་"
