msgctxt "bcfe73c878594ac18a7a69275138d797"
msgid "བསམས་པ་"
msgstr ""

msgctxt "6b3443af69054c878d286e6e9326c1ec"
msgid "བདག་ གང་ ནས་ ཤི་འཕོས་ ཏེ ། འདི␣ར་ སྐྱེས་ སྙམ་པ་ དང་"
msgstr ""

# དེས་མཉན་དུ་ཡོད་པ་<«གཡུང་»«པེ་»+ན།>དེ་ཉིད་དུ་རིགས་ངན་གྱི་བུ་མོ་མི་སྡུག་པའི་སྣ་གྲངས་བཅྭ་<«སྣར་»«ཅོ་»«ཞོལ་»བཅོ།>བརྒྱད་དང་ལྡན་པར་གྱུར་པ་ལས།
msgctxt "4a857df43ed549ed82c96d844ea8321d"
msgid ""
"དེ␣ས་ མཉན་ དུ་ཡོད་པ་ དེ་ཉིད་ དུ་ རིགས་ངན་ གྱི་ བུ་མོ་ མི་ སྡུག་པ␣འི་ "
"སྣ་གྲངས་ བཅྭ་ བརྒྱད་ དང་ ལྡན་པ␣ར་ གྱུར་པ་ ལ␣ས །"
msgstr ""

msgctxt "a54b3f6be65c4665bcf1bc7c3b73227d"
msgid "དེ␣ར་ བདག་ གིས་ འཕགས་པ་ ཀུན་དགའ་བོ་ ལ་ བརྟེན་ ནས་"
msgstr ""

msgctxt "c2d49f34950b485081f7e43e2d6bbafb"
msgid "རིགས་ངན་ གྱི་ སྐྱེ་གནས་ སྤངས་ ཏེ །"
