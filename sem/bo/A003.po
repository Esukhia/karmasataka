msgctxt "3a7eb733f8ef4d5eb54261774ba6fba7"
msgid "﻿གསེར་མདོག་ ཅེས་བྱ་བ་ ནི །"
msgstr ""

# གླེང་གཞི་<«གཡུང་»བཞི།
# «ཅོ་»གཞི་ནི།>མཉན་དུ་ཡོད་པ་ན་བཞུགས་ཏེ<«གཡུང་»«པེ་»སྟེ།>།
msgctxt "2e4e144f915f4ddf9866e9f94465a9a3"
msgid "གླེང་ གཞི་ མཉན་ དུ་ཡོད་པ་ ན་ བཞུགས་ ཏེ །"
msgstr ""

msgctxt "f33add3d597747deac5ce3cf7c822cb7"
msgid ""
"དེ␣འི་ ཚེ་ མཉན་ དུ་ཡོད་པ་ ན །  རིགས་ངན་ ཞིག་ གནས་པ་ དེ␣ས་ ཐབས་ ཟླ␣ར་ བབ་པ་ "
"ལ་ཆུང་ མ་ བླངས་པ་ ལ␣ས་"
msgstr ""

# ཕྱི་ཞིག་ན་དེའི་ཆུང་མ་:ལ་བུ་<«གཡུང་»འབུ།>ཆགས་ནས་
msgctxt "631a0763b84744ab8a45733e9db80e06"
msgid "ཕྱི་ ཞིག་ ན་ དེ␣འི་ ཆུང་མ་ ལ་ བུ་ ཆགས་ ནས་"
msgstr ""

# དེ་ཟླ་བ་དགུའམ་བཅུ་ལོན་པ་དང་བུ་མོ་མི་སྡུག་པ་<«ལི་»«སྣར་»«ཅོ་»«ཞོལ་»པའི།>མཆོག་ཏུ་མི་སྡུག་ལ་མི་སྡུག་པའི་སྣ་:གྲངས་བཅྭ་<«གཡུང་»«པེ་»«སྣར་»«ཅོ་»གྲངས་བཅོ།
# «ཞོལ་»དྲངས་བཅོ།>བརྒྱད་དང་ལྡན་པ་ཞིག་བཙས་ནས་
msgctxt "d18edebbf4824f6f8798a4f54f3b32b1"
msgid ""
"དེ་ ཟླ་བ་ དགུ␣འམ་ བཅུ་ ལོན་པ་ དང་ བུ་མོ་ མི་ སྡུག་པ་ མཆོག་ ཏུ་ མི་ སྡུག་ ལ་ "
"མི་ སྡུག་པ␣འི་ སྣ་གྲངས་ བཅྭ་ བརྒྱད་ དང་ ལྡན་པ་ ཞིག་ བཙས་ ནས་"
msgstr ""

# དེ་མཐོང་མ་ཐག་ཏུ་ཕ་མ་གཉིས་ཤིན་ཏུ་མི་དགའ་བར་གྱུར་ཏེ<«ལི་»«སྣར་»«ཅོ་»«ཞོལ་»ཏོ།>།
msgctxt "7e739ba49e0b4450a6a743923d2b649c"
msgid "དེ་ མཐོང་ མ་ ཐག་ ཏུ་ ཕ་མ་ གཉིས་ ཤིན་ཏུ་ མི་ དགའ་བ␣ར་ གྱུར་ ཏེ །"
msgstr ""

# བདག་ཅག་གཉིས་ལ་བུ་མོ་འདི་འདྲ་བ་མི་དགོས་ཀྱིས་<«ཞོལ་»ཀྱི།>
msgctxt "394e3908ba944f45845bbb89cd2d10ed"
msgid "བདག་ཅག་ གཉིས་ ལ་ བུ་མོ་ འདི་འདྲ་བ་ མི་ དགོས་ ཀྱིས་"
msgstr ""

msgctxt "8f6c3427039f4fab94d4b94a5cee9371"
msgid "ནམ་ སྲོས་པ་ དང་ ཕྱིར་ ཕྱུང་ ལ་"
msgstr ""

msgctxt "92cc6c146aba41ceae7fe5a3d1e4afdd"
msgid "ཁྱི་ རྣམས་ ལ་ སྦྱིན་ ནོ་ ཞེས་"
msgstr ""

msgctxt "35f9853355024321b75011d70bf7979b"
msgid "ཟེར་ རོ ། །"
msgstr ""

msgctxt "1375d48752954986a2c2535a060e6b9a"
msgid "དེ་ནས་ ཆུང་མ་ ན་རེ་"
msgstr ""

# ཇོ་བོ་དེ་སྐད་མ་གསུང་<«ཞོལ་»གསུངས།>ཞིག །
msgctxt "25c783936a6c48a9a131030dd634954b"
msgid "ཇོ་བོ་ དེ་སྐད་ མ་ གསུང་ ཞིག  །"
msgstr ""

# འཇིག་རྟེན་ན་མི་གསོད་<«གཡུང་»«པེ་»བསོད།>པ་ནི་རྣམ་པར་སྨད་<«ལི་»«སྣར་»དམད།>པ་ལགས་ཀྱིས<«ཞོལ་»ཀྱི།>།
msgctxt "c38d04acfb79448a9a6ee9d1fe38540f"
msgid "འཇིག་རྟེན་ ན་ མི་ གསོད་པ་ ནི་ རྣམ་པ␣ར་ སྨད་པ་ ལགས་ ཀྱིས །"
msgstr ""

# འདི་འདི་<«གཡུང་»«ལི་»«པེ་»«སྣར་»«ཅོ་»-འདི།>ཁོ་ན་ལྟར་མི་སྡུག་པའི་རྣམ་<«ཅོ་»སྣ།>གྲངས་བཅྭ་<«གཡུང་»«པེ་»«སྣར་»«ཞོལ་»བཅོ།>བརྒྱད་དང་ལྡན་དུ་ལགས་ཀྱིས་<«ཞོལ་»-ཀྱིས།>ཀྱང་<«གཡུང་»«ལི་»«པེ་»«སྣར་»«ཅོ་»-ཀྱང་།>།
# འདི་བདག་ཅག་གཉིས་ཀྱི་བུ་མོ་ལགས་པས་ན།
# འདི་བཙས་མ་ཐག་ཏུ་བདག་ཅག་གིས་དོར་མི་ནུས་ཀྱིས་<«ཞོལ་»ཀྱི།>
msgctxt "4347e2415ce548c8a9892351e95311d3"
msgid ""
"འདི་ འདི་ཁོ་ན་ ལྟར་ མི་ སྡུག་པ␣འི་ རྣམ་ གྲངས་ བཅྭ་ བརྒྱད་ དང་ ལྡན་ དུ་ ལགས་ "
"ཀྱིས་ ཀྱང་ །  འདི་ བདག་ཅག་ གཉིས་ ཀྱི་ བུ་མོ་ ལགས་པ␣ས་ ན །  འདི་ བཙས་མ་ ཐག་ "
"ཏུ་ བདག་ཅག་ གིས་ དོར་ མི་ ནུས་ ཀྱིས་"
msgstr ""

msgctxt "67392c139a3c4ddfb4387a3546f622f5"
msgid "ནམ་ ཆེ␣ར་ སྐྱེས་པ་ ན་ བསྐྲད་པ␣ར་ བགྱི␣འོ ། །"
msgstr ""

# འོན་ཀྱང་འདི་དེའི་ཚེ་ཅིས་ཀྱང་[76b]འཚོ་བ་ཚོལ་ནུས་པར་འགྱུར་རོ་<«གཡུང་»«པེ་»«ཁུ་»-རོ།>ཞེས་
msgctxt "e7fc8070ca524faf892bf429af0da9d0"
msgid "འོན་ཀྱང་ འདི་ དེ␣འི་ ཚེ་ ཅིས་ཀྱང་ འཚོ་བ་ ཚོལ་ ནུས་པ␣ར་ འགྱུར་ རོ་ ཞེས་"
msgstr ""

msgctxt "011d31cf4ebc4726a1746987ec6fc52d"
msgid "བྱས་ སོ ། །"
msgstr ""

msgctxt "fc9559ced18d4366919d1944fd96e594"
msgid "དེ་ནས་ འོ་ན་ དེ་ལྟར་ བྱ␣འོ་ ཞེས་"
msgstr ""

msgctxt "5e907422cfcf4dbdb1769f92ba2c4451"
msgid "སྨྲ␣ས་ ནས །"
msgstr ""

# དེ་ཕ་མ་གཉིས་ཀྱིས་ལྐོག་<«གཡུང་»རྐོག>ཏུ་གསོས་ཏེ།
msgctxt "b9955429793b4cb89918887c4cd2ce80"
msgid "དེ་ ཕ་མ་ གཉིས་ ཀྱིས་ ལྐོག་ ཏུ་ གསོས་ ཏེ །"
msgstr ""

msgctxt "952ae42ce6014521a5dc99cf745e600c"
msgid "ཅི་ ནས་ སུ␣ས་ ཀྱང་ མི་ ཤེས་པ␣ར་ བྱས་ སོ ། །"
msgstr ""

# དེ་ནས་དེ་གང་གི་ཚེ་འགྲོ་ནུས་པར་གྱུར་པ་དེའི་ཚེ་དེ་མི་སྡུག་ཆེས་<«གཡུང་»ཅེས།>ནས།
# ཕ་མ་གཉིས་ཀྱིས་ཁྱིམ་ནས་བསྐྲད་<«གཡུང་»«པེ་»སྐད། «མི་»«སྣར་»«ཅོ་»སྐྲད།>དེ་
msgctxt "3d77eeb6aad24b5e8207c0ac5b748d49"
msgid ""
"དེ་ནས་ དེ་ གང་ གི་ ཚེ་འགྲོ་ ནུས་པ␣ར་ གྱུར་པ་ དེ␣འི་ ཚེ་ དེ་ མི་ སྡུག་ ཆེས་ "
"ནས །  ཕ་མ་ གཉིས་ ཀྱིས་ ཁྱིམ་ ནས་ བསྐྲད་ དེ་"
msgstr ""

msgctxt "6c2815905bfb49119a229f617275411f"
msgid "བཏང་ ངོ་ ། །"
msgstr ""

# དེ་ནས་དེ་སློངས་<«སྣར་»«ཞོལ་»སློང་།>མོ་བྱེད་ཅིང་སྲང་དེ་ཡན་མན་ན་གནས་པ་ལས།
msgctxt "2e58e07171ba4fe7a4ce328b4a7a4296"
msgid "དེ་ནས་ དེ་ སློངས་ མོ་ བྱེད་ ཅིང་ སྲང་ དེ་ཡན་ མན་ ན་ གནས་པ་ ལ␣ས །"
msgstr ""

# དེས་ས་སྒྲེན་པོ་<«གཡུང་»སྒྲ་བོ།
# «མི་»«པེ་»«སྣར་»«ཅོ་»«ཞོལ་»སྒྲེ་བོ།>ལ་གནས་པ་དང་ཁ་ཟས་ངན་པ་ཟ་ཟ་བས་ལུས་ལ་མཛེ་ནད་<«གཡུང་»«ལི་»«པེ་»«སྣར་»«ཅོ་»«ཞོལ»-ནད།>དང་རྨ་བྱུང་བར་གྱུར་ཏེ།
msgctxt "91f4af3af39847ecb219140ec83f9bd0"
msgid ""
"དེ␣ས་ ས་ སྒྲེན་པོ་ ལ་ གནས་པ་ དང་ཁ་ ཟས་ངན་པ་ ཟ་ ཟ་བ␣ས་ ལུས་ ལ་ མཛེ་ནད་ དང་ "
"རྨ་ བྱུང་བ␣ར་ གྱུར་ ཏེ །"
msgstr ""

msgctxt "44af28951bcd497c883ec2814f169d88"
msgid "དེ་ ཕྱི་ ཞིག་ ན་ ནད་བུ་ ཅན་ དུ་ གྱུར་ ནས །"
msgstr ""

msgctxt "432e03f5854041539dfc647e51eca114"
msgid "སྲང་ དེ་ ན་ འགྱེལ་ ཞིང་ འདུག་ སྟེ །"
msgstr ""

msgctxt "196b683674004b2789f18249528c0115"
msgid "ཤི་ ལ་ ཐུག་པ་ དང་ ཚེ་ དང་ ལྡན་པ་ ཀུན་དགའ་ བོས་ མཐོང་ སྟེ །"
msgstr ""

msgctxt "cee9a4f4172141c29be582a9422779d7"
msgid "མཐོང་ མ་ ཐག་ ཏུ་ ཚེ་ དང་ ལྡན་པ་ ཀུན་དགའ་བོ་ སྙིང་རྗེ་ སྐྱེས་ ནས་"
msgstr ""

msgctxt "6a0b3c3bac27417094cf9825b546bac9"
msgid "དེ་དེ␣འི་ ཐད་ དུ་ སོང་ སྟེ་"
msgstr ""

msgctxt "8a8238753918425c8529427f824375a2"
msgid "སྨྲས་པ །"
msgstr ""

# ཆེ་གེ་<«གཡུང་»ཞེས།>ཅི་ཉེས་
msgctxt "e5a2533267f94d1aa5f27470368c9610"
msgid "ཆེ་ གེ་ ཅི་ ཉེ␣ས་"
msgstr ""

msgctxt "d2408cd1066748d2b37eb9bc1f79a1d9"
msgid "དེ␣ས་ སྨྲས་པ །"
msgstr ""

# བཙུན་པ་ཀུན་:དགའ་བོ་<«ལི་»དགའོ།>བདག་གིས་<«ཞོལ་»གི།>སྔོན་བསོད་ནམས་དག་མ་བགྱིས་པས་ན།
# དེས་བདག་འདི་ཁོ་ན་ལྟ་བུར་གྱུར་པ་ལགས་ཀྱིས<«ཞོལ་»ཀྱི།>།
msgctxt "a6665c68c2dc43b8935fdc4cac2bb6fe"
msgid ""
"བཙུན་པ་ ཀུན་དགའ་བོ་ བདག་ གིས་ སྔོན་ བསོད་ནམས་ དག་མ་ བགྱིས་པ␣ས་ ན །  དེ␣ས་ "
"བདག་ འདི་ཁོ་ན་ ལྟ་བུ␣ར་ གྱུར་པ་ ལགས་ ཀྱིས །"
msgstr ""

# བཙུན་པ་ཀུན་དགའ་བོ་ཅི་ནས་བདག་གི་<«གཡུང་»གིས།>མི་དགེ་བའི་ལས་འདི་ཡོངས་སུ་ཟད་པར་འགྱུར་བར་མཛད་དུ་གསོལ།
msgctxt "997f398c3ab749318ccc9cd558b54e3f"
msgid ""
"བཙུན་པ་ ཀུན་དགའ་བོ་ ཅི་ ནས་ བདག་ གི་ མི་དགེ་བ␣འི་ ལ␣ས་ འདི་ ཡོངས་སུ་ ཟད་པར་ "
"འགྱུར་བ␣ར་ མཛད་ དུ་ གསོལ །"
msgstr ""

# ཚེ་དང་ལྡན་པ་ཀུན་དགའ་<«གཡུང་»«པེ་»-དགའ།>བོས་སྨྲས་པ།
msgctxt "220abeada49144a4a9c5558a271e4d47"
msgid "ཚེ་ དང་ ལྡན་པ་ ཀུན་དགའ་ བོས་ སྨྲས་པ །"
msgstr ""

# ཆེ་གེ་<«གཡུང་»«ཅོ་»ཞེ།>སྙིང་མ་ཆུང་བར་སྤྲོ་བ་སྐྱེད་ཅིག་དང་།
msgctxt "85dbf559a5194c47bf61a3ecb2cee48b"
msgid "ཆེ་ གེ་ སྙིང་ མ་ ཆུང་བ␣ར་ སྤྲོ་བ་ སྐྱེད་ ཅིག་ དང་ །"
msgstr ""

msgctxt "47b039a606d5492b90a4c7f5c140dce3"
msgid "བདག་ གིས་ ཁྱོད་ ཀྱི་ མི་དགེ་བ␣འི་ ལ␣ས་ འདི་ བསལ་བ␣ར་ བྱ␣འོ ། །"
msgstr ""

msgctxt "c17b10085f074c5e85afa76507306003"
msgid "དེ་སྐད་ ཅེས་ སྨྲ␣ས་ ནས་"
msgstr ""

# ཚེ་དང་ལྡན་པ་ཀུན་དགའ་བོས་དེའི་དོན་དུ་ཏིལ་མར་དང་:སྤོས་བཙལ་<«གཡུང་»ང་ང་སྤོས་བརྩལ།
# «པེ་»སྤོས་བརྩལ།>ནས་
msgctxt "3780490c2bab4e6e88862b28072defa1"
msgid ""
"ཚེ་ དང་ ལྡན་པ་ ཀུན་དགའ་ བོས་ དེ␣འི་ དོན་ དུ་ ཏིལ་མར་ དང་ སྤོས་ བཙལ་ ནས་"
msgstr ""

# བུ་མོ་དེ་ག་ལ་བར་<«སྣར་»«ཞོལ་»བ་དེར།>སོང་སྟེ་
msgctxt "f5e8809b28824152a278239fc8a2a3c0"
msgid "བུ་མོ་ དེ་ག་ ལ་བ␣ར་ སོང་ སྟེ་"
msgstr ""

msgctxt "e2bac009769e4095a19bd6f66eaa0e92"
msgid "ཕྱིན་ ནས་"
msgstr ""

msgctxt "e69c621bc7b047708ee93880e69f6acf"
msgid "སྨྲ␣ས་ :␣པ །"
msgstr ""

# ཆེ་གེ་<«གཡུང་»ཆེ་ཞེ། «ཅོ་»པ་ཆི་ཞེ།>ལོངས་ལ་
msgctxt "7aa74c332cda46bbab9f54c703b61da2"
msgid "ཆེ་ གེ་ ལོངས་ ལ་"
msgstr ""

# བཅོམ་ལྡན་འདས་ཀྱི་དབུ་སྐྲ་<«གཡུང་»«པེ་»སྒྲ།>དང་སེན་<«གཡུང་»«པེ་»སེ།>མོའི་མཆོད་རྟེན་ལ་སྤོས་ཀྱིས་བྱུགས་ཤིག
# །
msgctxt "eb15dd2b106d427187e775dd150b1fad"
msgid ""
"བཅོམ་ལྡན་འདས་ ཀྱི་ དབུ་སྐྲ་ དང་ སེན་མོ␣འི་ མཆོད་རྟེན་ ལ་ སྤོས་ ཀྱིས་ བྱུགས་ "
"ཤིག  །"
msgstr ""

msgctxt "c2ba236004c14d39bfc93c2845176283"
msgid "དེ་ནས་ དེ་ ཐོས་ མ་ ཐག་ ཏུ་ བུ་མོ་ དེ་ དགའ་བ་ སྐྱེས་ ནས་"
msgstr ""

msgctxt "43ee0ceedff44767a05686e73c0df70e"
msgid "ལངས་ ཏེ་"
msgstr ""

msgctxt "899ecbe197a443d28c7b325ab48f97c9"
msgid "ཚེ་ དང་ ལྡན་པ་ ཀུན་དགའ་བོ␣འི་ ཕྱི་བཞིན་ སོང་ ནས །"
msgstr ""

# ཀུན་དགའ་<«གཡུང་»+དགའ།>བོས།
# དེ་བཞིན་གཤེགས་པའི་མཆོད་རྟེན་ལ་ཏིལ་མར་:གྱི་བྱུག་<«གཡུང་»«ལི་»«པེ་»«སྣར་»«ཅོ་»ཀྱིས་བྱུགས།>པ་དང་།
# སྦྱར་བའི་སྤོས་:ཀྱི་བྱུག་<«གཡུང་»«ལི་»«པེ་»«སྣར་»«ཅོ་»ཀྱིས་བྱུགས།>པ་དང་།
# གུར་གུམ་:གྱི་བྱུག་<«གཡུང་»«ལི་»«པེ་»«སྣར་»«ཅོ་»གྱིས་བྱུགས།>པ་<«ཅོ་»+འམ།>འབུལ་<«གཡུང་»འབུམ།>དུ་བཅུག་གོ།
# །
msgctxt "1881120fe5164412b5b586b3597f2ed3"
msgid ""
"ཀུན་དགའ་ བོས །  དེ་བཞིན་ གཤེགས་པ␣འི་ མཆོད་རྟེན་ ལ་ ཏིལ་མར་ གྱི་ བྱུག་པ་ དང་ "
"།  སྦྱར་བ␣འི་ སྤོས་ ཀྱི་ བྱུག་པ་ དང་ །  གུར་གུམ་ གྱི་ བྱུག་པ་ འབུལ་ དུ་ "
"བཅུག་ གོ ། །"
msgstr ""

msgctxt "82cc23ceb1254da3ab9f6798644479f5"
msgid "ཁྱིམ་བདག་ མགོན་མེད་ ཟས་ སྦྱིན་ ཡང་ ས་ཕྱོགས་ དེ␣ར་ འོངས་ ཏེ །"
msgstr ""

# དེས་ཚེ་དང་ལྡན་པ་ཀུན་དགའ་བོ་མཐོང་ནས་བཙུན་པ་ཀུན་:དགའ་བོ་<«གཡུང་»འོ་དགའོ།>ཅི་མཛད་ཅེས་
msgctxt "7d6ea17d36fd4b3086a0c0fe18eba87a"
msgid ""
"དེ␣ས་ ཚེ་ དང་ ལྡན་པ་ ཀུན་དགའ་བོ་ མཐོང་ ནས་ བཙུན་པ་ ཀུན་དགའ་བོ་ ཅི་ མཛད་ ཅེས་"
msgstr ""

msgctxt "a682000e55da46c5a4b9e9a2dc451250"
msgid "དྲིས་ སོ ། །"
msgstr ""

msgctxt "36910c0741374abb8128e3bed1022692"
msgid "ཚེ་ དང་ ལྡན་པ་ ཀུན་དགའ་ བོས་ སྨྲས་པ །"
msgstr ""

# བུ་མོ་འདིའི་དབུལ་བ་:ཡོངས་སུ་<«གཡུང་»«པེ་»ཡང་ཡོང་སུ། «ཅོ་»-སུ།>གཅད་པར་བྱའོ། །
msgctxt "9d1056344fe642238771eb20cb2f4ca1"
msgid "བུ་མོ་ འདི␣འི་ དབུལ་བ་ ཡོངས་སུ་ གཅད་པ␣ར་ བྱ␣འོ ། །"
msgstr ""

msgctxt "1830cbb24100410f868405f8086e492e"
msgid ""
"དེ་ནས་ ཁྱིམ་བདག་ མགོན་མེད་ ཟས་ སྦྱིན་ གྱིས་ ཀྱང་ བུ་མོ་ དེ་ སྒྲེན་མོ␣ར་ "
"འདུག་པ་ མཐོང་ ནས་"
msgstr ""

msgctxt "41fce0bcadde4ecf99d3b6d3a5cb6b37"
msgid "གོས་ བྱིན་ ནོ ། །"
msgstr ""

# བཅོམ་ལྡན་འདས་ཀྱང་བུ་མོ་དེ་<«པེ་»-དེ།>ཁོ་ན་ལ་ཐུགས་བརྩེ་བའི་ཕྱིར་ས་ཕྱོགས་དེར་གཤེགས་ནས་
msgctxt "e974e1a1be934030aeb735d77495ce3c"
msgid ""
"བཅོམ་ལྡན་འདས་ ཀྱང་ བུ་མོ་ དེ་ ཁོ་ན་ ལ་ ཐུགས་ བརྩེ་བ␣འི་ ཕྱིར་ ས་ཕྱོགས་ དེ␣ར་"
" གཤེགས་ ནས་"
msgstr ""

# བུ་མོ་དེས་སངས་རྒྱས་བཅོམ་ལྡན་འདས་མཛེས་ཤིང་ཡིད་དུ་འཐད་<«གཡུང་»འཆད།>ལ།
# དབང་པོ་ཞི་བ། :ཐུགས་ཞི་བ<«གཡུང་»«པེ་»-ཐུགས་ཞི་བ།>།
# ཐུགས་མཆོག་ཏུ་དུལ་བ་དང་རྣམ་པར་ཞི་བ་དང་ལྡན་པ་གསེར་གྱི་མཆོད་སྡོང་ལྟར་དཔལ་འབར་<«ཅོ་»བསར།>བ་མཐོང་ངོ་།
# །
msgctxt "0dff9a521d9f4a13b966be4d9c302dcf"
msgid ""
"བུ་མོ་ དེ␣ས་ སངས་རྒྱས་ བཅོམ་ལྡན་འདས་ མཛེས་ ཤིང་ ཡིད་ དུ་ འཐད་ ལ །  དབང་པོ་ "
"ཞི་བ །  ཐུགས་ ཞི་བ །  ཐུགས་ མཆོག་ ཏུ་ དུལ་བ་ དང་ རྣམ་པ␣ར་ ཞི་བ་ དང་ ལྡན་པ་ "
"གསེར་ གྱི་ མཆོད་སྡོང་ ལྟར་ དཔལ་འབར་བ་ མཐོང་ ངོ་ ། །"
msgstr ""

msgctxt "29b72525b99644ffbed53ab704dc35cd"
msgid "མཐོང་ ནས་ ཀྱང་ བུ་མོ་ དེ་ཡང་ མཆོག་ ཏུ་ དགའ་བ་ སྐྱེས་ སོ ། །"
msgstr ""

msgctxt "cf773be4b3a64b5da81b64c09d23b3aa"
msgid "དགའ་བ་ སྐྱེས་ ནས་ བསམས་པ །"
msgstr ""

# འདིའི་དབུ་སྐྲ་<«གཡུང་»སྐ།
# «པེ་»ས།>དང་སེན་<«གཡུང་»ས།>མོའི་མཆོད་རྟེན་དག་ལ་བཀུར་སྟི་བྱས་ན་ཡང་འབྲས་བུ་ཆེན་པོར་འགྱུར་ན།
# འདི་ཉིད་ལ་བྱས་པ་ལྟ་ཅི་སྨོས་ཏེ།
msgctxt "3c67bd4b3a124e80a3b304f6d487f80d"
msgid ""
"འདི␣འི་ དབུ་སྐྲ་ དང་ སེན་མོ␣འི་ མཆོད་རྟེན་ དག་ ལ་ བཀུར་སྟི་ བྱས་ ན་ ཡང་ "
"འབྲས་བུ་ ཆེན་པོ␣ར་ འགྱུར་ ན །  འདི་ཉིད་ ལ་ བྱས་པ་ ལྟ་ཅི་ སྨོས་ ཏེ །"
msgstr ""

# འབྲས་བུ་ཆེན་པོར་ཇི་<«ཞོལ་»ཅི།>སྟེ་མི་འགྱུར་སྙམ་ནས།
msgctxt "f1623dc1ef3a45758295ed2fcf97f0d3"
msgid "འབྲས་བུ་ ཆེན་པོ␣ར་ ཇི་སྟེ་ མི་ འགྱུར་ སྙམ་ ནས །"
msgstr ""

msgctxt "780026357b5e4fff93ecaddbb04a8fe4"
