msgctxt "07a77b84003f41dd9ddc782e2d0301a2"
msgid "﻿ལ␣ས་ བརྒྱ་ ཐམ་པ །  བམ་པོ་ བཅུ་པ །"
msgstr ""

# སྡོམ་ནི། ཀ་ཙང་ཀ་ལ་ཀཻ་ནེ་ཡ། བག་མ་གཏོང་གཉིས་གཅོད་པ་གཉིས།
# །ཟ་<«ཞོལ་»ཟེ།>བ་དགའ་བྱེད་སྣག་བུ་དང་། །བུར་འདུ་ཤེས་དང་ཞིང་པ་དང་།
# །ཤི་<«གཡུང་»«པེ་»ཤིང་།>དང་ཀོ་ཀ་ལི་ཀ་དང་། །ཐང་བཅད་ཐ་མ་ཁམ་ཞེས་བྱ། །
msgctxt "5c619a75644545b3936a13847c06fd93"
msgid ""
"སྡོམ་ ནི །  ཀ་ ཙང་ ཀ་ལ་ ཀཻ་ ནེ་ ཡ །  བག་མ་ གཏོང་ གཉིས་ གཅོད་པ་ གཉིས ། ། ཟ་བ་"
" དགའ་བྱེད་ སྣག་ བུ་ དང་ ། ། བུར་ འདུ་ཤེས་ དང་ ཞིང་པ་ དང་ ། ། ཤི་ དང་ ཀོ་ "
"ཀ་ལི་ ཀ་ དང་ ། ། ཐང་ བཅད་ ཐ་མ་ ཁམ་ ཞེས་ བྱ ། །"
msgstr ""

msgctxt "3a759e64b16b42d0b0bb375ba2f94851"
msgid "ཀ་ ཙང་ ཀ་ལ་ ཞེས་ བྱ་བ་ ནི །"
msgstr ""

msgctxt "c9b7a416c0a94dfd8697c4005bd0b057"
msgid "བཅོམ་ལྡན་འདས་ ཡུལ་ ཨོ་ ཏ་ལ་ ན་ ཨོ་ ཏ་ལ␣འི་ ནགས་ཁྲོད་ ན་ བཞུགས་ སོ ། །"
msgstr ""

# དེ་ནས་བཅོམ་ལྡན་འདས་སྔ་དྲོ་ཤམ་ཐབས་<«ལི་»ཐམས།>དང་ཆོས་གོས་གསོལ་ཏེ<«ཅོ་»ཏོ།>།
msgctxt "11076cfc8d404f0d873aec3233e8e4e3"
msgid "དེ་ནས་ བཅོམ་ལྡན་འདས་ སྔ་དྲོ་ ཤམ་ཐབས་ དང་ ཆོས་གོས་ གསོལ་ ཏེ །"
msgstr ""

msgctxt "492f6a60d46e4adebffc9bfdfedee689"
msgid "ལྷུང་བཟེད་ བསྣམས་ ནས་"
msgstr ""

msgctxt "d1580ecd2aee4faa899b1f33611720d0"
msgid "ཡུལ་ ཨོ་ ཏ་ལ␣ར་ བསོད་སྙོམས་ ལ་ གཤེགས་ སོ ། །"
msgstr ""

# དེའི་ཚེ་བུད་མེད་ཅིག་<«གཡུང་»«ལི་»«པེ་»«སྣར་»«ཅོ་»«ཞོལ་»གཅིག>བུམ་པ་ཁྱེར་ཏེ་
msgctxt "e470bbab6f414040bf3620fa17f36a2a"
msgid "དེ␣འི་ ཚེ་ བུད་མེད་ ཅིག་ བུམ་པ་ ཁྱེར་ ཏེ་"
msgstr ""

# ཆུ་ཆུར་<«ཞོལ་»འཆུར།>སོང་བ་ལས་
msgctxt "4e17c6f229834a59ac0de10513e4cf0a"
msgid "ཆུ་ཆུ␣ར་ སོང་བ་ ལ␣ས་"
msgstr ""

# བུད་མེད་དེས་ཐག་རིང་པོ་ཞིག་ནས། བཅོམ་ལྡན་འདས་མཛེས་པ།
# དད་པ་སྐྱེ་བར་འགྱུར་བ་དབང་པོ་ཞི་བ་ཐུགས་ཞི་བ།
# ཐུགས་<«གཡུང་»«པེ་»-ཐུགས།>མཆོག་ཏུ་ཞི་བ་དང་ལྡན་པ་གསེར་གྱི་མཆོད་སྡོང་བཞིན་དུ་དཔལ་དང་གཟི་བྱིན་འབར་བ་མཐོང་ངོ་།
# །
msgctxt "29106b0bc70e48789b99e8f0feef7623"
msgid ""
"བུད་མེད་ དེ␣ས་ ཐག་ རིང་པོ་ ཞིག་ ནས །  བཅོམ་ལྡན་འདས་ མཛེས་པ །  དད་པ་ "
"སྐྱེ་བ␣ར་ འགྱུར་བ་ དབང་པོ་ ཞི་བ་ ཐུགས་ ཞི་བ །  ཐུགས་ མཆོག་ ཏུ་ ཞི་བ་ དང་ "
"ལྡན་པ་ གསེར་ གྱི་ མཆོད་སྡོང་ བཞིན་ དུ་ དཔལ་ དང་ གཟི་བྱིན་ འབར་བ་ མཐོང་ ངོ་ "
"། །"
msgstr ""

msgctxt "34b15c389c314a60ad50c3d66ab3b62d"
msgid "མཐོང་ ནས་ ཀྱང་ དེ་ བཅོམ་ལྡན་འདས་ ལ་ བུ␣འི་ འདུ་ཤེས་ སྐྱེས་ ནས་"
msgstr ""

msgctxt "0ffcb193bf2041c79979e77f03e42d3b"
msgid "དེ␣ས་ བུམ་པ་ དེ་ ཕྱོགས་ གཅིག་ ཏུ་ བཞག་ སྟེ །"
msgstr ""

# ལག་པ་གཉིས་བརྐྱང་<«གཡུང་»བརྒྱང་། «ཞོལ་»བརྐྱངས།>ནས་
msgctxt "2ba494f4d07044ffa2fe71c647b47303"
msgid "ལག་པ་ གཉིས་ བརྐྱང་ ནས་"
msgstr ""

msgctxt "0a6c69701a3f4ef7b015f97c99a3d6e2"
msgid "བཅོམ་ལྡན་འདས་ ག་ལ་བ་ དེ␣ར་ སོང་ སྟེ་"
msgstr ""

msgctxt "2b217380f0f44abbbb27d303de664be4"
msgid "སྨྲས་པ་"
msgstr ""

msgctxt "d2e632ff67274811a3800e5db8a2898f"
msgid "བུ་ དང་ ཕྲད་ དོ ། །"
msgstr ""

# བུ་<«གཡུང་»«པེ་»+མ།>དང་ཕྲད་དོ་ཞེས་
msgctxt "568262b32af041a3aed1c85e731e64b2"
msgid "བུ་ དང་ ཕྲད་ དོ་ ཞེས་"
msgstr ""

msgctxt "c1ab6dae446c4327a8da5c795653e5e1"
msgid "ཟེར་ ཞིང་ །"
msgstr ""

msgctxt "544794a61116407dbbb3c8f6ad02b8d5"
msgid "བཅོམ་ལྡན་འདས་ ཀྱི་ མགུལ་ ནས་ འཁྱུད་པ␣ར་ འདོད་པ་ ལ␣ས་"
msgstr ""

msgctxt "953ac21d6051450bbdc81c9f77273f1f"
msgid "དགེ་སློང་ རྣམས་ ཀྱིས་ མཐོང་ ནས་"
msgstr ""

# བཀག་གོ<«གཡུང་»-གོ།>། །
msgctxt "fc00acaf752f446fb4c0df87407803b5"
msgid "བཀག་ གོ ། །"
msgstr ""

msgctxt "5a30b5f6028d47178e8d42185f546dd7"
msgid "དེ་ནས་ དེ␣ར་ བཅོམ་ལྡན་འདས་ ཀྱིས་ དགེ་སློང་ རྣམས་ ལ་ བཀའ་ སྩལ་པ །"
msgstr ""

msgctxt "630ee2faeb3849bdba67ee8e127646e0"
msgid "དགེ་སློང་ དག་ བུད་མེད་ འདི་ མ་ དགག་ ཅིག  །"
msgstr ""

# གལ་ཏེ་བུད་མེད་འདི་དེ་བཞིན་གཤེགས་པའི་མགུལ་ནས་འཁྱུད་<«གཡུང་»ཁྱུད།>དུ་མ་བཅུག་ན།
msgctxt "f21298db141f421682306174e2a26f7c"
msgid ""
"གལ་ཏེ་ བུད་མེད་ འདི་ དེ་བཞིན་ གཤེགས་པ␣འི་ མགུལ་ ནས་ འཁྱུད་ དུ་མ་ བཅུག་ ན །"
msgstr ""

msgctxt "47559750385b4491a6e95aae82271d3e"
msgid "ཁ་ ནས་ ཁྲག་དྲོན་ མོ་ སྐྱུགས་ ཏེ་ འཆི་བ␣ར་ འགྱུར་ རོ ། །"
msgstr ""

msgctxt "6df099175f414d5cafcff70196c2a909"
msgid "དེ་ནས་ དགེ་སློང་ རྣམས་ ཀྱིས་ དེ་ ཐོས་ ནས་ མ་ བཀག་པ␣ར་ བཞག་ གོ ། །"
msgstr ""

# དེ་ནས་དེས་བཅོམ་ལྡན་འདས་ཀྱི་མགུལ་ནས་འཁྱུད་<«གཡུང་»མཁྱུད། «ཁུ་»ཁྱུད།>ནས་
msgctxt "6c6357136f2448ab947461ca1ea697f9"
msgid "དེ་ནས་ དེ␣ས་ བཅོམ་ལྡན་འདས་ ཀྱི་ མགུལ་ ནས་ འཁྱུད་ ནས་"
msgstr ""

msgctxt "9a281da353554f32a708cc6548e563fa"
msgid "ཆོས་ མཉན་པ␣འི་ ཕྱིར་ སྤྱན་སྔ␣ར་ འདུག་ གོ ། །"
msgstr ""

# དེ་ནས་བཅོམ་ལྡན་འདས་ཀྱིས་དེའི་བསམ་<«ཅོ་»བསམས།>པ་དང་།
# བག་ལ་ཉལ་དང་ཁམས་དང་རང་བཞིན་ཐུགས་སུ་ཆུད་ནས་
msgctxt "f533a35d9c204382afc8c54dedaa1eeb"
msgid ""
"དེ་ནས་ བཅོམ་ལྡན་འདས་ ཀྱིས་ དེ␣འི་ བསམ་པ་ དང་ །  བག་ ལ་ ཉལ་ དང་ ཁམས་ དང་ "
"རང་བཞིན་ ཐུགས་ སུ་ ཆུད་ ནས་"
msgstr ""

msgctxt "2d23027025c544de820b0746b3d89d7d"
msgid "དེ་ དང་ འཐུན་པ␣འི་ ཆོས་ བསྟན་ ཏེ །"
msgstr ""

# :དེ་ནས་<«གཡུང་»«ལི་»«པེ་»«སྣར་»«ཅོ་»དེས་ན།>བུད་མེད་དེས་སྟན་དེ་ཁོ་ན་ལ་འདུག་བཞིན་དུ་འཇིག་ཚོགས་<«ཅོ་»ཙོགས།>ལ་ལྟ་བའི་རིའི་རྩེ་མོ་མཐོན་པོ་ཉི་ཤུ་ཡེ་ཤེས་རྡོ་རྗེས་བཅོམ་ནས།
msgctxt "5895d56720154fcca3271636abdccf86"
msgid ""
"དེ་ནས་ བུད་མེད་ དེ␣ས་ སྟན་ དེ་ ཁོ་ན་ ལ་ འདུག་ བཞིན་ དུ་ འཇིག་ཚོགས་ ལ་ "
"ལྟ་བ␣འི་ རི␣འི་ རྩེ་མོ་ མཐོན་པོ་ ཉི་ཤུ་ ཡེ་ཤེས་ རྡོ་རྗེ␣ས་ བཅོམ་ ནས །"
msgstr ""

msgctxt "2e06cd46542b484d94720766d923ce0c"
msgid "རྒྱུན་ དུ་ ཞུགས་པ␣འི་ འབྲས་བུ་ མངོན་སུམ་ དུ་ བྱས་ ཏེ །"
msgstr ""

msgctxt "033d411d0f93415bb9e7f04d2c7f52b1"
msgid "དེ␣ས་ བདེན་པ་ མཐོང་ ནས་"
msgstr ""

msgctxt "b726dace4b2041de8cbf278fc5b7e20c"
msgid "སྟན་ ལ␣ས་ ལངས་ ཏེ །"
msgstr ""

msgctxt "bb47f6bce1944b3c91af3ee0a1b8ccf1"
msgid "བླ་གོས་ ཕྲག་པ་ གཅིག་ ཏུ་ གཟར་ ནས །"
msgstr ""

# བཅོམ་ལྡན་འདས་ག་ལ་བ་དེ་<«གཡུང་»«པེ་»དེར།>ལོགས་སུ་ཐལ་མོ་སྦྱར་ཏེ་
msgctxt "17de2bc9131141dd8862bd7515b6f2a2"
msgid "བཅོམ་ལྡན་འདས་ ག་ལ་བ་ དེ་ ལོགས་ སུ་ ཐལ་མོ་ སྦྱར་ ཏེ་"
msgstr ""

msgctxt "0d4be9a8e9bf452cbacb3c42de9f562d"
msgid "བཅོམ་ལྡན་འདས་ ལ་ འདི་སྐད་ ཅེས་ གསོལ་ ཏོ ། །"
msgstr ""

# བཙུན་པ་བདག་ལེགས་པར་གསུངས་པའི་ཆོས་འདུལ་བ་ལ་རབ་ཏུ་འབྱུང་<«གཡུང»«པེ་»«ཞོལ་»བྱུང་།>བ་དང་བསྙེན་པར་རྫོགས་ཤིང་དགེ་སློང་མའི་དངོས་པོ་འཐོབ་ཏུ་རུང་ན།
msgctxt "e0dcfc9d2dbe4ebaae917b92b698dcb3"
msgid ""
"བཙུན་པ་ བདག་ ལེགས་པ␣ར་ གསུངས་པ␣འི་ ཆོས་ འདུལ་བ་ ལ་ རབ་ཏུ་ འབྱུང་བ་ དང་ "
"བསྙེན་པ␣ར་ རྫོགས་ ཤིང་ དགེ་སློང་ མ␣འི་ དངོས་པོ་ འཐོབ་ ཏུ་ རུང་ ན །"
msgstr ""

msgctxt "a9c9f55cae80432b8d67a8563912ac1f"
msgid ""
"བདག་ ཀྱང་ བཅོམ་ལྡན་འདས་ ཀྱི་ ཐད་ དུ་ ཚངས་པ␣ར་ སྤྱོད་པ་ སྤྱད་པ␣ར་ འཚལ་ ལོ ། །"
msgstr ""

# དེ་ནས་བཅོམ་ལྡན་འདས་ཀྱིས་སྐྱེ་དགུའི་<«ཞོལ་»རྒུའི།>བདག་མོ་ཆེན་མོ་གཽ་ཏ་མཱི་<«གཡུང་»«ལི་»«ཅོ་»མའི།
# «སྣར་»«ཞོལ་»མ།>ལ་གཏད་ནས།
msgctxt "e99f805e9b4947b186c179cb9b22cb21"
msgid ""
"དེ་ནས་ བཅོམ་ལྡན་འདས་ ཀྱིས་ སྐྱེ་དགུ␣འི་ བདག་མོ་ ཆེན་ མོ་ གཽ་ ཏ་ མཱི་ ལ་ གཏད་"
" ནས །"
msgstr ""

# དེས་དེ་རབ་ཏུ་ཕྱུང་<«སྣར་»བྱུང་།>སྟེ་
msgctxt "a9c714c52ce74400a36be942495ca10e"
msgid "དེ␣ས་ དེ་ རབ་ཏུ་ ཕྱུང་ སྟེ་"
msgstr ""

msgctxt "c28332fa5f024a8da279707512d0f9ce"
msgid "བསྙེན་པ␣ར་ རྫོགས་པ␣ར་ བྱས་ ནས་"
msgstr ""

# དེ་ལ་ལུང་ཡང་<«གཡུང་»«པེ་»-ཡང་།->ཕོག་གོ། །
msgctxt "26ae7f3cb7ac414cbf49db672134508d"
msgid "དེ་ ལ་ལུང་ ཡང་ ཕོག་ གོ ། །"
msgstr ""

# དེས་ཀྱང་བརྩོན་པ་དང་བསྒྲུབ་<«ཞོལ་»སྒྲུབ།>པ་དང་འབད་པས་ཉོན་མོངས་པ་ཐམས་ཅད་སྤངས་ནས་
msgctxt "195111abfd974f5fa2800dfef46505ec"
msgid ""
"དེ␣ས་ ཀྱང་ བརྩོན་པ་ དང་ བསྒྲུབ་པ་ དང་ འབད་པ␣ས་ ཉོན་མོངས་པ་ ཐམས་ཅད་ སྤངས་ ནས་"
msgstr ""

msgctxt "62b00aed482e4fe1978ab028f50b2a88"
msgid "དགྲ་བཅོམ་པ་ ཉིད་ མངོན་སུམ་ དུ་ བྱས་ ཏེ །"
msgstr ""

# དེས་<«ཞོལ་»དེ།>དགྲ་བཅོམ་མ་<«ཅོ་»«ཞོལ་»པ།>ཁམས་གསུམ་པའི་འདོད་ཆགས་དང་བྲལ་བར་གྱུར་ནས།
msgctxt "0aeb3e361728435c897885c2cce02103"
msgid "དེ␣ས་ དགྲ་བཅོམ་ མ་ ཁམས་ གསུམ་པ␣འི་ འདོད་ཆགས་ དང་ བྲལ་བ␣ར་ གྱུར་ ནས །"
msgstr ""

msgctxt "cec539981f304ead9fb69024097fdbcc"
msgid "གསེར་ དང་ བོང་བ␣ར་ མཉམ་པ །"
msgstr ""

msgctxt "f34b4a916c254c60a97e6e44242a0815"
msgid "ནམ་མཁའ་ དང་ ལག་མཐིལ་ དུ་ འདྲ་བ␣འི་ སེམས་ དང་ ལྡན་པ །"
msgstr ""

# ཙན་དན་<«གཡུང་»ན།>སྦངས་པ་ལྟ་བུར་བསིལ་བར་<«གཡུང་»རབ།>གྱུར་པ<«གཡུང་»«པེ་»+མ།>།
msgctxt "534f52bb43ee40a2bc1228d34fa2e6ed"
msgid "ཙན་དན་ སྦངས་པ་ ལྟ་བུ␣ར་ བསིལ་བ␣ར་ གྱུར་པ །"
msgstr ""

msgctxt "c60e418316404b7cba1e36ae6d011c3f"
msgid "རིག་པ␣ས་ སྒོ་ང␣འི་ སྦུབས་ བཅོམ་པ །"
msgstr ""

msgctxt "a1d03c238d464c879aca967cb1654bc8"
msgid "རིག་པ་ དང་ མངོན་པར་ ཤེས་པ་ དང་ སོ་སོ་ ཡང་དག་པ␣ར་ རིག་པ་ ཐོབ་པ །"
msgstr ""

msgctxt "9ea2f8eba790433fb00baf187c1d0354"
msgid "སྲིད་པ␣འི་ རྙེད་པ་ དང་ ཆགས་པ་ དང་ བཀུར་སྟི་ ལ་ མི་ལྟ་བ །"
msgstr ""

# དབང་པོ་དང་ཉེ་དབང་དང་བཅས་པའི་ལྷ་རྣམས་ཀྱིས་མཆོད་ཅིང་རྗེད་<«གཡུང་»«པེ་»«སྣར་»བརྗེད།>པ་དང་གུས་པར་སྨྲ་བའི་གནས་སུ་གྱུར་ཏོ།
# །
msgctxt "d1ca5ddc6cac42a6a51007a6dbdabf8a"
msgid ""
"དབང་པོ་ དང་ ཉེ་དབང་ དང་ བཅས་པ␣འི་ ལྷ་ རྣམས་ ཀྱིས་ མཆོད་ ཅིང་ རྗེད་པ་ དང་ "
"གུས་པ␣ར་ སྨྲ་བ␣འི་ གནས་ སུ་ གྱུར་ ཏོ ། །"
msgstr ""

# དེ་[120b]ནས་དེ་<«ཅོ་»-དེ།>བཅོམ་ལྡན་འདས་ཀྱིས་མཆོག་ཏུ་ལུང་བསྟན་ཏེ།
msgctxt "fd4efe0b61b7419596ab4996e1e19baf"
msgid "དེ་ནས་ དེ་ བཅོམ་ལྡན་འདས་ ཀྱིས་ མཆོག་ ཏུ་ ལུང་བསྟན་ ཏེ །"
msgstr ""

msgctxt "140b6b195f574f4b99dc4bb956d2e666"
msgid "དགེ་སློང་ དག་"
msgstr ""

msgctxt "e442705c86744bf698f9f1b3503b1305"
msgid ""
"ང␣འི་ ཉན་ཐོས་ ཀྱི་ དགེ་སློང་ མ་ མདོ་ རྣམ་པ␣ར་ འབྱེད་པ་ རྣམས་ ཀྱི་ མཆོག་ ནི་ "
"འདི་ལྟ་སྟེ །"
msgstr ""

msgctxt "a88263d0ede445a4b99f7cc855bd64fa"
msgid "དགེ་སློང་ མ་ ཀ་ ཙང་ ཀ་ལ་ འདི་ ཡིན་ ནོ་ ཞེས་"
msgstr ""

msgctxt "c848798b49c04e64b8bfbe05be6b9f1d"
msgid "ལུང་བསྟན་ ཏོ ། །"
msgstr ""

msgctxt "cdb393c5432e4e8f941ea02a2ba6326e"
msgid "དེ་ནས་ དགེ་སློང་ དག་ གིས་ སངས་རྒྱས་ བཅོམ་ལྡན་འདས་ ལ་ གསོལ་པ །"
msgstr ""

msgctxt "d859784efad643459bca0337ae66e19f"
msgid "བཙུན་པ་"
msgstr ""

# གཟིགས་ལགས་སམ་<«གཡུང་»«ལི་»«པེ་»«སྣར་»«ཅོ་»-སམ།>
msgctxt "cc0e6869cf334bf5a0e35ddf662b347d"
msgid "གཟིགས་ ལགས་ སམ་"
msgstr ""

# བུད་མེད་སྟོང་ཕྲག་དུ་མས་བཅོམ་ལྡན་འདས་མཐོང་ན་སུས་ཀྱང་བཅོམ་ལྡན་འདས་མཐོང་བ་<«གཡུང་»«པེ་»-བ།>ན་འདི་ལྟར་མ་བརྒྱུགས་<«གཡུང་»«པེ་»རྒྱུགས།>ལགས་སོ།
# །
msgctxt "1fcc59a7de4542d08bafc64e1b97ab39"
msgid ""
"བུད་མེད་ སྟོང་ཕྲག་ དུ་མ␣ས་ བཅོམ་ལྡན་འདས་ མཐོང་ ན་ སུ␣ས་ ཀྱང་ བཅོམ་ལྡན་འདས་ "
"མཐོང་བ་ ན་ འདི་ལྟར་ མ་ བརྒྱུགས་ ལགས་སོ ། །"
msgstr ""

msgctxt "3af52f995b974c2d9a2687bc475665e6"
msgid "བཅོམ་ལྡན་འདས་ ཀྱིས་ བཀའ་ སྩལ་པ །"
msgstr ""

# དགེ་སློང་དག་ཀ་ཙང་ཀ་ལ་འདི་ནི་ངའི་འདས་མ་<«གཡུང་»-མ།>ཐག་པའི་ཚེ་རབས་ལྔ་བརྒྱར་<«ལི་»«སྣར་»བརྒྱ།>མར་གྱུར་པས་ན།
msgctxt "3e39685e7de9461db0aa843413518f76"
msgid ""
"དགེ་སློང་ དག་ ཀ་ ཙང་ ཀ་ལ་ འདི་ ནི་ ང␣འི་ འདས་ མ་ ཐག་པ␣འི་ ཚེ་རབས་ ལྔ་ "
"བརྒྱ␣ར་ མར་ གྱུར་པ␣ས་ ན །"
msgstr ""

msgctxt "51b81737034548bebb9148cdc37c3fef"
msgid ""
"འདི་ ནི་ སྔོན་ གྱི་ བག་ཆགས་ ཀྱི་ གཞི་ ཡོད་པ␣འི་ ཕྱིར་ ང་ མཐོང་ ན་ རྒྱུག་ གོ "
"། །"
msgstr ""

msgctxt "df5f3f09488b45acb2513fa0e77b7ac4"
msgid "དགེ་སློང་ དག་ གིས་ གསོལ་པ །"
msgstr ""

msgctxt "045763bb2ad84a47acef1353448a08b7"
msgid "བཙུན་པ་"
msgstr ""

# གལ་ཏེ་འདི་བཅོམ་ལྡན་འདས་ཀྱི་<«གཡུང་»«ལི་»«པེ་»«སྣར་»«ཅོ་»«ཞོལ་»ཀྱིས།>འདས་མ་ཐག་པའི་ཚེ་རབས་ལྔ་བརྒྱར་ཡུམ་དུ་གྱུར་པ་ལགས་ན།
msgctxt "9be80ef158b74c94b28e47671b99e49a"
msgid ""
"གལ་ཏེ་ འདི་ བཅོམ་ལྡན་འདས་ ཀྱི་ འདས་ མ་ ཐག་པ␣འི་ ཚེ་རབས་ ལྔ་ བརྒྱ␣ར་ ཡུམ་ དུ་"
" གྱུར་པ་ ལགས་ ན །"
msgstr ""

# ཅིའི་སླད་དུ་ད་ལྟར་གྱི་<«ལི་»«སྣར་»«ཅོ་»གྱིས།>ཡུམ་དུ་མ་གྱུར་ལགས།
msgctxt "bdde0cee36e349daa74f08b24e84702e"
msgid "ཅིའི་སླད་དུ་ ད་ལྟར་ གྱི་ ཡུམ་ དུ་མ་ གྱུར་ ལགས །"
msgstr ""

msgctxt "d6bf4493da124c3d8e350620c746fccc"
msgid "བཅོམ་ལྡན་འདས་ ཀྱིས་ བཀའ་ སྩལ་པ །"
msgstr ""

msgctxt "07cf50ffcbdc4374a7e22297b69b3ea0"
msgid ""
"དགེ་སློང་ དག་ རྒྱུ་གཉིས་ དང་ རྐྱེན་ གཉིས་ ཀྱིས་ ན་ འདི་ ང␣འི་ ད་ལྟར་ གྱི་ "
"མར་ མ་ གྱུར་ ཏེ །"
msgstr ""

msgctxt "4c504509c8a842d0a35770fe80bff7a2"
msgid "ལྷ་མོ་ སྒྱུ་འཕྲུལ་ ཆེན་ མོས་ སྨོན་ལམ་ བཏབ་ སྟེ །"
msgstr ""

# :ཀྱེ་མ་<«ལི་»«ཅོ་»-ཀྱེ་མ།>བདག་སངས་རྒྱས་ཀྱི་ཡུམ་དུ་གྱུར་ཅིག་ཅེས་
msgctxt "77b1efaf60784537a71b842740751d06"
msgid "ཀྱེ་མ་ བདག་ སངས་རྒྱས་ ཀྱི་ ཡུམ་ དུ་ གྱུར་ཅིག་ ཅེས་"
msgstr ""

# བྱས་པ་དེ་དང་། ང་ཡང་འདིས་ཡིད་བྱུང་<«ཞོལ་»ཕྱུང་།>བར་བྱས་ནས་
msgctxt "f0a27d48f7b145a3b4409fa9b1ed36d4"
msgid "བྱས་པ་ དེ་ དང་ །  ང་ ཡང་ འདི␣ས་ ཡིད་ བྱུང་བ␣ར་ བྱས་ ནས་"
msgstr ""

msgctxt "5e7c42bd077044118372e6170a2c65bf"
msgid "ངས་ ཀྱང་ སྨོན་ལམ་ བཏབ་ སྟེ །"
msgstr ""

# ཕྱིན་ཅད་<«སྣར་»«ཞོལ་»ཆེད།>འདི་བདག་གི་མར་མ་<«གཡུང་»-མ།>གྱུར་ཅིག་ཅེས་
msgctxt "55672dc906864c3780ca6b83408496b0"
msgid "ཕྱིན་ ཅད་ འདི་ བདག་ གི་ མར་ མ་ གྱུར་ཅིག་ ཅེས་"
msgstr ""

msgctxt "aad81f13773d400ab30a9e581d21e724"
msgid "བྱས་པ␣འི་ ཕྱིར་ རོ ། །"
msgstr ""

msgctxt "7d3c29ea5f4d4688ad2470084fa864bd"
msgid "གསོལ་པ །"
msgstr ""

# འདིས་ཅི་<«ཞོལ་»ཇི།>ལྟར་བཅོམ་ལྡན་འདས་ཐུགས་བྱུང་<«ཞོལ»ཕྱུང་།>བར་བགྱིས་ལགས།
msgctxt "d2fd219282154bfbb686298d830f2130"
msgid "འདི␣ས་ ཅི་ ལྟར་ བཅོམ་ལྡན་འདས་ ཐུགས་ བྱུང་བ␣ར་ བགྱིས་ ལགས །"
msgstr ""

msgctxt "d71b22fa2bd547f1b095eee1b87a7196"
msgid "བཀའ་ སྩལ་པ །"
msgstr ""

msgctxt "e6811cc591c846bd87cc2a5436572ef4"
msgid ""
"དགེ་སློང་ དག་ བྱང་ཆུབ་ སེམས་དཔའ་ རྣམས་ ནི་ ངེས་པར་ འབྱུང་བ་ ལ་ དགའ་བ་ དང་ "
"སྦྱིན་པ་ ལ་ དགའ་བ་ ཡིན་ ན །  འདི␣ས་ ཐམས་ཅད་ དུ་ ང␣འི་ ངེས་པར་ འབྱུང་བ་ དང་ "
"སྦྱིན་པ␣འི་ བར་ཆད་ བྱས་ ཏེ་"
msgstr ""

# གཞི་དེས་ན་འདིས་ང་ཡིད་བྱུང་<«ཞོལ»ཕྱུང་།>བར་བྱས་སོ། །
msgctxt "dbd32f1d5a1c4fc2a38e823c606369e7"
msgid "གཞི་ དེས་ན་ འདི␣ས་ ང་ ཡིད་ བྱུང་བ␣ར་ བྱས་ སོ ། །"
msgstr ""

msgctxt "a5aac65e562a4fd1a164130fd13e0a73"
msgid "དེ་ནས་ དགེ་སློང་ དག་ གིས་ བཅོམ་ལྡན་འདས་ ལ་ ཞུས་པ །"
msgstr ""

msgctxt "1eaf35618738475ca486ca4f7d3434ca"
msgid ""
"བཙུན་པ་ དགེ་སློང་ མ་ ཀ་ ཙང་ ཀ་ལ␣ས །  ལ␣ས་ ཅི་ བགྱིས་ ན་ ལ␣ས་ དེ␣འི་ རྣམ་པ␣ར་"
" སྨིན་པ␣ས །  དབུལ་ མོ␣ར་ གྱུར་ ལ་"
msgstr ""

msgctxt "38b19bea70e54b688dbe0db6a98936e6"
msgid "རྒས་ ནས་ གདོད་ རབ་ཏུ་ བྱུང་ ལགས །"
msgstr ""

msgctxt "9096c1ce62cf442ea057370a8d1fc5cc"
msgid "བཅོམ་ལྡན་འདས་ ཀྱིས་ བཀའ་ སྩལ་པ །"
msgstr ""

msgctxt "940775671a914d41a6531063bff28b5c"
msgid ""
"དགེ་སློང་ དག་ བྱང་ཆུབ་ སེམས་དཔའ་ རྣམས་ ནི་ ངེས་པར་ འབྱུང་བ་ ལ་ དགའ་བ་ དང་ "
"སྦྱིན་པ་ ལ་ དགའ་བ་ ཡིན་ ན །  འདི␣ས་ ང␣འི་ ཚེ་རབས་ བགྲངས་པ␣ར་ ངེས་པར་ "
"འབྱུང་བ་ དང་ སྦྱིན་པ␣འི་ བར་ཆད་ བྱས་པ␣ས་ ན །"
msgstr ""

# ལས་དེའི་རྣམ་པར་སྨིན་པས་ད་ལྟར་དབུལ་མོར་<«ཞོལ་»མོ།>གྱུར་ལ་
msgctxt "0bd55a1fb887490ba0b0520d916b42ac"
msgid "ལ␣ས་ དེ␣འི་ རྣམ་པ␣ར་ སྨིན་པ␣ས་ ད་ལྟར་ དབུལ་ མོ␣ར་ གྱུར་ ལ་"
msgstr ""

msgctxt "e42c4061a3404369beeb2829ce7ed5cd"
msgid "རྒས་ ནས་ གདོད་ རབ་ཏུ་ བྱུང་ ངོ་ ། །"
msgstr ""

# བཙུན་པ་དགེ་སློང་མ་ཀ་ཙང་ཀ་ལས།
# ལས་ཅི་བགྱིས་ན་ལས་དེའི་རྣམ་པར་སྨིན་པས་ད་ལྟར་བཅོམ་ལྡན་འདས་ཀྱིས་མདོ་རྣམ་པར་འབྱེད་པ་རྣམས་ཀྱི་<«གཡུང་»«ལི་»«པེ་»«སྣར་»«ཅོ་»ཀྱིས།>མཆོག་ཏུ་བསྟན་ལ།
msgctxt "aec37951bcd44718b002c5db35f10e42"
msgid ""
"བཙུན་པ་ དགེ་སློང་ མ་ ཀ་ ཙང་ ཀ་ལ␣ས །  ལ␣ས་ ཅི་ བགྱིས་ ན་ ལ␣ས་ དེ␣འི་ རྣམ་པ␣ར་"
" སྨིན་པ␣ས་ ད་ལྟར་ བཅོམ་ལྡན་འདས་ ཀྱིས་ མདོ་ རྣམ་པ␣ར་ འབྱེད་པ་ རྣམས་ ཀྱི་ "
"མཆོག་ ཏུ་ བསྟན་ ལ །"
msgstr ""

msgctxt "db2e4ea5edf449e48f6ef823a6b2e8a7"
msgid "འདི␣ས་ བཅོམ་ལྡན་འདས་ མཉེས་པ␣ར་ བགྱིས་ ཏེ །"
msgstr ""

msgctxt "551197345c2941998ab9983d110eb98a"
msgid "མི་ མཉེས་པ␣ར་ མ་ བགྱིས་ ལ་"
msgstr ""

msgctxt "d2d4ca5055664cddad54403a0b8c839d"
msgid "བཅོམ་ལྡན་འདས་ ཀྱི་ བསྟན་པ་ ལ་ ཡང་ རབ་ཏུ་ བྱུང་ ནས་"
msgstr ""

msgctxt "8f570221f10742ef9b3be2dc3a397489"
msgid "ཉོན་མོངས་པ་ ཐམས་ཅད་ སྤངས་ ཏེ་"
msgstr ""

msgctxt "1d0fe500e7f64a7c9759824e3e8cbf73"
msgid "དགྲ་བཅོམ་པ་ ཉིད་ མངོན་སུམ་ དུ་ བགྱིས་ ལགས །"
msgstr ""

msgctxt "8066e33ea7f44333a87dc8e3827cf7ca"
msgid "བཅོམ་ལྡན་འདས་ ཀྱིས་ བཀའ་ སྩལ་པ །"
msgstr ""

msgctxt "12746ec6fdff447a9af5e388b9ce1271"
msgid "སྨོན་ལམ་ གྱི་ དབང་ ལ␣ས་ གྱུར་ ཏོ ། །"
msgstr ""

msgctxt "647483f204df44c6ad4d77e9a7dbf671"
msgid "བཙུན་པ་ འདི␣ས་ གང་ དུ་ སྨོན་ལམ་ བཏབ་ ལགས །"
msgstr ""

# དགེ་སློང་དག་སྔོན་བྱུང་བ་འདས་པའི་དུས་ན།
# བསྐལ་པ་བཟང་པོ་འདི་ཉིད་ལ་སྐྱེ་དགུའི་<«ཞོལ་»རྒུའི།>ཚེ་ལོ་ཉི་ཁྲི་ཐུབ་པ་ན་ཡང་དག་པར་རྫོགས་པའི་སངས་རྒྱས་རིག་པ་དང་ཞབས་སུ་ལྡན་པ།
# བདེ་བར་གཤེགས་པ། འཇིག་རྟེན་མཁྱེན་པ།
# སྐྱེས་བུ་གདུལ་བྱའི་<«གཡུང་»«པེ་»«ཞོལ་»འདུལ་བའི།>ཁ་ལོ་སྒྱུར་བ།
# བླ་ན་མེད་པ་ལྷ་དང་མི་རྣམས་ཀྱི་སྟོན་པ་སངས་རྒྱས་བཅོམ་ལྡན་འདས་འོད་སྲུང་ཞེས་བྱ་བ་འཇིག་རྟེན་དུ་བྱུང་སྟེ།
msgctxt "daceb5fd667841969978267c98ecffc8"
msgid ""
"དགེ་སློང་ དག་ སྔོན་བྱུང་བ་ འདས་པ␣འི་ དུས་ ན །  བསྐལ་པ་ བཟང་པོ་ འདི་ཉིད་ ལ་ "
"སྐྱེ་དགུ␣འི་ ཚེ་ལོ་ ཉི་ཁྲི་ ཐུབ་པ་ ན་ ཡང་དག་པ␣ར་ རྫོགས་པ␣འི་ སངས་རྒྱས་ "
"རིག་པ་ དང་ ཞབས་ སུ་ ལྡན་པ །  བདེ་བ␣ར་ གཤེགས་པ །  འཇིག་རྟེན་ མཁྱེན་པ །  "
"སྐྱེས་བུ་ གདུལ་བྱ␣འི་ ཁ་ལོ་ སྒྱུར་བ །  བླ་ ན་ མེད་པ་ ལྷ་ དང་ མི་ རྣམས་ ཀྱི་ "
"སྟོན་པ་ སངས་རྒྱས་ བཅོམ་ལྡན་འདས་ འོད་སྲུང་ ཞེས་ བྱ་བ་ འཇིག་རྟེན་ དུ་ བྱུང་ "
"སྟེ །"
msgstr ""

msgctxt "470500fc0130488fa9c50fd703b5b2bd"
msgid "དེ␣འི་ གསུང་རབ་ ལ་ འདི་ རབ་ཏུ་ བྱུང་ ནས །"
msgstr ""

msgctxt "238125c96d5643d98a3265c2bb027d40"
msgid ""
"གང་ ལ␣ས་ རབ་ཏུ་ བྱུང་བ␣འི་ མཁན་མོ་ དེ་ཡང་ དག་པ␣ར་ རྫོགས་པ␣འི་ སངས་རྒྱས་ "
"འོད་སྲུང་ གིས་ མདོ་ རྣམ་པ␣ར་ འབྱེད་པ་ རྣམས་ ཀྱི་ མཆོག་ ཏུ་ བསྟན་ ཏེ །"
msgstr ""

# དེས་<«གཡུང་»«ལི་»«པེ་»«སྣར་»«ཅོ་»«ཞོལ་»-དེས།>དེར་འདིས་ཚེ་གཅིག་ཏུ་ཚངས་པར་སྤྱོད་པ་སྤྱད་ནས་
msgctxt "e7ea08de263040e189a44cdc66dd3e8d"
msgid "དེ␣ས་ དེ␣ར་ འདི␣ས་ ཚེ་ གཅིག་ ཏུ་ ཚངས་པ␣ར་ སྤྱོད་པ་ སྤྱད་ ནས་"
msgstr ""

# འཆི་ཀར་<«ཞོལ་»ཁར།>སྨོན་ལམ་བཏབ་པ།
msgctxt "f3ac1e5fee074e6bbc1af901ad56f4d7"
msgid "འཆི་ཀ␣ར་ སྨོན་ལམ་ བཏབ་པ །"
msgstr ""

msgctxt "856dc5e53b504a20847b38ad21e1890f"
msgid "འདི་ལྟར་"
msgstr ""

# བདག་གིས་ཡང་དག་པར་རྫོགས་པའི་སངས་རྒྱས་འོད་སྲུང་གི་བསྟན་པ་ལ།
# ཚེ་གཅིག་ཏུ་ཚངས་པར་སྤྱོད་པ་སྤྱད་ཀྱང་ཡོན་ཏན་གྱི་ཚོགས་ཅི་ཡང་མ་ཐོབ་ཀྱིས<«ཞོལ་»ཀྱི།>།
msgctxt "744873077e0e41f08dba5b3980c97cde"
msgid ""
"བདག་ གིས་ ཡང་དག་པ␣ར་ རྫོགས་པ␣འི་ སངས་རྒྱས་ འོད་སྲུང་ གི་ བསྟན་པ་ ལ །  ཚེ་ "
"གཅིག་ ཏུ་ ཚངས་པ␣ར་ སྤྱོད་པ་ སྤྱད་ ཀྱང་ ཡོན་ཏན་ གྱི་ ཚོགས་ ཅི་ཡང་ མ་ ཐོབ་ "
"ཀྱིས །"
msgstr ""

# ཡང་དག་པར་རྫོགས་པའི་སངས་རྒྱས་འོད་སྲུང་གིས་བྲམ་ཟེའི་ཁྱེའུ་བླ་མ་ལུང་བསྟན་པ་གང་ཡིན་པའི་སངས་རྒྱས་བཅོམ་ལྡན་འདས་དེ་བདག་གིས་མཉེས་པར་བྱེད་པར་གྱུར་<«ཅོ་»-གྱུར།>ཅིག
# །
msgctxt "a02d4aa3116b4968bd30fafadc80254b"
msgid ""
"ཡང་དག་པ␣ར་ རྫོགས་པ␣འི་ སངས་རྒྱས་ འོད་སྲུང་ གིས་ བྲམ་ཟེ␣འི་ ཁྱེའུ་ བླ་མ་ "
"ལུང་བསྟན་པ་ གང་ཡིན་པ␣འི་ སངས་རྒྱས་ བཅོམ་ལྡན་འདས་ དེ་ བདག་ གིས་ མཉེས་པ␣ར་ "
"བྱེད་པ␣ར་ གྱུར་ཅིག  །"
msgstr ""

# མི་མཉེས་པར་<«ལི་»«ཅོ་»པ།>བྱེད་པར་མ་གྱུར་ཅིག །
msgctxt "16f06272ee0047b98c9ad86dd6fc92ee"
msgid "མི་ མཉེས་པ␣ར་ བྱེད་པ␣ར་ མ་ གྱུར་ཅིག  །"
msgstr ""

# དེ་ཁོ་ནའི་བསྟན་པ་ལ་རབ་ཏུ་བྱུང་<«ལི་»གྱུང་།>ནས་
msgctxt "3dbd6cd6d95b48c8a97bff563f295a7a"
msgid "དེ་ ཁོ་ན␣འི་ བསྟན་པ་ ལ་ རབ་ཏུ་ བྱུང་ ནས་"
msgstr ""

msgctxt "c7f0b86cf00c49e9a18cee56ccd8b285"
msgid "ཉོན་མོངས་པ་ ཐམས་ཅད་ སྤངས་ ཏེ་"
msgstr ""

msgctxt "7207a9318ccf4b58b1e1359230f8a4b1"
msgid "དགྲ་བཅོམ་པ་ ཉིད་ མངོན་སུམ་ དུ་ བྱེད་པ␣ར་ གྱུར་ཅིག  །"
msgstr ""

# ཡང་དག་པར་རྫོགས་པའི་སངས་རྒྱས་འོད་སྲུང་གིས་བདག་གི་མཁན་མོ་<«པེ་»པོ།>ཇི་ལྟར་མདོ་རྣམ་པར་འབྱེད་པ་རྣམས་ཀྱི་མཆོག་ཏུ་བསྟན་པ་དེ་བཞིན་དུ།
# བདག་ཀྱང་ཤཱཀྱ་ཐུབ་པ་ཤཱཀྱའི་<«མི་»ཤཱཀྱིའི།>རྒྱལ་པོའི་མཆོག་དེས་མདོ་རྣམ་པར་འབྱེད་པ་རྣམས་ཀྱི་མཆོག་ཏུ་བསྟན་པར་ཤོག་ཅིག་ཅེས་
msgctxt "7251aef6d23549a5bd6899739a5194bc"
msgid ""
"ཡང་དག་པ␣ར་ རྫོགས་པ␣འི་ སངས་རྒྱས་ འོད་སྲུང་ གིས་ བདག་ གི་ མཁན་མོ་ ཇི་ལྟར་ "
"མདོ་ རྣམ་པ␣ར་ འབྱེད་པ་ རྣམས་ ཀྱི་ མཆོག་ ཏུ་ བསྟན་པ་ དེ་བཞིན་ དུ །  བདག་ ཀྱང་"
" ཤཱཀྱ་ ཐུབ་པ་ ཤཱཀྱའི་ རྒྱལ་པོ␣འི་ མཆོག་ དེ␣ས་ མདོ་ རྣམ་པ␣ར་ འབྱེད་པ་ རྣམས་ "
"ཀྱི་ མཆོག་ ཏུ་ བསྟན་པ␣ར་ ཤོག་ ཅིག་ ཅེས་"
msgstr ""

msgctxt "d365c0b421c64da68b5067001c3f11cf"
msgid "བྱས་ སོ ། །"
msgstr ""

msgctxt "98c9e8e002bc40c489b741c0524eb59d"
msgid "དགེ་སློང་ དག་"
msgstr ""

msgctxt "9f5ba182f7ea4eb8be0c9c78f5993bf9"
msgid "ཇི་སྙམ་ དུ་ སེམས །"
msgstr ""

msgctxt "31d7bf13cc2e4b539be47b1df01f33b3"
msgid ""
"དེ␣འི་ ཚེ་ དགེ་སློང་ མར་ གྱུར་པ་ གང་ཡིན་པ་ དེ་ ནི་ ཀ་ ཙང་ ཀ་ལ་ འདི་ཉིད་ ཡིན་"
" ཏེ །"
msgstr ""

# དེར་འདིས་ཚེ་གཅིག་ཏུ་<«གཡུང་»«པེ་»+མཉེས་པར།>ཚངས་པར་སྤྱོད་པ་སྤྱད་ནས་
msgctxt "339adefad13c47c3a3535f0cbbdf5ec8"
msgid "དེ␣ར་ འདི␣ས་ ཚེ་ གཅིག་ ཏུ་ ཚངས་པ␣ར་ སྤྱོད་པ་ སྤྱད་ ནས་"
msgstr ""

# འཆི་ཀར་<«ཞོལ་»ཁར།>སྨོན་ལམ་བཏབ་སྟེ།
msgctxt "2c77d8d188364f26aebcbfef2750e2bd"
msgid "འཆི་ཀ␣ར་ སྨོན་ལམ་ བཏབ་ སྟེ །"
msgstr ""

msgctxt "e47865f4e756428a9e44a4234bb2a47c"
msgid ""
"ཡང་དག་པ␣ར་ རྫོགས་པ␣འི་ སངས་རྒྱས་ འོད་སྲུང་ གིས་ བྲམ་ཟེ␣འི་ ཁྱེའུ་ བླ་མ་ "
"ལུང་བསྟན་པ་ གང་ཡིན་པ␣འི་ སངས་རྒྱས་ བཅོམ་ལྡན་འདས་ དེ་ བདག་ གིས་ མཉེས་པ␣ར་ "
"བྱེད་པ␣ར་ གྱུར་ཅིག  །"
msgstr ""

msgctxt "4b73a3ada48c4e60bc8386f55d07b9cb"
msgid "མི་ མཉེས་པ␣ར་ བྱེད་པ␣ར་ མ་ གྱུར་ཅིག  །"
msgstr ""

msgctxt "adeb7a4b7e46486896c9c1a78bc13be1"
msgid "དེ་ ཁོ་ན␣འི་ བསྟན་པ་ ལ་ རབ་ཏུ་ བྱུང་ ནས་"
msgstr ""

msgctxt "1e6f1008157c4789b51dd376eeb4e193"
msgid "ཉོན་མོངས་པ་ ཐམས་ཅད་ སྤངས་ ཏེ །"
msgstr ""

msgctxt "372ca03b7be2494e88e2ab8042112692"
msgid "དགྲ་བཅོམ་པ་ ཉིད་ མངོན་སུམ་ དུ་ བྱེད་པ␣ར་ གྱུར་ཅིག  །"
msgstr ""

# ཡང་དག་པར་རྫོགས་པའི་སངས་རྒྱས་འོད་སྲུང་གིས་བདག་གི་མཁན་མོ་ཇི་ལྟར་མདོ་རྣམ་པར་འབྱེད་པ་རྣམས་ཀྱི་མཆོག་ཏུ་བསྟན་པ་དེ་བཞིན་དུ།
# བདག་ཀྱང་ཤཱཀྱ་ཐུབ་པ་ཤཱཀྱའི་<«ཅོ་»ཤཱཀྱིའི།>རྒྱལ་པོའི་མཆོག་དེས་མདོ་རྣམ་པར་འབྱེད་པ་རྣམས་ཀྱི་<«ཅོ་»ཀྱིས།>མཆོག་ཏུ་བསྟན་པར་ཤོག་ཅིག་ཅེས་
msgctxt "0cdd6733da134b5ab04e5d1afecd2b19"
msgid ""
"ཡང་དག་པ␣ར་ རྫོགས་པ␣འི་ སངས་རྒྱས་ འོད་སྲུང་ གིས་ བདག་ གི་ མཁན་མོ་ ཇི་ལྟར་ "
"མདོ་ རྣམ་པ␣ར་ འབྱེད་པ་ རྣམས་ ཀྱི་ མཆོག་ ཏུ་ བསྟན་པ་ དེ་བཞིན་ དུ །  བདག་ ཀྱང་"
" ཤཱཀྱ་ ཐུབ་པ་ ཤཱཀྱའི་ རྒྱལ་པོ␣འི་ མཆོག་ དེ␣ས་ མདོ་ རྣམ་པ␣ར་ འབྱེད་པ་ རྣམས་ "
"ཀྱི་ མཆོག་ ཏུ་ བསྟན་པ␣ར་ ཤོག་ ཅིག་ ཅེས་"
msgstr ""

msgctxt "44441d795b5842399c32676f05ffd22e"
msgid "བྱས་པ་ དེས་ན །"
msgstr ""

msgctxt "2b4b266481c64dedafb4dcc51a55fbc4"
msgid "དགེ་སློང་ དག་"
msgstr ""

msgctxt "25587188d96b426cb35611efd3ef0f17"
msgid ""
"ང་ ཡང་དག་པ␣ར་ རྫོགས་པ␣འི་ སངས་རྒྱས་ འོད་སྲུང་ དང་ ཤིན་ཏུ་ མཉམ་པ་ དང་ །  "
"ཤུགས་ མཉམ་པ་ དང་ །  མཛད་པ་ མཉམ་པ་ དང་ །  ཐབས་ མཉམ་པ་ བརྙེས་ ནས །"
msgstr ""

msgctxt "032e9e31ebd6431589e13db1bafdff30"
msgid "འདི␣ས་ ང་ མཉེས་པ␣ར་ བྱས་ ཏེ་"
msgstr ""

msgctxt "641e1e71f9534f909fe0b2a4657918d4"
msgid "མི་ མཉེས་པ␣ར་ མ་ བྱས་ ལ །"
msgstr ""

msgctxt "f091316675894308aeda5bdb03110473"
msgid "ང་ཉིད་ ཀྱི་ བསྟན་པ་ ལ་ རབ་ཏུ་ བྱུང་ ནས་"
msgstr ""

msgctxt "09864f08a3374e54827127b2d8294f3d"
msgid "ཉོན་མོངས་པ་ ཐམས་ཅད་ སྤངས་ ཏེ །"
msgstr ""

msgctxt "60865abee3af423da8a9c6705a5bdabf"
msgid "དགྲ་བཅོམ་པ་ ཉིད་ མངོན་སུམ་ དུ་ བྱས་ ནས་"
msgstr ""

msgctxt "7bf21fe4bb0f478b8acfdcc08ded8ccc"
msgid ""
"ངས་ འདི་ མདོ་ རྣམ་པ␣ར་ འབྱེད་པ་ རྣམས་ ཀྱི་ མཆོག་ ཏུ་ ཡང་ བསྟན་ ཏོ །། །།"
