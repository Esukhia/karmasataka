msgid "བདག་ ཁྲོ␣ས་ ནས་"
msgstr ""

msgctxt "ff1b85255179441db514030d9b641cc4"
msgid ""
"ཚངས་པ་ མཚུངས་པ␣ར་ སྤྱོད་པ་ རྣམས་ ལ་ ཚིག་རྩུབ་པོ་ སྨྲས་པ་ གང་ཡིན་པ␣འི་ ལ␣ས་ "
"དེ␣འི་ རྣམ་པ␣ར་ སྨིན་པ␣འི་ སྐལ་བ་ ཅན་ དུ་ བདག་ མ་ གྱུར་ཅིག་ ཅེས་"
msgstr ""

msgctxt "db9a5b0d1af844488964aab7896c481e"
msgid "བྱས་ སོ ། །"
msgstr ""

msgctxt "f0412f23c4e5443787281b9870fc1434"
msgid "དགེ་སློང་ དག་"
msgstr ""

msgctxt "bfb8d66d1cf64f608f69c7b71fae4e43"
msgid "ཇི་སྙམ་ དུ་ སེམས་"
msgstr ""

msgctxt "fa127da847dc4f2f82d6e7bcf947dce5"
msgid ""
"དེ␣འི་ ཚེ་ དགེ་སློང་ མར་ གྱུར་པ་ གང་ཡིན་པ་ དེ་ ནི་ དགེ་སློང་ མ་ འདི་ཉིད་ "
"ཡིན་ ཏེ །"
msgstr ""

# དེས་<«ཅོ་»འདིས།>དེར་ཚངས་པ་<«གཡུང་»«པེ་»པར།>མཚུངས་པར་སྤྱོད་པ་དེ་དག་ལ་ཚིག་རྩུབ་པོ་སྨྲས་པའི་ལས་དེའི་རྣམ་པར་སྨིན་པས་ནི་ཚེ་རབས་ལྔ་བརྒྱར་ཁྱིར་སྐྱེས་སོ།
# །
msgctxt "da6c46b7d99b4f22b6f0c072149c6658"
msgid ""
"དེ␣ས་ དེ␣ར་ ཚངས་པ་ མཚུངས་པ␣ར་ སྤྱོད་པ་ དེ་དག་ ལ་ ཚིག་རྩུབ་པོ་ སྨྲས་པ␣འི་ "
"ལ␣ས་ དེ␣འི་ རྣམ་པ␣ར་ སྨིན་པ␣ས་ ནི་ ཚེ་རབས་ ལྔ་ བརྒྱ␣ར་ ཁྱིར་ སྐྱེས་ སོ ། །"
msgstr ""

# དེས་དེར་འཆི་ཀར་<«ཞོལ་»གར།>སྨོན་ལམ་བཏབ་སྟེ།
msgctxt "70e57dd5604b48ed9ee7b118a78bc810"
msgid "དེ␣ས་ དེ␣ར་ འཆི་ཀ␣ར་ སྨོན་ལམ་ བཏབ་ སྟེ །"
msgstr ""

msgctxt "09e4bd8dae1942c4a59651227bb886bd"
msgid ""
"བདག་ གང་དང་གང་ དུ་ སྐྱེ་བ་ དེ་ དང་ དེ␣ར་ ཕྱུག་ ཅིང་ ནོར་ མང་ ལ་ ལོངས་སྤྱོད་ "
"ཆེ་བ␣འི་ རིགས་ སུ་ སྐྱེ་བ␣ར་ གྱུར་ཅིག  །"
msgstr ""

# ཡང་དག་པར་རྫོགས་པའི་སངས་རྒྱས་འོད་སྲུང་གིས་བྲམ་ཟེའི་ཁྱེའུ་བླ་མ་ལུང་བསྟན་པ་གང་ཡིན་པའི་སངས་རྒྱས་བཅོམ་ལྡན་འདས་དེ་བདག་<«ཁུ་»དག>གིས་མཉེས་པར་བྱེད་པར་གྱུར་ཅིག
# །
msgctxt "14c75a409c274dcebe0862358f40c541"
msgid ""
"ཡང་དག་པ␣ར་ རྫོགས་པ␣འི་ སངས་རྒྱས་ འོད་སྲུང་ གིས་ བྲམ་ཟེ␣འི་ ཁྱེའུ་ བླ་མ་ "
"ལུང་བསྟན་པ་ གང་ཡིན་པ␣འི་ སངས་རྒྱས་ བཅོམ་ལྡན་འདས་ དེ་ བདག་ གིས་ མཉེས་པ␣ར་ "
"བྱེད་པ␣ར་ གྱུར་ཅིག  །"
msgstr ""

# མི་མཉེས་<«ཁུ་»གཉེས།>པར་བྱེད་པར་མ་<«གཡུང་»བ་མ། «པེ་»པ་མ། «ཁུ་»པའི།>གྱུར་ཅིག །
msgctxt "567670fa496c44d49a9953cf6bab23c6"
msgid "མི་ མཉེས་པ␣ར་ བྱེད་པ␣ར་ མ་ གྱུར་ཅིག  །"
msgstr ""

msgctxt "5469f304ec3f4358827a268fe9ca1505"
msgid "དེ་ ཁོ་ན␣འི་ བསྟན་པ་ ལ་ རབ་ཏུ་ བྱུང་ ནས་"
msgstr ""

# ཉོན་མོངས་པ་ཐམས་ཅད་སྤངས་ཏེ<«གཡུང་»«པེ་»སྟེ།>།
msgctxt "471a39468803450dbfbfc397eac7aeb3"
msgid "ཉོན་མོངས་པ་ ཐམས་ཅད་ སྤངས་ ཏེ །"
msgstr ""

msgctxt "4db5f5c4cb9d4ce8b348343babaa57b8"
msgid "དགྲ་བཅོམ་པ་ ཉིད་ མངོན་སུམ་ དུ་ བྱེད་པ␣ར་ གྱུར་ཅིག་ ཅེས་"
msgstr ""

# བྱས་པའི་ལས་དེའི་རྣམ་པར་སྨིན་པས་ནི།
# འདི་ད་<«གཡུང་»«ལི་»«པེ་»«སྣར་»«ཅོ་»«ཞོལ་»–ད།>ལྟར་ཕྱུག་ཅིང་ནོར་མང་ལ་ལོངས་སྤྱོད་<«གཡུང་»སྤྱོ།>ཆེ་བའི་རིགས་སུ་སྐྱེས་<«སྣར་»སྐྱེས།>ཏེ།
msgctxt "016e4d39639c4dc389d4cad15976af9a"
msgid ""
"བྱས་པ␣འི་ ལ␣ས་ དེ␣འི་ རྣམ་པ␣ར་ སྨིན་པ␣ས་ ནི །  འདི་ ད་ལྟར་ ཕྱུག་ ཅིང་ ནོར་ "
"མང་ ལ་ ལོངས་སྤྱོད་ ཆེ་བ␣འི་ རིགས་ སུ་ སྐྱེས་ ཏེ །"
msgstr ""

msgctxt "312f2ae07a9e4c02a7a206f34771ede4"
msgid "དགེ་སློང་ དག་"
msgstr ""

# ང་ཡང་དག་པར་རྫོགས་པའི་སངས་རྒྱས་འོད་སྲུང་དང་ཤིན་ཏུ་མཉམ་པ་དང་ཤུགས་མཉམ་པ་དང་མཛད་པ་མཉམ་པ་དང་ཐབས་<«པེ་»+ཅད།>མཉམ་པ་བརྙེས་པར་གྱུར་ནས།
msgctxt "22fffde376fd45578fc169524a754bfe"
msgid ""
"ང་ ཡང་དག་པ␣ར་ རྫོགས་པ␣འི་ སངས་རྒྱས་ འོད་སྲུང་ དང་ ཤིན་ཏུ་ མཉམ་པ་ དང་ ཤུགས་ "
"མཉམ་པ་ དང་ མཛད་པ་ མཉམ་པ་ དང་ ཐབས་ མཉམ་པ་ བརྙེས་པ␣ར་ གྱུར་ ནས །"
msgstr ""

msgctxt "7e27b7f86acf4f509b4f9674139ad817"
msgid "འདི␣ས་ ང་ མཉེས་པ␣ར་ བྱས་ ཏེ །"
msgstr ""

msgctxt "2562066cadf840348924ffdd02df61db"
msgid "མི་ མཉེས་པ␣ར་ མ་ བྱས་ ལ་"
msgstr ""

msgctxt "78c101607cd94902831a8a6c38296f18"
msgid "ང་ཉིད་ ཀྱི་ བསྟན་པ་ ལ་ རབ་ཏུ་ བྱུང་ ནས །"
msgstr ""

msgctxt "40199e90b39a4bb8b364e5b94a9dfbab"
msgid "ཉོན་མོངས་པ་ ཐམས་ཅད་ སྤངས་ ཏེ །"
msgstr ""

# དགྲ་བཅོམ་པ་ཉིད་མངོན་སུམ་དུ་བྱས་པ་འདི་ནི་འདིས་འདས་པའི་ལས་<«གཡུང་»«པེ་»ལམ།>ཡིན་ནོ།
# །
msgctxt "b8acaff8d0984e069f45ee8285071900"
msgid ""
"དགྲ་བཅོམ་པ་ ཉིད་ མངོན་སུམ་ དུ་ བྱས་པ་ འདི་ ནི་ འདི␣ས་ འདས་པ␣འི་ ལ␣ས་ ཡིན་ ནོ"
" ། །"
msgstr ""

# ད་ལྟར་བྱུང་བའི་ལས་<«གཡུང་»«པེ་»ལམ།>ཅི་ཞིག་བྱས་ཞེ་<«གཡུང་»«ལི་»«པེ་»«སྣར་»«ཅོ་»«ཞོལ་»ཤེ།>ན།
msgctxt "9b6a0ccf420b42d0808296b79b540dcc"
msgid "ད་ལྟར་ བྱུང་བ␣འི་ ལ␣ས་ ཅི་ཞིག་ བྱས་ ཞེ་ ན །"
msgstr ""

# དེ་<«ཅོ་»འདི།>ཁྱིར་སྐྱེས་ནས་
msgctxt "c3f43c07b0d942438eb7a7aaa8a16612"
msgid "དེ་ ཁྱིར་ སྐྱེས་ ནས་"
msgstr ""

# ཤཱ་རིའི་བུ་ལ་སེམས་དགའ་བར་གྱུར་པ་དེས་ནི་མིའི་ནང་དུ་སྐྱེས་<«ཅོ་»སྐྱེས།>ཏེ།
msgctxt "61d1a5c8067c442eb9de8156478d13b6"
msgid ""
"ཤཱ་རི␣འི་ བུ་ ལ་ སེམས་ དགའ་བ␣ར་ གྱུར་པ་ དེ␣ས་ ནི་ མི␣འི་ ནང་ དུ་ སྐྱེས་ ཏེ །"
msgstr ""

msgctxt "9544646436e44fc8b6b984e4c491117c"
msgid "འདི་ ནི་ ད་ལྟར་ བྱུང་བ␣འི་ ལ␣ས་ ཡིན་ ནོ །། །།"
msgstr ""
