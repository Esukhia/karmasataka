msgctxt "bbb2ac611b754c7089a16191e5f2765b"
msgid ""
"དེ་ ཤི་བ␣འི་ ཚེ་ཚེ་ དང་ ལྡན་པ་ ཤཱ་རི␣འི་ བུ་ སླར་ ཁྱིམ་ དེ␣ར་ འོངས་ ནས །"
msgstr ""

# ཁྱིམ་བདག་གིས་ཚེ་དང་ལྡན་པ་ཤཱ་རིའི་བུ་ལ་བསྙད་<«གཡུང་»བརྙད།>དེ།
msgctxt "a5e9f6aa02c44a6186706d0328b6b6b2"
msgid "ཁྱིམ་བདག་ གིས་ ཚེ་ དང་ ལྡན་པ་ ཤཱ་རི␣འི་ བུ་ ལ་ བསྙད་ དེ །"
msgstr ""

# བཙུན་པ་ཤཱ་<«གཡུང་»«པེ་»ཤ>རིའི་བུ་
msgctxt "ad93ca635d474029be6cb122c45a427c"
msgid "བཙུན་པ་ ཤཱ་རི␣འི་ བུ་"
msgstr ""

msgctxt "3107343186174870970c37fd0d02720a"
msgid "ཁྱི་ མོ་ དེ་ གུམ་ ལགས་སོ་ ཞེས་"
msgstr ""

msgctxt "333a8b43c84b4bf4908f7d253d1e4cb0"
msgid "བྱས་ སོ ། །"
msgstr ""

msgctxt "ac4fcd26404b4e28ac000ab01f0acb53"
msgid "ཚེ་ དང་ ལྡན་པ་ ཤཱ་རི␣འི་ བུས་ སྨྲས་པ །"
msgstr ""

msgctxt "b68c072ebc6d4d6ebf5dbdd1ca5aca53"
msgid "ཁྱི་ མོ་ དེ␣འི་ རོ་ དེ་ ཕྱོགས་ དབེན་པ་ ཞིག་ ཏུ་ ཞོག་ ཤིག་ དང་ །"
msgstr ""

msgctxt "f956ddc937894019bdb0152182bfcc42"
msgid "རུས་པ་ དེ་དག་ ཕྱིས་ དེ་ཉིད་ ལ་ ཕན་ འདོགས་པ␣ར་ འགྱུར་ རོ ། །"
msgstr ""

msgctxt "a917288d97e744edb97bc15aa11198a2"
msgid "ཁྱིམ་བདག་ གིས་ སྨྲས་པ །"
msgstr ""

# བཙུན་པ་ཤཱ་རིའི་བུས་ཇི་སྐད་བཀའ་སྩལ་པ་བཞིན་དུ་<«གཡུང་»«ལི་»«པེ་»«སྣར་»«ཅོ་»«ཞོལ»–དུ།>བགྱིའོ་ཞེས་
msgctxt "6009b9db42c64c269b886ad9a70b12cd"
msgid "བཙུན་པ་ ཤཱ་རི␣འི་ བུས་ ཇི་སྐད་ བཀའ་ སྩལ་པ་ བཞིན་ དུ་ བགྱི␣འོ་ ཞེས་"
msgstr ""

msgctxt "5ac52ea8be6248fa8050f2f602b88c0d"
msgid "སྨྲ␣ས་ ནས །"
msgstr ""

msgctxt "785d150f76a14eb2ba36a84fb67f4307"
msgid ""
"ཁྱིམ་བདག་ དེ␣ས་ ཁྱི་རོ་ དེ་ གང་ དུ་ སུ␣ས་ ཀྱང་ མི་ མཐོང་བ␣ར་ འགྱུར་བ་ དེ་ལྟ་"
" བུ␣འི་ ཕྱོགས་ ཤིག་ ཏུ་ བཞག་ གོ ། །"
msgstr ""

# དེ་ནས་ཁྱིམ་བདག་གི་ཆུང་མ་དེ་ཟླ་བ་དགུའམ་བཅུ་ལོན་པ་དང་བུ་བྱུང་སྟེ<«ཁུ་»–བུ་བྱུང་སྟེ།>།
msgctxt "9a2379bb8ae94c32926d1d531f5636db"
msgid ""
"དེ་ནས་ ཁྱིམ་བདག་ གི་ ཆུང་མ་ དེ་ ཟླ་བ་ དགུ␣འམ་ བཅུ་ ལོན་པ་ དང་ བུ་ བྱུང་ སྟེ "
"།"
msgstr ""

# བུ་མོ་གཟུགས་བཟང་ཞིང་བལྟ་<«ལི་»«སྣར་»«ཅོ་»ལྟ།>ན་སྡུག་ལ་མཛེས་པ་<«གཡུང་»«པེ་»ལ།>མདོག་བཟང་བ་གསེར་གྱི་ཁ་དོག་ཅན་མགོ་བོ་གདུགས་ལྟར་འདུག་པ།
# ལག་པ་རིང་བ་དཔྲལ་བའི་དབྱེས་ཆེ་བ་སྣ་མཐོ་བ་སྣ་ལེགས་པ་སྨིན་མ་སྟུག་པ་ཞིག་བཙས་སོ།
# །
msgctxt "3dca2cd5e1b14d7ea05100c1d28c1bfa"
msgid ""
"བུ་མོ་ གཟུགས་བཟང་ ཞིང་ བལྟ་ ན་ སྡུག་ ལ་ མཛེས་པ་ མདོག་ བཟང་བ་ གསེར་ གྱི་ "
"ཁ་དོག་ ཅན་ མགོ་བོ་ གདུགས་ ལྟར་ འདུག་པ །  ལག་པ་ རིང་བ་ དཔྲལ་བ␣འི་ དབྱེས་ "
"ཆེ་བ་ སྣ་ མཐོ་བ་ སྣ་ ལེགས་པ་ སྨིན་མ་ སྟུག་པ་ ཞིག་ བཙས་ སོ ། །"
msgstr ""

# དེ་ནས་དེའི་བཙས་སྟོན་ཡང་རྒྱས་པར་<«གཡུང་»+བྱས་སོ།
