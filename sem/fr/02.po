msgctxt "361c1a463ace48b690cdbdf4297a0e7c"
msgid "ཕྱིར་ལོག་ ནས་"
msgstr "rentra [chez lui] et"

msgctxt "22b09f8a642642be9a0e77d8f2df3525"
