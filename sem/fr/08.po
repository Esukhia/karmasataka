msgctxt "5938935361bb4342b8fc7a98d7474d68"
msgid "དེ་ གཉིས་ གཅིག་ ལ་ གཅིག་ བཟོད་པ་ གསོལ་ ཏེ་"
msgstr "ils (lit. les deux) se demandèrent pardon l'un à l'autre et"

msgctxt "c276299bc02340c6b978a69c824c57e5"
msgid "མཛའ་བོ་ བྱས་ ནས་"
msgstr "ils devinrent (lit. faire) amis et"

# གཅིག་གིས་ཀྱང་བུ་མོ་ལ་གོས་དང་རྒྱན་མང་པོ་<«གཡུང་»«པེ་»པོས།>བསྐུར་རོ། །
msgctxt "0c47223ec6bf4bdc9bba48c211f7a154"
msgid "གཅིག་ གིས་ ཀྱང་ བུ་མོ་ ལ་ གོས་ དང་ རྒྱན་ མང་པོ་ བསྐུར་ རོ ། །"
msgstr "l'un envoya beaucoup de vêtements et d'ornements pour la fille."

# གཅིག་གིས་ཀྱང་ཁྱེའུ་ལ་གོས་དང་རྒྱན་མང་པོ་<«གཡུང་»«པེ་»པོས།>བསྐུར་ཏེ།
msgctxt "17c887b8a6ac4d0fb2f843513878c35b"
msgid "གཅིག་ གིས་ ཀྱང་ ཁྱེའུ་ ལ་ གོས་ དང་ རྒྱན་ མང་པོ་ བསྐུར་ ཏེ །"
msgstr ""
"L'autre aussi envoya beaucoup de vêtements et d'ornements au garçon. Puis,"

msgctxt "252f79eba4dd499bbee2ea201a9b55e0"
msgid "དེ་ གཉིས་ ཕན་ཚུན་ གཉེན་ དུ་ བྱས་ ནས་"
msgstr "ils firent [en sorte] de s'apparenter et"

msgctxt "c49babcbc04f4a6f8d1a5c6eea933091"
msgid "མགུལ་ ནས་ འཇུས་ ཏེ་"
msgstr "bras dessus bras dessous (lit. se tenant le cou),"

msgctxt "7e46c09722484ddb8bb9c50a0540b7dd"
msgid "ཡུལ་ དེ་ནས་ དོང་ ངོ་ ། །"
