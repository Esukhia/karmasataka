msgstr "après avoir éliminé toutes les émotions perturbatrices,"

msgctxt "47b52c4a496f4a72b4f27540bf47d104"
msgid "དགྲ་བཅོམ་པ་ ཉིད་ མངོན་སུམ་ དུ་ བྱས་ སོ ། །"
msgstr "il manifesta l'état d'arhat (lit. l'arhat-même)."

# ཅིག་<«གཡུང་»«ལི་»«པེ་»«སྣར་»«ཅོ་»«ཞོལ་»གཅིག>ཤོས་ཀྱིས་ནི་རབ་ཏུ་བྱུང་ནས་
msgctxt "7ddf002056d54551b9463668f9f38442"
msgid "ཅིག་ཤོས་ ཀྱིས་ ནི་ རབ་ཏུ་ བྱུང་ ནས་"
msgstr "L'autre se retira du monde et"

msgctxt "ee99925ee8f44e4e840d0b88b0c01270"
msgid "ཚེ་ གཅིག་ ཏུ་ ཚངས་པ␣ར་ སྤྱོད་པ་ སྤྱད་ དེ་"
msgstr "vécu chastement toute sa vie, puis"

# འཆི་ཀར་<«ཁུ་»གར། «ཞོལ་»ཁར།>སྨོན་ལམ་བཏབ་པ།
msgctxt "806e8a00940e4cad980dff3ef6684184"
msgid "འཆི་ཀ␣ར་ སྨོན་ལམ་ བཏབ་པ །"
msgstr "il fit ce souhait au moment de mourir : «"

msgctxt "583d0f025cf944db843ec62d3d938ede"
msgid ""
"བདག་ གིས་ འདི་ལྟར་ སེམས་ཅན་ དག་པ་ འདི་ ལྟ་བུ་ ལ་ ཚིག་རྩུབ་པོ་ སྨྲས་པ␣འི་ "
"ལ␣ས་ དེ␣འི་ སྐལ་བ་ ཅན་ དུ་ བདག་ མ་ གྱུར་ཅིག  །"
msgstr ""
"Puissé-je de pas devoir subir [le résultat] d'avoir dit des paroles "
"blessantes à un être comme lui."

# བརྒྱ་ལ་ལས་དེའི་འབྲས་བུ་བདག་ལ་སྨིན་པར་གྱུར་<«གཡུང་»«ལི་»«པེ་»«སྣར་»«ཅོ་»འགྱུར།>ན་ཡང་རབ་ཏུ་འབྱུང་བའི་སེམས་བསྐྱེད་མ་ཐག་ཏུ་མ་ལེགས་པ་མེད་པར་གྱུར་<«གཡུང་»«ལི་»«པེ་»«ཅོ་»འགྱུར།>ཏེ་
msgctxt "95c461155d9241229211906b10e31a1f"
msgid ""
"བརྒྱ་ལ་ ལ␣ས་ དེ␣འི་ འབྲས་བུ་ བདག་ ལ་ སྨིན་པ␣ར་ གྱུར་ ན་ ཡང་ རབ་ཏུ་ "
"འབྱུང་བ␣འི་ སེམས་བསྐྱེད་ མ་ ཐག་ ཏུ་ མ་ ལེགས་པ་ མེད་པ␣ར་ གྱུར་ ཏེ་"
msgstr ""
"Si par hasard (lit. sur cent), le résultat mûrit sur moi, puisse ce mal "
"disparaître dès que je conçois l'idée de me retirer du monde et"

# ལེགས་པ་<«སྣར་»«ཞོལ་»པར།>འབྱུང་བར་གྱུར་ཅིག །
msgctxt "ba8d44fbf28f41c8803a03bcd45c70cc"
msgid "ལེགས་པ་ འབྱུང་བ␣ར་ གྱུར་ཅིག  །"
msgstr "puisse ce qui est bien m'arriver."

msgctxt "7e27452daa4246ff8ea5fdc24381574d"
msgid "འདི་ལྟར་"
msgstr "Eh bien (lit. comme ceci),"

msgctxt "ea5d3354c98842fca7006bbed4d9e347"
msgid "བདག་ རབ་ཏུ་ བྱུང་ ནས་"
msgstr "je me suis retiré du monde et"

msgctxt "5d06d865cf804648a3aa44931ed7c3c2"
msgid "ཚེ་ གཅིག་ ཏུ་ ཚངས་པ␣ར་ སྤྱོད་པ་ སྤྱད་ ཀྱང་"
msgstr "j'ai vécu chastement toute ma vie (lit. une vie), et"

# ཡོན་ཏན་ཅི་ཡང་མ་ཐོབ་ཀྱིས་<«གཡུང་»«པེ་»ཀྱང་། «ཞོལ་»ཀྱི།>
msgctxt "fd4b356fd420428aabe0999cd9980271"
msgid "ཡོན་ཏན་ ཅི་ཡང་ མ་ ཐོབ་ ཀྱིས་"
msgstr "je n'ai obtenu aucune de toutes les qualités."

# ཡང་དག་པར་རྫོགས་པའི་སངས་རྒྱས་འོད་སྲུང་གིས་བྲམ་ཟེའི་ཁྱེའུ་བླ་མ་ལུང་བསྟན་པ་<«གཡུང་»«པེ་»–པ།>གང་ཡིན་པའི་སངས་རྒྱས་བཅོམ་ལྡན་འདས་དེ་བདག་<«ཅོ་»དག>གིས་མཉེས་པར་བྱེད་པར་གྱུར་ཅིག
# །
msgctxt "6cad72bb9ee54d7bbae84a381611bb52"
msgid ""
"ཡང་དག་པ␣ར་ རྫོགས་པ␣འི་ སངས་རྒྱས་ འོད་སྲུང་ གིས་ བྲམ་ཟེ␣འི་ ཁྱེའུ་ བླ་མ་ "
"ལུང་བསྟན་པ་ གང་ཡིན་པ␣འི་ སངས་རྒྱས་ བཅོམ་ལྡན་འདས་ དེ་ བདག་ གིས་ མཉེས་པ␣ར་ "
"བྱེད་པ␣ར་ གྱུར་ཅིག  །"
msgstr ""
"Puissé-je contenter par mes actes le Bienheureux Bouddha que [deviendra] le "
"jeune brahmane Uttara [d'après] la prophétie du complet et parfait Bouddha "
"Kāśyapa."

# མི་མཉེས་པར་<«ལི་»«སྣར་»«ཅོ་»«ཞོལ་»པ།>བྱེད་པར་མ་གྱུར་ཅིག །
msgctxt "b45d14f64e174612aa8d6248602fa4fe"
msgid "མི་ མཉེས་པ␣ར་ བྱེད་པ␣ར་ མ་ གྱུར་ཅིག  །"
msgstr "Puissé-je ne rien faire qui le mécontente."

# དེ་ཁོ་<«གཡུང་»«པེ་»ཁོའ།>ནའི་བསྟན་པ་ལ་རབ་ཏུ་བྱུང་ནས་
msgctxt "cc0af80f460d4983b2ecdb101c4f8a5d"
msgid "དེ་ ཁོ་ན␣འི་ བསྟན་པ་ ལ་ རབ་ཏུ་ བྱུང་ ནས་"
msgstr ""
"M'étant retirée du monde d'après son (lit. à lui-même) enseignement et"

msgctxt "ce5e800d23dc4b63b3ca0c0932973671"
msgid "ཉོན་མོངས་པ་ ཐམས་ཅད་ སྤངས་ ཏེ་"
msgstr "après avoir éliminé toutes les émotions perturbatrices,"

msgctxt "0e2e5e33f13d4ccbaa0a8d60a84596a5"
msgid "དགྲ་བཅོམ་པ་ ཉིད་ མངོན་སུམ་ དུ་ བྱེད་པ␣ར་ གྱུར་ཅིག་ ཅེས་"
msgstr "puissé-je manifester l'état d'arhat (lit. l'arhat-même) »,"

msgctxt "444484f04d044bce9a0b9bce66845c4c"
msgid "བྱས་ སོ ། །"
msgstr "dit-il."

msgctxt "ba0af9da50c84be68d894c14b8491684"
msgid "དགེ་སློང་ དག་"
msgstr "Moines,"

msgctxt "92d989cb7c144ed599967183213fc2aa"
msgid "ཇི་སྙམ་ དུ་ སེམས །"
msgstr "qu'en pensez-vous ?"

msgctxt "a250cc717da241ed947fa4721fa96ed8"
msgid ""
"དེ␣འི་ ཚེ་ ཡང་དག་པ␣ར་ རྫོགས་པ␣འི་ སངས་རྒྱས་ འོད་སྲུང་ གི་ གསུང་རབ་ ལ་ རབ་ཏུ་"
" བྱུང་བ␣འི་ དགེ་སློང་ དུ་ གྱུར་པ་ གང་ཡིན་པ་ དེ་ ནི་ འཕྱེ་བོ་ འདི་ཉིད་ ཡིན་ "
"ཏེ"
msgstr ""
"À cette époque, le moine qui s'était retiré du monde selon l'enseignement du"
" complet et parfait Bouddha Kāśyapa est Paṅgu lui-même et"

msgctxt "cb04077afc664bdaa959f6128fb6cf43"
msgid ""
"དེ␣ས་ དེ␣ར་ ཕུ་བོ་ ལ་ ཚིག་རྩུབ་པོ་ སྨྲས་པ␣འི་ ལ␣ས་ དེ␣ས་ ནི་ གང་དང་གང་ དུ་ "
"སྐྱེས་པ་ དེ་ དང་ དེ␣ར་ འཕྱེ་བོ␣ར་ གྱུར་ ཏོ ། །"
msgstr ""
"l'action d'avoir dit ces paroles blessantes à son frère le rendit estropié "
"partout où il naissait."

msgctxt "410edb3dc41347cb89d12cd074206060"
msgid "དེ་ འགྱོད་པ␣ར་ གྱུར་ ཏེ་"
msgstr "Il regretta et"

msgctxt "40fd1b21367c4943b470aa1d01e672fd"
msgid "རབ་ཏུ་ བྱུང་ ནས་"
msgstr "se retira du monde et"

msgctxt "371a8cc250214436aebc226ebc72b5e8"
msgid "ཚེ་ གཅིག་ ཏུ་ ཚངས་པ␣ར་ སྤྱོད་པ་ སྤྱད་ དེ་"
msgstr "vécut chastement toute sa vie et"

# འཆི་ཀར་<«ཞོལ་»ཁར།>སྨོན་ལམ་བཏབ་པ།
msgctxt "5305b9e820934757852d559a5907cc5d"
msgid "འཆི་ཀ␣ར་ སྨོན་ལམ་ བཏབ་པ །"
msgstr "fit ce souhait au moment de mourir : «"

msgctxt "e5ee746df7de4d30a991f259769c4fe0"
msgid "འདི་ལྟར་"
msgstr "Eh bien (lit. comme ceci),"

# བདག་གིས་སེམས་ཅན་དག་པ་འདི་ལྟ་བུ་ལ་ཚིག་རྩུབ་པོ་<«གཡུང་»«པེ་»མོ།>སྨྲས་པའི་ལས་དེའི་སྐལ་བ་ཅན་དུ་བདག་མ་གྱུར་ཅིག
# །
msgctxt "e53ab0d7c4934a6585088befa66cc24c"
msgid ""
"བདག་ གིས་ སེམས་ཅན་ དག་པ་ འདི་ ལྟ་བུ་ ལ་ ཚིག་རྩུབ་པོ་ སྨྲས་པ␣འི་ ལ␣ས་ དེ␣འི་ "
"སྐལ་བ་ ཅན་ དུ་ བདག་ མ་ གྱུར་ཅིག  །"
msgstr ""
"puissé-je de pas devoir subir [le résultat] d'avoir dit des paroles "
"blessantes à un être comme lui."

# བརྒྱ་ལ་ལས་དེའི་རྣམ་པར་སྨིན་པ་བདག་ལ་སྨིན་<«གཡུང་»«ལི་»«པེ་»«སྣར་»«ཅོ་»«ཞོལ་»–པ་བདག་ལ་སྨིན།>པར་གྱུར་ན་ཡང་རབ་ཏུ་འབྱུང་<«གཡུང་»«པེ་»«ཞོལ་»བྱུང་།>བའི་སེམས་བསྐྱེད་མ་ཐག་ཏུ་བདག་གི་<«ཁུ་»གིས།>མ་ལེགས་པ་མེད་པར་གྱུར་ཏེ།
msgctxt "6455f76e1ac84b81a3b8ec39b84748de"
msgid ""
"བརྒྱ་ལ་ ལ␣ས་ དེ␣འི་ རྣམ་པ␣ར་ སྨིན་པ་ བདག་ ལ་ སྨིན་པ␣ར་ གྱུར་ ན་ ཡང་ རབ་ཏུ་ "
"འབྱུང་བ␣འི་ སེམས་བསྐྱེད་ མ་ ཐག་ ཏུ་ བདག་ གི་ མ་ ལེགས་པ་ མེད་པ␣ར་ གྱུར་ ཏེ །"
msgstr ""
"Si par hasard (lit. sur cent), le résultat mûrit sur moi, puisse mon mal "
"disparaître dès que je conçois l'idée de me retirer du monde et"

# ལེགས་པ་འབྱུང་<«གཡུང་»«པེ་»བྱུང་།>བར་གྱུར་ཅིག་ཅེས་
msgctxt "5889887769434c41bb591cc9651e15ee"
msgid "ལེགས་པ་ འབྱུང་བ␣ར་ གྱུར་ཅིག་ ཅེས་"
msgstr "puisse ce qui est bien m'arriver. »"

# བྱས་པ་དེས་ནི་ད་<«གཡུང་»དེ།>ལྟར་རབ་ཏུ་བྱུང་བའི་སེམས་བསྐྱེད་མ་ཐག་ཏུ་ལུས་ཀྱི་ཡན་ལག་ཐམས་ཅད་ཡོངས་སུ་རྫོགས་པར་གྱུར་ཏོ།
# །
msgctxt "40fcbc1f17804c28b7ee572641729fdc"
msgid ""
"བྱས་པ་ དེ␣ས་ ནི་ ད་ལྟར་ རབ་ཏུ་ བྱུང་བ␣འི་ སེམས་བསྐྱེད་ མ་ ཐག་ ཏུ་ ལུས་ ཀྱི་ "
"ཡན་ལག་ ཐམས་ཅད་ ཡོངས་སུ་ རྫོགས་པ␣ར་ གྱུར་ ཏོ ། །"
msgstr ""
