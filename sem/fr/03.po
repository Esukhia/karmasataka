msgid "དེ་ ནི་ བདག་ ལ་ ཕན་ གདགས་པ་ ལགས་སོ་ ཞེས་"
msgstr "Ceci est un bienfait pour moi. »"

msgctxt "5f482e1077a049ca9e179a4d5b664fbf"
msgid "བྱས་ ནས །"
msgstr "dit-il. Puis,"

# ཁྱེའུ་དེ་ཚེ་དང་ལྡན་པ་མ་འགགས་པའི་ཕྱི་བཞིན་<«ཅོ་»བཞི།>འབྲང་སྟེ།
msgctxt "b24e7ea49d0b4958872b8984550aa1a8"
msgid "ཁྱེའུ་ དེ་ ཚེ་ དང་ ལྡན་པ་ མ་ འགགས་པ␣འི་ ཕྱི་བཞིན་ འབྲང་ སྟེ །"
msgstr "ce garçon suivit (lit. suivre en retrait) l'honorable Śāriputra et"

# ཁྱེའུ་དེ་ཚེ་དང་ལྡན་པ་མ་འགགས་པས། གཙུག་<«གཡུང་»«པེ་»ཙུག>ལག་ཁང་དུ་ཁྲིད་ནས།
msgctxt "02e858ab5fd74dbd9a7abe0937c0535f"
msgid "ཁྱེའུ་ དེ་ ཚེ་ དང་ ལྡན་པ་ མ་ འགགས་པ␣ས །  གཙུག་ལག་ཁང་ དུ་ ཁྲིད་ ནས །"
msgstr "l'honorable Aniruddha mena le garçon au monastère et"

msgctxt "85b45497238e43a6bb01f81b36207130"
msgid "རབ་ཏུ་ ཕྱུང་ སྟེ །"
msgstr "il [lui fit] se retirer du monde et"

msgctxt "5dd267d45f8c4279bd2113f4bb4cd145"
msgid "བསྙེན་པ␣ར་ རྫོགས་པ␣ར་ བྱས་ ནས་"
msgstr "lui donna l'ordination complète et"

# དེ་ལ་<«གཡུང་»«ལི་»«པེ་»«སྣར་»«ཅོ་»«ཞོལ་»–དེ་ལ།>ལུང་ཡང་ཕོག་གོ། །
msgctxt "7984a5cae10a4917ba4c584a6cc47945"
msgid "དེ་ ལ་ལུང་ ཡང་ ཕོག་ གོ ། །"
msgstr "lui [accorda] la transmission orale."

msgctxt "86921d968a334439a5ccc69005823422"
msgid ""
"དེ་ནས་ དེ་ སྲོད་ དང་ ཐོ་རངས་ མི་ ཉལ་བ␣ར་ བརྩོན་པ་ ལྷུར་ བྱེད་པ་ ལ་ གནས་ ཀྱང་"
msgstr ""
"Ensuite, sans dormir ni à l'aube ni au crépuscule, bien qu'il maintint de "
"grands efforts [dans sa pratique],"

msgctxt "5c492b2c17184af9b4f8ccf1e922b02e"
msgid "ཁྱད་པར་ ཅུང་ཟད་ ཀྱང་ མ་ ཐོབ་ ནས །"
msgstr "il n'obtint [rien] qui fit la moindre différence, et"

# དེ་<«གཡུང་»«ལི་»«པེ་»«སྣར་»«ཅོ་»དེའི།>ཕྱི་ཞིག་ན་ནད་ཀྱིས་ཐེབས་ནས།
msgctxt "bfcf17a5083d4350a52f2c232bae7863"
msgid "དེ་ ཕྱི་ ཞིག་ ན་ ནད་ ཀྱིས་ ཐེབས་ ནས །"
msgstr "plus tard, il tomba malade et"

msgctxt "ee59f805189e4ddabfe82875aee2bc47"
msgid "དེ␣འི་ ཕ་མ་ གཉིས་ ཀྱིས་ བུ་ནད་ ཀྱིས་ ཐེབས་ སོ་ ཞེས་"
msgstr "« [votre] enfant est tombé malade », ses deux parents"

msgctxt "5cc8c8210f1c410aa5ad124899cd22e8"
msgid "ཐོས་ ནས །"
msgstr "l'entendirent et"

# ཐོས་མ་ཐག་ཏུ་དེའི་<«གཡུང་»«ལི་»«པེ་»«སྣར་»«ཅོ་»«ཞོལ་»–དེའི།>ཕ་མ་གཉིས་ཀྱིས་སྨན་པ་ཁྲིད་དེ།
msgctxt "73d47a17331444a98a5c9ff40130b422"
msgid "ཐོས་ མ་ ཐག་ ཏུ་ དེ␣འི་ ཕ་མ་ གཉིས་ ཀྱིས་ སྨན་པ་ ཁྲིད་ དེ །"
msgstr ""
"dès qu'ils l'eurent entendu, ses deux parents prirent avec eux un médecin et"

msgctxt "abe3dbdc22ca401198654095bc71eb9f"
msgid "ཡོ་བྱད་ ཐམས་ཅད་ ཁྱེར་ ནས །"
msgstr "prenant [avec eux] tous les objets nécessaires,"

msgctxt "2fcacc396de44de890f62a397b70c01a"
msgid "གཙུག་ལག་ཁང་ དུ་ སོང་ སྟེ །"
msgstr "ils allèrent au vihāra, et"

msgctxt "7e587ad67cce412cbad5a9e950a48249"
msgid "ཡོ་བྱད་ ཐམས་ཅད་ ཀྱིས་ བསྙེན་བཀུར་ བྱས་ ཀྱང་"
msgstr "même s'ils le soignèrent à l'aide de tous les objets,"

# སོས་པར་བྱེད་<«ལི་»བྱེས།>མ་ནུས་ནས།
msgctxt "185c3ad820a04cb9b41d3b4a4178bc37"
msgid "སོས་པ␣ར་ བྱེད་ མ་ནུ␣ས་ ནས །"
msgstr "ils ne purent pas le guérir et"

msgctxt "f56220b836d7485fb69739ef1959db9b"
msgid "དེ་ གཉིས་ ཀྱིས་ བསམས་པ །"
msgstr "ils pensèrent [tous] les deux : «"

msgctxt "4d6765d9c36c4b0cb81e07a394cbc0e8"
msgid ""
"བདག་ཅག་ གཉིས་ གཙུག་ལག་ཁང་ དུ་ འདུག་ ན་ ནི་ ཁྱིམ་ གྱི་ བྱ་བ་ མང་པོ་ དག་ "
"ཤོར་བ␣ར་ འགྱུར་ ན་"
msgstr ""
"Si, tous les deux, nous restons au vihāra, nous laisserons passer beaucoup "
"de [choses] à faire de la maison. Donc,"

msgctxt "143727859e274ec39e70e431e14cc750"
msgid "མ་ལ་"
msgstr "décidément,"

msgctxt "71cb7ec6e9f9447cbd7c3f69f4a12b91"
msgid "བུ་ འདི་ ཁྱིམ་ དུ་ ཁྲིད་ ལ་"
msgstr "nous prendrons (lit. mènerons) ce fils à la maison et"

msgctxt "ac3a34ffe42c4b2a9d2adc81ec773c8d"
msgid "དེ་ཉིད་ དུ་ ནད་གཡོག་ བྱ␣འོ་ སྙམ་ དུ་"
msgstr ""
"nous le soignerons (lit. ferons les serviteurs des malades) là-bas (lit. là-"
"bas même) »"

msgctxt "b91c25aa47354bf48dab8fc5398a7737"
msgid "བསམས་ ནས །"
msgstr "pensèrent-ils. Puis,"

msgctxt "f9ba255172d64fb8b19abe6c6139f7f0"
msgid "ཚེ་ དང་ ལྡན་པ་ མ་ འགགས་པ␣འི་ རྐང་པ་ ལ་ ཕྱག་ འཚལ་ ཏེ་"
msgstr "ils se prosternèrent aux pieds de l'honorable Aniruddha et"

msgctxt "46af14b140ea4c1680353810ccdaf673"
msgid "སྨྲས་པ །"
msgstr "dirent : «"

msgctxt "0b06d4a5efcb4c2a987eb166e038bbc5"
msgid "འཕགས་པ་"
msgstr "« [Être] sublime,"

msgctxt "bdd270f4701040828e7771c7e4b9aa1f"
msgid "མཁྱེན་པ␣ར་ མཛད་ དུ་ གསོལ །"
msgstr "veuillez écouter (lit. savoir) [ce qui suit]."

msgctxt "70f6c2992dbc46648ce296921e9bc529"
msgid ""
"བདག་ཅག་ གཉིས་ འདི་ ན་ མཆིས་ ན་ ནི་ ཁྱིམ་ གྱི་ བགྱི་བ་ མང་པོ་ དག་ ཤོར་བ␣ར་ "
"འགྱུར་ ན །"
msgstr ""
"si, tous les deux, nous restons ici, nous laisserons passer beaucoup de "
"[choses] à faire de la maison. Donc,"

# བདག་ཅག་གཉིས་ཀྱིས་<«གཡུང་»«པེ་»ཀྱི།>བུ་འདི་<«ཁུ་»དེ།>ཁྱིམ་དུ་ཁྲིད་དེ་
msgctxt "2c92ef45c38d49c8ab59f6b2f7f14f3f"
msgid "བདག་ཅག་ གཉིས་ ཀྱིས་ བུ་ འདི་ ཁྱིམ་ དུ་ ཁྲིད་ དེ་"
msgstr "nous prendrons (lit. mènerons) ce fils à la maison et"

msgctxt "6d650ee11bbb4be491ee52c551c55d26"
msgid "ནད་གཡོག་ བགྱི་ བར་ འཚལ་ ན་"
msgstr ""
"nous aimerions [l'y] soigner (lit. faire le serviteur des malades). Donc,"

msgctxt "718c59f2c12e4ec891e8f8bec9e33516"
msgid "གནང་ བར་ མཛད་ དུ་ གསོལ་ ཞེས་"
msgstr "Veuillez nous accorder [la permission de le faire]. »"

msgctxt "c473b7a9238f4b3ca254c00e573fc9e2"
msgid "བྱས་ སོ ། །"
msgstr "dirent-ils."

msgctxt "661cf05cfc2c4762bb8ec0ec2ad2dac5"
msgid "དེ་ནས་ ཚེ་ དང་ ལྡན་པ་ མ་ འགགས་པ␣ས་ བསམས་པ །"
msgstr "Ensuite, l'honorable Aniruddha pensa : «"

msgctxt "d2e9e622a82c43358ecd963e58f98026"
msgid ""
"དགེ་སློང་ འདི␣ས་ ཁྱིམ་ དེ་ཉིད་ དུ་ དགྲ་བཅོམ་པ་ ཉིད་ མངོན་སུམ་ དུ་ བྱེད་པ␣ར་ "
"འགྱུར་ ལ །"
msgstr ""
"Ce moine manifestera l'état d'arhat (lit. l'arhat-même) dans cette maison là"
" (lit. même). De plus,"

msgctxt "485b938dc076424389807a67fed32e22"
msgid ""
"ཕ་མ་ གཉིས་ དང་ ཁྱིམ་མི་ རྣམས་ ཀྱང་ བདེན་པ་ རྣམས་ ལ་ འགོད་པ␣ར་ འགྱུར་ རོ་ "
"སྙམ་ དུ་"
msgstr ""
"il établira ses deux parents et les personnes de la maison dans les vérités."
" »"

# རིག་<«སྣར་»རིགས།>ནས།
msgctxt "2d30cbc62d3242a1b0cac82bc8f9a43e"
msgid "རིག་ ནས །"
msgstr "vit-il. Puis,"

msgctxt "7e3ca1f95b934b67be31499d4652b6f3"
msgid "ཚེ་ དང་ ལྡན་པ་ མ་ འགགས་པ␣ས་ སྨྲས་པ །"
msgstr "L'honorable Aniruddha dit : «"

msgctxt "d8c041a9077b485f8e21764c6bbe071f"
msgid "ཁྱེད་ གཉིས་ ཇི་ལྟར་ དགའ་བ་ བཞིན་ དུ་ གྱིས་ ཤིག  །"
msgstr "Faites comme il vous plaît à [tous] les deux. »"

# དེ་ནས་དེ་སྐད་<«གཡུང་»«ལི་»«པེ་»«སྣར་»«ཅོ་»«ཞོལ»–སྐད།>ཐོས་པའི་མོད་ལ།
msgctxt "de150af5dbfd47efb7d885c600f1ad77"
msgid "དེ་ནས་ དེ་སྐད་ ཐོས་པ␣འི་ མོད་ ལ །"
msgstr "Ensuite, dès qu'ils eurent entendu ces paroles,"

# ཁྱིམ་བདག་དེས། དགེ་སློང་དེ་བདག་གི་<«ལི་»«སྣར་»«ཅོ་»གིས།>ཁྱིམ་དུ་ཁྲིད་དེ།
msgctxt "d776a4500052438980a6fe493150e5ac"
msgid "ཁྱིམ་བདག་ དེ␣ས །  དགེ་སློང་ དེ་ བདག་ གི་ ཁྱིམ་ དུ་ ཁྲིད་ དེ །"
msgstr ""
"ce père de famille prit (lit. mena) ce moine dans sa (lit. de moi-même) "
"maison et"

msgctxt "473ce670050845e2af76e2a112549d25"
msgid "སྨན་པ␣འི་ ལུང་ བཞིན་ དུ་ ཡོ་བྱད་ ཐམས་ཅད་ ཀྱིས་ ནད་གཡོག་ བྱས་ སོ ། །"
msgstr ""
"il le soigna avec tous les objets en accord avec la prescription (lit. "
"transmission orale) du médecin."

msgctxt "26ed16a0ec7743c984ad332753f44635"
msgid ""
"དགེ་སློང་ དེ␣ས་ ཀྱང་ ཁྱིམ་ དེ་ཉིད་ ན་ འདུག་ བཞིན་ དུ་ ནད་ ཀྱིས་ སྐྱོ་བ␣ར་ "
"གྱུར་ ནས །"
msgstr ""
"Ce moine devint désillusionné du fait de cette maladie tandis qu'il se "
"trouvait dans cette même maison et,"

# བརྩོན་པ་དང་བསྒྲུབ་<«ཞོལ་»སྒྲུབ།>པ་དང་འབད་པས་ཉོན་མོངས་པ་ཐམས་ཅད་སྤངས་ནས།
msgctxt "109f20e37639415280990de67c007ce2"
msgid "བརྩོན་པ་ དང་ བསྒྲུབ་པ་ དང་ འབད་པ␣ས་ ཉོན་མོངས་པ་ ཐམས་ཅད་ སྤངས་ ནས །"
msgstr ""
"il élimina toutes les émotions perturbatrices en s'efforçant, s'appliquant "
"et s'évertuant. Puis,"

# དགྲ་བཅོམ་པ་ཉིད་མངོན་སུམ་དུ་བྱས་ཏེ<«གཡུང་»«པེ་»སྟེ།>།
msgctxt "4dc43c2559cc481ea09af4d7150cf8d9"
msgid "དགྲ་བཅོམ་པ་ ཉིད་ མངོན་སུམ་ དུ་ བྱས་ ཏེ །"
msgstr "il manifesta l'état d'arhat (lit. l'arhat-même). Alors,"

msgctxt "d393b7f920284e778dc2a57a54a362fb"
msgid "དེ␣ས་ དགྲ་བཅོམ་པ་ ཐོབ་ ནས་"
msgstr "après être devenu un arhat,"

