msgctxt "45b8d0557b43460694c3c510916c4137"
msgid "དགྲ་བཅོམ་པ་ ཉིད་ མངོན་སུམ་ དུ་ བྱེད་པ␣ར་ གྱུར་ཅིག་ ཅེས་"
msgstr "puissé-je manifester l'état d'arhat (lit. l'arhat-même).\""

msgctxt "cdcb90c8f3e74937b87c6dafbaf81cf0"
msgid "བྱས་ སོ ། །"
msgstr "dit-il."

msgctxt "d86e1e1af55043808c906514e43e97c2"
msgid "དགེ་སློང་ དག་"
msgstr "Moines,"

msgctxt "f977901f0fdf4222904a79ae2839e2e6"
msgid "ཇི་སྙམ་ དུ་ སེམས །"
msgstr "qu'en pensez-vous ?"

