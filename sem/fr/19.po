# ལས་བརྒྱ་ཐམ་<«གཡུང་»ཐམས།>པ་
msgctxt "a8cb50b652b74751821cf6a8dec338dd"
msgid "ལ␣ས་ བརྒྱ་ ཐམ་པ་"
msgstr "Les cent karmas"

msgctxt "e43098ba32084cde8063a10c8b3e263a"
msgid "བམ་པོ་ དྲུག་པ །"
msgstr "Sixième feuillet"

msgctxt "972a914978244e71b159c1ce8b4f7a7e"
msgid "ས་འཚོ་ མ་ ཞེས་ བྱ་བ་ ནི །"
msgstr "Celle nommée Gopā"

msgctxt "4ce523a49297404fa3895703a5a632c3"
msgid "གླེང་ གཞི་ སེར་སྐྱ␣འི་ གནས་ ན་ བཞུགས་ ཏེ །"
msgstr ""
"Cette histoire [fut contée] alors que le Bienheureux séjournait à "
"Kapilavastu."

# གང་གི་ཚེ་ལྷ་སྦྱིན་གྱིས་ཨུད་པ་<«གཡུང་»«ལི་»«པེ་»«ཅོ་»ལྷ་བྱིན་གྱིས་ཨུད་པ།
# «སྣར་»«ཞོལ»ལྷས་བྱིན་གྱིས་ཨུཏྠ།>ལའི་ཁ་དོག་མ་བསད་<«གཡུང་»གསད།>ནས་
msgctxt "f8932cc5d2a94a878d720d81e247b76a"
msgid "གང་ གི་ ཚེ་ ལྷ་ སྦྱིན་ གྱིས་ ཨུད་པ་ ལ␣འི་ ཁ་དོག་ མ་ བསད་ ནས་"
msgstr "Lorsque Devadatta eut tué Utpalavarṇā et"

# དེ་རྒྱལ་པོ་གསལ་རྒྱལ་གྱིས་ཡུལ་ནས་བསྐྲད་པ་དེའི་ཚེ།
# དེ་<«གཡུང་»«ལི་»«པེ་»«སྣར་»«ཅོ་»«ཞོལ་»–དེ།>ཡུལ་སེར་སྐྱའི་གནས་སུ་སོང་ནས་
msgctxt "99291f230ddc4ee095b93a2f23340dec"
msgid ""
"དེ་ རྒྱལ་པོ་ གསལ་རྒྱལ་ གྱིས་ ཡུལ་ ནས་ བསྐྲད་པ་ དེ␣འི་ ཚེ །  དེ་ ཡུལ་ "
"སེར་སྐྱ␣འི་ གནས་ སུ་ སོང་ ནས་"
msgstr ""
"qu'il fut expulsé du royaume du roi Prasenajit, il alla dans le pays de "
"Kapilavastu et"

msgctxt "592389a8548e4613bd3112d257af61eb"
msgid "བསམས་པ་"
msgstr "il pensa : «"

msgctxt "16d0dcdf56a6418fbe5619b0bb2ac391"
msgid "བདག་ གིས་ དགེ་སྦྱོང་ གཽ་ ཏ་ མ་ གསོད་ ཀྱང་ མ་ནུ␣ས་ ལ །"
msgstr "Je ne parviendrai pas à tuer l'ascète Gautama. De plus,"

# སངས་རྒྱས་ཀྱི་ཐབས་ཀྱང་མ་ཐོབ་ཀྱིས་<«ཞོལ་»གོས་ཀྱི།>
msgctxt "bc5268a70b7c43bba21520c764d3829f"
msgid "སངས་རྒྱས་ ཀྱི་ ཐབས་ ཀྱང་ མ་ ཐོབ་ ཀྱིས་"
msgstr "je n'ai pas obtenu les moyens du Bouddha. Donc,"

msgctxt "2e423cbff4474e1fb1eaf6fde49dd6db"
msgid "བདག་ སོང་ ལ་"
msgstr "je vais aller et"

# དོན་གྲུབ་ཀྱི་བཙུན་མོ་<«ཞོལ་»མོའི།>འཁོར་རྣམས་ཁ་དྲངས་ཏེ།
msgctxt "6c4e724eb8594d5588f1c156adeef730"
msgid "དོན་གྲུབ་ ཀྱི་ བཙུན་མོ་ འཁོར་ རྣམས་ ཁ་དྲངས་ ཏེ །"
msgstr "je vais attirer à moi la reine et l'entourage de Siddhārtha et"

msgctxt "45811f81ea6449738a85ac4c1b45aba4"
msgid "དེ་དག་ དང་ བསྡོངས་ ལ་"
msgstr "je vais rester en leur compagnie et"

msgctxt "68250fa04a034a61ae6e5d6feec112de"
msgid "རྒྱལ་ས་ ཐོབ་ ནས་"
msgstr "j'obtiendrai la royauté et"

msgctxt "bff83f876225406c8a8ecfb32cde818a"
msgid "རྒྱལ་པོ་ བྱ␣འོ་ སྙམ་ དུ་"
msgstr "je régnerai »,"

msgctxt "f290761bbc7f459bba8d2f0cf3e5d6a6"
msgid "བསམས་ ཏེ །"
msgstr "pensa-t-il. Puis,"

# དེས་གྲགས་འཛིན་མ་ལ་ཕྲིན་<«ཞོལ་»འཕྲིན།>པ་ཞིག་བཏང་སྟེ།
msgctxt "7827feba229846959460bf8243b1a5af"
msgid "དེ␣ས་ གྲགས་ འཛིན་མ་ ལ་ ཕྲིན་པ་ ཞིག་ བཏང་ སྟེ །"
msgstr "il envoya un message à Yaśodharā : «"

msgctxt "42341e7474794eb683917d7b8f2facfa"
msgid "གྲགས་ འཛིན་མ་"
msgstr "Yaśodharā,"

# ད་<«པེ་»དེ།>ནི་སྡིག་པ་ལ་དགའ་བ་དེས་ཅི་བྱས་<«ཁུ་»ནི་བྲིས།>པ་ལ་ལྟོས།
msgctxt "5e0e682633a5455d988e9cfd66d7d0a8"
msgid "ད་ནི་ སྡིག་པ་ ལ་ དགའ་བ་ དེ␣ས་ ཅི་ བྱས་པ་ ལ་ ལྟོས །"
msgstr "regarde ce que celui qui aime les mauvaises actions fait maintenant."

msgctxt "af9ba23252804113a9cf3d46d09579bb"
msgid "བུད་མེད་ དྲུག་ ཁྲི་ ཆུང་མ␣ར་ བླངས་ ནས་"
msgstr "Il prend soixante mille femmes comme épouses et"

msgctxt "d8a9c194b5194f8bbb24245a5cc9a5d4"
msgid ""
"སྡུག་པ␣ར་ བྱས་པ་ འདི␣ས །  གལ་ཏེ་ དེ␣ས་ འདོད་པ་ རྣམས་ ཡོངས་སུ་ སྤྱད་པ␣ར་ མི་ "
"འདོད་ ན་ ནི་ བཙུན་མོ་ བླངས་ ཏེ་"
msgstr ""
"prend soin d'elles. Par rapport à ça, s'il ne veut pas profiter entièrement "
"des désirs, prendre des reines et"

msgctxt "7a52a34cf60f427bb8335a749aaeea9c"
msgid "གཞག་ ཀྱང་ ཅི་ དགོས །"
msgstr "les garder, à quoi ça sert ?"

# ད་<«སྣར་»«ཞོལ་»དེ།>ཡང་གལ་ཏེ་ཁྱོད་དགའ་ན་ནི་བདག་ཅག་གིས་ཀྱང་རྒྱལ་པོ་ཐོབ་པར་འགྱུར་ལ་
msgctxt "44f919772a684706b7ed6a3bd3077110"
msgid ""
"ད་ ཡང་ གལ་ཏེ་ ཁྱོད་ དགའ་ ན་ ནི་ བདག་ཅག་ གིས་ ཀྱང་ རྒྱལ་པོ་ ཐོབ་པ␣ར་ འགྱུར་ "
"ལ་"
msgstr ""
"Maintenant, si tel est ton désir, nous pourrons obtenir le statut de roi et"

# [65a]ཁྱོད་ཀྱང་སྔོན་བཞིན་དུ་དཔལ་དང་སྐལ་བ་<«གཡུང་»དཔལ་དང་བསྐལ་བ།
# «པེ་»དལ་དང་བསྐལ་པ།>དང་ལྡན་པར་འགྱུར་རོ་ཞེས་
msgctxt "db6a6d1b352840a4b5a71296c095e5fc"
msgid "ཁྱོད་ ཀྱང་ སྔོན་བཞིན་ དུ་ དཔལ་ དང་ སྐལ་བ་ དང་ ལྡན་པ␣ར་ འགྱུར་ རོ་ ཞེས་"
msgstr "tu obtiendras comme avant la gloire et les privilèges »,"

msgctxt "04352277a15d4098a52a4af1fa2daf4e"
msgid "སྤྲིང་ ངོ་ ། །"
msgstr "fit-il dire."

msgctxt "0596329e1f3e4cb497a683545f817396"
msgid "དེ་ནས་ དེ་ ཐོས་ མ་ ཐག་ ཏུ་ གྲགས་ འཛིན་མ␣ས་ ས་འཚོ་ མ་ལ་ བསྙད་ དེ །"
msgstr ""
"Ensuite, dès qu'elle eut entendu ceci, Yaśodharā le raconta à Gopā : «"

msgctxt "6d25bec633cc481abd9e513c31b5e2e3"
msgid "ས་འཚོ་ མ་"
msgstr "Gopā,"

# ལྷ་སྦྱིན་<«གཡུང་»འཚོ་བ་ལྷ་བྱིན། «པེ་»འཚོ་མ་ལྷ་བྱིན།
# «སྣར་»«ཞོལ་»འཚོ་མ་ལྷས་བྱིན།>གྱིས་ནི་བདག་ལ་འདི་སྐད་ཅེས་སྤྲིང་ངོ་ཞེས་
msgctxt "988f8b1ae67b42b38924ba315722b0f9"
msgid "ལྷ་ སྦྱིན་ གྱིས་ ནི་ བདག་ ལ་ འདི་སྐད་ ཅེས་ སྤྲིང་ ངོ་ ཞེས་"
msgstr "voici ce que Devadatta m'a fait dire »,"

msgctxt "cda1ef74da8e495cb108c118c78c5a6c"
msgid "བྱས་ སོ ། །"
msgstr "dit-elle."

msgctxt "af7b016d5c404819a7d190b8fcbc0de4"
msgid "དེ་ནས་ ས་འཚོ་ མ་ ན་རེ་"
msgstr "Ensuite, Gopā dit : «"

# འུ་བུ་ཅག་<«གཡུང་»«པེ་»ཅིག>གིས་ཐབས་<«ཅོ་»ཐོབས།>ཤིག་གིས་དེ་<«གཡུང་»ནེ།
# «པེ་»ནི།>ནང་དུ་བཀུག་ལ་
msgctxt "2ed356bc7aa84496a880b11464eb3108"
msgid "འུ་བུ་ཅག་ གིས་ ཐབས་ ཤིག་ གིས་ དེ་ ནང་ དུ་ བཀུག་ ལ་"
msgstr "Nous utiliserons un moyen pour le faire venir à l'intérieur et"

# དམའ་ཕབ་སྟེ་<«གཡུང་»སྨ་ཕབ་སྟེ། «ལི་»«ཅོ་»དམའ་ཕབ་ཏེ། «པེ་»སྨས་ཕབ་སྟེ།
# «སྣར་»«ཞོལ་»དམའ་ཕབ་སྟེ་མ།>
msgctxt "3f63352d067d46e495c0eb3693bd24bb"
msgid "དམའ་ ཕབ་ སྟེ་"
msgstr "nous le rabaisserons et"

# བཏང་ན<«ཁུ་»ནས།>།
msgctxt "87e8f3a0e82c4c51891f34f40861e60e"
msgid "བཏང་ ན །"
msgstr "nous le renverrons. Alors,"

# དེ་ཐ་མལ་པར་འདུག་པར་<«གཡུང་»«པེ་»«སྣར་»«ཞོལ་»+མི།>འགྱུར་རོ་ཞེས་
msgctxt "b7ff98cd803440dfa4285fa99637a5d9"
msgid "དེ་ ཐ་མལ་པ␣ར་ འདུག་པ␣ར་ འགྱུར་ རོ་ ཞེས་"
msgstr "il se trouvera [être] un être ordinaire »,"

msgctxt "62a11fc5a91a45b0834f0c25c9fc8b70"
msgid "ཟེར་ རོ ། །"
msgstr "dit-elle."

# དེ་ནས་དེ་རྣམས་ཀྱིས་ལྷ་སྦྱིན་<«སྣར་»«ཞོལ་»ལྷས་བྱིན།>ལ་སྤྲིང་བ།
msgctxt "72cf08c945ed42648ba43ab894502fe5"
msgid "དེ་ནས་ དེ་ རྣམས་ ཀྱིས་ ལྷ་ སྦྱིན་ ལ་ སྤྲིང་བ །"
msgstr "Ensuite, elles firent dire à Devadatta : «"

# ལྷ་སྦྱིན་<«སྣར་»«ཞོལ་»ལྷས་བྱིན།>
msgctxt "fc170e900abf48d9af66e2c44218cdf9"
msgid "ལྷ་ སྦྱིན་"
msgstr "Devadatta,"

msgctxt "e1b496c8f2cf4a64b32056c9ef2c550e"
msgid "རྒྱལ་པོ␣འི་ ཕོ་བྲང་ གི་ ནང་ དུ་ སྤྱོན་ ཅིག་ དང་"
msgstr "viens donc dans le palais royal."

msgctxt "353053afaaf6470da0cf01d937c3a53e"
msgid "འདི␣ར་ གཤེགས་ ནས་"
msgstr "Quand tu seras venu,"

msgctxt "a0fdd0a8f37242119d4b4c5e306c7854"
msgid ""
"ཐབས་ ཇི་ལྟ་བུ་ ཞིག་ གིས་ བདག་ཅག་ གིས་ ཁྱོད་ རྒྱལ་ས␣ར་ བཞུགས་པ་ བཀའ་གྲོས་ "
"མཛད་ དོ་ ཞེས་"
msgstr "nous discuterons du moyen pour t'établir dans la royauté »,"

msgctxt "e332052e4ce54a5d8752c5ea48f84531"
msgid "སྤྲིང་ ངོ་ ། །"
msgstr "firent-elles dire."

# དེ་ནས་ལྷ་སྦྱིན་<«སྣར་»«ཞོལ་»ལྷས་བྱིན།>གྱིས་དེ་ཐོས་མ་ཐག་ཏུ་དགའ་ཆེས་ནས་
msgctxt "a523fa05712f48668669a5d5608fa860"
