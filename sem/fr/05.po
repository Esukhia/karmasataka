msgid "ཆེ་ཞེ་"
msgstr "Sœur aînée,"

# བདག་ཅག་གིས་ནི་སྡོང་པོ་ཡོན་པོ་རྣམས་ཐིག་གིས་<«གཡུང་»གི།>བཏབ་ནས་
msgctxt "b4f650db95f04892899c4f106309f6e6"
msgid "བདག་ཅག་ གིས་ ནི་ སྡོང་པོ་ ཡོན་པོ་ རྣམས་ ཐིག་ གིས་ བཏབ་ ནས་"
msgstr "Nous traçons des lignes sur les troncs tordus et"

# སྟ་རེས་བཞོགས་<«གཡུང་»ལྟ་རེས་བཞོགས། «ཞོལ་»སྟ་རེས་གཞོགས།>ཏེ་
msgctxt "c6d77559b14d448bb4ec77c6d9f055e5"
msgid "སྟ་རེ␣ས་ བཞོགས་ ཏེ་"
msgstr "nous les taillons à la hache et"

# དྲང་པོར་སྲོང་ན<«གཡུང་»«པེ་»ནས།>།
msgctxt "d891e9d47b524c48809439a2bd886568"
msgid "དྲང་པོ␣ར་ སྲོང་ ན །"
msgstr "nous les redressons. Donc,"

# གལ་ཏེ་ཁྱོད་<«གཡུང་»«པེ་»ཁྱེད།>ཀྱིས་གནོད་པ་དེ་ལྟ་བུ་བཟོད་ནུས་ན་ནི།
# བདག་ཅག་གིས་ཁྱོད་ཀྱི་རྒྱབ་ཐིག་གིས་བཏབ་སྟེ་
msgctxt "a34ea2e1467041a492664f9c9a9bbcc7"
msgid ""
"གལ་ཏེ་ ཁྱོད་ ཀྱིས་ གནོད་པ་ དེ་ལྟ་ བུ་ བཟོད་ ནུས་ ན་ ནི །  བདག་ཅག་ གིས་ ཁྱོད་"
" ཀྱི་ རྒྱབ་ ཐིག་ གིས་ བཏབ་ སྟེ་"
msgstr ""
"si vous êtes capables de supporter un tel traitement (lit. un tel mal), nous"
" tracerons des lignes sur votre dos et"

# སྟ་རེས་བཞོགས་<«ཞོལ་»གཞོགས།>ལ་
msgctxt "ff11cd907713423ab0d74b22da48675d"
msgid "སྟ་རེ␣ས་ བཞོགས་ ལ་"
msgstr "nous le taillerons à la hache et"

msgctxt "720d6fa267c649c9ab949c364576be38"
msgid "དྲང་པོ␣ར་ བསྲང་ ངོ་ ། །"
msgstr "nous le redresserons. »"

msgctxt "255fc23054ca4aafa214c5390371880c"
msgid "དེ␣ས་ སྨྲས་པ །"
msgstr "Elle répondit : «"

msgctxt "11f82087ed524975a582f9fccf28b444"
msgid "ཐབས་ དེ␣ས་ ནི་ བདག་ འཆི་བ␣ར་ འགྱུར་ ཏེ །"
msgstr "De cette manière, je vais mourrir ;"

msgctxt "725a9ffca54248708f140c7ecba35ef2"
msgid "བདག་ འཚོ་བ␣འི་ སྐབས་ ཀྱང་ མེད་ དོ ། །"
msgstr "je n'ai aucune chance (lit. aucun opportunité de) de vivre. »"

msgctxt "363bfe0034e74db9b2ee00b2d0e2b24b"
msgid "ཤིང་ མཁན་ རྣམས་ ཀྱིས་ སྨྲས་པ །"
msgstr "Les charpentiers dirent : «"

# ཁོ་བོ་<«ཅོ་»+ཅ།>ཅག་གི་<«ཞོལ་»གིས།>ཤེས་པ་ནི་དེ་ཁོ་ནར་ཟད་དེ<«ཅོ་»དོ།>།
msgctxt "57b532b7c59c40349592d86ed8ef4051"
msgid "ཁོ་བོ་ ཅག་ གི་ ཤེས་པ་ ནི་ དེ་ ཁོ་ན␣ར་ ཟད་ དེ །"
msgstr "Nos connaissances se limitent à ça. Alors,"

# དེ་ཕན་ཅད་<«སྣར་»«ཞོལ་»ཆད།>ཇི་ལྟར་བྱ་བ་ནི་ཁྱོད་ཉིད་ཤེས་སུ་ཟད་དོ། །
msgctxt "aedd4e7e24e54958938b72f464e9749c"
msgid "དེ་ ཕན་ ཅད་ ཇི་ལྟར་ བྱ་བ་ ནི་ ཁྱོད་ ཉིད་ ཤེས་ སུ་ ཟད་ དོ ། །"
msgstr ""
"après ça, ce que vous ferez dépend de vous (lit. vous le savez vous-même)."

msgctxt "3567ff2dce7f4e2f9214336952eb3c8e"
msgid "དེ་ནས་ དེ་ སྐྱེངས་ ཏེ་"
msgstr "Ensuite, elle devint honteuse et"

msgctxt "cff9f0b42a3b4dde9013e0d12d68a8f1"
msgid "རེ་ཐག་ ཆད་ ནས །"
msgstr "perdit tout espoir et"

msgctxt "3c32f677da254323a6a8e4d017fadb1d"
msgid "ཁ་རོག་པ␣ར་ འདུག་ སྟེ །"
msgstr "se tint en silence et"

msgctxt "dfce486da1f24188ab43ee94a5b42dc5"
msgid "སུ་ ལ་ ཡང་ ཅུང་ཟད་ ཀྱང་ སྨྲ་བ་ མ་ ཕོད་ དོ ། །"
msgstr "n'osa en toucher le moindre mot (lit. même un peu) à personne."

msgctxt "1d417bfc05574cfa9e58ddfaa73c72ea"
msgid "དགེ་སློང་ དག་"
msgstr "Moines,"

msgctxt "18faed851e11460ca5e8e886d8ef235f"
msgid "ཇི་སྙམ་ དུ་ སེམས་"
msgstr "qu'en pensez-vous ?"

# དེའི་ཚེ། རྒན་མོར་<«གཡུང་»བགོད་པར།
# «པེ་»བརྒན་མོར།>གྱུར་པ་གང་ཡིན་པ་དེ་ནི་ཚོན་མོ་དགའ་མོ་འདི་ཉིད་ཡིན་ནོ། །
msgctxt "59a6a0a93bbd4c8e82a1366b296319e5"
msgid ""
"དེ␣འི་ ཚེ །  རྒན་མོ␣ར་ གྱུར་པ་ གང་ཡིན་པ་ དེ་ ནི་ ཚོན་ མོ་ དགའ་མོ་ འདི་ཉིད་ "
"ཡིན་ ནོ ། །"
msgstr "À cette époque, cette vieille dame est Sthūlanandā elle-même."

# དེའི་ཚེ་ཤིང་[24a]མཁན་དུ་གྱུར་པ་དེ་དག་ནི་དྲུག་སྡེ་<«ལི་»«ཅོ་»སྟེ།>འདི་དག་ཉིད་ཡིན་ཏེ།
msgctxt "b6a3592f52e24053bb00b3ec972d2ffa"
msgid ""
"དེ␣འི་ ཚེ་ ཤིང་ མཁན་ དུ་ གྱུར་པ་ དེ་དག་ ནི་ དྲུག་ སྡེ་ འདི་དག་ ཉིད་ ཡིན་ ཏེ "
"།"
msgstr ""
"À cette époque, ceux qui étaient les charpentiers sont les [membres du] "
"Groupe des Six. Ainsi,"

msgctxt "d37653bc728d4548a334f51bf1c68612"
msgid "དེ␣འི་ ཚེ་ ཡང་ འདི་ དེ་དག་ གིས་ བྲིད་ ནས་"
msgstr "à cette époque, ils la trompèrent et"

msgctxt "8a65802260354076bb7d1d5761084ac4"
msgid "དོན་ དེ་ མ་ གྲུབ་བོ ། །"
msgstr "elle n'obtint pas ce qu'elle voulait (lit. son sens)."

# ད་ལྟར་ཡང་འདི་དེ་<«གཡུང་»–དེ།>དག་གིས་བྲིད་ནས་
msgctxt "08306e3cb4cb42e391790b0f7ba1e7bd"
msgid "ད་ལྟར་ ཡང་ འདི་ དེ་དག་ གིས་ བྲིད་ ནས་"
msgstr "Maintenant aussi, ils la trompèrent et"

msgctxt "480eef8ab12b4dffbc42cd3532088c9f"
msgid "འདི་ རྫུ་འཕྲུལ་ ཡང་ མ་གྲུབ་པ་ ཡིན་ ནོ །། །།"
msgstr "elle n'obtint pas non plus les pouvoirs surnaturels."
