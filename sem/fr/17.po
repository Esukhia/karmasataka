msgctxt "b38db65b1c244306adfc4164d778b17d"
msgid "ཤིང་རྟ་ བཞི་པ་ ནི །"
msgstr "Le quatrième chariot"

# གླེང་གཞི་མཉན་<«གཡུང་»བཞི་གཉན།>དུ་ཡོད་པ་ན་བཞུགས་ཏེ།
msgctxt "ee88e0c963f94d5f890b617abdf90767"
msgid "གླེང་ གཞི་ མཉན་ དུ་ཡོད་པ་ ན་ བཞུགས་ ཏེ །"
msgstr ""
"Cette histoire [fut contée] alors que le Bienheureux séjournait à Śrāvastī."

# དེའི་ཚེ་བཅོམ་ལྡན་འདས་མཉན་<«གཡུང་»གཉན།
# «ཁུ་»མཉེན།>དུ་ཡོད་པ་ནས་རྒྱལ་པོའི་ཁབ་ཏུ་གཤེགས་སོ། །
msgctxt "8eea873bb70a402c84a7fa0fb29145c4"
msgid ""
"དེ␣འི་ ཚེ་ བཅོམ་ལྡན་འདས་ མཉན་ དུ་ཡོད་པ་ ནས་ རྒྱལ་པོ␣འི་ ཁབ་ ཏུ་ གཤེགས་ སོ ། "
"།"
msgstr "À cette époque, le Bienheureux était parti de Śrāvastī vers Rājagṛha."

msgctxt "4f9cfaeb3874444384fc74e62cf5f4b1"
msgid "བྲམ་ཟེ་ ཞིག་ ཀྱང་ ཤིང་རྟ་ ལ་ ཞོན་ ཏེ་"
msgstr "Un brahmane monta sur un chariot et"

msgctxt "d9f85b4895c9493dbabf568254f82b04"
msgid "ལམ་ དུ་ སྣོལ་མ␣ར་ འགྲོ་བ་ ལ␣ས །"
msgstr "se déplaçait en direction opposée (lit. mêlée) et"

msgctxt "d139896e64344d12862aafa4b22d9792"
msgid "བྲམ་ཟེ་ དེ␣ས་ བཅོམ་ལྡན་འདས་ མཐོང་ ནས་"
msgstr "ce brahmane vit le Bienheureux et"

msgctxt "4403ce1c903f41e485b23cea8f21d2da"
msgid "ཤིང་རྟ་ ལ␣ས་ བབས་ ཏེ་"
msgstr "descendit de son chariot et"

msgctxt "d0a5ba05c5814d7ea0190d7f40f2d9fa"
msgid "བཅོམ་ལྡན་འདས་ ལ་ ཤིང་རྟ་ བསྟབས་ སོ ། །"
msgstr "offrit le chariot au Bienheureux."

# བཅོམ་ལྡན་འདས་ཀྱིས་ཀྱང་དེ་ཉིད་ལ་ཐུགས་བརྩེ་བའི་ཕྱིར།
# ཤིང་རྟའི་སྟེང་<«གཡུང་»སྟའི།>གི་ནམ་མཁའ་ལ་བཞུགས་སོ། །
msgctxt "fe63cfda9c1f4fe291d387f457b05879"
msgid ""
"བཅོམ་ལྡན་འདས་ ཀྱིས་ ཀྱང་ དེ་ཉིད་ ལ་ ཐུགས་ བརྩེ་བ␣འི་ ཕྱིར །  ཤིང་རྟ␣འི་ "
"སྟེང་ གི་ ནམ་མཁའ་ ལ་ བཞུགས་ སོ ། །"
msgstr ""
"Le Bienheureux, par compassion pour lui, se tint dans l'espace au dessus du "
"chariot."

# དེ་ནས་བྲམ་ཟེ་དེ་<«གཡུང་»«པེ་»–དེ།>རྒྱུ་དེ་ལས་དགའ་བ་ཆེན་པོ་སྐྱེས་སོ། །
msgctxt "55933435e11443bf9b2f9f47b46e0c02"
msgid "དེ་ནས་ བྲམ་ཟེ་ དེ་ རྒྱུ་ དེ་ལས་ དགའ་བ་ ཆེན་པོ་ སྐྱེས་ སོ ། །"
msgstr ""
"Ensuite, grâce à ça (lit. cette cause), le brahmane ressentit une grande "
"joie."

msgctxt "3bca7841cc8647b6bbb4390e3c1b5745"
msgid "དགའ་བ་ ཆེན་པོ་ སྐྱེས་ ནས་"
msgstr "Ayant ressenti une grande joie,"

msgctxt "1dd8df5e58944b60b880e0e927bc8841"
msgid "བཅོམ་ལྡན་འདས་ ཀྱི་ ཐད་ ནས་ སོང་ ངོ་ ། །"
msgstr "il partit [d'avoir été] auprès du Bienheureux."

# དེའི་སྐབས་སུ་བཅོམ་ལྡན་འདས་ཀྱིས་འཛུམ་པ་མཛད་དོ<«གཡུང་»«པེ་»དེ།>། །
msgctxt "9a69a9af1e774c16a34866c046000fb2"
msgid "དེ␣འི་ སྐབས་ སུ་ བཅོམ་ལྡན་འདས་ ཀྱིས་ འཛུམ་པ་ མཛད་ དོ ། །"
msgstr "À ce moment, le Bienheureux sourit."

msgctxt "6218437a0f294c9ab3ce80d69401d984"
msgid ""
"འདི་ ནི་ སངས་རྒྱས་ བཅོམ་ལྡན་འདས་ རྣམས་ གང་ གི་ ཚེ །  འཛུམ་པ་ མཛད་པ␣འི་ "
"ཆོས་ཉིད་ དེ །"
msgstr ""
"ceci, quand les Bienheureux Bouddhas sourient, [c'est] la nature [des "
"sourires] que"

msgctxt "111a058a66a24fac94655768dfa27760"
msgid ""
"དེ␣འི་ ཚེ་ ཞལ་ ནས་ འོད་ཟེར་ སྔོན་པོ་ དང་ །  སེར་པོ་ དང་ །  དམར་པོ་ དང་ །  "
"དཀར་པོ་ རྣམས་ བྱུང་ ནས་"
msgstr ""
"à ces moments, des rayons de lumière bleus, jaunes, rouges et blancs sont "
"émis de sa bouche et"

msgctxt "893ae838717846ea97a1f6b6260fcccc"
msgid "ཁ་ཅིག་ ནི་ འོག་ ཏུ་ འགྲོ␣འོ ། །"
msgstr "certains vont vers le bas."

msgctxt "b472103a422c4746b64369108f4db89a"
msgid "ཁ་ཅིག་ ནི་ སྟེང་ དུ་ འགྲོ␣འོ ། །"
msgstr "Certains vont vers le haut."

# འོག་ཏུ་འགྲོ་བ་གང་ཡིན་པ་དེ་དག་ནི། སེམས་ཅན་དམྱལ་བ་ཡང་སོས་དང་། ཐིག་ནག་དང་།
# བསྡུས་འཇོམས་དང་། ངུ་འབོད་དང་། ངུ་འབོད་ཆེན་པོ་དང་། ཚ་བ་དང་། རབ་ཏུ་ཚ་བ་དང་།
# མནར་མེད་པ་དང་། ཆུ་བུར་ཅན་དང་། ཆུ་བུར་རྡོལ་བ་<«གཡུང་»«པེ་»–བ།>དང་།
# སོ་ཐམས་ཐམས་<«སྣར་»«ཞོལ་»ཐམ་ཐམ།>དང་། ཀྱི་ཧུད་ཟེར་བ་<«གཡུང་»–བ།>དང་།
# ཨ་ཆུ་ཟེར་བ་དང་། ཨུད་པལ་ལྟར་གས་པ་དང་། པད་མ་ལྟར་གས་པ་དང་།
# པད་མ་ལྟར་གས་པ་ཆེན་པོར་སོང་ནས།
msgctxt "8da2242c7cb745af9f7d3195b993c425"
msgid ""
"འོག་ ཏུ་ འགྲོ་བ་ གང་ཡིན་པ་ དེ་དག་ ནི །  སེམས་ཅན་ དམྱལ་བ་ ཡང་སོས་ དང་ །  "
"ཐིག་ནག་ དང་ །  བསྡུས་འཇོམས་ དང་ །  ངུ་འབོད་ དང་ །  ངུ་འབོད་ ཆེན་པོ་ དང་ །  "
"ཚ་བ་ དང་ །  རབ་ཏུ་ ཚ་བ་ དང་ །  མནར་མེད་པ་ དང་ །  ཆུ་བུར་ ཅན་ དང་ །  ཆུ་བུར་ "
"རྡོལ་བ་ དང་ །  སོ་ ཐམས་ ཐམས་ དང་ །  ཀྱི་ཧུད་ ཟེར་བ་ དང་ །  ཨ་ཆུ་ ཟེར་བ་ དང་ "
"།  ཨུད་ པལ་ ལྟར་ གས་པ་ དང་ །  པད་མ་ ལྟར་ གས་པ་ དང་ །  པད་མ་ ལྟར་ གས་པ་ "
"ཆེན་པོ␣ར་ སོང་ ནས །"
msgstr ""
"Ceux qui vont vers le bas vont chez les êtres infernaux de Résurrections, "
"Lignes-Noires, Réunion-et-Écrasement, Pleurs-et-Hurlements, Grands-Pleurs-"
"et-Hurlements, Brûlant, Extrêmement-Brûlant, Tourments-Insurpassables, "
"Couvert-de-Cloques, Cloques-Éclatées, Dents-qui-Claquent, Lamentations, "
"Quel-Froid, Fendu-comme-un-Lotus-Bleu, Fendu-comme-un-Lotus, Fendu-comme-un-"
"Grand-Lotus et"

# ཚ་བའི་སེམས་ཅན་དམྱལ་བ་གང་ཡིན་པ་དེ་དག་ཏུ་<«གཡུང་»«པེ་»–ཏུ།
# «ལི»«ཅོ་»–དུ།>ནི་བསིལ་བར་གྱུར་ཏེ་
msgctxt "778511dc3dab403a88ab27f41af32501"
msgid "ཚ་བ␣འི་ སེམས་ཅན་ དམྱལ་བ་ གང་ཡིན་པ་ དེ་དག་ ཏུ་ ནི་ བསིལ་བ␣ར་ གྱུར་ ཏེ་"
msgstr "ils rafraîchissent les êtres des enfers chauds et"

msgctxt "91eb356a540747c18bc09950c0ed4b5a"
msgid "འབབ་བོ ། །"
msgstr "ils les touchent (lit. tombent)."

# གྲང་བའི་སེམས་ཅན་དམྱལ་བ་གང་ཡིན་པ་དེ་དག་ཏུ་<«གཡུང་»«པེ་»–ཏུ།>ནི་དྲོ་བར་གྱུར་ཏེ་
msgctxt "fea066f94f22469980b682427d407271"
msgid "གྲང་བ␣འི་ སེམས་ཅན་ དམྱལ་བ་ གང་ཡིན་པ་ དེ་དག་ ཏུ་ ནི་ དྲོ་བ␣ར་ གྱུར་ ཏེ་"
msgstr "Ils réchauffent les être des enfers froids et"

msgctxt "6644d33ad7724d86b75381776dd3f928"
msgid "འབབ་བོ ། །"
msgstr "ils les touchent (lit. tombent)."

# དེས་ན་སེམས་ཅན་དེ་དག་གི་གནོད་པའི་ཁྱད་པར་རྣམས་རྩེ་ཞིལ་བར་གྱུར་ཏོ<«གཡུང་»«ལི་»«པེ་»«སྣར་»«ཅོ་»འགྱུར་རོ།>།
# །
msgctxt "9481e08c0fbc4acfa77802c6e8b8f220"
msgid ""
"དེས་ན་ སེམས་ཅན་ དེ་དག་ གི་ གནོད་པ␣འི་ ཁྱད་པར་ རྣམས་ རྩེ་ ཞིལ་ བར་ གྱུར་ ཏོ །"
" །"
msgstr "Ensuite, les souffrances particulières de ces êtres sont amenuisées."

# དེ་ནས་དེ་དག་འདི་སྙམ་དུ་<«གཡུང་»«པེ་»–དུ།>སེམས་ཏེ།
msgctxt "dbdcc6c635134fa0b2cd09c055d129f0"
msgid "དེ་ནས་ དེ་དག་ འདི་ སྙམ་ དུ་ སེམས་ ཏེ །"
msgstr "Ensuite, ils pensent ceci : «"

msgctxt "0789e5f8e14f4de194e1071ed25bd3b1"
msgid "ཀྱེ་"
msgstr "Hé !"

msgctxt "4829f4de209549469db346da35d513b2"
msgid "ཅི་"
msgstr "Qu'[en est-il] ?"

# བདག་ཅག་<«གཡུང་»«ལི་»«པེ་»«སྣར་»«ཅོ་»«ཞོལ་»–ཅག>འདི་ནས་ཤི་འཕོས་སམ་
msgctxt "6e1ddfe307b7403a8d323181d0a9e8e8"
msgid "བདག་ཅག་ འདི་ ནས་ ཤི་འཕོས་ སམ་"
msgstr "Sommes-nous morts et partis de cet endroit ?"

msgctxt "c9f08a9c3d0d479683d3bb95eb1508eb"
msgid "འོན་ཏེ་ གཞན་ དུ་ སྐྱེས་ སམ་ སྙམ་ མོ ། །"
msgstr "Ou bien sommes-nous nés ailleurs ? »"

# དེ་ནས་དེ་དག་དད་པ་བསྐྱེད་པའི་ཕྱིར་བཅོམ་ལྡན་འདས་སྤྲུལ་པ་འགྱེད་པར་མཛད་<«པེ་»འགྱིད་པར་མཛད།
# «ཅོ་»ཛད།>དེ།
msgctxt "0279de9c8fb54c68b9eb4f524e58944f"
msgid ""
