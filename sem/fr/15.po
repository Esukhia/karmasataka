msgctxt "740a08be2ac24f6f97921bc89ba42caf"
msgid "ཤིང་རྟ་ གཉིས་པ་ ནི །"
msgstr "Le deuxième chariot"

msgctxt "6a8ce7e0e71944b584c46fbc6f40ea97"
msgid "གླེང་ གཞི་ མཉན་ དུ་ཡོད་པ་ ན་ བཞུགས་ ཏེ །"
msgstr ""
"Cette histoire [fut contée] alors que le Bienheureux séjournait à Śrāvastī."

msgctxt "3431cc9340084e7c9088d0735ba88a3d"
msgid "དེ␣འི་ ཚེ་ བྲམ་ཟེ␣འི་ ཁྱེའུ་ ཞིག་ གིས་ མཆོད་སྦྱིན་ བྱས་ ནས་"
msgstr "À cette époque, un brahmane fit une offrande rituelle et"

msgctxt "278874b0b77c45db86bb0dc862588916"
msgid "ཤིང་རྟ་ ལ་ ཞོན་ ཏེ་"
msgstr "il monta sur un chariot et"

msgctxt "b1dd6e12203a44e3bd7c3ce4788dfa17"
msgid "འཁོར་ གྱི་ ཚོགས་ ཀྱིས་ གཡོག་ བྱས་ ནས་"
msgstr "avec [comme] serviteurs l'assemblée de son entourage,"

msgctxt "e6dfbe68a9764e6ca57cdc211df203aa"
msgid "མཉན་ དུ་ཡོད་པ␣འི་ ནང་ དུ་ ཆ␣ས་ སོ ། །"
msgstr "il entra dans Śrāvastī."

# བཅོམ་ལྡན་འདས་ཀྱང་མཉན་དུ་ཡོད་པར་བསོད་སྙོམས་མཛད་<«ཁུ་»འཛད།>ནས།
msgctxt "8c8815f3fe494050977606be6c9a1ba2"
msgid "བཅོམ་ལྡན་འདས་ ཀྱང་ མཉན་ དུ་ཡོད་པ␣ར་ བསོད་སྙོམས་ མཛད་ ནས །"
msgstr "Le Bienheureux, lui aussi, faisait l'aumône à Śrāvastī et"

msgctxt "f71332aeb8404888b4de7e4df8ccd8d1"
msgid "མཉན་ དུ་ཡོད་པ་ ནས་ སླར་ གཤེགས་པ་ དང་"
msgstr "il rentra de Śrāvastī. Puis,"

msgctxt "b4c23d797d904627b69743e25e8d1fa3"
msgid "དེ་ དགའ་བ་ སྐྱེས་ ནས་"
msgstr "il ressentit de la joie et"

msgctxt "6871e30a55df4773af81976a3b664206"
msgid "བཅོམ་ལྡན་འདས་ ལ་ བལྟས་ སོ ། །"
msgstr "il regarda le Bienheureux."

# དེའི་སྐབས་སུ་བཅོམ་ལྡན་འདས་ཀྱིས་འཛུམ་<«ཁུ་»མཛུམ།>པ་མཛད་དེ།
msgctxt "ea6a4d8a6a7c4f25a082b4b9be9a894d"
msgid "དེ␣འི་ སྐབས་ སུ་ བཅོམ་ལྡན་འདས་ ཀྱིས་ འཛུམ་པ་ མཛད་ དེ །"
msgstr "À ce moment, le Bienheureux sourit et"

msgctxt "8bcbb893482f4a10aab4f955a6d9ef5c"
msgid ""
"འདི་ ནི་ སངས་རྒྱས་ བཅོམ་ལྡན་འདས་ རྣམས་ གང་ གི་ ཚེ་ འཛུམ་པ་ མཛད་པ␣འི་ "
"ཆོས་ཉིད་ དེ །"
msgstr ""
"ceci, quand les Bienheureux Bouddhas sourient, [c'est] la nature [des "
"sourires] que"

msgctxt "56fd50048bb7496bb4c2d7d126a2a24b"
msgid ""
"དེ␣འི་ ཚེ་ ཞལ་ ནས་ འོད་ཟེར་ སྔོན་པོ་ དང་ །  སེར་པོ་ དང་ །  དམར་པོ་ དང་ །  "
"དཀར་པོ་ རྣམས་ བྱུང་ ནས་"
msgstr ""
"à ces moments, des rayons de lumière bleus, jaunes, rouges et blancs sont "
"émis de sa bouche et"

msgctxt "54e5919de4d546c09578339962cf6ea4"
msgid "ཁ་ཅིག་ ནི་ འོག་ ཏུ་ འགྲོ␣འོ ། །"
msgstr "certains vont vers le bas."

msgctxt "dc2b3b8a40d0436ab1569a8c9917d605"
msgid "ཁ་ཅིག་ ནི་ སྟེང་ དུ་ འགྲོ␣འོ ། །"
msgstr "Certains vont vers le haut."

# འོག་ཏུ་འགྲོ་བ་གང་ཡིན་པ་དེ་དག་ནི་སེམས་ཅན་དམྱལ་བ་ཡང་སོས་དང་ཐིག་ནག་[54b]དང་།
# བསྡུས་འཇོམས་དང་། ངུ་འབོད་དང་། ངུ་འབོད་ཆེན་པོ་དང་། ཚ་བ་དང་། རབ་ཏུ་ཚ་བ་དང་།
# མནར་མེད་པ་དང་། ཆུ་བུར་ཅན་དང་། ཆུ་བུར་རྡོལ་དང་།
# ས་<«གཡུང་»+སོ།>ཐམས་ཐམས་<«ཅོ་»+པ།>དང་། ཀྱི་ཧུད་ཟེར་བ་<«གཡུང་»–བ།>དང་།
# ཨ་ཆུ་ཟེར་བ་<«གཡུང་»–བ།>དང་། ཨུད་པལ་ལྟར་གས་པ་དང་། པད་མ་ལྟར་གས་པ་དང་།
# པད་མ་ལྟར་གས་པ་ཆེན་པོར་སོང་ནས།
msgctxt "13fbf5ec8d7d407da8a917010aa05b74"
msgid ""
"འོག་ ཏུ་ འགྲོ་བ་ གང་ཡིན་པ་ དེ་དག་ ནི་ སེམས་ཅན་ དམྱལ་བ་ ཡང་སོས་ དང་ ཐིག་ནག་ "
"དང་ །  བསྡུས་འཇོམས་ དང་ །  ངུ་འབོད་ དང་ །  ངུ་འབོད་ ཆེན་པོ་ དང་ །  ཚ་བ་ དང་ "
"།  རབ་ཏུ་ ཚ་བ་ དང་ །  མནར་མེད་པ་ དང་ །  ཆུ་བུར་ ཅན་ དང་ །  ཆུ་བུར་ རྡོལ་ དང་"
" །  ས་ཐམས་ ཐམས་ དང་ །  ཀྱི་ཧུད་ ཟེར་བ་ དང་ །  ཨ་ཆུ་ ཟེར་བ་ དང་ །  ཨུད་ པལ་ "
"ལྟར་ གས་པ་ དང་ །  པད་མ་ ལྟར་ གས་པ་ དང་ །  པད་མ་ ལྟར་ གས་པ་ ཆེན་པོ␣ར་ སོང་ ནས"
" །"
msgstr ""
"Ceux qui vont vers le bas vont chez les êtres infernaux de Résurrections, "
"Lignes-Noires, Réunion-et-Écrasement, Pleurs-et-Hurlements, Grands-Pleurs-"
"et-Hurlements, Brûlant, Extrêmement-Brûlant, Tourments-Insurpassables, "
"Couvert-de-Cloques, Cloques-Éclatées, Dents-qui-Claquent, Lamentations, "
"Quel-Froid, Fendu-comme-un-Lotus-Bleu, Fendu-comme-un-Lotus, Fendu-comme-un-"
"Grand-Lotus et"

msgctxt "967fd15ce0854d6eb7f1e8f58efbd73c"
msgid "ཚ་བ␣འི་ སེམས་ཅན་ དམྱལ་བ་ གང་ཡིན་པ་ དེ་དག་ ཏུ་ བསིལ་བ␣ར་ གྱུར་ ཏེ་"
msgstr "ils rafraîchissent les êtres des enfers chauds et"

# འབབ་<«གཡུང་»འབུབ།>བོ། །
msgctxt "54b19bdde83e4fef9baf0de6cf964e44"
msgid "འབབ་བོ ། །"
msgstr "ils les touchent (lit. tombent)."

# གྲང་བའི་སེམས་ཅན་དམྱལ་བ་གང་ཡིན་པ་དེ་དག་ཏུ་ནི་<«གཡུང་»«ལི་»«པེ་»«སྣར་»«ཅོ་»«ཞོལ་»–ནི།>དྲོ་བར་གྱུར་ཏེ་
msgctxt "9c8d6f14b739426a85adb3df0f5ade60"
msgid "གྲང་བ␣འི་ སེམས་ཅན་ དམྱལ་བ་ གང་ཡིན་པ་ དེ་དག་ ཏུ་ ནི་ དྲོ་བ␣ར་ གྱུར་ ཏེ་"
msgstr "Ils réchauffent les être des enfers froids et"

msgctxt "c68af7898748432d8db0da500f3cf49b"
msgid "འབབ་བོ ། །"
msgstr "ils les touchent (lit. tombent)."

# དེས་ན་<«གཡུང་»«མི་»«པེ་»«སྣར་»«ཅོ་»«ཞོལ་»དེ་ནས།>སེམས་ཅན་དེ་དག་གི་<«གཡུང་»«ལི་»«པེ་»«སྣར་»«ཅོ་»གིས།>གནོད་པའི་ཁྱད་པར་རྣམས་རྩེ་ཞིལ་<«སྣར་»གཞིལ།>བར་གྱུར་ཏོ།
# །
msgctxt "1469c7d66b2d4169aa81f679c6991b0a"
msgid ""
"དེས་ན་ སེམས་ཅན་ དེ་དག་ གི་ གནོད་པ␣འི་ ཁྱད་པར་ རྣམས་ རྩེ་ ཞིལ་ བར་ གྱུར་ ཏོ །"
" །"
msgstr "Ensuite, les souffrances particulières de ces êtres sont amenuisées."

msgctxt "68a08fcb28c74f16adb1f220a792189c"
msgid "དེ་ནས་ དེ་དག་ འདི་ སྙམ་ དུ་ སེམས་ ཏེ །"
msgstr "Ensuite, ils pensent ceci : «"

msgctxt "bfb67c8c5fa34a5d991bf8d8c5fc2056"
msgid "ཀྱེ་"
msgstr "Hé !"

msgctxt "ca1a123e022045538a4748a9354fecf4"
msgid "ཅི་"
msgstr "Qu'[en est-il] ?"

msgctxt "e329714d46174829afec4e058bc28239"
msgid "བདག་ཅག་ འདི་ ནས་ ཤི་འཕོས་ སམ་"
msgstr "Sommes-nous morts et partis de cet endroit ?"

msgctxt "1568aff1da9545398f8aef3a282e9dc0"
msgid "འོན་ཏེ་ གཞན་ དུ་ སྐྱེས་ སམ་ སྙམ་ མོ ། །"
msgstr "Ou bien sommes-nous nés ailleurs ? »"

msgctxt "242b21eb508d40a08b1e31084bd0d6cc"
msgid ""
"དེ་ནས་ དེ་དག་ དད་པ་ བསྐྱེད་པ␣འི་ ཕྱིར་ བཅོམ་ལྡན་འདས་ སྤྲུལ་པ་ འགྱེད་པ␣ར་ "
"མཛད་ དེ །"
msgstr ""
"Ensuite, pour développer leur dévotion, le Bienheureux émet une émanation et"

msgctxt "a839468019614e269542ced8c43e9429"
msgid "དེ་དག་ གིས་ སྤྲུལ་པ་ མཐོང་ ནས་"
msgstr "ils voient cette émanation et"

msgctxt "491fd392b02c4c5195f5fa9eca38c042"
msgid "འདི་ སྙམ་ དུ་ སེམས་ ཏེ །"
msgstr "ils pensent ceci : «"

# ཀྱེ་མ་<གཡུང་»«ལི་»«པེ་»«སྣར་»«ཅོ་»«ཞོལ་»–མ།>
msgctxt "a3dbbb8ca44147418430562dbed2a427"
msgid "ཀྱེ་མ་"
msgstr "Quelle merveille !"

msgctxt "7a1da618bc724a6383516a19c5dd6405"
msgid "ཅི་"
msgstr "Qu'[en est-il] ?"

msgctxt "c01fab1878524de4b99e589846934450"
msgid "བདག་ཅག་ ནི་ འདི་ ནས་ ཀྱང་ ཤི་འཕོས་ པར་མ་ གྱུར་ ལ་"
msgstr "Nous ne sommes pas morts ni partis de cet endroit. De plus,"

# གཞན་དུ་ཡང་<«གཡུང་»«པེ་»–ཡང་།>མ་སྐྱེས་མོད་ཀྱི<«གཡུང་»«པེ་»ཀྱིས།>།
msgctxt "ba0b5794072c4f2182447af3d4de117b"
msgid "གཞན་ དུ་ ཡང་ མ་ སྐྱེས་ མོད་ ཀྱི །"
msgstr "nous ne sommes pas nés ailleurs, mais"

# འོན་ཀྱང་སྔོན་མ་བྱུང་བའི་སེམས་ཅན་སྣང་བ་འདིའི་མཐུས།
# བདག་ཅག་གི་<«གཡུང་»«པེ་»གིས།>གནོད་པའི་ཁྱད་པར་རྣམས་རྩེ་ཞིལ་བ
# ར་གྱུར་ཏོ་<«གཡུང་»རོ།>སྙམ་མོ། །
msgctxt "cf5779e232a2475cbf42da6e15200218"
msgid ""
"འོན་ཀྱང་ སྔོན་མ་ བྱུང་བ␣འི་ སེམས་ཅན་ སྣང་བ་ འདི␣འི་ མཐུ␣ས །  བདག་ཅག་ གི་ "
"གནོད་པ␣འི་ ཁྱད་པར་ རྣམས་ རྩེ་ ཞིལ་ བ ར་ གྱུར་ ཏོ་ སྙམ་ མོ ། །"
msgstr ""
"pourtant, cet être que nous voyons et qui n'était pas apparu avant, c'est "
"grâce à lui que les particularités de nos maux sont amenuisées. »"

# དེ་ནས་དེ་དག་སྤྲུལ་པ་ལ་སེམས་མངོན་པར་<«ཞོལ་»པ།>དགའ་བར་གྱུར་ནས་
msgctxt "ff2aaf2f04e14490be1ee4fc31ba18fd"
msgid "དེ་ནས་ དེ་དག་ སྤྲུལ་པ་ ལ་ སེམས་ མངོན་པར་ དགའ་བ␣ར་ གྱུར་ ནས་"
msgstr "Ensuite, ils sont emplis d'une joie entière pour cette émanation et"

# སེམས་ཅན་དམྱལ་བ་<«ཅོ་»བར།>མྱོང་བར་འགྱུར་བའི་ལས་དེ་ཟད་དེ་
msgctxt "9a96ecf8e42a45f7b969f1e5b557121a"
msgid "སེམས་ཅན་ དམྱལ་བ་ མྱོང་བ␣ར་ འགྱུར་བ␣འི་ ལ␣ས་ དེ་ ཟད་ དེ་"
msgstr ""
"ils épuisent les actions qui leur font faire l'expérience des êtres des "
"enfers et"

# ལྷ་དང་མི་རྣམས་ཀྱི་ནང་དུ་གང་དུ་བདེན་པ་རྣམས་ཀྱི་སྣོད་<«གཡུང་»གནོད།>དུ་རུང་བར་འགྱུར་བ་དེར་ཉིང་མཚམས་འཛིན་པར་འགྱུར་རོ།
# །
msgctxt "83bff49e597a4baf9b4f6143dddb2561"
msgid ""
"ལྷ་ དང་ མི་ རྣམས་ ཀྱི་ ནང་ དུ་ གང་ དུ་ བདེན་པ་ རྣམས་ ཀྱི་ སྣོད་ དུ་ རུང་བ␣ར་"
" འགྱུར་བ་ དེ␣ར་ ཉིང་མཚམས་ འཛིན་པ␣ར་ འགྱུར་ རོ ། །"
msgstr ""
"ils reprennent naissance chez les dieux et les hommes où ils pourront "
"appréhender (lit. seront des réceptacles pour) les vérités."

# སྟེང་དུ་འགྲོ་བ་གང་ཡིན་པ་དེ་དག་ནི་རྒྱལ་ཆེན་བཞིའི་རིས་རྣམས་དང་།
# སུམ་ཅུ་<«གཡུང་»«ལི་»«པེ་»བཅུ།>རྩ་གསུམ་པ་རྣམས་དང་། འཐབ་བྲལ་རྣམས་དང་།
# དགའ་ལྡན་རྣམས་དང་། འཕྲུལ་དགའ་རྣམས་དང་། གཞན་འཕྲུལ་དབང་བྱེད་རྣམས་དང་།
# ཚངས་རིས་རྣམས་དང་། ཚངས་པའི་མདུན་ན་འདོན་རྣམས་དང་། ཚངས་པ་ཆེན་པོ་རྣམས་དང་།
# འོད་ཆུང་རྣམས་དང་། ཚད་མེད་འོད་རྣམས་དང་། འོད་གསལ་<«གཡུང་»བསད།>རྣམས་དང་།
# དགེ་ཆུང་རྣམས་དང་། ཚད་མེད་དགེ་རྣམས་དང་། དགེ་རྒྱས་རྣམས་དང་།
# སྤྲིན་<«གཡུང་»སྡིན།>མེད་རྣམས་དང་། བསོད་ནམས་སྐྱེས་རྣམས་དང་།
# འབྲས་བུ་ཆེ་<«ཅོ་»+བ།>རྣམས་དང་། མི་ཆེ་བ་<«གཡུང་»«པེ་»–བ།>རྣམས་དང་།
# མི་གདུང་བ་རྣམས་དང་། གྱ་ནོམ་སྣང་རྣམས་དང་། ཤིན་ཏུ་མཐོང་རྣམས་དང་།[55a]
# འོག་མིན་གྱི་ལྷ་རྣམས་ཀྱི་བར་དུ་སོང་ནས་
msgctxt "f5c12410f0a0441a9d4fa11476fb132c"
msgid ""
"སྟེང་ དུ་ འགྲོ་བ་ གང་ཡིན་པ་ དེ་དག་ ནི་ རྒྱལ་ཆེན་ བཞི␣འི་ རིས་ རྣམས་ དང་ །  "
"སུམ་ཅུ་རྩ་གསུམ་པ་ རྣམས་ དང་ །  འཐབ་བྲལ་ རྣམས་ དང་ །  དགའ་ལྡན་ རྣམས་ དང་ །  "
"འཕྲུལ་དགའ་ རྣམས་ དང་ །  གཞན་འཕྲུལ་ དབང་ བྱེད་ རྣམས་ དང་ །  ཚངས་རིས་ རྣམས་ "
"དང་ །  ཚངས་པ␣འི་ མདུན་ན་འདོན་ རྣམས་ དང་ །  ཚངས་པ་ ཆེན་པོ་ རྣམས་ དང་ །  "
"འོད་ཆུང་ རྣམས་ དང་ །  ཚད་ མེད་ འོད་ རྣམས་ དང་ །  འོད་གསལ་ རྣམས་ དང་ །  "
"དགེ་ཆུང་ རྣམས་ དང་ །  ཚད་ མེད་ དགེ་ རྣམས་ དང་ །  དགེ་རྒྱས་ རྣམས་ དང་ །  "
"སྤྲིན་ མེད་ རྣམས་ དང་ །  བསོད་ནམས་ སྐྱེས་ རྣམས་ དང་ །  འབྲས་བུ་ ཆེ་ རྣམས་ "
"དང་ །  མི་ཆེ་བ་ རྣམས་ དང་ །  མི་གདུང་བ་ རྣམས་ དང་ །  གྱ་ནོམ་ སྣང་ རྣམས་ དང་ "
"།  ཤིན་ཏུ་ མཐོང་ རྣམས་ དང་ །  འོག་མིན་ གྱི་ ལྷ་ རྣམས་ ཀྱི་ བར་ དུ་ སོང་ ནས་"
msgstr ""
"Ceux qui vont vers le haut vont chez les dieux des Quatre Grands Rois, ceux "
"du monde des Trente-Trois, ceux de Sans-Affrontement, ceux de Tuṣita, ceux "
"de Délices-des-Productions, ceux de Appropriation-des-Productions-d'Autrui, "
"ceux du monde de Brahmā, ceux des Prêtres-de-Brahmā, ceux de Grand-Brahmā, "
"ceux de Lueur, ceux de Lumière-Infinie, ceux de Lumière-Claire, ceux de "
"Petite-Vertu, ceux de Lumière-Infinie, ceux de Vertu-Étendue, ceux de Sans-"
"Nuages, ceux de Naissance-des-Vertus, ceux de Grands-Fruits, ceux de Sans-"
"Grandeur, ceux de Sans-Affliction, ceux de Vision-Excellente, ceux de "
"Vision-Inouïe, et jusqu'à chez ceux de Culminant et"

# མི་རྟག་པ་<«ཅོ་»+རྣམས།>དང་། སྡུག་བསྔལ་བ་དང་། སྟོང་པ་དང་།
# བདག་མེད་པའི་སྒྲ་སྒྲོགས་སོ<«ཞོལ་»སྒྲོག་གོ།>། །
msgctxt "d687fff3ef134991b44f5066364f00d1"
msgid ""
"མི་རྟག་པ་ དང་ །  སྡུག་བསྔལ་བ་ དང་ །  སྟོང་པ་ དང་ །  བདག་མེད་པ␣འི་ སྒྲ་ "
"སྒྲོགས་ སོ ། །"
msgstr ""
"font résonner les sons de l'impermanence, de la douleur, de l'absence "
"d'existence et de l'absence de moi."

# ཚིགས་སུ་བཅད་པ་གཉིས་ཀྱང་རྗོད་<«སྣར་»བརྗོད།>པར་བྱེད་དེ།
msgctxt "d242f6f09b424bee96a2201f670c7877"
msgid "ཚིགས་ སུ་ བཅད་པ་ གཉིས་ ཀྱང་ རྗོད་པ␣ར་ བྱེད་ དེ །"
msgstr "Deux versets sont aussi proclamés :"

msgctxt "e49952e57ba4411394195d385f72cb0e"
msgid "ངེས་པར་ འབྱུང་བ་ བརྩམ་པ␣ར་ གྱིས ། །"
msgstr "Faites les efforts pour vous retirer du monde."

# སངས་རྒྱས་བསྟན་ལ་སྦྱོར་བར་<«གཡུང་»–བར།>གྱིས། །
msgctxt "1d77663067a14f01b2b52e2301214db5"
msgid "སངས་རྒྱས་ བསྟན་ ལ་ སྦྱོར་བ␣ར་ གྱིས ། །"
msgstr "Appliquez l'enseignement du Bouddha."

msgctxt "4e5c2c9407f54935a13093bc8c0d7828"
msgid ""
"འདམ་བུ␣འི་ ཁྱིམ་ ལ་ གླང་ཆེན་ བཞིན ། །  འཆི་བདག་ སྡེ་ ནི་ གཞོམ་པ␣ར་ གྱིས ། །"
msgstr ""
"Comme un éléphant dans une maison en argile, détruisez les armées (lit. "
"groupes) du seigneur de la mort."

msgctxt "0df3c026982e44ccb95e24d7b514f24b"
msgid ""
"གང་ཞིག་ ཆོས་ འདུལ་ འདི་ཉིད་ ལ ། །  བག་ ཡོད་ སྤྱོད་པ␣ར་ བྱེད་པ་ ནི ། །  "
"སྐྱེ་བ␣འི་ འཁོར་བ་ རབ་ སྤངས་ ནས ། །"
msgstr ""
"Celui qui pratique (lit. se conduit) avec soin ce Dharma du Vinaya, "
"abandonnera la roue des naissances et"

msgctxt "35c9c5f8956c463f8964cd23cc9b62d6"
msgid "སྡུག་བསྔལ་ ཟད་པར་ བྱེད་པ␣ར་ འགྱུར ། །  ཞེས་"
msgstr "épuisera les souffrances."

# རྗོད་<«སྣར་»བརྗོད།>པར་བྱེད་དོ། །
msgctxt "959cd08131ba48cc9dc0b16d1ceacd70"
msgid "རྗོད་པ␣ར་ བྱེད་ དོ ། །"
msgstr "fut-il proclamé."

msgctxt "73b268501a9844db948d24f51f686764"
msgid ""
"དེ་ནས་ འོད་ཟེར་ དེ་དག་ གིས་ སྟོང་གསུམ་ གྱི་ སྟོང་ ཆེན་པོ␣འི་ འཇིག་རྟེན་ གྱི་"
" ཁམས་ བསྐོར་ ནས་"
msgstr ""
"Donc, ces rayons de lumière font le tour du trichiliocosme (lit. le monde de"
" l'étendue du grand millier des trois mille) et"

msgctxt "89e3fb033c7c4a07b4a7f983912d63df"
msgid "བཅོམ་ལྡན་འདས་ ཀྱི་ ཕྱི་བཞིན་ འབྲང་ སྟེ །"
msgstr "ils reviennent vers (lit. suivent) le Bienheureux et"

msgctxt "6c000b5fc2af4fa9b34db9bf71221b90"
msgid "གལ་ཏེ་ བཅོམ་ལྡན་འདས་ ཀྱིས་ འདས་པ␣འི་ ལ␣ས་ ལུང་སྟོན་པ␣ར་ བཞེད་ ན་ ནི །"
msgstr "si le Bienheureux a l'intention de révéler des actions passées,"

msgctxt "4826b0e17ff74e80bbe432577e0574a9"
msgid "བཅོམ་ལྡན་འདས་ ཀྱི་ སྣམ་ལོགས་ སུ་ མི་སྣང་བ␣ར་ འགྱུར་ རོ ། །"
msgstr "ils disparaissent par l'arrière du Bienheureux."

msgctxt "dc9d659eda7f41c3b8c5eb9ef16a108b"
msgid "མ་འོངས་པ␣འི་ ལ␣ས་ ལུང་སྟོན་པ␣ར་ བཞེད་ ན་ ནི་"
msgstr "S'il a l'intention de révéler des actions futures,"

msgctxt "468321ca469a4219abff422a362e268e"
msgid "མདུན་ལོགས་ སུ་ མི་སྣང་བ␣ར་ འགྱུར་ རོ ། །"
msgstr "ils disparaissent par l'avant."

# སེམས་ཅན་དམྱལ་བར་སྐྱེ་བར་<«གཡུང་»བར། «ཞོལ་»བ་སྐྱེ་བར།>ལུང་སྟོན་པར་བཞེད་ན་ནི་
msgctxt "03c311f8f7e34fac8e6a6f2ac1e082de"
msgid "སེམས་ཅན་ དམྱལ་བ␣ར་ སྐྱེ་བ␣ར་ ལུང་སྟོན་པ␣ར་ བཞེད་ ན་ ནི་"
msgstr "S'il a l'intention de révéler une naissance dans les enfers,"

msgctxt "14d045d5b85d4658860bce2ab1cb2acb"
msgid "ཞབས་ ཀྱི་ མཐིལ་ དུ་ མི་སྣང་བ␣ར་ འགྱུར་ རོ ། །"
