msgstr ""

msgctxt "line 10, A001-1 sentences.txt"
msgid ""
"འདས་པའི་དུས་སུ་ཡང་ཀཽཎྜི་<«ལི་»«ཁུ་»ཀཽཎྚི།>ནྱ་ལ་སོགས་པ།<«ཞོལ་»+དང་།> "
"ལྷ་བརྒྱད་ཁྲི་པོ་འདི་དག་ཉིད་ངས་ཇི་ལྟར་རང་གི་ཤ་དང་ཁྲག་གིས་ཚིམ་པར་བྱས་ནས། "
msgstr ""

msgctxt "line 11, A001-1 sentences.txt"
msgid "སྲོག་ཕངས་པ་བྱིན་པ་དེ་ཉོན་ཅིག །"
msgstr ""

msgctxt "line 12, A001-1 sentences.txt"
msgid ""
