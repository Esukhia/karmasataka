msgctxt "line 3, A001-1 sentences.txt"
msgid "གཏན་ཚིགས་རྒྱས་པར་ནི་དབྱིག་དགའི་རྟོགས་པ་བརྗོད་པ་ལས་འབྱུང་བ་བཞིན་ཏེ། "
msgstr ""

msgctxt "line 4, A001-1 sentences.txt"
msgid ""
"གང་གི་ཚེ་ཚེ་དང་ལྡན་པ་ཀུན་ཤེས་ཀཽཎྜི་<«ལི་»«ཁུ་»ཀཽཎྚི།>ནྱས་བདེན་པ་རྣམས་མཐོང་བར་གྱུར་པ་དེའི་ཚེ།"
" ལྷ་བརྒྱད་ཁྲིས་ཀྱང་བདེན་པ་མཐོང་བར་གྱུར་ཏོ། །"
msgstr ""

msgctxt "line 5, A001-1 sentences.txt"
msgid "དེ་ནས་དགེ་སློང་རྣམས་ཀྱིས་སངས་རྒྱས་བཅོམ་ལྡན་འདས་ལ་གསོལ་པ། "
msgstr ""

msgctxt "line 6, A001-1 sentences.txt"
msgid ""
"བཙུན་པ་བཅོམ་ལྡན་འདས་ཀྱིས་ཀཽཎྜི་<«ལི་»«ཁུ་»ཀཽཎྚི།>ནྱ་ལ་སོགས་པ་<«ཞོལ་»+དང༌།>ལྷ་བརྒྱད་ཁྲི་དམ་པའི་ཆོས་ཀྱི་བཅུད་ཀྱིས་ཚིམ་པར་མཛད་ནས།"
" "
msgstr ""

msgctxt "line 7, A001-1 sentences.txt"
msgid ""
"གཡུང་དྲུང་གི་མཐར་ཐུག་པ་གྲུབ་པ་དང་བདེ་བའི་མྱ་ངན་ལས་འདས་པ་ལ་:བཀོད་པ་<«གཡུང་»བཀོས།"
" «པེ་»-དཀོད་པ།>ལ་གཟིགས། "
msgstr ""

msgctxt "line 8, A001-1 sentences.txt"
msgid "བཅོམ་ལྡན་འདས་ཀྱིས་བཀའ་སྩལ་པ། "
msgstr ""

msgctxt "line 9, A001-1 sentences.txt"
