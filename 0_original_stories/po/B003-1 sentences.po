msgctxt "line 10, B003-1 sentences.txt"
msgid "སྐྱེད་<«གཡུང་»བསྐྱེད།>མོས་ཚལ་གྱི་ནང་དུ་ཆས་སོ། །"
msgstr ""

msgctxt "line 11, B003-1 sentences.txt"
msgid ""
"བཅོམ་ལྡན་འདས་ཀྱང་དེ་དག་ལ་ཐུགས་བརྩེ་བའི་ཕྱིར་སྔ་དྲོ་ཤམ་ཐབས་དང་ཆོས་གོས་གསོལ་ཏེ།"
