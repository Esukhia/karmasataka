msgctxt "line 1, B002.txt"
msgid "﻿པད་མ་གཉིས་པ་ནི། "
msgstr ""

msgctxt "line 2, B002.txt"
msgid "གླེང་གཞི་མཉན་དུ་ཡོད་པ་ན་བཞུགས་ཏེ། "
msgstr ""

# དེའི་ཚེ་མཉན་དུ་ཡོད་པ་དེ་ཉིད་ན། ཀུན་དགའ་ར་:བ་སྲུང་བ་<«ཅོ་»བ་སྲུང་མ།
# «ཞོལ་»བའི་སྲུང་མ།>ཞིག་འདུག་པ།
msgctxt "line 3, B002.txt"
msgid "དེའི་ཚེ་མཉན་དུ་ཡོད་པ་དེ་ཉིད་ན། ཀུན་དགའ་ར་བ་སྲུང་བ་ཞིག་འདུག་པ། "
msgstr ""

# དེའི་<«གཡུང་»«པེ་»«སྣར་»«ཞོལ་》པའི།>མཚོའི་ནང་ནས་<«ལི་»«ཅོ་»ན།>དུས་མ་ཡིན་པར་<«གཡུང་»«པེ་»པའི་བར།>པད་མ་ཞིག་སྐྱེས་ཏེ།
msgctxt "line 4, B002.txt"
msgid "དེའི་མཚོའི་ནང་ནས་དུས་མ་ཡིན་པར་པད་མ་ཞིག་སྐྱེས་ཏེ། "
msgstr ""

msgctxt "line 5, B002.txt"
msgid "དེའི་བློ་སྐྱེས་པ། "
msgstr ""

# པད་མ་འདི་བདག་<«གཡུང་»«པེ་»དག
# །>གིས་རྒྱལ་པོ་གསལ་རྒྱལ་ལ་དབུལ་<«ལི་»«ཅོ་»འབུལ།>བར་བྱའོ། །
msgctxt "line 6, B002.txt"
msgid "པད་མ་འདི་བདག་གིས་རྒྱལ་པོ་གསལ་རྒྱལ་ལ་དབུལ་བར་བྱའོ། །"
msgstr ""

# འོན་ཏང་<«གཡུང་»«པེ་»ཀྱང་།>བདག་གི་ལོངས་སྤྱོད་ཆེན་པོ་དག་:གི་འབྱུང་ཁུངས་སུ་<«གཡུང་»«པེ་»གིས་འབྱུང་ཁུང་དུ།
# «ལི་»«ཅོ་»གི༑ 《སྣར་》གི་འབྱུང་ཀུང་དུ།>འགྱུར་རོ་སྙམ་ནས།
msgctxt "line 7, B002.txt"
msgid "འོན་ཏང་བདག་གི་ལོངས་སྤྱོད་ཆེན་པོ་དག་གི་འབྱུང་ཁུངས་སུ་འགྱུར་རོ་སྙམ་ནས། "
msgstr ""

# དེ་ཁྱེར་ཏེ་<«ཁུ་»ཕྱིར།>
msgctxt "line 8, B002.txt"
msgid "དེ་ཁྱེར་ཏེ་"
msgstr ""

msgctxt "line 9, B002.txt"
msgid "རྒྱལ་པོའི་ཕོ་བྲང་དུ་ཆས་ཏེ་"
msgstr ""

msgctxt "line 10, B002.txt"
msgid "སོང་ངོ་། །"
msgstr ""

# སངས་རྒྱས་བཅོམ་ལྡན་འདས་བགྲོད་པ་གཅིག་<«ལི་»ཅིག>པུའི་ལམ་སྟོན་པ།
# རྣམ་པ་གཉིས་ཀྱི་ཤེས་བྱ་དང་། ཡེ་ཤེས་ལ་མངའ་བརྙེས་པ།
# མ་འདྲེས་པའི་དྲན་པ་ཉེ་བར་གཞག་པ་<«གཡུང་«པེ་»«སྣར་》བཞག>གསུམ་ལ་མཁས་པ<«གཡུང་»«པེ་»-པ།>།
# མི་འཇིགས་པ་བཞིས་མི་འཇིགས་པ<«གཡུང་»«པེ་»པར།>།
# འགྲོ་བ་ལྔར་འགྲོ་བ་ལས་རྣམ་པར་གྲོལ་བ། སྐྱེ་མཆེད་དྲུག་ལ་མཁས་པ།
# བྱང་ཆུབ་ཀྱི་ཡན་ལག་བདུན་ལ་སྤྱོད་ཡུལ་བ། རྣམ་པར་ཐར་པ་བརྒྱད་ལ་བསམ་གཏན་པ།
# མཐར་གྱིས་གནས་པའི་སྙོམས་པར་འཇུག་པ་དགུ་ལ་སྙོམས་པར་འཇུག་པ།
# སྟོབས་བཅུའི་སྟོབས་དང་ལྡན་པ།
# ཡང་དག་པའི་<«སྣར་»«ཞོལ་»པར།>སེང་གེའི་སྒྲ་ཆེན་པོ་སྒྲོགས་<«ཞོལ་»སྒྲོག>པ་རྣམས་ནི།
# ཆོས་ཉིད་ཀྱིས་ཉིན་དུས་གསུམ་མཚན་དུས་གསུམ་སྟེ།
# ཉིན་མཚན་དུས་དྲུག་ཏུ་སངས་རྒྱས་ཀྱི་སྤྱན་གྱིས་འཇིག་རྟེན་ལ་གཟིགས་ཏེ།
msgctxt "line 11, B002.txt"
msgid ""
"སངས་རྒྱས་བཅོམ་ལྡན་འདས་བགྲོད་པ་གཅིག་པུའི་ལམ་སྟོན་པ། "
"རྣམ་པ་གཉིས་ཀྱི་ཤེས་བྱ་དང་། ཡེ་ཤེས་ལ་མངའ་བརྙེས་པ། "
"མ་འདྲེས་པའི་དྲན་པ་ཉེ་བར་གཞག་པ་གསུམ་ལ་མཁས་པ། མི་འཇིགས་པ་བཞིས་མི་འཇིགས་པ། "
"འགྲོ་བ་ལྔར་འགྲོ་བ་ལས་རྣམ་པར་གྲོལ་བ། སྐྱེ་མཆེད་དྲུག་ལ་མཁས་པ། "
"བྱང་ཆུབ་ཀྱི་ཡན་ལག་བདུན་ལ་སྤྱོད་ཡུལ་བ། རྣམ་པར་ཐར་པ་བརྒྱད་ལ་བསམ་གཏན་པ། "
"མཐར་གྱིས་གནས་པའི་སྙོམས་པར་འཇུག་པ་དགུ་ལ་སྙོམས་པར་འཇུག་པ། "
"སྟོབས་བཅུའི་སྟོབས་དང་ལྡན་པ། ཡང་དག་པའི་སེང་གེའི་སྒྲ་ཆེན་པོ་སྒྲོགས་པ་རྣམས་ནི། "
"ཆོས་ཉིད་ཀྱིས་ཉིན་དུས་གསུམ་མཚན་དུས་གསུམ་སྟེ། "
"ཉིན་མཚན་དུས་དྲུག་ཏུ་སངས་རྒྱས་ཀྱི་སྤྱན་གྱིས་འཇིག་རྟེན་ལ་གཟིགས་ཏེ། "
msgstr ""

# སུ་ནི་རྒུད<«གཡུང་»གུད། «ལི་»དགུད།>། 
msgctxt "line 12, B002.txt"
msgid "སུ་ནི་རྒུད། "
msgstr ""

msgctxt "line 13, B002.txt"
msgid "སུ་ནི་དར། "
msgstr ""

# སུ་ནི་ཕོངས་<«གཡུང་»«པེ་»«ཅོ་»འཕོངས། «ལི་»འཕོང་།>ཤིང་འདུག །
msgctxt "line 14, B002.txt"
msgid "སུ་ནི་ཕོངས་ཤིང་འདུག །"
msgstr ""

# སུ་ནི་ཉམ་ང་བར་གྱུར<«ལི་»«ཅོ་»འགྱུར། «སྣར་»«ཞོལ་»འདུག>། 
msgctxt "line 15, B002.txt"
msgid "སུ་ནི་ཉམ་ང་བར་གྱུར། "
msgstr ""

msgctxt "line 16, B002.txt"
msgid "སུ་ནི་གནོད་པ་དང་ལྡན། "
msgstr ""

# སུ་ནི་ཕོངས་<«གཡུང་»«པེ་»«ཅོ་»འཕོངས༑
# «ལི་»འཕོང་།>པ་དང་ཉམ་<«སྣར་»ཉམས།>ང་བ་དང་གནོད་པ་དང་ལྡན།
msgctxt "line 17, B002.txt"
msgid "སུ་ནི་ཕོངས་པ་དང་ཉམ་ང་བ་དང་གནོད་པ་དང་ལྡན། "
msgstr ""

msgctxt "line 18, B002.txt"
msgid "སུ་ནི་ངན་སོང་དུ་གཞོལ། "
msgstr ""

# སུ་ནི་ངན་སོང་དུ་འབབ<«ལི་»འབོབ།>། 
msgctxt "line 19, B002.txt"
msgid "སུ་ནི་ངན་སོང་དུ་འབབ། "
msgstr ""

# :སུ་ནི་ངན་སོང་དུ་བབ<«ལི་»སུ་ནི་ངན་སང་དུ་བབ། «པེ་»སུ་ནི་ངན་སོང་རུ་ང་།
# «སྣར་»-སུ་ནི་ངན་སོང་དུ་བབ།>།
msgctxt "line 20, B002.txt"
msgid "སུ་ནི་ངན་སོང་དུ་བབ། "
msgstr ""

# སུ་ནི་ངས་<«ལི་»«ཅོ་»-ངས།>ངན་སོང་ནས་ཕྱུང་སྟེ་
msgctxt "line 21, B002.txt"
msgid "སུ་ནི་ངས་ངན་སོང་ནས་ཕྱུང་སྟེ་"
msgstr ""

# མཐོ་རིས་དང་ཐར་:པ་དང་<«ལི་»«ཅོ་»པའི།>འབྲས་བུ་ལ་གཞག་<«ལི་»བཞག>པར་བྱ། 
msgctxt "line 22, B002.txt"
msgid "མཐོ་རིས་དང་ཐར་པ་དང་འབྲས་བུ་ལ་གཞག་པར་བྱ། "
msgstr ""

# སུ་ནི་ཉེས་པར་སྤྱོད་པའི་འདམ་དུ་བྱིང་བ་ལས་ལག་ནས་:དྲང་བར་<«གཡུང་»«པེ་»«ཁུ་»དྲངས་པར།>བྱ།
msgctxt "line 23, B002.txt"
msgid "སུ་ནི་ཉེས་པར་སྤྱོད་པའི་འདམ་དུ་བྱིང་བ་ལས་ལག་ནས་དྲང་བར་བྱ། "
msgstr ""

# སུ་ནི་:འཕགས་པའི་ནོར་བདུན་མེད་པ་ལས་<«སྣར་»«ཞོལ་»-འཕགས་པའི་ནོར་བདུན་མེད་པ་ལས།>
msgctxt "line 24, B002.txt"
msgid "སུ་ནི་འཕགས་པའི་ནོར་བདུན་མེད་པ་ལས་"
msgstr ""

